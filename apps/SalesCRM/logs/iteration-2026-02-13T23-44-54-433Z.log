 }\n    31→    return Response.json(rows[0])\n    32→  }\n    33→\n    34→  // GET /clients\n    35→  if (req.method === 'GET') {\n    36→    const page = parseInt(url.searchParams.get('page') ?? '1')\n    37→    const pageSize = 50\n    38→    const offset = (page - 1) * pageSize\n    39→    const search = url.searchParams.get('search') ?? ''\n    40→    const status = url.searchParams.get('status') ?? ''\n    41→    const tag = url.searchParams.get('tag') ?? ''\n    42→    const source = url.searchParams.get('source') ?? ''\n    43→    const sort = url.searchParams.get('sort') ?? 'updated_at'\n    44→\n    45→    let whereClause = 'WHERE 1=1'\n    46→    const params: unknown[] = []\n    47→    let paramIdx = 1\n    48→\n    49→    if (search) {\n    50→      whereClause += ` AND (c.name ILIKE $${paramIdx} OR EXISTS (SELECT 1 FROM unnest(c.tags) AS t WHERE t ILIKE $${paramIdx}) OR EXISTS (SELECT 1 FROM client_individuals ci JOIN individuals i ON ci.individual_id = i.id WHERE ci.client_id = c.id AND i.name ILIKE $${paramIdx}))`\n    51→      params.push(`%${search}%`)\n    52→      paramIdx++\n    53→    }\n    54→    if (status) {\n    55→      whereClause += ` AND c.status = $${paramIdx}`\n    56→      params.push(status)\n    57→      paramIdx++\n    58→    }\n    59→    if (tag) {\n    60→      whereClause += ` AND $${paramIdx} = ANY(c.tags)`\n    61→      params.push(tag)\n    62→      paramIdx++\n    63→    }\n    64→    if (source) {\n    65→      whereClause += ` AND c.source_type = $${paramIdx}`\n    66→      params.push(source)\n    67→      paramIdx++\n    68→    }\n    69→\n    70→    let orderBy = 'c.updated_at DESC'\n    71→    if (sort === 'name_asc') orderBy = 'c.name ASC'\n    72→    else if (sort === 'name_desc') orderBy = 'c.name DESC'\n    73→    else if (sort === 'status') orderBy = 'c.status ASC'\n    74→\n    75→    const countQuery = `SELECT COUNT(*) as count FROM clients c ${whereClause}`\n    76→    const dataQuery = `\n    77→      SELECT