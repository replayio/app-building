lex flex-col gap-1\">\n    41→          {tasks.map((task) => (\n    42→            <div\n    43→              key={task.id}\n    44→              className=\"flex items-center gap-3 px-3 py-2.5 rounded-[4px] hover:bg-hover transition-colors duration-100 cursor-pointer group\"\n    45→            >\n    46→              <button\n    47→                onClick={(e) => { e.stopPropagation(); onToggleTask(task.id, true) }}\n    48→                className=\"flex-shrink-0 text-text-muted hover:text-status-active transition-colors duration-100\"\n    49→              >\n    50→                {task.completed ? (\n    51→                  <CheckSquare size={16} strokeWidth={1.75} />\n    52→                ) : (\n    53→                  <Square size={16} strokeWidth={1.75} />\n    54→                )}\n    55→              </button>\n    56→              <div\n    57→                className=\"flex-1 min-w-0\"\n    58→                onClick={() => {\n    59→                  if (task.deal_id) {\n    60→                    navigate(`/deals/${task.deal_id}`)\n    61→                  }\n    62→                }}\n    63→              >\n    64→                <span className=\"text-[13px] text-text-primary\">{task.title}</span>\n    65→                {task.due_date && (\n    66→                  <span className=\"text-[12px] text-text-muted ml-2\">\n    67→                    · Due: {formatDueDate(task.due_date)}\n    68→                  </span>\n    69→                )}\n    70→                {task.deal_name && (\n    71→                  <span className=\"text-[12px] text-text-muted ml-1\">\n    72→                    , Deal: &apos;{task.deal_name}&apos;\n    73→                  </span>\n    74→                )}\n    75→              </div>\n    76→            </div>\n    77→          ))}\n    78→        </div>\n    79→      )}\n    80→    </div>\n    81→  )\n    82→}\n    83→"}]},"parent_tool_use_id":"toolu_01HLtdoLDB7UTzSNyYyExRmT","session_id":"7ce698e0-1bf0-