ching clients', async ({ page }) => {\n   263→    await page.goto('/clients');\n   264→    await page.waitForLoadState('networkidle');\n   265→\n   266→    // Select Active status\n   267→    await page.getByTestId('filter-status').selectOption('active');\n   268→    await page.waitForLoadState('networkidle');\n   269→\n   270→    // All visible status badges should be \"Active\"\n   271→    const statusBadges = page.locator('[data-testid=\"client-status\"]');\n   272→    const count = await statusBadges.count();\n   273→    for (let i = 0; i < count; i++) {\n   274→      await expect(statusBadges.nth(i).locator('[data-testid=\"status-badge-active\"]')).toBeVisible();\n   275→    }\n   276→  });\n   277→\n   278→  test('CLP-FLT-03: Tags filter dropdown shows available tags', async ({ page }) => {\n   279→    await page.goto('/clients');\n   280→    await page.waitForLoadState('networkidle');\n   281→\n   282→    const tagsFilter = page.getByTestId('filter-tags');\n   283→    await expect(tagsFilter).toBeVisible();\n   284→\n   285→    // Should have \"All\" option plus available tags\n   286→    const options = tagsFilter.locator('option');\n   287→    const texts = await options.allTextContents();\n   288→    expect(texts.some(t => t.includes('All'))).toBeTruthy();\n   289→    expect(texts.length).toBeGreaterThanOrEqual(1);\n   290→  });\n   291→\n   292→  test('CLP-FLT-04: Filtering by tag shows only matching clients', async ({ page }) => {\n   293→    await page.goto('/clients');\n   294→    await page.waitForLoadState('networkidle');\n   295→\n   296→    const tagsFilter = page.getByTestId('filter-tags');\n   297→    const options = tagsFilter.locator('option');\n   298→    const allTexts = await options.allTextContents();\n   299→\n   300→    // Find a non-\"All\" tag option\n   301→    const tagOption = allTexts.find(t => !t.includes('All'));\n   302→    if (tagOption) {\n   303→      // Extract the value - the value is the tag name, option text is \"Tags: <ta