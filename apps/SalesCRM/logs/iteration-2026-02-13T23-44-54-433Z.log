101→      }\n   102→    }\n   103→    if (body.title !== undefined) {\n   104→      updates.push(`title = $${paramIdx}`)\n   105→      params.push(body.title)\n   106→      paramIdx++\n   107→    }\n   108→    if (body.description !== undefined) {\n   109→      updates.push(`description = $${paramIdx}`)\n   110→      params.push(body.description)\n   111→      paramIdx++\n   112→    }\n   113→    if (body.due_date !== undefined) {\n   114→      updates.push(`due_date = $${paramIdx}`)\n   115→      params.push(body.due_date)\n   116→      paramIdx++\n   117→    }\n   118→    if (body.priority !== undefined) {\n   119→      updates.push(`priority = $${paramIdx}`)\n   120→      params.push(body.priority)\n   121→      paramIdx++\n   122→    }\n   123→\n   124→    updates.push('updated_at = NOW()')\n   125→\n   126→    const query = `UPDATE tasks SET ${updates.join(', ')} WHERE id = $${paramIdx} RETURNING *`\n   127→    params.push(taskId)\n   128→\n   129→    const rows = await sql(query, params)\n   130→    if (rows.length === 0) {\n   131→      return new Response(JSON.stringify({ error: 'Not found' }), { status: 404 })\n   132→    }\n   133→\n   134→    // Create timeline event for completion\n   135→    if (body.completed) {\n   136→      const task = rows[0]\n   137→      await sql`\n   138→        INSERT INTO timeline_events (client_id, event_type, description, user_name, related_entity_id, related_entity_type)\n   139→        VALUES (${task.client_id}, 'task_completed', ${'Task Completed: \\'' + task.title + '\\''}, 'System', ${taskId}, 'task')\n   140→      `\n   141→    }\n   142→\n   143→    return Response.json(rows[0])\n   144→  }\n   145→\n   146→  // DELETE /client-tasks/:taskId\n   147→  if (req.method === 'DELETE' && taskId) {\n   148→    await sql`DELETE FROM tasks WHERE id = ${taskId}`\n   149→    return Response.json({ success: true })\n   150→  }\n   151→\n   152→  return new Response(JSON.stringify({ error: 'Method not allowed' }), { status: 405 })