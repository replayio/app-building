\">\n          {attachments.map((att) => (\n            <div\n              key={att.id}\n              className=\"flex items-center gap-3 px-3 py-2.5 rounded-[4px] hover:bg-hover transition-colors duration-100\"\n            >\n              <FileText size={16} strokeWidth={1.5} className=\"text-text-muted flex-shrink-0\" />\n              <div className=\"flex-1 min-w-0\">\n                <span className=\"text-[13px] text-text-primary font-medium\">{att.filename}</span>\n                <span className=\"text-[12px] text-text-muted ml-2\">{formatSize(att.size)}</span>\n              </div>\n              <div className=\"flex items-center gap-1 flex-shrink-0\">\n                <a\n                  href={att.url}\n                  download={att.filename}\n                  className=\"text-[12px] text-accent hover:underline\"\n                >\n                  Download\n                </a>\n                <button\n                  onClick={() => onDelete(att.id)}\n                  className=\"text-[12px] text-status-churned hover:underline ml-2\"\n                >\n                  Delete\n                </button>\n              </div>\n            </div>\n          ))}\n        </div>\n      )}\n    </div>\n  )\n}\n","structuredPatch":[{"oldStart":1,"oldLines":4,"newStart":1,"newLines":4,"lines":["-import { FileText, Upload, Download, Trash2 } from 'lucide-react'","+import { FileText, Upload } from 'lucide-react'"," import type { Attachment } from '../../types'"," "," interface DealAttachmentsSectionProps {"]}],"userModified":false,"replaceAll":false}}
