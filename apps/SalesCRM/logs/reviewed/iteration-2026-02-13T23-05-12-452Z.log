[2026-02-13T23:06:16.547Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_018nv519jgkkfe3dH2NyPUqw","type":"tool_result","content":"     1→import { neon } from '@neondatabase/serverless'\n     2→\n     3→function getDb() {\n     4→  const url = process.env.DATABASE_URL\n     5→  if (!url) throw new Error('DATABASE_URL not set')\n     6→  return neon(url)\n     7→}\n     8→\n     9→interface IndividualRow {\n    10→  id: string\n    11→  name: string\n    12→  title: string | null\n    13→  email: string | null\n    14→  phone: string | null\n    15→  location: string | null\n    16→  created_at: string\n    17→  updated_at: string\n    18→}\n    19→\n    20→interface ClientAssociationRow {\n    21→  client_id: string\n    22→  client_name: string\n    23→  client_status: string\n    24→  industry: string | null\n    25→  role: string | null\n    26→}\n    27→\n    28→interface RelationshipRow {\n    29→  id: string\n    30→  related_individual_id: string\n    31→  related_individual_name: string\n    32→  relationship_type: string\n    33→  role: string | null\n    34→  company: string | null\n    35→}\n    36→\n    37→interface ContactHistoryRow {\n    38→  id: string\n    39→  individual_id: string\n    40→  date: string\n    41→  type: string\n    42→  summary: string\n    43→  team_member: string | null\n    44→  created_at: string\n    45→  updated_at: string\n    46→}\n    47→\n    48→export default async function handler(req: Request) {\n    49→  const sql = getDb()\n    50→  const url = new URL(req.url)\n    51→  const pathParts = url.pathname.split('/').filter(Boolean)\n    52→  // Path: /.netlify/functions/individuals/:individualId/:subResource/:subId\n    53→  const individualId = pathParts.length >= 3 ? pathParts[2] : null\n    54→  const subResource = pathParts.length >= 4 ? pathParts[3] : null\n    55→  const subId = pathParts.length >= 5 ? pathParts[4] : null\n    56→\n    57→  // GET /individuals/:id — fetch individual with associations, relationships, and contact history\n    58→  if (req.method === 'GET' && individualId && !subResource) {\n    59→    const rows = await sql`\n    60→      SELECT * FROM individuals WHERE id = ${individualId}\n    61→    ` as IndividualRow[]\n    62→\n    63→    if (rows.length === 0) {\n    64→      return new Response(JSON.stringify({ error: 'Individual not found' }), { status: 404 })\n    65→    }\n    66→\n    67→    const individual = rows[0]\n    68→\n    69→    // Fetch client associations\n    70→    const clientAssociations = await sql`\n    71→      SELECT ci.client_id, c.name AS client_name, c.status AS client_status, ci.industry, ci.role\n    72→      FROM client_individuals ci\n    73→      JOIN clients c ON ci.client_id = c.id\n    74→      WHERE ci.individual_id = ${individualId}\n    75→      ORDER BY c.name ASC\n    76→    ` as ClientAssociationRow[]\n    77→\n    78→    // Fetch relationships\n    79→    const relationships = await sql`\n    80→      SELECT ir.id, ir.related_individual_id, i.name AS related_individual_name,\n    81→             ir.relationship_type, ci.role, ci.industry AS company\n    82→      FROM individual_relationships ir\n    83→      JOIN individuals i ON ir.related_individual_id = i.id\n    84→      LEFT JOIN client_individuals ci ON ci.individual_id = ir.related_individual_id\n    85→        AND ci.client_id = (\n    86→          SELECT ci2.client_id FROM client_individuals ci2\n    87→          WHERE ci2.individual_id = ${individualId} LIMIT 1\n    88→        )\n    89→      WHERE ir.individual_id = ${individualId}\n    90→      ORDER BY i.name ASC\n    91→    ` as RelationshipRow[]\n    92→\n    93→    // Fetch contact history\n    94→    const contactHistory = await sql`\n    95→      SELECT * FROM contact_history\n    96→      WHERE individual_id = ${individualId}\n    97→      ORDER BY date DESC\n    98→    ` as ContactHistoryRow[]\n    99→\n   100→    return Response.json({\n   101→      ...individual,\n   102→      client_associations: clientAssociations,\n   103→      relationships,\n   104→      contact_history: contactHistory,\n   105→    })\n   106→  }\n   107→\n   108→  // PUT /individuals/:id — update individual info\n   109→  if (req.method === 'PUT' && individualId && !subResource) {\n   110→    const body = await req.json() as Partial<{\n   111→      name: string\n   112→      title: string\n   113→      email: string\n   114→      phone: string\n   115→      location: string\n   116→    }>\n   117→\n   118→    const rows = await sql`\n   119→      UPDATE individuals SET\n   120→        name = COALESCE(${body.name ?? null}, name),\n   121→        title = COALESCE(${body.title ?? null}, title),\n   122→        email = COALESCE(${body.email ?? null}, email),\n   123→        phone = COALESCE(${body.phone ?? null}, phone),\n   124→        location = COALESCE(${body.location ?? null}, location),\n   125→        updated_at = NOW()\n   126→      WHERE id = ${individualId}\n   127→      RETURNING *\n   128→    ` as IndividualRow[]\n   129→\n   130→    if (rows.length === 0) {\n   131→      return new Response(JSON.stringify({ error: 'Individual not found' }), { status: 404 })\n   132→    }\n   133→\n   134→    return Response.json(rows[0])\n   135→  }\n   136→\n   137→  // POST /individuals/:id/relationships — add a relationship\n   138→  if (req.method === 'POST' && individualId && subResource === 'relationships') {\n   139→    const body = await req.json() as {\n   140→      related_individual_id: string\n   141→      relationship_type: string\n   142→    }\n   143→\n   144→    const rows = await sql`\n   145→      INSERT INTO individual_relationships (individual_id, related_individual_id, relationship_type)\n   146→      VALUES (${individualId}, ${body.related_individual_id}, ${body.relationship_type})\n   147→      RETURNING *\n   148→    `\n   149→\n   150→    return Response.json(rows[0], { status: 201 })\n   151→  }\n   152→\n   153→  // DELETE /individuals/:id/relationships/:relId — delete a relationship\n   154→  if (req.method === 'DELETE' && individualId && subResource === 'relationships' && subId) {\n   155→    await sql`DELETE FROM individual_relationships WHERE id = ${subId} AND individual_id = ${individualId}`\n   156→    return Response.json({ success: true })\n   157→  }\n   158→\n   159→  // POST /individuals/:id/contact-history — add a contact history entry\n   160→  if (req.method === 'POST' && individualId && subResource === 'contact-history') {\n   161→    const body = await req.json() as {\n   162→      date: string\n   163→      type: string\n   164→      summary: string\n   165→      team_member: string\n   166→    }\n   167→\n   168→    const rows = await sql`\n   169→      INSERT INTO contact_history (individual_id, date, type, summary, team_member)\n   170→      VALUES (${individualId}, ${body.date}, ${body.type}, ${body.summary}, ${body.team_member || null})\n   171→      RETURNING *\n   172→    ` as ContactHistoryRow[]\n   173→\n   174→    return Response.json(rows[0], { status: 201 })\n   175→  }\n   176→\n   177→  // PUT /individuals/:id/contact-history/:entryId — update a contact history entry\n   178→  if (req.method === 'PUT' && individualId && subResource === 'contact-history' && subId) {\n   179→    const body = await req.json() as Partial<{\n   180→      date: string\n   181→      type: string\n   182→      summary: string\n   183→      team_member: string\n   184→    }>\n   185→\n   186→    const rows = await sql`\n   187→      UPDATE contact_history SET\n   188→        date = COALESCE(${body.date ?? null}, date),\n   189→        type = COALESCE(${body.type ?? null}, type),\n   190→        summary = COALESCE(${body.summary ?? null}, summary),\n   191→        team_member = COALESCE(${body.team_member ?? null}, team_member),\n   192→        updated_at = NOW()\n   193→      WHERE id = ${subId} AND individual_id = ${individualId}\n   194→      RETURNING *\n   195→    ` as ContactHistoryRow[]\n   196→\n   197→    if (rows.length === 0) {\n   198→      return new Response(JSON.stringify({ error: 'Entry not found' }), { status: 404 })\n   199→    }\n   200→\n   201→    return Response.json(rows[0])\n   202→  }\n   203→\n   204→  // DELETE /individuals/:id/contact-history/:entryId — delete a contact history entry\n   205→  if (req.method === 'DELETE' && individualId && subResource === 'contact-history' && subId) {\n   206→    await sql`DELETE FROM contact_history WHERE id = ${subId} AND individual_id = ${individualId}`\n   207→    return Response.json({ success: true })\n   208→  }\n   209→\n   210→  return new Response(JSON.stringify({ error: 'Method not allowed' }), { status: 405 })\n   211→}\n   212→"}]},"parent_tool_use_id":"toolu_01TDKajd9uE1ZMWHRvacssvs","session_id":"a47b4ab7-2fa2-4041-b9a2-ae128b707e7a","uuid":"91d37665-d48c-4e27-a2b7-2d6669e27876"}
