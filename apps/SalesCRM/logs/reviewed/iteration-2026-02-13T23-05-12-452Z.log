_URL not set')\\\\n  return neon(url)\\\\n}\\\\n\\\\ninterface IndividualRow {\\\\n  id: string\\\\n  name: string\\\\n  title: string | null\\\\n  email: string | null\\\\n  phone: string | null\\\\n  location: string | null\\\\n  created_at: string\\\\n  updated_at: string\\\\n}\\\\n\\\\ninterface ClientAssociationRow {\\\\n  client_id: string\\\\n  client_name: string\\\\n  client_status: string\\\\n  industry: string | null\\\\n  role: string | null\\\\n}\\\\n\\\\ninterface RelationshipRow {\\\\n  id: string\\\\n  related_individual_id: string\\\\n  related_individual_name: string\\\\n  relationship_type: string\\\\n  role: string | null\\\\n  company: string | null\\\\n}\\\\n\\\\ninterface ContactHistoryRow {\\\\n  id: string\\\\n  individual_id: string\\\\n  date: string\\\\n  type: string\\\\n  summary: string\\\\n  team_member: string | null\\\\n  created_at: string\\\\n  updated_at: string\\\\n}\\\\n\\\\nexport default async function handler(req: Request) {\\\\n  const sql = getDb()\\\\n  const url = new URL(req.url)\\\\n  const pathParts = url.pathname.split('/').filter(Boolean)\\\\n  // Path: /.netlify/functions/individuals/:individualId/:subResource/:subId\\\\n  const individualId = pathParts.length >= 3 ? pathParts[2] : null\\\\n  const subResource = pathParts.length >= 4 ? pathParts[3] : null\\\\n  const subId = pathParts.length >= 5 ? pathParts[4] : null\\\\n\\\\n  // GET /individuals/:id — fetch individual with associations, relationships, and contact history\\\\n  if (req.method === 'GET' && individualId && !subResource) {\\\\n    const rows = await sql`\\\\n      SELECT * FROM individuals WHERE id = ${individualId}\\\\n    ` as IndividualRow[]\\\\n\\\\n    if (rows.length === 0) {\\\\n      return new Response(JSON.stringify({ error: 'Individual not found' }), { status: 404 })\\\\n    }\\\\n\\\\n    const individual = rows[0]\\\\n\\\\n    // Fetch client associations\\\\n    const clientAssociations = await sql`\\\\n      SELECT ci.client_id, c.nam