  50→  const sql = neon(connectionUri)\n    51→\n    52→  console.log('Creating database schema...')\n    53→\n    54→  await sql`\n    55→    CREATE TABLE IF NOT EXISTS clients (\n    56→      id UUID PRIMARY KEY DEFAULT gen_random_uuid(),\n    57→      name TEXT NOT NULL,\n    58→      type TEXT NOT NULL CHECK (type IN ('organization', 'individual')),\n    59→      status TEXT NOT NULL DEFAULT 'prospect' CHECK (status IN ('active', 'inactive', 'prospect', 'churned')),\n    60→      tags TEXT[] DEFAULT '{}',\n    61→      source_type TEXT,\n    62→      source_detail TEXT,\n    63→      campaign TEXT,\n    64→      channel TEXT,\n    65→      date_acquired DATE,\n    66→      created_at TIMESTAMPTZ DEFAULT NOW(),\n    67→      updated_at TIMESTAMPTZ DEFAULT NOW()\n    68→    )\n    69→  `\n    70→\n    71→  await sql`\n    72→    CREATE TABLE IF NOT EXISTS individuals (\n    73→      id UUID PRIMARY KEY DEFAULT gen_random_uuid(),\n    74→      name TEXT NOT NULL,\n    75→      title TEXT,\n    76→      email TEXT,\n    77→      phone TEXT,\n    78→      location TEXT,\n    79→      created_at TIMESTAMPTZ DEFAULT NOW(),\n    80→      updated_at TIMESTAMPTZ DEFAULT NOW()\n    81→    )\n    82→  `\n    83→\n    84→  await sql`\n    85→    CREATE TABLE IF NOT EXISTS client_individuals (\n    86→      id UUID PRIMARY KEY DEFAULT gen_random_uuid(),\n    87→      client_id UUID NOT NULL REFERENCES clients(id) ON DELETE CASCADE,\n    88→      individual_id UUID NOT NULL REFERENCES individuals(id) ON DELETE CASCADE,\n    89→      role TEXT,\n    90→      industry TEXT,\n    91→      is_primary BOOLEAN DEFAULT false,\n    92→      UNIQUE(client_id, individual_id)\n    93→    )\n    94→  `\n    95→\n    96→  await sql`\n    97→    CREATE TABLE IF NOT EXISTS individual_relationships (\n    98→      id UUID PRIMARY KEY DEFAULT gen_random_uuid(),\n    99→      individual_id UUID NOT NULL REFERENCES individuals(id) ON DELETE CASCADE,\n   100→      related_individual_id UUID NOT N