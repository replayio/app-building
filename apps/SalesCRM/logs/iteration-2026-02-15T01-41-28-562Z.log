[2026-02-15T01:07:14.027Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01PV5vPUWhQY37P2kMUcKP7A","type":"tool_result","content":"     1→import { useState, useEffect, useCallback } from 'react'\n     2→import { useAppDispatch, useAppSelector } from '../store/hooks'\n     3→import { fetchDeals, createDeal, deleteDeal, setDealsPage } from '../store/dealsSlice'\n     4→import { DealsPageHeader } from '../components/deals/DealsPageHeader'\n     5→import { DealsSummaryCards } from '../components/deals/DealsSummaryCards'\n     6→import { DealsViewToggle } from '../components/deals/DealsViewToggle'\n     7→import { DealsFilterControls } from '../components/deals/DealsFilterControls'\n     8→import { DealsTable } from '../components/deals/DealsTable'\n     9→import { DealsPipelineView } from '../components/deals/DealsPipelineView'\n    10→import { DealsPagination } from '../components/deals/DealsPagination'\n    11→import { CreateDealModal } from '../components/deals/CreateDealModal'\n    12→import { ConfirmDialog } from '../components/shared/ConfirmDialog'\n    13→import type { DealStage } from '../types'\n    14→\n    15→export function DealsListPage() {\n    16→  const dispatch = useAppDispatch()\n    17→  const { items, total, page, pageSize, loading, metrics, availableClients } = useAppSelector(\n    18→    (state) => state.deals\n    19→  )\n    20→\n    21→  const [view, setView] = useState<'table' | 'pipeline'>('table')\n    22→  const [searchInput, setSearchInput] = useState('')\n    23→  const [search, setSearch] = useState('')\n    24→  const [stageFilter, setStageFilter] = useState('')\n    25→  const [clientFilter, setClientFilter] = useState('')\n    26→  const [statusFilter, setStatusFilter] = useState('')\n    27→  const [sort, setSort] = useState('close_date_desc')\n    28→  const [dateFrom, setDateFrom] = useState('')\n    29→  const [dateTo, setDateTo] = useState('')\n    30→  const [createModalOpen, setCreateModalOpen] = useState(false)\n    31→  const [deleteConfirm, setDeleteConfirm] = useState<string | null>(null)\n    32→\n    33→  const loadDeals = useCallback(() => {\n    34→    dispatch(\n    35→      fetchDeals({\n    36→        page,\n    37→        search: search || undefined,\n    38→        stage: stageFilter || undefined,\n    39→        client: clientFilter || undefined,\n    40→        status: statusFilter || undefined,\n    41→        sort,\n    42→        dateFrom: dateFrom || undefined,\n    43→        dateTo: dateTo || undefined,\n    44→      })\n    45→    )\n    46→  }, [dispatch, page, search, stageFilter, clientFilter, statusFilter, sort, dateFrom, dateTo])\n    47→\n    48→  useEffect(() => {\n    49→    loadDeals()\n    50→  }, [loadDeals])\n    51→\n    52→  // Debounce search input\n    53→  useEffect(() => {\n    54→    const timer = setTimeout(() => {\n    55→      setSearch(searchInput)\n    56→      dispatch(setDealsPage(1))\n    57→    }, 300)\n    58→    return () => clearTimeout(timer)\n    59→  }, [searchInput, dispatch])\n    60→\n    61→  function handleCreateDeal(data: {\n    62→    name: string\n    63→    client_id: string\n    64→    value: number\n    65→    stage: DealStage\n    66→    owner: string\n    67→    expected_close_date: string\n    68→  }) {\n    69→    dispatch(createDeal(data)).then(() => {\n    70→      setCreateModalOpen(false)\n    71→      return dispatch(fetchDeals({\n    72→        page,\n    73→        search: search || undefined,\n    74→        stage: stageFilter || undefined,\n    75→        client: clientFilter || undefined,\n    76→        status: statusFilter || undefined,\n    77→        sort,\n    78→        dateFrom: dateFrom || undefined,\n    79→        dateTo: dateTo || undefined,\n    80→      }))\n    81→    })\n    82→  }\n    83→\n    84→  function handleDeleteDeal() {\n    85→    if (!deleteConfirm) return\n    86→    dispatch(deleteDeal(deleteConfirm)).then(() => {\n    87→      setDeleteConfirm(null)\n    88→      loadDeals()\n    89→    })\n    90→  }\n    91→\n    92→  function handlePageChange(newPage: number) {\n    93→    dispatch(setDealsPage(newPage))\n    94→  }\n    95→\n    96→  // Also fetch all clients for the create modal (not just ones with deals)\n    97→  const [allClients, setAllClients] = useState<{ id: string; name: string }[]>([])\n    98→  useEffect(() => {\n    99→    fetch('/.netlify/functions/clients?pageSize=1000')\n   100→      .then((res) => res.json())\n   101→      .then((data: { clients: { id: string; name: string }[] }) => {\n   102→        setAllClients(data.clients.map((c) => ({ id: c.id, name: c.name })))\n   103→      })\n   104→      .catch(() => {})\n   105→  }, [])\n   106→\n   107→  return (\n   108→    <div className=\"p-6\">\n   109→      <DealsPageHeader\n   110→        searchValue={searchInput}\n   111→        onSearchChange={setSearchInput}\n   112→        onCreateDeal={() => setCreateModalOpen(true)}\n   113→      />\n   114→\n   115→      <DealsSummaryCards\n   116→        totalActive={Number(metrics.total_active)}\n   117→        pipelineValue={Number(metrics.pipeline_value)}\n   118→        wonCount={Number(metrics.won_count)}\n   119→        wonValue={Number(metrics.won_value)}\n   120→        lostCount={Number(metrics.lost_count)}\n   121→        lostValue={Number(metrics.lost_value)}\n   122→      />\n   123→\n   124→      <DealsViewToggle view={view} onViewChange={setView} />\n   125→\n   126→      <DealsFilterControls\n   127→        stage={stageFilter}\n   128→        client={clientFilter}\n   129→        status={statusFilter}\n   130→        sort={sort}\n   131→        dateFrom={dateFrom}\n   132→        dateTo={dateTo}\n   133→        availableClients={availableClients}\n   134→        onStageChange={(v) => { setStageFilter(v); dispatch(setDealsPage(1)) }}\n   135→        onClientChange={(v) => { setClientFilter(v); dispatch(setDealsPage(1)) }}\n   136→        onStatusChange={(v) => { setStatusFilter(v); dispatch(setDealsPage(1)) }}\n   137→        onSortChange={setSort}\n   138→        onDateFromChange={setDateFrom}\n   139→        onDateToChange={setDateTo}\n   140→      />\n   141→\n   142→      {loading ? (\n   143→        <div className=\"flex items-center justify-center py-12 text-[13px] text-text-muted\">\n   144→          Loading deals...\n   145→        </div>\n   146→      ) : view === 'table' ? (\n   147→        <div className=\"border border-border rounded-[6px] bg-surface\">\n   148→          <DealsTable\n   149→            deals={items}\n   150→            sort={sort}\n   151→            onSortChange={setSort}\n   152→            onDeleteDeal={(id) => setDeleteConfirm(id)}\n   153→          />\n   154→          <DealsPagination\n   155→            page={page}\n   156→            pageSize={pageSize}\n   157→            total={total}\n   158→            onPageChange={handlePageChange}\n   159→          />\n   160→        </div>\n   161→      ) : (\n   162→        <div className=\"border border-border rounded-[6px] bg-surface p-4\">\n   163→          <DealsPipelineView deals={items} />\n   164→        </div>\n   165→      )}\n   166→\n   167→      <CreateDealModal\n   168→        open={createModalOpen}\n   169→        availableClients={allClients}\n   170→        onClose={() => setCreateModalOpen(false)}\n   171→        onSave={handleCreateDeal}\n   172→      />\n   173→\n   174→      <ConfirmDialog\n   175→        open={deleteConfirm !== null}\n   176→        title=\"Delete Deal\"\n   177→        message=\"Are you sure you want to delete this deal? This action cannot be undone.\"\n   178→        confirmLabel=\"Delete\"\n   179→        onConfirm={handleDeleteDeal}\n   180→        onCancel={() => setDeleteConfirm(null)}\n   181→      />\n   182→    </div>\n   183→  )\n   184→}\n   185→"}]},"parent_tool_use_id":"toolu_01Rw8CZYYDGeVVnNDZNhL7yY","session_id":"1b3cdbe8-d256-4cc5-8532-0000270df63f","uuid":"bd678c57-c694-4e2c-998d-f30cf29d0b3f"}
