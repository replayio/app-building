ity: TaskPriority\n    51→  completed: boolean\n    52→  completed_at: string | null\n    53→  client_id: string | null\n    54→  client_name: string | null\n    55→  deal_id: string | null\n    56→  deal_name: string | null\n    57→  assignee_name: string | null\n    58→  assignee_role: string | null\n    59→  created_at: string\n    60→  updated_at: string\n    61→}\n    62→\n    63→export interface Individual {\n    64→  id: string\n    65→  name: string\n    66→  title: string | null\n    67→  email: string | null\n    68→  phone: string | null\n    69→  location: string | null\n    70→  created_at: string\n    71→  updated_at: string\n    72→  client_associations: ClientAssociation[]\n    73→  relationships: Relationship[]\n    74→  contact_history: ContactHistoryEntry[]\n    75→}\n    76→\n    77→export interface ClientAssociation {\n    78→  client_id: string\n    79→  client_name: string\n    80→  client_status: ClientStatus\n    81→  industry: string | null\n    82→  role: string | null\n    83→}\n    84→\n    85→export interface Relationship {\n    86→  id: string\n    87→  related_individual_id: string\n    88→  related_individual_name: string\n    89→  relationship_type: RelationshipType\n    90→  role: string | null\n    91→  company: string | null\n    92→}\n    93→\n    94→export interface ContactHistoryEntry {\n    95→  id: string\n    96→  individual_id: string\n    97→  date: string\n    98→  type: ContactHistoryType\n    99→  summary: string\n   100→  team_member: string | null\n   101→  created_at: string\n   102→  updated_at: string\n   103→}\n   104→\n   105→export interface Attachment {\n   106→  id: string\n   107→  filename: string\n   108→  type: AttachmentType\n   109→  url: string\n   110→  size: number | null\n   111→  client_id: string\n   112→  deal_id: string | null\n   113→  deal_name: string | null\n   114→  created_at: string\n   115→}\n   116→\n   117→export interface TimelineEvent {\n   118→  id: string\n   119→  client_id: string\n   120→  event_type: string\n   121→  description: string\n   122→  user_name: string | null\n   123→  related_entity_id: string | null\n   124→  related_entity_type: string | null\n   125→  created_at: string\n   126→}\n   127→\n   128→export interface DealHistory {\n   129→  id: string\n   130→  deal_id: string\n   131→  old_stage: DealStage\n   132→  new_stage: DealStage\n   133→  changed_by: string | null\n   134→  created_at: string\n   135→}\n   136→\n   137→export interface Writeup {\n   138→  id: string\n   139→  deal_id: string\n   140→  title: string\n   141→  content: string\n   142→  author: string\n   143→  version: number\n   144→  created_at: string\n   145→  updated_at: string\n   146→}\n   147→\n   148→export interface DealContact {\n   149→  individual_id: string\n   150→  individual_name: string\n   151→  role: string\n   152→  company: string | null\n   153→}\n   154→"}]},"parent_tool_use_id":"toolu_016rW6sbXKaTyCWuYqPyqqAb","session_id":"1b3cdbe8-d256-4cc5-8532-0000270df63f","uuid":"561a13cd-0904-4680-ac5d-b6388c047835"}
