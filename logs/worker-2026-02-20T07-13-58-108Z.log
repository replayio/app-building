llback: try to find any individual via the clients list\n    42→  await page.goto('/clients');\n    43→  await page.waitForLoadState('networkidle');\n    44→  await rows.first().waitFor({ state: 'visible', timeout: 15000 });\n    45→\n    46→  for (let i = 1; i < count && i < 10; i++) {\n    47→    const rowTestId = await rows.nth(i).getAttribute('data-testid');\n    48→    const cId = rowTestId!.replace('client-row-', '');\n    49→    await page.goto(`/clients/${cId}`);\n    50→    await page.waitForLoadState('networkidle');\n    51→    await page.getByTestId('people-section').waitFor({ state: 'visible', timeout: 10000 });\n    52→\n    53→    const pItems = page.locator('[data-testid^=\"person-item-\"]');\n    54→    const pCount = await pItems.count();\n    55→    if (pCount > 0) {\n    56→      const pTestId = await pItems.first().getAttribute('data-testid');\n    57→      const pId = pTestId!.replace('person-item-', '');\n    58→      await page.goto(`/individuals/${pId}`);\n    59→      await page.waitForLoadState('networkidle');\n    60→      return pId;\n    61→    }\n    62→  }\n    63→\n    64→  throw new Error('No individuals found across clients');\n    65→}\n    66→\n    67→test.describe('PersonDetailPage - PersonHeader (PDP-HDR)', () => {\n    68→  test('PDP-HDR-01: Header displays person name, title, and contact info', async ({ page }) => {\n    69→    await navigateToFirstPersonDetail(page);\n    70→\n    71→    const header = page.getByTestId('person-header');\n    72→    await expect(header).toBeVisible();\n    73→\n    74→    // Person name is displayed\n    75→    const nameEl = page.getByTestId('person-header-name');\n    76→    await expect(nameEl).toBeVisible();\n    77→    const nameText = await nameEl.textContent();\n    78→    expect(nameText!.length).toBeGreaterThan(0);\n    79→\n    80→    // Contact info area exists (may be empty if the individual has no email/phone/location)\n    81→    const contactInfo = page.getByTestId('person-heade