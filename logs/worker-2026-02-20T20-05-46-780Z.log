[2026-02-20T19:59:45.690Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_015MSspM7QSU3zvsEHVKWPcM","type":"tool_result","content":"     1→import { getDb } from '../utils/db'\n     2→import { scryptSync, randomBytes, timingSafeEqual } from 'crypto'\n     3→import * as jose from 'jose'\n     4→import { sendConfirmationEmail, sendPasswordResetEmail } from '../utils/email'\n     5→\n     6→const TEST_JWT_SECRET = 'test-jwt-secret-for-playwright'\n     7→\n     8→function getJwtSecret(): Uint8Array {\n     9→  if (process.env.IS_TEST === 'true') {\n    10→    return new TextEncoder().encode(TEST_JWT_SECRET)\n    11→  }\n    12→  const secret = process.env.JWT_SECRET || 'dev-jwt-secret-do-not-use-in-prod'\n    13→  return new TextEncoder().encode(secret)\n    14→}\n    15→\n    16→function hashPassword(password: string): string {\n    17→  const salt = randomBytes(16).toString('hex')\n    18→  const hash = scryptSync(password, salt, 64).toString('hex')\n    19→  return `${salt}:${hash}`\n    20→}\n    21→\n    22→function verifyPassword(password: string, stored: string): boolean {\n    23→  const [salt, hash] = stored.split(':')\n    24→  const hashBuffer = Buffer.from(hash, 'hex')\n    25→  const derivedBuffer = scryptSync(password, salt, 64)\n    26→  return timingSafeEqual(hashBuffer, derivedBuffer)\n    27→}\n    28→\n    29→async function generateToken(userId: string, email: string, name: string): Promise<string> {\n    30→  const secret = getJwtSecret()\n    31→  return new jose.SignJWT({ sub: userId, email, name })\n    32→    .setProtectedHeader({ alg: 'HS256' })\n    33→    .setIssuedAt()\n    34→    .setExpirationTime('7d')\n    35→    .sign(secret)\n    36→}\n    37→\n    38→function generateEmailToken(): string {\n    39→  return randomBytes(32).toString('hex')\n    40→}\n    41→\n    42→export default async function handler(req: Request): Promise<Response> {\n    43→  const url = new URL(req.url)\n    44→  const action = url.searchParams.get('action')\n    45→  const headers = { 'Content-Type': 'application/json' }\n    46→  const isTest = process.env.IS_TEST === 'true'\n    47→\n    48→  // GET ?action=me — returns current user from token\n    49→  if (req.method === 'GET' && action === 'me') {\n    50→    const authHeader = req.headers.get('authorization')\n    51→    if (!authHeader?.startsWith('Bearer ')) {\n    52→      return new Response(JSON.stringify({ error: 'Not authenticated' }), { status: 401, headers })\n    53→    }\n    54→    try {\n    55→      const secret = getJwtSecret()\n    56→      const { payload } = await jose.jwtVerify(authHeader.slice(7), secret)\n    57→      const authUserId = payload.sub\n    58→      const email = (payload.email as string) || ''\n    59→      const name = (payload.name as string) || email.split('@')[0] || 'User'\n    60→\n    61→      const sql = getDb()\n    62→      const rows = await sql`\n    63→        INSERT INTO users (auth_user_id, email, name, provider)\n    64→        VALUES (${authUserId}::uuid, ${email}, ${name}, 'email')\n    65→        ON CONFLICT (auth_user_id) DO UPDATE SET\n    66→          email = EXCLUDED.email,\n    67→          name = EXCLUDED.name,\n    68→          updated_at = NOW()\n    69→        RETURNING id, email, name, avatar_url\n    70→      `\n    71→      return new Response(JSON.stringify(rows[0]), { status: 200, headers })\n    72→    } catch {\n    73→      return new Response(JSON.stringify({ error: 'Invalid token' }), { status: 401, headers })\n    74→    }\n    75→  }\n    76→\n    77→  if (req.method !== 'POST') {\n    78→    return new Response(JSON.stringify({ error: 'Method not allowed' }), { status: 405, headers })\n    79→  }\n    80→\n    81→  const sql = getDb()\n    82→\n    83→  // POST ?action=confirm-email — confirm email with token\n    84→  if (action === 'confirm-email') {\n    85→    const body = await req.json() as { token?: string }\n    86→    const { token } = body\n    87→    if (!token) {\n    88→      return new Response(JSON.stringify({ error: 'Token is required' }), { status: 400, headers })\n    89→    }\n    90→\n    91→    const rows = await sql`\n    92→      SELECT et.id AS token_id, et.user_id, et.expires_at, et.used_at, u.auth_user_id, u.email, u.name, u.avatar_url\n    93→      FROM email_tokens et\n    94→      JOIN users u ON u.id = et.user_id\n    95→      WHERE et.token = ${token} AND et.type = 'confirm'\n    96→    `\n    97→    if (rows.length === 0) {\n    98→      return new Response(JSON.stringify({ error: 'Invalid or expired confirmation link' }), { status: 400, headers })\n    99→    }\n   100→\n   101→    const row = rows[0]\n   102→    if (row.used_at) {\n   103→      return new Response(JSON.stringify({ error: 'This confirmation link has already been used' }), { status: 400, headers })\n   104→    }\n   105→    if (new Date(row.expires_at as string) < new Date()) {\n   106→      return new Response(JSON.stringify({ error: 'This confirmation link has expired' }), { status: 400, headers })\n   107→    }\n   108→\n   109→    await sql`UPDATE email_tokens SET used_at = NOW() WHERE id = ${row.token_id}`\n   110→    await sql`UPDATE users SET email_confirmed = true, updated_at = NOW() WHERE id = ${row.user_id}`\n   111→\n   112→    const jwtToken = await generateToken(row.auth_user_id as string, row.email as string, row.name as string)\n   113→\n   114→    return new Response(JSON.stringify({\n   115→      access_token: jwtToken,\n   116→      user: { id: row.user_id, email: row.email, name: row.name, avatar_url: row.avatar_url },\n   117→    }), { status: 200, headers })\n   118→  }\n   119→\n   120→  // POST ?action=forgot-password — send password reset email\n   121→  if (action === 'forgot-password') {\n   122→    const body = await req.json() as { email?: string }\n   123→    const { email } = body\n   124→    if (!email) {\n   125→      return new Response(JSON.stringify({ error: 'Email is required' }), { status: 400, headers })\n   126→    }\n   127→\n   128→    const userRows = await sql`SELECT id, email FROM users WHERE email = ${email}`\n   129→    if (userRows.length === 0) {\n   130→      return new Response(JSON.stringify({ message: 'If an account with that email exists, a password reset link has been sent.' }), { status: 200, headers })\n   131→    }\n   132→\n   133→    const user = userRows[0]\n   134→    const token = generateEmailToken()\n   135→    const expiresAt = new Date(Date.now() + 60 * 60 * 1000)\n   136→\n   137→    await sql`\n   138→      INSERT INTO email_tokens (user_id, token, type, expires_at)\n   139→      VALUES (${user.id}, ${token}, 'reset', ${expiresAt.toISOString()})\n   140→    `\n   141→\n   142→    await sendPasswordResetEmail(user.email as string, token)\n   143→\n   144→    return new Response(JSON.stringify({ message: 'If an account with that email exists, a password reset link has been sent.' }), { status: 200, headers })\n   145→  }\n   146→\n   147→  // POST ?action=reset-password — reset password with token\n   148→  if (action === 'reset-password') {\n   149→    const body = await req.json() as { token?: string; password?: string }\n   150→    const { token, password } = body\n   151→    if (!token || !password) {\n   152→      return new Response(JSON.stringify({ error: 'Token and new password are required' }), { status: 400, headers })\n   153→    }\n   154→    if (password.length < 6) {\n   155→      return new Response(JSON.stringify({ error: 'Password must be at least 6 characters' }), { status: 400, headers })\n   156→    }\n   157→\n   158→    const rows = await sql`\n   159→      SELECT et.id AS token_id, et.user_id, et.expires_at, et.used_at, u.auth_user_id, u.email, u.name, u.avatar_url\n   160→      FROM email_tokens et\n   161→      JOIN users u ON u.id = et.user_id\n   162→      WHERE et.token = ${token} AND et.type = 'reset'\n   163→    `\n   164→    if (rows.length === 0) {\n   165→      return new Response(JSON.stringify({