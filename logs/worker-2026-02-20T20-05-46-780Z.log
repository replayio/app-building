] } });\n   110→    const page = await context.newPage();\n   111→\n   112→    await page.goto('/clients');\n   113→    await page.waitForLoadState('networkidle');\n   114→\n   115→    // Open sign-in form\n   116→    await page.getByTestId('sidebar-sign-in').click();\n   117→    await expect(page.getByTestId('sidebar-auth-form')).toBeVisible();\n   118→\n   119→    // Click forgot password link\n   120→    await page.getByTestId('auth-forgot-password').click();\n   121→\n   122→    // Should navigate to forgot password page\n   123→    await expect(page).toHaveURL(/\\/auth\\/forgot-password/);\n   124→    await expect(page.getByTestId('forgot-password-page')).toBeVisible();\n   125→    await expect(page.getByTestId('forgot-password-form')).toBeVisible();\n   126→    await expect(page.getByTestId('forgot-password-email')).toBeVisible();\n   127→    await expect(page.getByTestId('forgot-password-submit')).toBeVisible();\n   128→    await expect(page.getByTestId('forgot-password-back-to-signin')).toBeVisible();\n   129→\n   130→    await context.close();\n   131→  });\n   132→\n   133→  test('AUTH-FP-02: Forgot password form submits and shows success message', async ({ browser }) => {\n   134→    const context = await browser.newContext({ storageState: { cookies: [], origins: [] } });\n   135→    const page = await context.newPage();\n   136→\n   137→    await page.goto('/auth/forgot-password');\n   138→    await page.waitForLoadState('networkidle');\n   139→\n   140→    // Enter email and submit\n   141→    await page.getByTestId('forgot-password-email').fill('test@example.com');\n   142→    await page.getByTestId('forgot-password-submit').click();\n   143→\n   144→    // Should show success message\n   145→    await expect(page.getByTestId('forgot-password-success')).toBeVisible({ timeout: 10000 });\n   146→    await expect(page.getByTestId('forgot-password-back')).toBeVisible();\n   147→\n   148→    await context.close();\n   149→  });\n   150→});\n   151→\n   152→