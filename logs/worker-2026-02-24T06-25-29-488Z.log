Errors] = useState<Record<string, string>>({})\n   182→  const [saving, setSaving] = useState(false)\n   183→\n   184→  const handleSubmit = async () => {\n   185→    const newErrors: Record<string, string> = {}\n   186→    if (!taskName.trim()) newErrors.title = 'Task name is required'\n   187→    if (Object.keys(newErrors).length > 0) {\n   188→      setErrors(newErrors)\n   189→      return\n   190→    }\n   191→\n   192→    setSaving(true)\n   193→    try {\n   194→      const res = await fetch('/.netlify/functions/tasks', {\n   195→        method: 'POST',\n   196→        headers: { 'Content-Type': 'application/json' },\n   197→        body: JSON.stringify({\n   198→          title: taskName.trim(),\n   199→          deal_id: dealId,\n   200→          client_id: clientId,\n   201→          due_date: dueDate || null,\n   202→        }),\n   203→      })\n   204→      if (res.ok) {\n   205→        onSaved()\n   206→      }\n   207→    } finally {\n   208→      setSaving(false)\n   209→    }\n   210→  }\n   211→\n   212→  return (\n   213→    <div className=\"modal-overlay\" data-testid=\"add-task-modal\">\n   214→      <div className=\"modal-content\" onClick={e => e.stopPropagation()}>\n   215→        <div className=\"modal-header\">\n   216→          <h3 className=\"modal-title\">Add Task</h3>\n   217→          <button className=\"modal-close\" onClick={onClose} data-testid=\"add-task-close\">\n   218→            <svg width=\"16\" height=\"16\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" strokeWidth=\"2\" strokeLinecap=\"round\" strokeLinejoin=\"round\">\n   219→              <line x1=\"18\" y1=\"6\" x2=\"6\" y2=\"18\"/><line x1=\"6\" y1=\"6\" x2=\"18\" y2=\"18\"/>\n   220→            </svg>\n   221→          </button>\n   222→        </div>\n   223→\n   224→        <div className=\"modal-form\">\n   225→          <div className=\"form-field\">\n   226→            <label className=\"form-label\">Task Name *</label>\n   227→            <input\n   