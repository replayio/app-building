[2026-02-24T06:15:13.993Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01LHm2q7FS4BFKojsv1KStKu","type":"tool_result","content":"     1→import { useState, useEffect, useCallback, useRef } from 'react'\n     2→\n     3→interface Attachment {\n     4→  id: string\n     5→  filename: string\n     6→  file_type: string\n     7→  file_url: string | null\n     8→  file_size: string | null\n     9→  created_at: string\n    10→}\n    11→\n    12→interface DealAttachmentsSectionProps {\n    13→  dealId: string\n    14→  clientId: string\n    15→}\n    16→\n    17→function getFileIcon(fileType: string, filename: string) {\n    18→  const ext = filename.split('.').pop()?.toLowerCase() || ''\n    19→  const isImage = fileType.startsWith('image/') || ['png', 'jpg', 'jpeg', 'gif', 'svg', 'webp'].includes(ext)\n    20→  const isPdf = fileType === 'application/pdf' || ext === 'pdf'\n    21→  const isSpreadsheet = ['csv', 'xls', 'xlsx'].includes(ext) || fileType.includes('spreadsheet') || fileType.includes('csv')\n    22→  const isDoc = ['doc', 'docx'].includes(ext) || fileType.includes('document') || fileType.includes('msword')\n    23→\n    24→  if (isImage) {\n    25→    return (\n    26→      <svg width=\"16\" height=\"16\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" strokeWidth=\"2\" strokeLinecap=\"round\" strokeLinejoin=\"round\">\n    27→        <rect width=\"18\" height=\"18\" x=\"3\" y=\"3\" rx=\"2\" ry=\"2\"/><circle cx=\"9\" cy=\"9\" r=\"2\"/>\n    28→        <path d=\"m21 15-3.086-3.086a2 2 0 0 0-2.828 0L6 21\"/>\n    29→      </svg>\n    30→    )\n    31→  }\n    32→  if (isPdf) {\n    33→    return (\n    34→      <svg width=\"16\" height=\"16\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" strokeWidth=\"2\" strokeLinecap=\"round\" strokeLinejoin=\"round\">\n    35→        <path d=\"M14.5 2H6a2 2 0 0 0-2 2v16a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V7.5L14.5 2z\"/>\n    36→        <polyline points=\"14 2 14 8 20 8\"/>\n    37→        <path d=\"M10 12h4\"/><path d=\"M10 16h4\"/>\n    38→      </svg>\n    39→    )\n    40→  }\n    41→  if (isSpreadsheet) {\n    42→    return (\n    43→      <svg width=\"16\" height=\"16\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" strokeWidth=\"2\" strokeLinecap=\"round\" strokeLinejoin=\"round\">\n    44→        <path d=\"M14.5 2H6a2 2 0 0 0-2 2v16a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V7.5L14.5 2z\"/>\n    45→        <polyline points=\"14 2 14 8 20 8\"/>\n    46→        <path d=\"M8 13h2\"/><path d=\"M14 13h2\"/><path d=\"M8 17h2\"/><path d=\"M14 17h2\"/>\n    47→      </svg>\n    48→    )\n    49→  }\n    50→  if (isDoc) {\n    51→    return (\n    52→      <svg width=\"16\" height=\"16\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" strokeWidth=\"2\" strokeLinecap=\"round\" strokeLinejoin=\"round\">\n    53→        <path d=\"M14.5 2H6a2 2 0 0 0-2 2v16a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V7.5L14.5 2z\"/>\n    54→        <polyline points=\"14 2 14 8 20 8\"/>\n    55→        <line x1=\"16\" y1=\"13\" x2=\"8\" y2=\"13\"/><line x1=\"16\" y1=\"17\" x2=\"8\" y2=\"17\"/>\n    56→      </svg>\n    57→    )\n    58→  }\n    59→  // Generic file icon\n    60→  return (\n    61→    <svg width=\"16\" height=\"16\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" strokeWidth=\"2\" strokeLinecap=\"round\" strokeLinejoin=\"round\">\n    62→      <path d=\"M14.5 2H6a2 2 0 0 0-2 2v16a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V7.5L14.5 2z\"/>\n    63→      <polyline points=\"14 2 14 8 20 8\"/>\n    64→    </svg>\n    65→  )\n    66→}\n    67→\n    68→export default function DealAttachmentsSection({ dealId, clientId }: DealAttachmentsSectionProps) {\n    69→  const [attachments, setAttachments] = useState<Attachment[]>([])\n    70→  const [loading, setLoading] = useState(true)\n    71→  const [uploading, setUploading] = useState(false)\n    72→  const [deleteConfirm, setDeleteConfirm] = useState<Attachment | null>(null)\n    73→  const [toast, setToast] = useState('')\n    74→  const fileInputRef = useRef<HTMLInputElement>(null)\n    75→\n    76→  const fetchAttachments = useCallback(async () => {\n    77→    try {\n    78→      const res = await fetch(`/.netlify/functions/deals/${dealId}/attachments`)\n    79→      const data = await res.json()\n    80→      setAttachments(data.attachments || [])\n    81→    } catch {\n    82→      // silently fail\n    83→    } finally {\n    84→      setLoading(false)\n    85→    }\n    86→  }, [dealId])\n    87→\n    88→  useEffect(() => {\n    89→    fetchAttachments()\n    90→  }, [fetchAttachments])\n    91→\n    92→  const showToast = (msg: string) => {\n    93→    setToast(msg)\n    94→    setTimeout(() => setToast(''), 3000)\n    95→  }\n    96→\n    97→  const handleUpload = async (e: React.ChangeEvent<HTMLInputElement>) => {\n    98→    const file = e.target.files?.[0]\n    99→    if (!file) return\n   100→\n   101→    setUploading(true)\n   102→    try {\n   103→      const formData = new FormData()\n   104→      formData.append('file', file)\n   105→\n   106→      const uploadRes = await fetch('/api/uploadthing', {\n   107→        method: 'POST',\n   108→        body: formData,\n   109→      })\n   110→\n   111→      let fileUrl = ''\n   112→      const fileSize = formatFileSize(file.size)\n   113→\n   114→      if (uploadRes.ok) {\n   115→        const uploadData = await uploadRes.json()\n   116→        fileUrl = uploadData.url || ''\n   117→      } else {\n   118→        fileUrl = URL.createObjectURL(file)\n   119→      }\n   120→\n   121→      const res = await fetch(`/.netlify/functions/deals/${dealId}/attachments`, {\n   122→        method: 'POST',\n   123→        headers: { 'Content-Type': 'application/json' },\n   124→        body: JSON.stringify({\n   125→          filename: file.name,\n   126→          file_type: file.type || 'application/octet-stream',\n   127→          file_url: fileUrl,\n   128→          file_size: fileSize,\n   129→          client_i