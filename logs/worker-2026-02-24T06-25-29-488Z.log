h > 0 ? (\n   100→        <div className=\"linked-tasks-list\" data-testid=\"linked-tasks-list\">\n   101→          {tasks.map(task => (\n   102→            <div key={task.id} className={`linked-task-item ${task.completed ? 'completed' : ''}`} data-testid=\"linked-task-item\">\n   103→              <label className=\"linked-task-checkbox-label\">\n   104→                <input\n   105→                  type=\"checkbox\"\n   106→                  checked={task.completed}\n   107→                  onChange={() => toggleTask(task)}\n   108→                  className=\"linked-task-checkbox\"\n   109→                  data-testid=\"linked-task-checkbox\"\n   110→                />\n   111→              </label>\n   112→              <button\n   113→                className={`linked-task-name ${task.completed ? 'completed' : ''}`}\n   114→                onClick={() => navigate(`/tasks`)}\n   115→                data-testid=\"linked-task-name\"\n   116→              >\n   117→                {task.title}\n   118→              </button>\n   119→              <span className=\"linked-task-date\" data-testid=\"linked-task-date\">\n   120→                {task.completed\n   121→                  ? `Completed: ${formatDate(task.updated_at)}`\n   122→                  : task.due_date ? `Due: ${formatDate(task.due_date)}` : ''}\n   123→              </span>\n   124→              <button\n   125→                className=\"btn-ghost linked-task-remove\"\n   126→                onClick={() => setUnlinkConfirm(task.id)}\n   127→                data-testid=\"linked-task-remove-button\"\n   128→              >\n   129→                <svg width=\"14\" height=\"14\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" strokeWidth=\"2\" strokeLinecap=\"round\" strokeLinejoin=\"round\">\n   130→                  <line x1=\"18\" y1=\"6\" x2=\"6\" y2=\"18\"/><line x1=\"6\" y1=\"6\" x2=\"18\" y2=\"18\"/>\n   131→                </svg>\n   132→              </button>\n   133→      