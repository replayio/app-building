 gap: 0;\n  border-bottom: 1px solid var(--color-border-default);\n  margin-bottom: 12px;\n}\n\n.person-tab {\n  padding: 8px 16px;\n  border: none;\n  border-bottom: 2px solid transparent;\n  background: transparent;\n  color: var(--color-text-muted);\n  font-size: 13px;\n  font-weight: 500;\n  cursor: pointer;\n  transition: all 0.15s ease;\n}\n\n.person-tab:hover {\n  color: var(--color-text-primary);\n}\n\n.person-tab.active {\n  color: var(--color-accent);\n  border-bottom-color: var(--color-accent);\n}\n\n/* Relationships List */\n.relationships-list {\n  display: flex;\n  flex-direction: column;\n  gap: 0;\n}\n\n.relationship-entry {\n  display: flex;\n  align-items: center;\n  flex-wrap: wrap;\n  gap: 2px;\n  padding: 10px 0;\n  font-size: 13px;\n  color: var(--color-text-secondary);\n  border-bottom: 1px solid var(--color-border-light);\n}\n\n.relationship-entry:last-child {\n  border-bottom: none;\n}\n\n.relationship-name {\n  font-weight: 600;\n  color: var(--color-text-primary);\n}\n\n.relationship-type {\n  color: var(--color-text-muted);\n}\n\n.relationship-detail {\n  color: var(--color-text-secondary);\n}\n\n.relationship-link {\n  border: none;\n  background: none;\n  color: var(--color-accent);\n  font-size: 13px;\n  cursor: pointer;\n  padding: 0;\n  margin-left: 6px;\n  text-decoration: none;\n  transition: opacity 0.1s ease;\n}\n\n.relationship-link:hover {\n  opacity: 0.8;\n  text-decoration: underline;\n}\n\n.relationship-delete {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 28px;\n  height: 28px;\n  border: none;\n  border-radius: var(--radius-sm);\n  background: transparent;\n  color: var(--color-text-muted);\n  cursor: pointer;\n  margin-left: auto;\n  flex-shrink: 0;\n  transition: background 0.1s ease;\n}\n\n.relationship-delete:hover {\n  background: var(--color-bg-hover);\n  color: var(--color-status-churned);\n}\n\n/* Relationships Graph */\n.relationships-graph-view {\n  display: flex;\n  flex-wrap: wrap;\n  gap: 16px;\n  padding: 24px;\n  justify-content: center;\n  align-items: center;\n  min-height: 200px;\n}\n\n.graph-center-node {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 80px;\n  height: 80px;\n  border-radius: 50%;\n  background: var(--color-accent);\n  color: white;\n  font-size: 14px;\n  font-weight: 600;\n}\n\n.graph-node {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  gap: 4px;\n  padding: 12px;\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-md);\n  background: var(--color-bg-base);\n}\n\n.graph-node-name {\n  font-size: 13px;\n  font-weight: 500;\n  color: var(--color-text-primary);\n}\n\n.graph-node-type {\n  font-size: 11px;\n  color: var(--color-text-muted);\n}\n\n/* Contact History */\n.contact-history-list {\n  display: flex;\n  flex-direction: column;\n  gap: 0;\n}\n\n.contact-history-entry {\n  position: relative;\n  padding: 12px 0;\n  font-size: 13px;\n  border-bottom: 1px solid var(--color-border-light);\n}\n\n.contact-history-entry:last-child {\n  border-bottom: none;\n}\n\n.contact-history-entry-header {\n  margin-bottom: 4px;\n}\n\n.contact-history-date {\n  font-weight: 500;\n  color: var(--color-text-primary);\n}\n\n.contact-history-separator {\n  color: var(--color-text-muted);\n}\n\n.contact-history-type {\n  color: var(--color-accent-muted);\n  font-weight: 500;\n}\n\n.contact-history-body {\n  color: var(--color-text-secondary);\n  margin-bottom: 2px;\n}\n\n.contact-history-label {\n  font-weight: 500;\n  color: var(--color-text-muted);\n}\n\n.contact-history-summary {\n  color: var(--color-text-secondary);\n}\n\n.contact-history-team {\n  font-size: 12px;\n  color: var(--color-text-muted);\n}\n\n.contact-history-edit-btn {\n  position: absolute;\n  top: 12px;\n  right: 0;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 28px;\n  height: 28px;\n  border: none;\n  border-radius: var(--radius-sm);\n  background: transparent;\n  color: var(--color-text-muted);\n  cursor: pointer;\n  transition: background 0.1s ease;\n}\n\n.contact-history-edit-btn:hover {\n  background: var(--color-bg-hover);\n  color: var(--color-text-primary);\n}\n\n/* Form textarea */\n.form-textarea {\n  height: auto;\n  padding: 8px 10px;\n  resize: vertical;\n  min-height: 80px;\n  font-family: var(--font-sans);\n}\n\n/* Selected person chip */\n.selected-person-chip {\n  display: inline-flex;\n  align-items: center;\n  gap: 6px;\n  height: 32px;\n  padding: 0 10px;\n  border: 1px solid var(--color-accent);\n  border-radius: var(--radius-sm);\n  background: #eef0ff;\n  color: var(--color-accent-muted);\n  font-size: 13px;\n}\n\n/* Associated Clients */\n.associated-clients-grid {\n  display: flex;\n  flex-direction: column;\n  gap: 12px;\n}\n\n.associated-client-card {\n  display: flex;\n  align-items: center;\n  gap: 12px;\n  padding: 12px;\n  border: 1px solid var(--color-border-light);\n  border-radius: var(--radius-md);\n  transition: background 0.1s ease;\n}\n\n.associated-client-card:hover {\n  background: var(--color-bg-hover);\n}\n\n.associated-client-icon {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 40px;\n  height: 40px;\n  border-radius: var(--radius-md);\n  background: var(--color-bg-sidebar);\n  color: var(--color-text-muted);\n  flex-shrink: 0;\n}\n\n.associated-client-info {\n  flex: 1;\n  min-width: 0;\n}\n\n.associated-client-name {\n  font-size: 14px;\n  font-weight: 600;\n  color: var(--color-text-primary);\n  margin-bottom: 2px;\n}\n\n.associated-client-detail {\n  font-size: 12px;\n  color: var(--color-text-muted);\n}\n\n.associated-client-label {\n  font-weight: 500;\n}\n\n.associated-client-btn {\n  flex-shrink: 0;\n}\n\n/* ===== Deal Detail Page ===== */\n\n/* Deal Header */\n.deal-header {\n  margin-bottom: 24px;\n}\n\n.deal-header-title {\n  font-size: 20px;\n  font-weight: 600;\n  color: var(--color-text-primary);\n  margin-bottom: 16px;\n  text-transform: uppercase;\n  letter-spacing: 0.01em;\n}\n\n.deal-header-fields {\n  display: flex;\n  flex-wrap: wrap;\n  gap: 16px;\n  align-items: center;\n}\n\n.deal-header-field {\n  display: flex;\n  align-items: center;\n  gap: 6px;\n  font-size: 13px;\n}\n\n.deal-header-label {\n  font-weight: 500;\n  color: var(--color-text-muted);\n  white-space: nowrap;\n}\n\n.deal-header-value-btn {\n  border: none;\n  background: none;\n  color: var(--color-text-primary);\n  font-size: 13px;\n  font-weight: 500;\n  cursor: pointer;\n  padding: 2px 6px;\n  border-radius: var(--radius-sm);\n  transition: background 0.1s ease;\n}\n\n.deal-header-value-btn:hover {\n  background: var(--color-bg-hover);\n}\n\n.deal-header-input {\n  height: 28px;\n  padding: 0 8px;\n  border: 1px solid var(--color-accent);\n  border-radius: var(--radius-sm);\n  background: var(--color-bg-base);\n  font-size: 13px;\n  color: var(--color-text-primary);\n  outline: none;\n  min-width: 100px;\n}\n\n.deal-header-stage-field {\n  margin-left: auto;\n}\n\n.deal-header-stage-controls {\n  display: flex;\n  align-items: center;\n  gap: 8px;\n}\n\n/* Deal Stage Dropdown */\n.deal-stage-dropdown-wrapper {\n  position: relative;\n}\n\n.deal-stage-dropdown-trigger {\n  display: flex;\n  align-items: center;\n  gap: 6px;\n  height: 28px;\n  padding: 0 10px;\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-sm);\n  background: var(--color-bg-base);\n  font-size: 13px;\n  color: var(--color-text-primary);\n  cursor: pointer;\n  transition: border-color 0.15s ease;\n}\n\n.deal-stage-dropdown-trigger:hover {\n  border-color: var(--color-accent);\n}\n\n.deal-stage-dropdown-menu {\n  position: absolute;\n  top: 100%;\n  left: 0;\n  margin-top: 4px;\n  min-width: 160px;\n  background: var(--color-bg-base);\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-md);\n  box-shadow: var(--shadow-md);\n  z-index: 50;\n  overflow: hidden;\n}\n\n.deal-stage-dropdown-option {\n  display: block;\n  width: 100%;\n  padding: 6px 12px;\n  border: none;\n  background: none;\n  text-align: left;\n  font-size: 13px;\n  color: var(--color-text-primary);\n  cursor: pointer;\n  transition: background 0.1s ease;\n}\n\n.deal-stage-dropdown-option:hover {\n  background: var(--color-bg-hover);\n}\n\n.deal-stage-dropdown-option.selected {\n  background: var(--color-bg-selected);\n  font-weight: 500;\n}\n\n/* Stage Progress Bar */\n.stage-progress-bar {\n  display: flex;\n  align-items: flex-start;\n  margin-bottom: 24px;\n  padding: 16px;\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-md);\n}\n\n.stage-progress-step {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  flex: 1;\n  min-width: 0;\n}\n\n.stage-progress-indicator {\n  display: flex;\n  align-items: center;\n  width: 100%;\n  position: relative;\n}\n\n.stage-progress-circle {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 24px;\n  height: 24px;\n  border-radius: 50%;\n  flex-shrink: 0;\n  margin: 0 auto;\n  position: relative;\n  z-index: 1;\n}\n\n.stage-progress-circle.completed {\n  background: var(--color-accent);\n  color: white;\n}\n\n.stage-progress-circle.incomplete {\n  background: var(--color-bg-sidebar);\n  border: 2px solid var(--color-border-default);\n}\n\n.stage-progress-dot {\n  width: 8px;\n  height: 8px;\n  border-radius: 50%;\n  background: var(--color-border-default);\n}\n\n.stage-progress-line {\n  position: absolute;\n  top: 50%;\n  left: calc(50% + 12px);\n  right: calc(-50% + 12px);\n  height: 2px;\n  transform: translateY(-50%);\n}\n\n.stage-progress-line.completed {\n  background: var(--color-accent);\n}\n\n.stage-progress-line.incomplete {\n  background: var(--color-border-default);\n}\n\n.stage-progress-label {\n  font-size: 11px;\n  margin-top: 6px;\n  text-align: center;\n  white-space: nowrap;\n  overflow: hidden;\n  text-overflow: ellipsis;\n  max-width: 100%;\n}\n\n.stage-progress-label.completed {\n  color: var(--color-accent);\n  font-weight: 500;\n}\n\n.stage-progress-label.incomplete {\n  color: var(--color-text-muted);\n}\n\n/* Deal History */\n.deal-history-list {\n  display: flex;\n  flex-direction: column;\n  gap: 0;\n}\n\n.deal-history-entry {\n  padding: 8px 0;\n  font-size: 13px;\n  border-bottom: 1px solid var(--color-border-light);\n}\n\n.deal-history-entry:last-child {\n  border-bottom: none;\n}\n\n.deal-history-date {\n  font-weight: 500;\n  color: var(--color-text-primary);\n}\n\n.deal-history-separator {\n  color: var(--color-text-muted);\n}\n\n.deal-history-text {\n  color: var(--color-text-secondary);\n}\n\n.deal-history-user {\n  color: var(--color-text-muted);\n}\n\n/* Deal Metrics */\n.deal-metrics-grid {\n  display: flex;\n  gap: 32px;\n}\n\n.deal-metrics-item {\n  display: flex;\n  align-items: center;\n  gap: 6px;\n  font-size: 13px;\n}\n\n.deal-metrics-label {\n  font-weight: 500;\n  color: var(--color-text-muted);\n}\n\n.deal-metrics-edit {\n  display: flex;\n  align-items: center;\n  gap: 4px;\n  font-size: 13px;\n  color: var(--color-text-primary);\n}\n\n/* Writeups */\n.writeups-list {\n  display: flex;\n  flex-direction: column;\n  gap: 0;\n}\n\n.writeup-entry {\n  padding: 12px 0;\n  border-bottom: 1px solid var(--color-border-light);\n}\n\n.writeup-entry:last-child {\n  border-bottom: none;\n}\n\n.writeup-entry-header {\n  margin-bottom: 4px;\n  font-size: 13px;\n}\n\n.writeup-title {\n  font-weight: 600;\n  color: var(--color-text-primary);\n}\n\n.writeup-meta {\n  color: var(--color-text-muted);\n}\n\n.writeup-author {\n  color: var(--color-text-muted);\n}\n\n.writeup-content {\n  font-size: 13px;\n  color: var(--color-text-secondary);\n  margin-bottom: 8px;\n  line-height: 1.5;\n}\n\n.writeup-actions {\n  display: flex;\n  gap: 8px;\n  align-items: center;\n}\n\n.btn-danger-text {\n  color: var(--color-status-churned) !important;\n}\n\n.btn-danger-text:hover {\n  background: var(--color-status-churned-bg) !important;\n}\n\n/* Writeup Version History */\n.writeup-version-entry {\n  padding: 12px;\n  border: 1px solid var(--color-border-light);\n  border-radius: var(--radius-sm);\n  margin-bottom: 8px;\n}\n\n.writeup-version-header {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  margin-bottom: 8px;\n}\n\n.writeup-version-label {\n  font-weight: 600;\n  font-size: 13px;\n  color: var(--color-text-primary);\n}\n\n.writeup-version-date {\n  font-size: 12px;\n  color: var(--color-text-muted);\n}\n\n.writeup-version-content {\n  font-size: 13px;\n  color: var(--color-text-secondary);\n  line-height: 1.5;\n}\n\n/* Linked Tasks */\n.linked-tasks-list {\n  display: flex;\n  flex-direction: column;\n  gap: 0;\n}\n\n.linked-task-item {\n  display: flex;\n  align-items: center;\n  gap: 10px;\n  padding: 8px 0;\n  border-bottom: 1px solid var(--color-border-light);\n}\n\n.linked-task-item:last-child {\n  border-bottom: none;\n}\n\n.linked-task-checkbox-label {\n  display: flex;\n  align-items: center;\n  flex-shrink: 0;\n}\n\n.linked-task-checkbox {\n  width: 16px;\n  height: 16px;\n  accent-color: var(--color-accent);\n  cursor: pointer;\n}\n\n.linked-task-name {\n  flex: 1;\n  min-width: 0;\n  border: none;\n  background: none;\n  font-size: 13px;\n  font-weight: 500;\n  color: var(--color-text-primary);\n  cursor: pointer;\n  text-align: left;\n  padding: 0;\n  overflow: hidden;\n  text-overflow: ellipsis;\n  white-space: nowrap;\n  transition: color 0.1s ease;\n}\n\n.linked-task-name:hover {\n  color: var(--color-accent);\n}\n\n.linked-task-name.completed {\n  text-decoration: line-through;\n  color: var(--color-text-muted);\n}\n\n.linked-task-date {\n  font-size: 12px;\n  color: var(--color-text-muted);\n  white-space: nowrap;\n  flex-shrink: 0;\n}\n\n.linked-task-remove {\n  flex-shrink: 0;\n  color: var(--color-text-muted);\n}\n\n.linked-task-remove:hover {\n  color: var(--color-status-churned);\n}\n\n/* Deal Attachments */\n.deal-attachments-list {\n  display: flex;\n  flex-direction: column;\n  gap: 0;\n}\n\n.deal-attachment-item {\n  display: flex;\n  align-items: center;\n  gap: 8px;\n  padding: 8px 0;\n  font-size: 13px;\n  border-bottom: 1px solid var(--color-border-light);\n}\n\n.deal-attachment-item:last-child {\n  border-bottom: none;\n}\n\n.deal-attachment-icon {\n  display: flex;\n  align-items: center;\n  color: var(--color-text-muted);\n  flex-shrink: 0;\n}\n\n.deal-attachment-name {\n  font-weight: 500;\n  color: var(--color-text-primary);\n}\n\n.deal-attachment-size {\n  color: var(--color-text-muted);\n}\n\n.deal-attachment-separator {\n  color: var(--color-text-disabled);\n}\n\n.deal-attachment-link {\n  border: none;\n  background: none;\n  color: var(--color-accent);\n  font-size: 13px;\n  cursor: pointer;\n  padding: 0;\n  text-decoration: none;\n  transition: opacity 0.1s ease;\n}\n\n.deal-attachment-link:hover {\n  opacity: 0.8;\n  text-decoration: underline;\n}\n\n.deal-attachment-delete {\n  color: var(--color-status-churned);\n}\n\n.deal-attachment-upload-progress {\n  margin-top: 8px;\n  font-size: 13px;\n  color: var(--color-text-muted);\n  padding: 8px 0;\n}\n\n/* Deal Contacts */\n.deal-contacts-list {\n  display: flex;\n  flex-direction: column;\n  gap: 0;\n}\n\n.deal-contact-entry {\n  display: flex;\n  align-items: center;\n  gap: 12px;\n  padding: 10px 0;\n  border-bottom: 1px solid var(--color-border-light);\n}\n\n.deal-contact-entry:last-child {\n  border-bottom: none;\n}\n\n.deal-contact-avatar {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 32px;\n  height: 32px;\n  border-radius: 50%;\n  background: var(--color-accent);\n  color: white;\n  font-size: 11px;\n  font-weight: 600;\n  flex-shrink: 0;\n}\n\n.deal-contact-info {\n  flex: 1;\n  min-width: 0;\n  font-size: 13px;\n}\n\n.deal-contact-name {\n  font-weight: 600;\n  color: var(--color-text-primary);\n}\n\n.deal-contact-meta {\n  color: var(--color-