.search || ''\n   139→  const stageFilter = p.stage || ''\n   140→  const clientFilter = p.client || ''\n   141→  const statusFilter = p.status || ''\n   142→  const sort = p.sort || 'close_date_newest'\n   143→  const dateFrom = p.date_from || null\n   144→  const dateTo = p.date_to || null\n   145→  const clientId = p.client_id || ''\n   146→  const searchPattern = search ? `%${search}%` : ''\n   147→\n   148→  const countResult = await sql`\n   149→    SELECT COUNT(*)::int as total FROM deals d\n   150→    LEFT JOIN clients c ON d.client_id = c.id\n   151→    WHERE\n   152→      (${clientId} = '' OR d.client_id::text = ${clientId})\n   153→      AND (${stageFilter} = '' OR d.stage = ${stageFilter})\n   154→      AND (${clientFilter} = '' OR c.name = ${clientFilter})\n   155→      AND (\n   156→        ${statusFilter} = ''\n   157→        OR (${statusFilter} = 'Active' AND d.status NOT IN ('Won', 'Lost'))\n   158→        OR (${statusFilter} != '' AND ${statusFilter} != 'Active' AND d.status = ${statusFilter})\n   159→      )\n   160→      AND (${searchPattern} = '' OR d.name ILIKE ${searchPattern})\n   161→      AND (${dateFrom} IS NULL OR d.close_date >= ${dateFrom}::date)\n   162→      AND (${dateTo} IS NULL OR d.close_date <= ${dateTo}::date)\n   163→  `\n   164→  const total = countResult[0].total\n   165→\n   166→  const rows = await sql`\n   167→    SELECT d.*, c.name as client_name\n   168→    FROM deals d\n   169→    LEFT JOIN clients c ON d.client_id = c.id\n   170→    WHERE\n   171→      (${clientId} = '' OR d.client_id::text = ${clientId})\n   172→      AND (${stageFilter} = '' OR d.stage = ${stageFilter})\n   173→      AND (${clientFilter} = '' OR c.name = ${clientFilter})\n   174→      AND (\n   175→        ${statusFilter} = ''\n   176→        OR (${statusFilter} = 'Active' AND d.status NOT IN ('Won', 'Lost'))\n   177→        OR (${statusFilter} != '' AND ${statusFilter} != 'Active' AND d.status = ${statusFilter})\n   178→      )\n   179→      AND (${searchPattern} = '' OR d.name ILIKE ${searchPattern})\n   180→      AND (${dateFrom} IS NULL OR d.close_date >= ${dateFrom}::date)\n   181→      AND (${dateTo} IS NULL OR d.close_date <= ${dateTo}::date)\n   182→    ORDER BY\n   183→      CASE WHEN ${sort} = 'close_date_oldest' THEN d.close_date END ASC NULLS LAST,\n   184→      CASE WHEN ${sort} = 'value_highest' THEN d.value END DESC NULLS LAST,\n   185→      CASE WHEN ${sort} = 'value_lowest' THEN d.value END ASC NULLS LAST,\n   186→      CASE WHEN ${sort} NOT IN ('close_date_oldest', 'value_highest', 'value_lowest') THEN d.close_date END DESC NULLS LAST\n   187→    LIMIT ${pageSize} OFFSET ${offset}\n   188→  `\n   189→\n   190→  const now = new Date()\n   191→  const currentQuarter = Math.ceil((now.getMonth() + 1) / 3)\n   192→  const qYear = now.getFullYear()\n   193→  const qStartMonth = (currentQuarter - 1) * 3\n   194→  const qStart = `${qYear}-${String(qStartMonth + 1).padStart(2, '0')}-01`\n   195→  const qEndDate = new Date(qYear, qStartMonth + 3, 0)\n   196→  const qEnd = `${qYear}-${String(qStartMonth + 3).padStart(2, '0')}-${String(qEndDate.getDate()).padStart(2, '0')}`\n   197→\n   198→  const summary = await sql`\n   199→    SELECT\n   200→      COUNT(*) FILTER (WHERE status NOT IN ('Won', 'Lost'))::int as total_active,\n   201→      COALESCE(SUM(value) FILTER (WHERE status NOT IN ('Won', 'Lost')), 0)::numeric as pipeline_value,\n   202→      COUNT(*) FILTER (WHERE status = 'Won' AND close_date >= ${qStart}::date AND close_date <= ${qEnd}::date)::int as won_count,\n   203→      COALESCE(SUM(value) FILTER (WHERE status = 'Won' AND close_date >= ${qStart}::date AND close_date <= ${qEnd}::date), 0)::numeric as won_value,\n   204→      COUNT(*) FILTER (WHERE status = 'Lost' AND close_date >= ${qStart}::date AND close_date <= ${qEnd}::date)::int as lost_count,\n   205→      COALESCE(SUM(value) FILTER (WHERE status = 'Lost' AND close_date >= ${qStart}::date AND close_date <= ${qEnd}::date), 0)::numeric as lost_value\n   206→    FROM deals\n   207→  `\n   208→\n   209→  const availableClients = await sql`\n   210→    SELECT DISTINCT c.name FROM clients c\n   211→    JOIN deals d ON d.client_id = c.id\n   212→    WHERE c.name IS NOT NULL\n   213→    ORDER BY c.name\n   214→  `\n   215→\n   216→  return json(200, {\n   217→    deals: rows,\n   218→    total,\n   219→    page,\n   220→    pageSize,\n   221→    summary: {\n   222→      totalActive: summary[0].total_active,\n   223→      pipelineValue: Number(summary[0].pipeline_value),\n   224→      wonCount: summary[0].won_count,\n   225→      wonValue: Number(summary[0].won_value),\n   226→      lostCount: summary[0].lost_count,\n   227→      lostValue: Number(summary[0].lost_value),\n   228→      currentQuarter,\n   229→    },\n   230→    availableClients: availableClients.map((c: Record<string, string>) => c.name),\n   231→  })\n   232→}\n   233→\n   234→async function handleCreateDeal(sql: SqlFn, body: string | null) {\n   235→  const data = JSON.parse(body || '{}')\n   236→  const { name, client_id, stage, value, owner, status, probability, expected_close_date, close_date } = data\n   237→  if (!name) return json(400, { error: 'Deal name is required' })\n   238→  if (!client_id) return json(400, { error: 'Client is required' })\n   239→\n   240→  const result = await sql`\n   241→    INSERT INTO deals (name, client_id, stage, value, owner, status, probability, expected_close_date, close_date)\n   242→    VALUES (\n   243→      ${name},\n   244→      ${client_id},\n   245→      ${stage || 'Lead'},\n   246→      ${value || null},\n   247→      ${owner || null},\n   248→      ${status || 'On Track'},\n   249→      ${probability === undefined ? null : probability},\n   250→      ${expected_close_date || null},\n   251→      ${close_date || null}\n   252→    )\n   253→    RETURNING *\n   254→  `\n   255→\n   256→  const newDeal = result[0]\n   257→\n   258→  await sql`\n   259→    INSERT INTO timeline_events (client_id, event_type, title, description, actor, reference_id, reference_type)\n   260→    VALUES (\n   261→      ${client_id},\n   262→      'deal_created',\n   263→      ${'Deal Created: ' + name},\n   264→      ${'New deal \"' + name + '\" was created' + (stage ? ' in stage ' + (stage || 'Lead') : '')},\n   265→      ${owner || null},\n   266→      ${newDeal.id},\n   267→      'deal'\n   268→    )\n   269→  `\n   270→\n   271→  const dealWithClient = await sql`\n   272→    SELECT d.*, c.name as client_name\n   273→    FROM deals d\n   274→    LEFT JOIN clients c ON d.client_id = c.id\n   275→    WHERE d.id = ${newDeal.id}\n   276→  `\n   277→\n   278→  return json(201, dealWithClient[0])\n   279→}\n   280→\n   281→async function handleUpdateDeal(sql: SqlFn, id: string, body: string | null) {\n   282→  const data = JSON.parse(body || '{}')\n   283→  const { name, client_id, stage, value, owner, status, probability, expected_close_date, close_date } = data\n   284→\n   285→  const current = await sql`SELECT * FROM deals WHERE id = ${id}`\n   286→  if (!current.length) return json(404, { error: 'Not found' })\n   287→  const oldDeal = current[0]\n   288→\n   289→  const skipName = name === undefined\n   290→  const skipClientId = client_id === undefined\n   291→  const skipStage = stage === undefined\n   292→  const skipValue = value === undefined\n   293→  const skipOwner = owner === undefined\n   294→  const skipStatus = status === undefined\n   295→  const skipProbability = probability === undefined\n   296→  const skipExpectedClose = expected_close_date === undefined\n   297→  const skipCloseDate = close_date === undefined\n   298→\n   299→  await sql`\n   300→    UPDATE deals SET\n   301→      name = CASE WHEN ${skipName}::boolean THEN name ELSE ${name || null} END,\n   302→      client_id = CASE WHEN ${skipClientId}::boolean THEN client_id ELSE ${client_id || null} END,\n   303→      stage = CASE WHEN ${skipStage}::boolean THEN stage ELSE ${stage || null} END,\n   304→      value = CASE WHEN ${skipValue}::boolean THEN value ELSE ${value === '' ? null : value} END,\n   305→      owner = CASE WHEN ${skipOwner}::boolean THEN owner ELSE ${owner || null} END,\n   306→      status = CASE WHEN ${skipStatus}::boolean THEN status ELSE ${status || null} END,\n   307→      probability = CASE WHEN ${skipProbability}::boolean THEN probability ELSE ${probability === null || probability === '' ? null : probability} END,\n   308→      expected_close_date = CASE WHEN ${skipExpectedClose}::boolean THEN expected_close_date ELSE ${expected_close_date || null} END,\n   309→      close_date = CASE WHEN ${skipCloseDate}::boolean THEN close_date ELSE ${close_date || null} END,\n   310→      updated_at = now()\n   311→    WHERE id = ${id}\n   312→  `\n   313→\n   314→  if (!skipStage && stage !== oldDeal.stage) {\n   315→    await sql`\n   316→      INSERT INTO deal_stage_history (deal_id, old_stage, new_stage, changed_by)\n   317→      VALUES (${id}, ${oldDeal.stage}, ${stage}, ${owner || oldDeal.owner || null})\n   318→    `\n   319→    await sql`\n   320→      INSERT INTO timeline_events (client_id, event_type, title, description, actor, reference_id, reference_type)\n   321→      VALUES (\n   322→        ${oldDeal.client_id},\n   323→        'deal_stage_change',\n   324→        ${'Deal Stage Changed: ' + (name || oldDeal.name)},\n   325→        ${'Deal \"' + (name || oldDeal.name) + '\" stage changed from ' + oldDeal.stage + ' to ' + stage},\n   326→        ${owner || oldDeal.owner || null},\n   327→        ${id},\n   328→        'deal'\n   329→      )\n   330→    `\n   331→  }\n   332→\n   333→  const updated = await sql`\n   334→    SELECT d.*, c.name as client_name\n   335→    FROM deals d\n   336→    LEFT JOIN clients c ON d.client_id = c.id\n   337→    WHERE d.id = ${id}\n   338→  `\n   339→  if (!updated.length) return json(404, { error: 'Not found' })\n   340→  return json(200, updated[0])\n   341→}\n   342→\n   343→async function handleDeleteDeal(sql: SqlFn, id: string) {\n   344→  const current = await sql`\n   345→    SELECT d.*, c.name as client_name\n   346→    FROM deals d\n   347→    LEFT JOIN clients c ON d.client_id = c.id\n   348→    WHERE d.id = ${id}\n   349→  `\n   350→  if (current.length) {\n   351→    const deal = current[0]\n   352→    await sql`\n   353→      INSERT INTO timeline_events (client_id, event_type, title, description, actor, reference_id, reference_type)\n   354→      VALUES (\n   355→        ${deal.client_id},\n   356→        'deal_deleted',\n   357→        ${'Deal Deleted: ' + deal.name},\n   358→        ${'Deal \"' + deal.name + '\" was deleted'},\n   359→        ${deal.owner || null},\n   360→        ${id},\n   361→        'deal'\n   362→      )\n   363→    `\n   364→  }\n   365→  await sql`DELETE FROM deals WHERE id = ${id}`\n   366→  return json(200, { deleted: true })\n   367→}\n   368→\n   369→async function handleGetHistory(sql: SqlFn, dealId: string) {\n   370→  const rows = await sql`\n   371→    SELECT * FROM deal_stage_history\n   372→    WHERE deal_id = ${dealId}\n   373→    ORDER BY changed_at DESC\n   374→  `\n   375→  return json(200, { history: rows })\n   376→}\n   377→\n   378→async function handleCreateHistory(sql: SqlFn, dealId: string, body: string | null) {\n   379→  const data = JSON.parse(body || '{}')\n   380→  const { old_stage, new_stage, changed_by } = data\n   381→  if (!new_stage) return json(400, { error: 'New stage is required' })\n   382→\n   383→  const result = await sql`\n   384→    INSERT INTO deal_stage_history (deal_id, old_stage, new_stage, changed_by)\n   385→    VALUES (${dealId}, ${old_stage || null}, ${new_stage}, ${changed_by || null})\n   386→    RETURNING *\n   387→  `\n   388→  return json(201, result[0])\n   389→}\n   390→\n   391→async function handleGetWriteups(sql: SqlFn, dealId: string) {\n   392→  const rows = await sql`\n   393→    SELECT * FROM deal_writeups\n   394→    WHERE deal_id = ${dealId}\n   395→    ORDER BY created_at DESC\n   396→  `\n   397→  return json(200, { writeups: rows })\n   398→}\n   399→\n   400→async function handleGetWriteupVersions(sql: SqlFn, writeupId: string) {\n   401→  const current = await sql`SELECT * FROM deal_writeups WHERE id = ${writeupId}`\n   402→  if (!current.length) return json(404, { error: 'Not found' })\n   403→  return json(200, { versions: current })\n   404→}\n   405→\n   406→async function handleCreateWriteup(sql: SqlFn, dealId: string, body: string | null) {\n   407→  const data = JSON.parse(body || '{}')\n   408→  const { title, content, author } = data\n   409→  if (!title) return json(400, { error: 'Title is required' })\n   410→\n   411→  const result = await sql`\n   412→    INSERT INTO deal_writeups (deal_id, title, content, author)\n   413→    VALUES (${dealId}, ${title}, ${content || null}, ${author || null})\n   414→    RETURNING *\n   415→  `\n   416→  return json(201, result[0])\n   417→}\n   418→\n   419→async function handleUpdateWriteup(sql: SqlFn, id: string, body: string | null) {\n   420→  const data = JSON.parse(body || '{}')\n   421→  const { title, content } = data\n   422→\n   423→  const current = await sql`SELECT * FROM deal_writeups WHERE id = ${id}`\n   424→  if (!current.length) return json(404, { error: 'Not found' })\n   425→\n   426→  const newVersion = (current[0].version || 1) + 1\n   427→\n   428→  await sql`\n   429→    UPDATE deal_writeups SET\n   430→      title = COALESCE(${title || null}, title),\n   431→      content = COALESCE(${content || null}, content),\n   432→      version = ${newVersion},\n   433→      updated_at = now()\n   434→    WHERE id = ${id}\n   435→  `\n   436→\n   437→  const updated = await sql`SELECT * FROM deal_writeups WHERE id = ${id}`\n   438→  return json(200, updated[0])\n   439→}\n   440→\n   441→async function handleGetTasks(sql: SqlFn, dealId: string) {\n   442→  const rows = await sql`\n   443→    SELECT t.*,\n   444→      c.name as client_name,\n   445→      d.name as deal_name\n   446→    FROM tasks t\n   447→    LEFT JOIN clients c ON t.client_id = c.id\n   448→    LEFT JOIN deals d ON t.deal_id = d.id\n   449→    WHERE t.deal_id = ${dealId}\n   450→    ORDER BY t.completed ASC, t.due_date ASC NULLS LAST, t.created_at DESC\n   451→  `\n   452→  return json(200, { tasks: rows })\n   453→}\n   454→\n   455→async function handleGetAttachments(sql: SqlFn, dealId: string) {\n   456→  const rows = await sql`\n   457→    SELECT * FROM attachments\n   458→    WHERE deal_id = ${dealId}\n   459→    ORDER BY created_at DESC\n   460→  `\n   461→  return json(200, { attachments: rows })\n   462→}\n   463→\n   464→async function handleCreateAttachment(sql: SqlFn, dealId: string, body: string | null) {\n   465→  const data = JSON.parse(body || '{}')\n   466→  const { filename, file_type, file_url, file_size, client_id } = data\n   467→  if (!filename) return json(400, { error: 'Filename is required' })\n   468→\n   469→  const result = await sql`\n   470→    INSERT INTO attachments (deal_id, client_id, filename, file_type, file_url, file_size)\n   471→    VALUES (\n   472→      ${dealId},\n   473→      ${client_id || null},\n   474→      ${filename},\n   475→      ${file_type || 'application/octet-stream'},\n   476→      ${file_url || null},\n   477→      ${file_size || null}\n   478→    )\n   479→    RETURNING *\n   480→  `\n   481→  return json(201, result[0])\n   482→}\n   483→\n   484→async function handleGetContacts(sql: SqlFn, dealId: string) {\n   485→  const rows = await sql`\n   486→    SELECT dc.role,\n   487→      i.id, i.name, i.title, i.email,\n   488→      (\n   489→        SELECT c.name FROM client_individuals ci\n   490→        JOIN clients c ON ci.client_id = c.id\n   491→        WHERE ci.individual_id = i.id\n   492→        LIMIT 1\n   493→      ) as client_name\n   494→    FROM deal_contacts dc\n   495→    JOIN individuals i ON dc.individual_id = i.id\n   496→    WHERE dc.deal_id = ${dealId}\n   497→    ORDER BY i.name\