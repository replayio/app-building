n()\n    99→        setDeal(updated)\n   100→        setHistoryRefreshKey(k => k + 1)\n   101→      }\n   102→    } catch {\n   103→      // silently fail\n   104→    }\n   105→  }\n   106→\n   107→  const handleUpdateMetrics = async (field: string, value: string | number | null) => {\n   108→    if (!dealId) return\n   109→    const body: Record<string, unknown> = { [field]: value }\n   110→    try {\n   111→      const res = await fetch(`/.netlify/functions/deals/${dealId}`, {\n   112→        method: 'PUT',\n   113→        headers: { 'Content-Type': 'application/json' },\n   114→        body: JSON.stringify(body),\n   115→      })\n   116→      if (res.ok) {\n   117→        const updated = await res.json()\n   118→        setDeal(updated)\n   119→      }\n   120→    } catch {\n   121→      // silently fail\n   122→    }\n   123→  }\n   124→\n   125→  if (loading) {\n   126→    return (\n   127→      <div className=\"page-content p-6 max-sm:p-3\" data-testid=\"deal-detail-page\">\n   128→        <p>Loading...</p>\n   129→      </div>\n   130→    )\n   131→  }\n   132→\n   133→  if (!deal) {\n   134→    return (\n   135→      <div className=\"page-content p-6 max-sm:p-3\" data-testid=\"deal-detail-page\">\n   136→        <p>Deal not found</p>\n   137→      </div>\n   138→    )\n   139→  }\n   140→\n   141→  return (\n   142→    <div className=\"page-content p-6 max-sm:p-3\" data-testid=\"deal-detail-page\">\n   143→      <DealHeader\n   144→        dealName={deal.name}\n   145→        clientName={deal.client_name}\n   146→        value={deal.value}\n   147→        owner={deal.owner}\n   148→        stage={deal.stage}\n   149→        onUpdateField={handleUpdateField}\n   150→        onChangeStage={handleChangeStage}\n   151→      />\n   152→\n   153→      <StageProgressBar currentStage={deal.stage} />\n   154→\n   155→      <DealHistorySection dealId={deal.id} refreshKey={historyRefreshKey} />\n   156→\n   157→      <DealMetricsSection\n   158→        probability={de