: clientId }\n   187→    const res = await fetch('/.netlify/functions/client-people', {\n   188→      method: 'POST',\n   189→      headers: { 'Content-Type': 'application/json' },\n   190→      body: JSON.stringify(body),\n   191→    })\n   192→    if (res.ok) {\n   193→      setAddPersonOpen(false)\n   194→      loadClientData()\n   195→    }\n   196→  }\n   197→\n   198→  async function handleToggleTask(taskId: string, completed: boolean) {\n   199→    const res = await fetch(`/.netlify/functions/client-tasks/${taskId}`, {\n   200→      method: 'PUT',\n   201→      headers: { 'Content-Type': 'application/json' },\n   202→      body: JSON.stringify({ completed }),\n   203→    })\n   204→    if (res.ok) {\n   205→      loadClientData()\n   206→    }\n   207→  }\n   208→\n   209→  if (loading) {\n   210→    return (\n   211→      <div className=\"p-6\" data-testid=\"client-detail-loading\">\n   212→        <div className=\"text-[13px] text-text-muted\">Loading client...</div>\n   213→      </div>\n   214→    )\n   215→  }\n   216→\n   217→  if (!currentClient) {\n   218→    return (\n   219→      <div className=\"p-6\" data-testid=\"client-detail-error\">\n   220→        <div className=\"text-[13px] text-text-muted\">{error || 'Client not found.'}</div>\n   221→        <button\n   222→          onClick={() => { if (clientId) dispatch(fetchClient(clientId)) }}\n   223→          className=\"mt-2 text-[13px] text-accent hover:underline\"\n   224→        >\n   225→          Retry\n   226→        </button>\n   227→      </div>\n   228→    )\n   229→  }\n   230→\n   231→  return (\n   232→    <div className=\"p-3 sm:p-6\" data-testid=\"client-detail-page\">\n   233→      {/* Breadcrumb */}\n   234→      <div className=\"flex items-center gap-1 text-[12px] text-text-muted mb-4\" data-testid=\"client-detail-breadcrumb\">\n   235→        <span\n   236→          className=\"hover:text-accent cursor-pointer\"\n   237→          onClick={() => navigate('/clients')}\n   238→        >\n   239→          ClientDetailPage\n   240→        </span>\n   241→        <span>/</span>\n   242→        <span>/clients/{clientId}</span>\n   243→      </div>\n   244→\n   245→      {/* Header */}\n   246→      <ClientHeader client={currentClient} onUpdate={handleUpdateClient} />\n   247→\n   248→      {/* Quick Actions */}\n   249→      <QuickActions\n   250→        onAddTask={() => setAddTaskOpen(true)}\n   251→        onAddDeal={() => setAddDealOpen(true)}\n   252→        onAddAttachment={() => setAddAttachmentOpen(true)}\n   253→        onAddPerson={() => setAddPersonOpen(true)}\n   254→      />\n   255→\n   256→      {/* Source Info */}\n   257→      <SourceInfoSection client={currentClient} onUpdate={handleUpdateClient} />\n   258→\n   259→      {/* Two-column layout */}\n   260→      <div className=\"grid grid-cols-1 md:grid-cols-2 gap-4\">\n   261→        <div>\n   262→          {/* Tasks */}\n   263→          <TasksSection tasks={tasks} onToggleTask={handleToggleTask} />\n   264→\n   265→          {/* Deals */}\n   266→          <DealsSection deals={deals} />\n   267→\n   268→          {/* Attachments */}\n   269→          <AttachmentsSection\n   270→            attachments={attachments}\n   271→            onDelete={(id) => setDeleteAttachmentId(id)}\n   272→          />\n   273→        </div>\n   274→        <div>\n   275→          {/* People */}\n   276→          <PeopleSection people={people} />\n   277→\n   278→          {/* Timeline */}\n   279→          <TimelineSection events={timeline} />\n   280→        </div>\n   281→      </div>\n   282→\n   283→      {/* Modals */}\n   284→      <AddTaskModal\n   285→        open={addTaskOpen}\n   286→        onClose={() => setAddTaskOpen(false)}\n   287→        onSave={handleAddTask}\n   288→        deals={deals}\n   289→      />\n   290→      <AddDealModal\n   291→        open={addDealOpen}\n   292→        availableUsers={availableUsers}\n   293→        onClose={() => setAddDealOpen(false)}\n   294→        onSave={handleAddDeal}\n   295→      />\n   296→      <AddAttachmentModal\n   297→        open={addAttachmentOpen}\n   298→        onClose={() => setAddAttachmentOpen(false)}\n   299→        onSave={handleAddAttachment}\n   300→        deals={deals}\n   301→      />\n   302→      <AddPersonModal\n   303→        open={addPersonOpen}\n   304→        onClose={() => setAddPersonOpen(false)}\n   305→        onSave={handleAddPerson}\n   306→      />\n   307→      <ConfirmDialog\n   308→        open={deleteAttachmentId !== null}\n   309→        title=\"Delete Attachment\"\n   310→        message=\"Are you sure you want to delete this attachment? This action cannot be undone.\"\n   311→        confirmLabel=\"Delete\"\n   312→        onConfirm={handleDeleteAttachment}\n   313→        onCancel={() => setDeleteAttachmentId(null)}\n   314→      />\n   315→    </div>\n   316→  )\n   317→}\n   318→"}]},"parent_tool_use_id":"toolu_011DxdhNZVekXPJwzdrgVa8x","session_id":"763af48d-ec2d-4286-a67d-65762ca15462","uuid":"c6f55893-f0dc-4c4e-bf43-a760d27f34a5"}
