[2026-02-20T10:29:21.956Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_019wpcf63gJTx4eZYX3Q8ryc","type":"tool_result","content":"     1→import type { TimelineEvent } from '../../types'\n     2→\n     3→interface TimelineSectionProps {\n     4→  events: TimelineEvent[]\n     5→}\n     6→\n     7→function formatRelativeDate(dateStr: string): string {\n     8→  const date = new Date(dateStr)\n     9→  const now = new Date()\n    10→  const diffMs = now.getTime() - date.getTime()\n    11→  const diffDays = Math.floor(diffMs / 86400000)\n    12→\n    13→  if (diffDays === 0) return 'Today'\n    14→  if (diffDays === 1) return 'Yesterday'\n    15→  if (diffDays <= 7) return `${diffDays} days ago`\n    16→  if (diffDays <= 14) return 'Last Week'\n    17→  if (diffDays <= 30) return `${Math.floor(diffDays / 7)} weeks ago`\n    18→  if (diffDays <= 60) return 'Last Month'\n    19→  return date.toLocaleDateString()\n    20→}\n    21→\n    22→function getEventIcon(eventType: string): string {\n    23→  switch (eventType) {\n    24→    case 'task_created': return '●'\n    25→    case 'task_completed': return '●'\n    26→    case 'deal_created': return '●'\n    27→    case 'deal_stage_changed': return '●'\n    28→    case 'note_added': return '●'\n    29→    case 'email_sent': return '●'\n    30→    case 'contact_added': return '●'\n    31→    case 'attachment_added': return '●'\n    32→    case 'status_changed': return '●'\n    33→    default: return '●'\n    34→  }\n    35→}\n    36→\n    37→function getEventColor(eventType: string): string {\n    38→  switch (eventType) {\n    39→    case 'task_created': return 'text-accent'\n    40→    case 'task_completed': return 'text-status-active'\n    41→    case 'deal_created': return 'text-accent-purple'\n    42→    case 'deal_stage_changed': return 'text-accent-blue'\n    43→    case 'note_added': return 'text-text-muted'\n    44→    case 'email_sent': return 'text-accent-blue'\n    45→    case 'contact_added': return 'text-status-prospect'\n    46→    case 'attachment_added': return 'text-text-muted'\n    47→    case 'status_changed': return 'text-accent'\n    48→    default: return 'text-text-muted'\n    49→  }\n    50→}\n    51→\n    52→export function TimelineSection({ events }: TimelineSectionProps) {\n    53→  // Group events by relative date\n    54→  const grouped = events.reduce<Record<string, TimelineEvent[]>>((acc, event) => {\n    55→    const label = formatRelativeDate(event.created_at)\n    56→    if (!acc[label]) acc[label] = []\n    57→    acc[label].push(event)\n    58→    return acc\n    59→  }, {})\n    60→\n    61→  return (\n    62→    <div className=\"border border-border rounded-[6px] p-4 mb-4\" data-testid=\"timeline-section\">\n    63→      <h2 className=\"text-[14px] font-semibold text-text-primary mb-3\">Timeline</h2>\n    64→\n    65→      {events.length === 0 ? (\n    66→        <div className=\"text-[13px] text-text-muted py-2\" data-testid=\"timeline-empty-state\">No timeline events</div>\n    67→      ) : (\n    68→        <div className=\"relative\">\n    69→          {Object.entries(grouped).map(([dateLabel, groupEvents]) => (\n    70→            <div key={dateLabel} className=\"mb-4 last:mb-0\" data-testid=\"timeline-date-group\">\n    71→              <div className=\"text-[12px] font-medium text-text-muted mb-2\" data-testid=\"timeline-date-label\">{dateLabel}</div>\n    72→              {groupEvents.map((event) => (\n    73→                <div key={event.id} className=\"flex items-start gap-3 mb-2 last:mb-0 pl-2\" data-testid={`timeline-entry-${event.id}`}>\n    74→                  <span className={`flex-shrink-0 text-[8px] mt-1.5 ${getEventColor(event.event_type)}`}>\n    75→                    {getEventIcon(event.event_type)}\n    76→                  </span>\n    77→                  <div className=\"flex-1 min-w-0\">\n    78→                    <div className=\"text-[13px] text-text-primary\" data-testid={`timeline-event-description-${event.id}`}>\n    79→                      {event.description}\n    80→                      {event.user_name && (\n    81→                        <span className=\"text-accent-blue ml-1\" data-testid={`timeline-event-user-${event.id}`}>\n    82→                          by {event.user_name}\n    83→                        </span>\n    84→                      )}\n    85→                    </div>\n    86→                  </div>\n    87→                </div>\n    88→              ))}\n    89→            </div>\n    90→          ))}\n    91→        </div>\n    92→      )}\n    93→    </div>\n    94→  )\n    95→}\n    96→"}]},"parent_tool_use_id":"toolu_011DxdhNZVekXPJwzdrgVa8x","session_id":"763af48d-ec2d-4286-a67d-65762ca15462","uuid":"e4dc05ee-20d3-43cb-945d-6fec61ae5f97"}
