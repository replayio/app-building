ext('Acme Corporation');\n    80→\n    81→    // Restore original name\n    82→    await page.getByTestId('client-header-edit-button').click();\n    83→    await page.getByTestId('client-header-name-input').clear();\n    84→    await page.getByTestId('client-header-name-input').fill(originalName!);\n    85→    await page.getByTestId('client-header-save-button').click();\n    86→    await expect(page.getByTestId('client-header-title')).toHaveText(originalName!);\n    87→  });\n    88→\n    89→  test('CDP-HDR-03: Cancel edit mode restores original values', async ({ page }) => {\n    90→    await navigateToFirstClientDetail(page);\n    91→\n    92→    // Get original name\n    93→    const originalName = await page.getByTestId('client-header-title').textContent();\n    94→\n    95→    // Enter edit mode\n    96→    await page.getByTestId('client-header-edit-button').click();\n    97→\n    98→    // Change name\n    99→    const nameInput = page.getByTestId('client-header-name-input');\n   100→    await nameInput.clear();\n   101→    await nameInput.fill('Changed Name');\n   102→\n   103→    // Cancel\n   104→    await page.getByTestId('client-header-cancel-button').click();\n   105→\n   106→    // Name should be restored to original\n   107→    await expect(page.getByTestId('client-header-title')).toHaveText(originalName!);\n   108→  });\n   109→\n   110→  test('CDP-HDR-04: Status dropdown shows all status options', async ({ page }) => {\n   111→    await navigateToFirstClientDetail(page);\n   112→\n   113→    // Click status dropdown button\n   114→    await page.getByTestId('status-dropdown-button').click();\n   115→\n   116→    // All status options should be visible\n   117→    const options = page.getByTestId('status-dropdown-options');\n   118→    await expect(options).toBeVisible();\n   119→    await expect(page.getByTestId('status-dropdown-option-active')).toBeVisible();\n   120→    await expect(page.getByTestId('status-dropdown-option-inactive')).toBeVisible()