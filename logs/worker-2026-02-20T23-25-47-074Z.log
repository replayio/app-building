[2026-02-20T23:06:47.108Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01XA369Aatv5BBkRbGugqFCa","type":"tool_result","content":"     1→import type { TimelineEvent } from '../../types'\n     2→\n     3→interface TimelineSectionProps {\n     4→  events: TimelineEvent[]\n     5→}\n     6→\n     7→function formatRelativeDate(dateStr: string): string {\n     8→  const date = new Date(dateStr)\n     9→  const now = new Date()\n    10→  const diffMs = now.getTime() - date.getTime()\n    11→  const diffDays = Math.floor(diffMs / 86400000)\n    12→\n    13→  if (diffDays === 0) return 'Today'\n    14→  if (diffDays === 1) return 'Yesterday'\n    15→  if (diffDays <= 7) return `${diffDays} days ago`\n    16→  if (diffDays <= 14) return 'Last Week'\n    17→  if (diffDays <= 30) return `${Math.floor(diffDays / 7)} weeks ago`\n    18→  if (diffDays <= 60) return 'Last Month'\n    19→  return date.toLocaleDateString()\n    20→}\n    21→\n    22→function getEventIcon(eventType: string): string {\n    23→  switch (eventType) {\n    24→    case 'task_created': return '●'\n    25→    case 'task_completed': return '●'\n    26→    case 'deal_created': return '●'\n    27→    case 'deal_stage_changed': return '●'\n    28→    case 'note_added': return '●'\n    29→    case 'email_sent': return '●'\n    30→    case 'contact_added': return '●'\n    31→    case 'attachment_added': return '●'\n    32→    case 'status_changed': return '●'\n    33→    case 'name_changed': return '●'\n    34→    case 'tags_changed': return '●'\n    35→    case 'type_changed': return '●'\n    36→    default: return '●'\n    37→  }\n    38→}\n    39→\n    40→function getEventColor(eventType: string): string {\n    41→  switch (eventType) {\n    42→    case 'task_created': return 'text-accent'\n    43→    case 'task_completed': return 'text-status-active'\n    44→    case 'deal_created': return 'text-accent-purple'\n    45→    case 'deal_stage_changed': return 'text-accent-blue'\n    46→    case 'note_added': return 'text-text-muted'\n    47→    case 'email_sent': return 'text-accent-blue'\n    48→    case 'contact_added': return 'text-status-prospect'\n    49→    case 'attachment_added': return 'text-text-muted'\n    50→    case 'status_changed': return 'text-accent'\n    51→    case 'name_changed': return 'text-accent'\n    52→    case 'tags_changed': return 'text-accent-blue'\n    53→    case 'type_changed': return 'text-accent-purple'\n    54→    default: return 'text-text-muted'\n    55→  }\n    56→}\n    57→\n    58→export function TimelineSection({ events }: TimelineSectionProps) {\n    59→  // Group events by relative date\n    60→  const grouped = events.reduce<Record<string, TimelineEvent[]>>((acc, event) => {\n    61→    const label = formatRelativeDate(event.created_at)\n    62→    if (!acc[label]) acc[label] = []\n    63→    acc[label].push(event)\n    64→    return acc\n    65→  }, {})\n    66→\n    67→  return (\n    68→    <div className=\"border border-border rounded-[6px] p-4 mb-4\" data-testid=\"timeline-section\">\n    69→      <h2 className=\"text-[14px] font-semibold text-text-primary mb-3\">Timeline</h2>\n    70→\n    71→      {events.length === 0 ? (\n    72→        <div className=\"text-[13px] text-text-muted py-2\" data-testid=\"timeline-empty-state\">No timeline events</div>\n    73→      ) : (\n    74→        <div className=\"relative\">\n    75→          {Object.entries(grouped).map(([dateLabel, groupEvents]) => (\n    76→            <div key={dateLabel} className=\"mb-4 last:mb-0\" data-testid=\"timeline-date-group\">\n    77→              <div className=\"text-[12px] font-medium text-text-muted mb-2\" data-testid=\"timeline-date-label\">{dateLabel}</div>\n    78→              {groupEvents.map((event) => (\n    79→                <div key={event.id} className=\"flex items-start gap-3 mb-2 last:mb-0 pl-2\" data-testid={`timeline-entry-${event.id}`}>\n    80→                  <span className={`flex-shrink-0 text-[8px] mt-1.5 ${getEventColor(event.event_type)}`}>\n    81→                    {getEventIcon(event.event_type)}\n    82→                  </span>\n    83→                  <div className=\"flex-1 min-w-0\">\n    84→                    <div className=\"text-[13px] text-text-primary\" data-testid={`timeline-event-description-${event.id}`}>\n    85→                      {event.description}\n    86→                      {event.user_name && (\n    87→                        <span className=\"text-accent-blue ml-1\" data-testid={`timeline-event-user-${event.id}`}>\n    88→                          by {event.user_name}\n    89→                        </span>\n    90→                      )}\n    91→                    </div>\n    92→                  </div>\n    93→                </div>\n    94→              ))}\n    95→            </div>\n    96→          ))}\n    97→        </div>\n    98→      )}\n    99→    </div>\n   100→  )\n   101→}\n   102→"}]},"parent_tool_use_id":"toolu_012sxUDaCXoSKekSpP2NEcUQ","session_id":"c9f2fb8c-a17e-40e0-adbc-d295fa359972","uuid":"22708f85-abe6-4a76-9645-106ef0320292"}
