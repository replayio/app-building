ength]);\n    35→\n    36→  const categoriesFilterString = selectedCategories.length === 5 ? null : selectedCategories.join(\",\");\n    37→\n    38→  const handleSubmit = async () => {\n    39→    if (!startDate || !endDate) return;\n    40→    setSubmitting(true);\n    41→\n    42→    const name = defaultAccountName\n    43→      ? `${reportType === \"budget_vs_actual\" ? \"Budget vs Actual\" : reportType === \"detailed\" ? \"Detailed Transactions\" : \"Summary Overview\"} - ${defaultAccountName}`\n    44→      : `${reportType === \"budget_vs_actual\" ? \"Budget vs Actual\" : reportType === \"detailed\" ? \"Detailed Transactions\" : \"Summary Overview\"} Report`;\n    45→\n    46→    try {\n    47→      await dispatch(\n    48→        createReport({\n    49→          name,\n    50→          report_type: reportType,\n    51→          date_range_start: startDate,\n    52→          date_range_end: endDate,\n    53→          categories_filter: categoriesFilterString,\n    54→          include_zero_balance: includeZeroBalance,\n    55→        })\n    56→      ).unwrap();\n    57→      onClose();\n    58→    } catch {\n    59→      setSubmitting(false);\n    60→    }\n    61→  };\n    62→\n    63→  const canSubmit = !!startDate && !!endDate && !submitting;\n    64→\n    65→  return (\n    66→    <div className=\"modal-overlay create-report-dialog\" data-testid=\"create-report-dialog\" onClick={onClose}>\n    67→      <div className=\"modal-content\" onClick={(e) => e.stopPropagation()}>\n    68→        <div className=\"modal-header\">\n    69→          <h2 className=\"modal-title\">Create New Report</h2>\n    70→          <button className=\"modal-close-btn\" data-testid=\"report-dialog-close-btn\" onClick={onClose}>\n    71→            <svg width=\"16\" height=\"16\" viewBox=\"0 0 16 16\" fill=\"none\">\n    72→              <path d=\"M4 4L12 12M12 4L4 12\" stroke=\"currentColor\" strokeWidth=\"1.5\" strokeLinecap=\"round\" />\n    73→            </svg>\n    74→          </button>\n    75→        </div>\n    76→\n    77→        <p className=\"create-report-dialog-subtitle\">\n    78→          Configure parameters to generate a financial report based on accounts and transactions.\n    79→        </p>\n    80→\n    81→        <div className=\"create-report-dialog-body\">\n    82→          <div className=\"create-report-dialog-left\">\n    83→            <div className=\"create-report-settings-title\">Report Settings</div>\n    84→\n    85→            <ReportTypeSelector value={reportType} onChange={setReportType} />\n    86→\n    87→            <DateRangeSelector\n    88→              startDate={startDate}\n    89→              endDate={endDate}\n    90→              onStartDateChange={setStartDate}\n    91→              onEndDateChange={setEndDate}\n    92→            />\n    93→\n    94→            <AccountCategoryFilter\n    95→              subCategoryState={subCategoryState}\n    96→              onSubCategoryChange={setSubCategoryState}\n    97→              includeZeroBalance={includeZeroBalance}\n    98→              onIncludeZeroBalanceChange={setIncludeZeroBalance}\n    99→            />\n   100→          </div>\n   101→\n   102→          <ReportPreview\n   103→            reportType={reportType}\n   104→            startDate={startDate}\n   105→            endDate={endDate}\n   106→            selectedCategories={selectedCategories}\n   107→            includeZeroBalance={includeZeroBalance}\n   108→            subCategoryState={subCategoryState}\n   109→          />\n   110→        </div>\n   111→\n   112→        <div className=\"modal-footer\">\n   113→          <button className=\"btn btn--secondary\" data-testid=\"report-cancel-btn\" onClick={onClose}>Cancel</button>\n   114→          <button\n   115→            className=\"btn btn--primary\"\n   116→            data-testid=\"submit-report-btn\"\n   117→            disabled={!canSubmit}\n   118→            onClick={handleSubmit}\n   119→          >\n   120→          