s$/, { timeout: 10000 });\n    35→    await expect(page.getByTestId(\"accounts-page\")).toBeVisible({ timeout: 30000 });\n    36→  });\n    37→\n    38→  test(\"AccountHeader New Transaction button opens NewTransactionModal\", async ({ page }) => {\n    39→    await navigateToCheckingAccount(page);\n    40→    await page.getByTestId(\"account-new-transaction-btn\").click();\n    41→    await expect(page.getByTestId(\"new-transaction-modal\")).toBeVisible({ timeout: 10000 });\n    42→  });\n    43→\n    44→  test(\"AccountHeader search bar filters transactions\", async ({ page }) => {\n    45→    await navigateToCheckingAccount(page);\n    46→    await expect(page.getByTestId(\"transactions-table\")).toBeVisible({ timeout: 10000 });\n    47→\n    48→    // Type \"Grocery\" in search bar\n    49→    await page.getByTestId(\"account-search-input\").fill(\"Grocery\");\n    50→\n    51→    // Verify only matching transactions are shown\n    52→    const table = page.getByTestId(\"transactions-table\");\n    53→    await expect(table.locator(\"tbody tr\").filter({ hasText: \"Grocery Store\" })).toBeVisible();\n    54→    await expect(table.locator(\"tbody tr\").filter({ hasText: \"Salary Deposit\" })).toHaveCount(0, { timeout: 5000 });\n    55→    await expect(table.locator(\"tbody tr\").filter({ hasText: \"Electric Bill\" })).toHaveCount(0, { timeout: 5000 });\n    56→  });\n    57→\n    58→  test(\"AccountHeader search bar clears and restores all transactions\", async ({ page }) => {\n    59→    await navigateToCheckingAccount(page);\n    60→    await expect(page.getByTestId(\"transactions-table\")).toBeVisible({ timeout: 10000 });\n    61→\n    62→    // Filter first\n    63→    await page.getByTestId(\"account-search-input\").fill(\"Grocery\");\n    64→    await expect(\n    65→      page.getByTestId(\"transactions-table\").locator(\"tbody tr\").filter({ hasText: \"Salary Deposit\" })\n    66→    ).toHaveCount(0, { timeout: 5000 });\n    67→\n    68→    // Clear searc