n \"unchecked\";\n    42→  if (checkedCount === total) return \"checked\";\n    43→  return \"indeterminate\";\n    44→}\n    45→\n    46→export function AccountCategoryFilter({\n    47→  subCategoryState,\n    48→  onSubCategoryChange,\n    49→  includeZeroBalance,\n    50→  onIncludeZeroBalanceChange,\n    51→}: AccountCategoryFilterProps): React.ReactElement {\n    52→  const [expandedCategories, setExpandedCategories] = useState<Set<AccountCategory>>(new Set());\n    53→\n    54→  const toggleExpand = (cat: AccountCategory) => {\n    55→    setExpandedCategories((prev) => {\n    56→      const next = new Set(prev);\n    57→      if (next.has(cat)) {\n    58→        next.delete(cat);\n    59→      } else {\n    60→        next.add(cat);\n    61→      }\n    62→      return next;\n    63→    });\n    64→  };\n    65→\n    66→  const handleParentToggle = (node: CategoryNode) => {\n    67→    const currentState = getParentCheckState(node, subCategoryState);\n    68→    const newChecked = currentState !== \"checked\";\n    69→    const newState = { ...subCategoryState };\n    70→    for (const sub of node.subCategories) {\n    71→      newState[sub.id] = newChecked;\n    72→    }\n    73→    onSubCategoryChange(newState);\n    74→  };\n    75→\n    76→  const handleSubToggle = (subId: string) => {\n    77→    const newState = { ...subCategoryState };\n    78→    newState[subId] = !newState[subId];\n    79→    onSubCategoryChange(newState);\n    80→  };\n    81→\n    82→  return (\n    83→    <div data-testid=\"account-category-filter\">\n    84→      <div className=\"form-label\" style={{ marginBottom: \"8px\" }}>Filter Accounts &amp; Categories</div>\n    85→      <div className=\"category-filter-tree\">\n    86→        {CATEGORY_TREE.map((node) => {\n    87→          const isExpanded = expandedCategories.has(node.category);\n    88→          const parentState = getParentCheckState(node, subCategoryState);\n    89→\n    90→          return (\n    91→            <div