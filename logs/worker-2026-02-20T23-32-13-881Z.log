\n   242→    // Restore original stage\n   243→    await selectFilterOption(page, 'deal-header-stage-select', currentStage);\n   244→    await expect(changeStageBtn).not.toBeDisabled();\n   245→    await changeStageBtn.click();\n   246→    await expect(page.getByTestId('deal-header-stage-select')).toHaveAttribute('data-value', currentStage, { timeout: 10000 });\n   247→  });\n   248→});\n   249→\n   250→test.describe('DealDetailPage - StagePipeline (DDP-PIP)', () => {\n   251→  test('DDP-PIP-01: Stage pipeline displays all stages with visual progress', async ({ page }) => {\n   252→    await navigateToFirstDealDetail(page);\n   253→\n   254→    const pipeline = page.getByTestId('deal-stage-pipeline');\n   255→    await expect(pipeline).toBeVisible();\n   256→\n   257→    // All 6 stages should be visible (excluding closed_lost which is separate)\n   258→    await expect(page.getByTestId('pipeline-stage-lead')).toBeVisible();\n   259→    await expect(page.getByTestId('pipeline-stage-qualification')).toBeVisible();\n   260→    await expect(page.getByTestId('pipeline-stage-discovery')).toBeVisible();\n   261→    await expect(page.getByTestId('pipeline-stage-proposal')).toBeVisible();\n   262→    await expect(page.getByTestId('pipeline-stage-negotiation')).toBeVisible();\n   263→    await expect(page.getByTestId('pipeline-stage-closed_won')).toBeVisible();\n   264→\n   265→    // Progress bar should be visible\n   266→    await expect(page.getByTestId('pipeline-progress-bar')).toBeVisible();\n   267→    await expect(page.getByTestId('pipeline-progress-fill')).toBeVisible();\n   268→\n   269→    // One stage should be marked as current\n   270→    const currentStage = page.locator('[data-current=\"true\"]');\n   271→    const currentCount = await currentStage.count();\n   272→    expect(currentCount).toBe(1);\n   273→  });\n   274→\n   275→  test('DDP-PIP-02: Pipeline updates when stage changes', async ({ page }) => {\n   276→    await navigateToFirstDealDetail(page);\n 