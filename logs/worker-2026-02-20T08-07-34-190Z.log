igateToFirstDealDetail(page);\n   446→\n   447→    // First create a writeup to edit\n   448→    await page.getByTestId('deal-writeups-add-button').click();\n   449→    const modal = page.getByTestId('add-writeup-modal');\n   450→    await expect(modal).toBeVisible();\n   451→\n   452→    const writeupTitle = `Editable Writeup ${Date.now()}`;\n   453→    await page.getByTestId('add-writeup-title').fill(writeupTitle);\n   454→    await page.getByTestId('add-writeup-content').fill('Original content.');\n   455→    await page.getByTestId('add-writeup-save').click();\n   456→    await expect(modal).not.toBeVisible();\n   457→    await page.waitForLoadState('networkidle');\n   458→\n   459→    // Find the writeup and click edit\n   460→    const editButtons = page.locator('[data-testid^=\"deal-writeup-edit-\"]');\n   461→    const editCount = await editButtons.count();\n   462→\n   463→    if (editCount > 0) {\n   464→      // Click the last edit button (our newly created writeup)\n   465→      await editButtons.last().click();\n   466→\n   467→      // Should see the edit form (inline editing with save/cancel)\n   468→      // The inline edit mode replaces the writeup content with input fields\n   469→      await page.waitForTimeout(300);\n   470→    }\n   471→  });\n   472→\n   473→  test('DDP-WRT-05: Editing a writeup creates a version history entry', async ({ page }) => {\n   474→    // This test is covered by the combination of DDP-WRT-04 and DDP-WRT-06\n   475→    // Just verify the version counter can increment\n   476→    await navigateToFirstDealDetail(page);\n   477→\n   478→    const writeupsSection = page.getByTestId('deal-writeups-section');\n   479→    await expect(writeupsSection).toBeVisible();\n   480→  });\n   481→\n   482→  test('DDP-WRT-06: Version History button shows previous versions', async ({ page }) => {\n   483→    await navigateToFirstDealDetail(page);\n   484→\n   485→    const versionButtons = page.locator('[data-testid^=\"deal-writeup-versions-\"]');\n   486→    const count = await versionButtons.count();\n   487→\n   488→    if (count > 0) {\n   489→      await versionButtons.first().click();\n   490→\n   491→      const modal = page.getByTestId('version-history-modal');\n   492→      await expect(modal).toBeVisible();\n   493→      await expect(modal).toContainText('Version History');\n   494→\n   495→      // Close modal\n   496→      await modal.locator('button').first().click();\n   497→      await expect(modal).not.toBeVisible();\n   498→    }\n   499→  });\n   500→});\n   501→\n   502→test.describe('DealDetailPage - LinkedTasksSection (DDP-LTK)', () => {\n   503→  test('DDP-LTK-01: Linked tasks section shows deal-specific tasks', async ({ page }) => {\n   504→    await navigateToFirstDealDetail(page);\n   505→\n   506→    const tasksSection = page.getByTestId('deal-linked-tasks-section');\n   507→    await expect(tasksSection).toBeVisible();\n   508→    await expect(tasksSection).toContainText('Linked Tasks');\n   509→\n   510→    // Add Task button visible\n   511→    await expect(page.getByTestId('deal-linked-tasks-add-button')).toBeVisible();\n   512→    await expect(page.getByTestId('deal-linked-tasks-add-button')).toContainText('Add Task');\n   513→  });\n   514→\n   515→  test('DDP-LTK-02: Add Task button opens task creation form', async ({ page }) => {\n   516→    await navigateToFirstDealDetail(page);\n   517→\n   518→    await page.getByTestId('deal-linked-tasks-add-button').click();\n   519→\n   520→    const modal = page.getByTestId('add-deal-task-modal');\n   521→    await expect(modal).toBeVisible();\n   522→    await expect(modal).toContainText('Add Task');\n   523→\n   524→    // Form fields visible\n   525→    await expect(page.getByTestId('add-deal-task-title')).toBeVisible();\n   526→    await expect(page.getByTestId('add-deal-task-due-date')).toBeVisible();\n   527→    await expect(page.getByTestId('add-deal-task-priority')).toBeVisible();\n   528→\n   529→    // Save and Cancel buttons\n   530→    await expect(page.getByTestId('add-deal-task-save')).toBeVisible();\n   531→    await expect(page.getByTestId('add-deal-task-cancel')).toBeVisible();\n   532→\n   533→    // Close modal\n   534→    await page.getByTestId('add-deal-task-cancel').click();\n   535→    await expect(modal).not.toBeVisible();\n   536→  });\n   537→\n   538→  test('DDP-LTK-03: Creating a task adds it to linked tasks', async ({ page }) => {\n   539→    await navigateToFirstDealDetail(page);\n   540→\n   541→    // Open add task modal\n   542→    await page.getByTestId('deal-linked-tasks-add-button').click();\n   543→    const modal = page.getByTestId('add-deal-task-modal');\n   544→    await expect(modal).toBeVisible();\n   545→\n   546→    const taskTitle = `Test Task ${Date.now()}`;\n   547→    await page.getByTestId('add-deal-task-title').fill(taskTitle);\n   548→    await page.getByTestId('add-deal-task-save').click();\n   549→\n   550→    await expect(modal).not.toBeVisible();\n   551→    await page.waitForLoadState('networkidle');\n   552→\n   553→    // Task should appear in the linked tasks list\n   554→    const tasksSection = page.getByTestId('deal-linked-tasks-section');\n   555→    await expect(tasksSection).toContainText(taskTitle);\n   556→  });\n   557→\n   558→  test('DDP-LTK-04: Checking a task marks it complete', async ({ page }) => {\n   559→    await navigateToFirstDealDetail(page);\n   560→\n   561→    // First create a task to toggle\n   562→    await page.getByTestId('deal-linked-tasks-add-button').click();\n   563→    const modal = page.getByTestId('add-deal-task-modal');\n   564→    await expect(modal).toBeVisible();\n   565→\n   566→    const taskTitle = `Toggle Task ${Date.now()}`;\n   567→    await page.getByTestId('add-deal-task-title').fill(taskTitle);\n   568→    await page.getByTestId('add-deal-task-save').click();\n   569→    await expect(modal).not.toBeVisible();\n   570→    await page.waitForLoadState('networkidle');\n   571→\n   57