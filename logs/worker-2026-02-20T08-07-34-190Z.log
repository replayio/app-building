sible();\n   211→    await expect(page.getByTestId('pipeline-stage-qualification')).toBeVisible();\n   212→    await expect(page.getByTestId('pipeline-stage-discovery')).toBeVisible();\n   213→    await expect(page.getByTestId('pipeline-stage-proposal')).toBeVisible();\n   214→    await expect(page.getByTestId('pipeline-stage-negotiation')).toBeVisible();\n   215→    await expect(page.getByTestId('pipeline-stage-closed_won')).toBeVisible();\n   216→\n   217→    // Progress bar should be visible\n   218→    await expect(page.getByTestId('pipeline-progress-bar')).toBeVisible();\n   219→    await expect(page.getByTestId('pipeline-progress-fill')).toBeVisible();\n   220→\n   221→    // One stage should be marked as current\n   222→    const currentStage = page.locator('[data-current=\"true\"]');\n   223→    const currentCount = await currentStage.count();\n   224→    expect(currentCount).toBe(1);\n   225→  });\n   226→\n   227→  test('DDP-PIP-02: Pipeline updates when stage changes', async ({ page }) => {\n   228→    await navigateToFirstDealDetail(page);\n   229→\n   230→    // Get current stage\n   231→    const currentStage = await getFilterValue(page, 'deal-header-stage-select');\n   232→\n   233→    // Get the current indicator status\n   234→    const currentIndicator = page.getByTestId(`pipeline-stage-indicator-${currentStage}`);\n   235→    await expect(currentIndicator).toHaveAttribute('data-current', 'true');\n   236→\n   237→    // Change to a different stage\n   238→    const newStage = currentStage === 'discovery' ? 'proposal' : 'discovery';\n   239→    await selectFilterOption(page, 'deal-header-stage-select', newStage);\n   240→    const pipelineChangeBtn = page.getByTestId('deal-header-change-stage-button');\n   241→    await expect(pipelineChangeBtn).not.toBeDisabled({ timeout: 5000 });\n   242→    await pipelineChangeBtn.click();\n   243→    await page.waitForLoadState('networkidle');\n   244→\n   245→    // New stage should now be current\n   246→    const newIndicator = page.getByTestId(`pipeline-stage-indicator-${newStage}`);\n   247→    await expect(newIndicator).toHaveAttribute('data-current', 'true');\n   248→\n   249→    // Restore original stage\n   250→    await selectFilterOption(page, 'deal-header-stage-select', currentStage);\n   251→    await expect(pipelineChangeBtn).not.toBeDisabled({ timeout: 5000 });\n   252→    await pipelineChangeBtn.click();\n   253→    await page.waitForLoadState('networkidle');\n   254→  });\n   255→});\n   256→\n   257→test.describe('DealDetailPage - DealHistorySection (DDP-HIS)', () => {\n   258→  test('DDP-HIS-01: Deal history shows stage change log', async ({ page }) => {\n   259→    await navigateToFirstDealDetail(page);\n   260→\n   261→    const historySection = page.getByTestId('deal-history-section');\n   262→    await expect(historySection).toBeVisible();\n   263→\n   264→    // Should have the heading\n   265→    await expect(historySection).toContainText('Deal History');\n   266→\n   267→    // History entries or empty state\n   268→    const entries = page.locator('[data-testid^=\"deal-history-entry-\"]');\n   269→    const emptyState = page.getByTestId('deal-history-empty');\n   270→    const hasEntries = await entries.count() > 0;\n   271→    const hasEmpty = await emptyState.isVisible().catch(() => false);\n   272→\n   273→    // Either entries or empty state should be visible\n   274→    expect(hasEntries || hasEmpty).toBeTruthy();\n   275→\n   276→    if (hasEntries) {\n   277→      // Each entry should contain stage change text\n   278→      const firstEntry = entries.first();\n   279→      const entryText = await firstEntry.textContent();\n   280→      expect(entryText).toContain('Changed Stage from');\n   281→    }\n   282→  });\n   283→\n   284→  test('DDP-HIS-02: New stage change adds history entry', async ({ page }) => {\n   285→    await navigateToFirstDealDetail(page);\n   286→\n   287→    // Count current history entries\n   288→    const entriesBefore = page.locator('[data-testid^=\"deal-history-entry-\"]');\n   289→    const countBefore = await entriesBefore.count();\n   290→\n   291→    // Change stage to trigger new history entry\n   292→    const currentStage = await getFilterValue(page, 'deal-header-stage-select');\n   293→    const newStage = currentStage === 'discovery' ? 'proposal' : 'discovery';\n   294→    const stageDisplayNames: Record<string, string> = {\n   295→      'lead': 'Lead', 'qualification': 'Qualification', 'discovery': 'Discovery',\n   296→      'proposal': 'Proposal', 'negotiation': 'Negotiation', 'closed_won': 'Closed Won',\n   297→    };\n   298→    const expectedFrom = stageDisplayNames[currentStage] || currentStage;\n   299→    const expectedTo = stageDisplayNames[newStage] || newStage;\n   300→\n   301→    await selectFilterOption(page, 'deal-header-stage-select', newStage);\n   302→\n   303→    // Wait for the Change Stage button to become enabled after the stage selection\n   304→    const changeStageBtn = page.getByTestId('deal-header-change-stage-button');\n   305→    await expect(changeStageBtn).not.toBeDisabled({ timeout: 5000 });\n   306→    await changeStageBtn.click();\n   307→\n   308→    // Wait for a history entry matching this specific stage change to appear\n   309→    const expectedText = `Changed Stage from ${expectedFrom} to ${expectedTo}`;\n   310→    await expect(async () => {\n   311→      const entriesAfter = page.locator('[data-testid^=\"deal-history-entry-\"]');\n   312→      const countAfter = await entriesAfter.count();\n   313→      expect(countAfter).toBeGreaterThan(countBefore);\n   314→\n   315→      // Verify the specific stage change entry exists\n   316→      const matchingEntry = page.locator('[data-testid^=\"deal-history-entry-\"]', { hasText: expectedText });\n   317→      const matchCount = await matchingEntry.count();\n   318→      expect(matchCount).toBeGreaterThanOrEqual(1);\n   319→    }).toPass({ timeout: 10000 });\n   320→\n   321→    // Restore original stage\n   322→    await selectFilterOption(page, 'deal-header-stage-select', currentStage);\n   323→    await expect(changeStageBtn).not.toBeDisabled({ timeout: 5000 });\n   324→    await changeStageBtn.click();\n   325→    await page.waitForLoadState('networkidle');\n   326→  });\n   327→});\n   328→\n   329→test.describe('DealDetailPage - DealMetricsSection (DDP-MET)', () => {\n   330→  test('DDP-MET-01: Deal metrics displays probability and expected close', async ({ page }) => {\n   331→    await navigateToFirstDealDetail(page);\n   332→\n   333→    const metricsSection = page.getByTestId('deal-metrics-section');\n   334→    await expect(metricsSection).toBeVisible();\n   335→    await expect(metricsSection).toContainText('Deal Metrics');\n   336→\n   337→    // Probability should be visible with % sign\n   338→    const probability = page.getByTestId('deal-metrics-probability');\n   339→    await expect(probability).toBeVisible();\n   340→    const probText = await probability.textContent();\n   341→    expect(probText).toContain('%');\n   342→\n   343→    // Expected close date should be visible\n   344→    const closeDate = page.getByTestId('deal-metrics-close-date');\n   345→    await expect(closeDate).toBeVisible();\n   346→  });\n   347→\n   348→  test('DDP-MET-02: Metrics are editable', async ({ page }) => {\n   349→    await navigateToFirstDealDetail(page);\n   350→\n   351→    // Get current probability\n   352→    const probBefore = await page.getByTestId('deal-metrics-probability').textContent();\n   353→\n   354→    // Click edit\n   355→    await page.getByTestId('deal-metrics-edit-button').click();\n   356→\n   357→    // Input fields should appear\n   358→    const probInput = page.getByTestId('deal-metrics-probability-input');\n   359→    await expect(probInput).toBeVisible();\n   360→    const closeDateInput = page.getByTestId('deal-metrics-close-date-input');\n   361→    await expect(closeDateInput).toBeVisible();\n   362→\n   363→    // Change probability to 60\n   364→    await probInput.clear();\n   365→    await probInput.fill('60');\n   366→\n   367→    // Save\n   368→    await page.getByTestId('deal-metrics-save-button').click();\n   369→    await page.waitForLoadState('networkidle');\n   370→\n   371→    // Probability should update\n   372→    const probAfter = await page.getByTestId('deal-metrics-probability').textContent();\n   373→    expect(probAfter).toContain('60%');\n   374→\n   375→    // Restore original\n   376→    await page.getByTestId('deal-metrics-edit-button').click();\n   377→    const restoreInput = page.getByTestId('deal-metrics-probability-input');\n   378→    await restoreInput.clear();\n   379→    const originalProb = probBefore!.replace('%', '').trim();\n   380→    await restoreInput.fill(originalProb);\n   381→    await page.getByTestId('deal-metrics-save-button').click();\n   382→    await page.waitForLoadState('networkidle');\n   383→  });\n   384→});\n   385→\n   386→test.describe('DealDetailPage - WriteupsSection (DDP-WRT)', () => {\n   387→  test('DDP-WRT-01: Writeups section shows existing writeups', async ({ page }) => {\n   388→    await navigateToFirstDealDetail(page);\n   389→\n   390→    const writeupsSection = page.getByTestId('deal-writeups-section');\n   391→    await expect(writeupsSection).toBeVisible();\n   392→    await expect(writeupsSection).toContainText('Writeups');\n   393→\n   394→    // New Entry button visible\n   395→    await expect(page.getByTestId('deal-writeups-add-button')).toBeVisible();\n   396→    await expect(page.getByTestId('deal-writeups-add-button')).toContainText('New Entry');\n   397→  });\n   398→\n   399→  test('DDP-WRT-02: New Entry button opens writeup creation form', async ({ page }) => {\n   400→    await navigateToFirstDealDetail(page);\n   401→\n   402→    await page.getByTestId('deal-writeups-add-button').click();\n   403→\n   404→    const modal = page.getByTestId('add-writeup-modal');\n   405→    await ex