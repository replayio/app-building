ng;\n    95→  description: string;\n    96→  status: string;\n    97→}\n    98→\n    99→function AddSupplierModal({\n   100→  onClose,\n   101→  onSave,\n   102→}: {\n   103→  onClose: () => void;\n   104→  onSave: (data: AddSupplierForm) => Promise<void>;\n   105→}) {\n   106→  const [form, setForm] = useState<AddSupplierForm>({\n   107→    name: \"\",\n   108→    address: \"\",\n   109→    contact_name: \"\",\n   110→    phone: \"\",\n   111→    email: \"\",\n   112→    description: \"\",\n   113→    status: \"Active\",\n   114→  });\n   115→  const [errors, setErrors] = useState<Record<string, string>>({});\n   116→  const [saving, setSaving] = useState(false);\n   117→  const [statusOpen, setStatusOpen] = useState(false);\n   118→\n   119→  const update = (field: keyof AddSupplierForm, value: string) => {\n   120→    setForm((f) => ({ ...f, [field]: value }));\n   121→    if (errors[field]) setErrors((e) => ({ ...e, [field]: \"\" }));\n   122→  };\n   123→\n   124→  const handleSave = async () => {\n   125→    const newErrors: Record<string, string> = {};\n   126→    if (!form.name.trim()) newErrors.name = \"Supplier name is required\";\n   127→    if (Object.keys(newErrors).length > 0) {\n   128→      setErrors(newErrors);\n   129→      return;\n   130→    }\n   131→    setSaving(true);\n   132→    try {\n   133→      await onSave(form);\n   134→    } finally {\n   135→      setSaving(false);\n   136→    }\n   137→  };\n   138→\n   139→  return (\n   140→    <div className=\"modal-overlay\" data-testid=\"add-supplier-modal\" onClick={onClose}>\n   141→      <div className=\"modal-content\" onClick={(e) => e.stopPropagation()}>\n   142→        <div className=\"modal-header\">\n   143→          <h2 className=\"modal-title\">Add New Supplier</h2>\n   144→          <button className=\"modal-close-btn\" data-testid=\"add-supplier-modal-close\" onClick={onClose}>\n   145→            <svg width=\"16\" height=\"16\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentC