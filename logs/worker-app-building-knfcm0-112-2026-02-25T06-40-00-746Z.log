[2026-02-25T06:37:00.513Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_0172cmBkE7HhVyqyCySL4cvN","type":"tool_result","content":"     1→import React, { useState, useMemo } from \"react\";\n     2→import { useNavigate } from \"react-router-dom\";\n     3→import { useAppDispatch } from \"../hooks\";\n     4→import { createReport, deleteReport } from \"../slices/reportsSlice\";\n     5→import { Pagination } from \"../../../shared/components/Pagination\";\n     6→import { formatDateRange } from \"../../../shared/utils/date\";\n     7→import { generateCsv } from \"../../../shared/utils/csv\";\n     8→import type { Report } from \"../types\";\n     9→\n    10→const ITEMS_PER_PAGE = 10;\n    11→\n    12→const REPORT_TYPE_LABELS: Record<string, string> = {\n    13→  summary: \"Summary\",\n    14→  detailed: \"Detailed\",\n    15→  budget_vs_actual: \"Budget vs Actual\",\n    16→};\n    17→\n    18→interface ReportTableProps {\n    19→  reports: Report[];\n    20→  searchQuery: string;\n    21→}\n    22→\n    23→export function ReportTable({\n    24→  reports,\n    25→  searchQuery,\n    26→}: ReportTableProps): React.ReactElement {\n    27→  const navigate = useNavigate();\n    28→  const dispatch = useAppDispatch();\n    29→  const [sortAsc, setSortAsc] = useState(true);\n    30→  const [currentPage, setCurrentPage] = useState(1);\n    31→\n    32→  const filtered = useMemo(() => {\n    33→    if (!searchQuery.trim()) return reports;\n    34→    const q = searchQuery.toLowerCase();\n    35→    return reports.filter((r) => r.name.toLowerCase().includes(q));\n    36→  }, [reports, searchQuery]);\n    37→\n    38→  const sorted = useMemo(() => {\n    39→    const items = [...filtered];\n    40→    items.sort((a, b) => {\n    41→      const cmp = a.name.localeCompare(b.name);\n    42→      return sortAsc ? cmp : -cmp;\n    43→    });\n    44→    return items;\n    45→  }, [filtered, sortAsc]);\n    46→\n    47→  const totalPages = Math.max(1, Math.ceil(sorted.length / ITEMS_PER_PAGE));\n    48→  const safePage = Math.min(currentPage, totalPages);\n    49→  const paged = sorted.slice(\n    50→    (safePage - 1) * ITEMS_PER_PAGE,\n    51→    safePage * ITEMS_PER_PAGE\n    52→  );\n    53→\n    54→  // Reset to page 1 when search query changes\n    55→  React.useEffect(() => {\n    56→    setCurrentPage(1);\n    57→  }, [searchQuery]);\n    58→\n    59→  function handleToggleSort() {\n    60→    setSortAsc((prev) => !prev);\n    61→  }\n    62→\n    63→  async function handleRefresh(report: Report) {\n    64→    await dispatch(\n    65→      createReport({\n    66→        name: report.name,\n    67→        report_type: report.report_type,\n    68→        date_range_start: report.date_range_start,\n    69→        date_range_end: report.date_range_end,\n    70→        categories_filter: report.categories_filter,\n    71→        include_zero_balance: report.include_zero_balance,\n    72→      })\n    73→    ).unwrap();\n    74→    await dispatch(deleteReport(report.id));\n    75→  }\n    76→\n    77→  async function handleDownload(report: Report) {\n    78→    try {\n    79→      const res = await fetch(`/.netlify/functions/reports/${report.id}`);\n    80→      if (!res.ok) return;\n    81→      const data = await res.json();\n    82→      const items = (data.items || []) as Record<string, unknown>[];\n    83→      const csvData = generateCsv(\n    84→        items.map((item) => ({\n    85→          name: (item.item_name as string) ?? \"\",\n    86→          budget: String(item.budget_amount ?? 0),\n    87→          actual: String(item.actual_amount ?? 0),\n    88→          variance: String(item.variance ?? 0),\n    89→          variance_pct: `${item.variance_pct ?? 0}%`,\n    90→        })),\n    91→        [\n    92→          { header: \"Account / Item\", key: \"name\" },\n    93→          { header: \"Budget ($)\", key: \"budget\" },\n    94→          { header: \"Actual ($)\", key: \"actual\" },\n    95→         