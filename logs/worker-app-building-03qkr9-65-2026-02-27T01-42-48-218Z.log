\n   292→    monday += 7; // Second Monday\n   293→    const friday = monday + 4;\n   294→\n   295→    const monthStr = String(month + 1).padStart(2, \"0\");\n   296→    const startDate = `${year}-${monthStr}-${String(monday).padStart(2, \"0\")}T08:00:00Z`;\n   297→    const endDate = `${year}-${monthStr}-${String(friday).padStart(2, \"0\")}T16:00:00Z`;\n   298→\n   299→    const response = await page.request.post(\"/.netlify/functions/runs\", {\n   300→      data: {\n   301→        start_date: startDate,\n   302→        end_date: endDate,\n   303→        planned_quantity: 1000,\n   304→        unit: \"Units\",\n   305→        status: \"Scheduled\",\n   306→        notes: \"\",\n   307→      },\n   308→    });\n   309→    const run = await response.json();\n   310→\n   311→    try {\n   312→      await page.goto(\"/calendar\");\n   313→      await expect(page.getByTestId(\"calendar-page\")).toBeVisible({ timeout: 30000 });\n   314→\n   315→      // The event should appear on the start date\n   316→      const startCell = page.getByTestId(\n   317→        `calendar-day-${year}-${monthStr}-${String(monday).padStart(2, \"0\")}`\n   318→      );\n   319→      const eventCard = startCell.locator(`[data-testid=\"calendar-event-${run.id}\"]`);\n   320→      await expect(eventCard).toBeVisible({ timeout: 15000 });\n   321→\n   322→      // Multi-day event should have the span class\n   323→      await expect(eventCard).toHaveClass(/calendar-event-card--span/);\n   324→\n   325→      // The event should NOT be rendered separately in the intermediate day cells (Tue-Thu)\n   326→      for (let d = monday + 1; d < friday; d++) {\n   327→        const intermediateCell = page.getByTestId(\n   328→          `calendar-day-${year}-${monthStr}-${String(d).padStart(2, \"0\")}`\n   329→        );\n   330→        await expect(\n   331→          intermediateCell.locator(`[data-testid=\"calendar-event-${run.id}\"]`)\n   332→        ).toHaveCount(0);\n   333→      }\n   334→    } finally {