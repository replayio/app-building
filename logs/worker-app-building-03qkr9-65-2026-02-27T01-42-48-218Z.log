ify/functions/runs\", {\n   374→      data: {\n   375→        start_date: `${dateStr}T08:00:00Z`,\n   376→        end_date: `${dateStr}T12:00:00Z`,\n   377→        planned_quantity: 100,\n   378→        unit: \"Units\",\n   379→        status: \"Scheduled\",\n   380→        notes: \"First run\",\n   381→      },\n   382→    });\n   383→    const run1 = await response1.json();\n   384→\n   385→    const response2 = await page.request.post(\"/.netlify/functions/runs\", {\n   386→      data: {\n   387→        start_date: `${dateStr}T13:00:00Z`,\n   388→        end_date: `${dateStr}T17:00:00Z`,\n   389→        planned_quantity: 200,\n   390→        unit: \"Units\",\n   391→        status: \"Scheduled\",\n   392→        notes: \"Second run\",\n   393→      },\n   394→    });\n   395→    const run2 = await response2.json();\n   396→\n   397→    try {\n   398→      await page.goto(\"/calendar\");\n   399→      await expect(page.getByTestId(\"calendar-page\")).toBeVisible({ timeout: 30000 });\n   400→\n   401→      // Both events should be visible in the same day cell\n   402→      const dayCell = page.getByTestId(`calendar-day-${dateStr}`);\n   403→      await expect(dayCell).toBeVisible();\n   404→\n   405→      await expect(\n   406→        dayCell.locator(`[data-testid=\"calendar-event-${run1.id}\"]`)\n   407→      ).toBeVisible({ timeout: 15000 });\n   408→      await expect(\n   409→        dayCell.locator(`[data-testid=\"calendar-event-${run2.id}\"]`)\n   410→      ).toBeVisible({ timeout: 15000 });\n   411→\n   412→      // Both events should be stacked (both visible, count >= 2)\n   413→      const eventsInCell = dayCell.locator('[data-testid^=\"calendar-event-\"]');\n   414→      const count = await eventsInCell.count();\n   415→      expect(count).toBeGreaterThanOrEqual(2);\n   416→    } finally {\n   417→      await page.request.delete(`/.netlify/functions/runs/${run1.id}`);\n   418→      await page.request.delete(`/.netlify/functions/runs/${run2.id}`);\n   419→    }\n   420→  });\n   421→\n   422→  test(\"CAL-GRID-13: Empty day cells show no event cards\", async ({ page }) => {\n   423→    await page.goto(\"/calendar\");\n   424→    await expect(page.getByTestId(\"calendar-page\")).toBeVisible({ timeout: 30000 });\n   425→\n   426→    // Wait for events to load (at least one event should exist on some day)\n   427→    await expect(\n   428→      page.locator('[data-testid^=\"calendar-event-\"]').first()\n   429→    ).toBeVisible({ timeout: 15000 });\n   430→\n   431→    // Find a day cell in the current month that has no events\n   432→    const now = new Date();\n   433→    const year = now.getFullYear();\n   434→    const month = String(now.getMonth() + 1).padStart(2, \"0\");\n   435→\n   436→    // Day 1 is unlikely to have seed data events (seed data is on specific dates)\n   437→    // Check that a day cell with no events has zero event cards\n   438→    const emptyCell = page.getByTestId(`calendar-day-${year}-${month}-01`);\n   439→    await expect(emptyCell).toBeVisible();\n   440→    await expect(\n   441→      emptyCell.locator('[data-testid^=\"calendar-event-\"]')\n   442→    ).toHaveCount(0);\n   443→  });\n   444→\n   445→  test(\"CAL-GRID-14: Drag-and-drop an event card to reschedule a run\", async ({ page }) => {\n   446→    test.slow();\n   447→\n   448→    // Create a run on a known date for drag testing\n   449→    const now = new Date();\n   450→    const year = now.getFullYear();\n   451→    const month = String(now.getMonth() + 1).padStart(2, \"0\");\n   452→    const sourceDay = \"16\";\n   453→    const targetDay = \"15\";\n   454→\n   455→    const response = await page.request.post(\"/.netlify/functions/runs\", {\n   456→      data: {\n   457→        start_date: `${year}-${month}-${sourceDay}T08:00:00Z`,\n   458→        end_date: `${year}-${month}-${sourceDay}T16:00:00Z`,\n   459→        planned_quantity: 300,\n   460→        unit: \"Units\",\n   461→        status: \"Scheduled\",\n   462→        notes: \"\",\n   463→      },\n   464→    });\n   465→    const run = await response.json();\n   466→\n   467→    try {\n   468→      await page.goto(\"/calendar\");\n   469→      await expect(page.getByTestId(\"calendar-page\")).toBeVisible({ timeout: 30000 });\n   470→\n   471→      const eventCard = page.getByTestId(`calendar-event-${run.id}`);\n   472→      await expect(eventCard).toBeVisible({ timeout: 15000 });\n   473→\n   474→      const targetCell = page.getByTestId(`calendar-day-${year}-${month}-${targetDay}`);\n   475→\n   476→      // Drag event from source to target\n   477→      await eventCard.dragTo(targetCell);\n   478→\n   479→      // Reschedule tooltip should appear\n   480→      await expect(page.getByTestId(\"reschedule-tooltip\")).toBeVisible({ timeout: 15000 });\n   481→      await expect(page.getByTestId(\"reschedule-tooltip-text\")).toBeVisible();\n   482→\n   483→      // Confirm the reschedule\n   484→      await page.getByTestId(\"reschedule-confirm-btn\").click();\n   485→\n   486→      // Tooltip should close\n   487→      await expect(page.getByTestId(\"reschedule-tooltip\")).toBeHidden({ timeout: 15000 });\n   488→\n   489→      // Event should now be on the target date\n   490→      await expect(\n   491→        page.getByTestId(`calendar-day-${year}-${month}-${targetDay}`)\n   492→          .locator(`[data-testid=\"calendar-event-${run.id}\"]`)\n   493→      ).toBeVisible({ timeout: 15000 });\n   494→\n   495→      // Verify persistence by reloading\n   496→      await page.goto(\"/calendar\");\n   497→      await expect(page.getByTestId(\"calendar-page\")).toBeVisible({ timeout: 30000 });\n   498→\n   499→      await expect(\n   500→        page.getByTestId(`calendar-day-${year}-${month}-${targetDay}`)\n   501→          .locator(`[data-testid=\"calendar-event-${run.id}\"]`)\n   502→      ).toBeVisible({ timeout: 15000 });\n   503→    } finally {\n   504→      await page.request.delete(`/.netlify/functions/runs/${run.id}`);\n   505→    }\n   506→  });\n   507→\n   508→  test(\"CAL-GRID-15: Drag-and-drop reschedule can be cancelled\", async ({ page }) => {\n   509→    test.slow();\n   510→    // Create a run for drag testing\n   511→    const now = new Date();\n   512→    const year = now.getFullYear();\n   513→    const month = String(now.getMonth() + 1).padStart(2, \"0\");\n   514→    const sourceDay = \"18\";\n   515→    const targetDay = \"19\";\n   516→\n   517→    const response = await page.request.post(\"/.netlify/functions/runs\", {\n   518→      data: {\n   519→        start_date: `${year}-${month}-${sourceDay}T08:00:00Z`,\n   520→        end_date: `${year}-${month}-${sourceDay}T16:00:00Z`,\n   521→        planned_quantity: 300,\n   522→        unit: \"Units\",\n   523→        status: \"Scheduled\",\n   524→        notes: \"\",\n   525→      },\n   526→    });\n   527→    const run = await response.json();\n   528→\n   529→    try {\n   530→      await page.goto(\"/calendar\");\n   531→      await expect(page.getByTestId(\"calendar-page\")).toBeVisible({ timeout: 30000 });\n   532→\n   533→      const eventCard = page.getByTestId(`calendar-event-${run.id}`);\n   534→      await expect(eventCard).toBeVisible({ timeout: 15000 });\n   535→\n   536→      const targetCell = page.getByTestId(`calendar-day-${year}-${month}-${targetDay}`);\n   537→      await expect(targetCell).toBeVisible({ timeout: 15000 });\n   538→\n   539→      // Use manual HTML5 drag event dispatch for Replay browser reliability\n   540→      const sourceTestId = `calendar-event-${run.id}`;\n   541→      const targetTestId = `calendar-day-${year}-${month}-${targetDay}`;\n   542→      await page.evaluate(({ sourceId, targetId }) => {\n   543→        const source = document.querySelector(`[data-testid=\"${sourceId}\"]`)!;\n   544→        const target = document.querySelector(`[data-testid=\"${targetId}\"]`)!;\n   545→        const targetRect = target.getBoundingClientRect();\n   546→\n   547→        const dataTransfer = 