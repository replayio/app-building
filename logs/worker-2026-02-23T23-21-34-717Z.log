[2026-02-23T22:08:59.778Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_014evAb2VCsm4MU7sBwidQRi","type":"tool_result","content":"     1→import { execSync } from 'child_process'\n     2→import { readFileSync, existsSync } from 'fs'\n     3→import { resolve, join } from 'path'\n     4→import { homedir } from 'os'\n     5→import { initSchema, runMigrations } from './schema.ts'\n     6→\n     7→const NEON_API_BASE = 'https://console.neon.tech/api/v2'\n     8→\n     9→function parseEnvFile(filePath: string): Record<string, string> {\n    10→  const env: Record<string, string> = {}\n    11→  if (!existsSync(filePath)) return env\n    12→  const content = readFileSync(filePath, 'utf-8')\n    13→  for (const line of content.split('\\n')) {\n    14→    const trimmed = line.trim()\n    15→    if (!trimmed || trimmed.startsWith('#')) continue\n    16→    const eqIndex = trimmed.indexOf('=')\n    17→    if (eqIndex === -1) continue\n    18→    const key = trimmed.slice(0, eqIndex).trim()\n    19→    let value = trimmed.slice(eqIndex + 1).trim()\n    20→    if ((value.startsWith('\"') && value.endsWith('\"')) || (value.startsWith(\"'\") && value.endsWith(\"'\"))) {\n    21→      value = value.slice(1, -1)\n    22→    }\n    23→    env[key] = value\n    24→  }\n    25→  return env\n    26→}\n    27→\n    28→async function neonApi(path: string, apiKey: string, options?: { method?: string; body?: unknown }) {\n    29→  const res = await fetch(`${NEON_API_BASE}${path}`, {\n    30→    method: options?.method || 'GET',\n    31→    headers: {\n    32→      'Authorization': `Bearer ${apiKey}`,\n    33→      'Content-Type': 'application/json',\n    34→    },\n    35→    body: options?.body ? JSON.stringify(options.body) : undefined,\n    36→  })\n    37→  if (!res.ok) {\n    38→    const text = await res.text()\n    39→    throw new Error(`Neon API error ${res.status}: ${text}`)\n    40→  }\n    41→  if (res.status === 204) return null\n    42→  return res.json()\n    43→}\n    44→\n    45→function killStaleProcesses() {\n    46→  try { execSync('pkill -f \"netlify\" 2>/dev/null || true', { stdio: 'ignore' }) } catch { /* ok */ }\n    47→  try { execSync('pkill -f \"vite\" 2>/dev/null || true', { stdio: 'ignore' }) } catch { /* ok */ }\n    48→}\n    49→\n    50→async function cleanupStaleBranches(projectId: string, apiKey: string) {\n    51→  const data = await neonApi(`/projects/${projectId}/branches`, apiKey)\n    52→  const branches = (data as { branches: Array<{ id: string; name: string }> }).branches\n    53→  for (const branch of branches) {\n    54→    if (branch.name.startsWith('test-worker-')) {\n    55→      console.log(`Deleting stale branch: ${branch.name}`)\n    56→      await neonApi(`/projects/${projectId}/branches/${branch.id}`, apiKey, { method: 'DELETE' })\n    57→    }\n    58→  }\n    59→}\n    60→\n    61→async function createEphemeralBranch(projectId: string, apiKey: string): Promise<{ branchId: string; databaseUrl: string }> {\n    62→  const runId = Date.now().toString(36) + Math.random().toString(36).slice(2, 8)\n    63→  const branchName = `test-worker-${runId}`\n    64→\n    65→  const data = await neonApi(`/projects/${projectId}/branches`, apiKey, {\n    66→    method: 'POST',\n    67→    body: {\n    68→      branch: { name: branchName },\n    69→      endpoints: [{ type: 'read_write' }],\n    70→    },\n    71→  })\n    72→\n    73→  const result = data as {\n    74→    branch: { id: string }\n    75→    connection_uris: Array<{ connection_uri: string }>\n    76→  }\n    77→  const branchId = result.branch.id\n    78→  const connectionUri = result.connection_uris[0].connection_uri\n    79→\n    80→  console.log(`Created ephemeral branch: ${branchName} (${branchId})`)\n    81→  return { branchId, databaseUrl: connectionUri }\n    82→}\n    83→\n    84→async function deleteBranch(projectId: string, branchId: string, apiKey: string) {\n    85→  try {\n    86→    await neonApi(`/projects/${projectId}/branches/${branchId}`, apiKey, { method: 'DELETE' })\n    87→    console.log(`Deleted ephemeral branch: ${branchId}`)\n    88→  } catch (err) {\n    89→    console.error(`Failed to delete branch ${branchId}:`, err)\n    90→  }\n    91→}\n    92→\n    93→interface RecordingInfo {\n    94→  id: string\n    95→  metadata?: { test?: { result?: string; title?: string; file?: string } }\n    96→  duration: number\n    97→  writeFinished: boolean\n    98→}\n    99→\n   100→function parseRecordingsLog(): RecordingInfo[] {\n   101→  const logPath = join(homedir(), '.replay', 'recordings.log')\n   102→  if (!existsSync(logPath)) return []\n   103→  const content = readFileSync(logPath, 'utf-8')\n   104→  const recordings = new Map<string, RecordingInfo>()\n   105→\n   106→  for (const line of content.split('\\n')) {\n   107→    const trimmed = line.trim()\n   108→    if (!trimmed) continue\n   109→    try {\n   110→      const entry = JSON.parse(trimmed) as {\n   111→        kind: string\n   112→        id?: string\n   113→        metadata?: RecordingInfo['metadata']\n   114→        duration?: number\n   115→      }\n   116→      if (!entry.id) continue\n   117→\n   118→      if (entry.kind === 'createRecording') {\n   119→        recordings.set(entry.id, { id: entry.id, duration: entry.duration ?? 0, writeFinished: false })\n   120→      } else if (entry.kind === 'addMetadata') {\n   121→        const rec = recordings.get(entry.id)\n   122→        if (rec && entry.metadata) {\n   123→          rec.metadata = { ...rec.metadata, ...entry.metadata }\n   124→        }\n   125→      } else if (entry.kind === 'writeFinished') {\n   126→        const rec = recordings.get(entry.id)\n   127→        if (rec) {\n   128→          rec.writeFinished = true\n   129→          if (entry.duration !== undefined) rec.duration = entry.duration\n   130→        }\n   131→      }\n   132→    } catch { /* skip malformed lines */ }\n   133→  }\n   134→\n   135→  return [...recordings.values()]\n   136→}\n   137→\n   138→function handleFailedRecordings() {\n   139→  const recordings = parseRecordingsLog()\n   140→\n   141→  const withTestMetadata = recordings.filter(r => r.metadata?.test)\n   142→  if (withTestMetadata.length > 0) {\n   143→    console.log('=== REPLAY RECORDINGS METADATA ===')\n   144→    for (const rec of withTestMetadata) {\n   145→      console.log(JSON.stringify({\n   146→        id: rec.id,\n   147→        testResult: rec.metadata?.test?.result,\n   148→        testTitle: rec.metadata?.test?.title,\n   149→        specFile: rec.metadata?.test?.file,\n   150→      }))\n   151→    }\n   152→    console.log('=== END REPLAY RECORDINGS METADATA ===')\n   153→  }\n   154→\n   155→  const failedRecordings = recordings\n   156→    .filter(r => r.writeFinished && (r.metadata?.test?.result === 'failed' || r.metadata?.test?.result === 'timedOut'))\n   157→    .sort((a, b) => b.duration - a.duration)\n   158→\n   159→  if (failedRecordings.length > 0) {\n   160→    const toUpload = failedRecordings[0]\n   161→    console.log(`Uploading recording ${toUpload.id}...`)\n   162→    try {\n   163→      execSync(`npx replayio upload ${toUpload.id}`, { stdio: 'inherit' })\n   164→      console.log(`REPLAY UPLOADED: ${toUpload.id}`)\n   165→    } catch (err) {\n   166→      const exitCode = (err as { status?: number }).status ?? 1\n   167→      console.error(`Upload failed with exit code ${exitCode}`)\n   168→    }\n   169→  }\n   170→}\n   171→\n   172→async function main() {\n   173→  const testFile = process.argv[2]\n   174→  if (!testFile) {\n   175→    console.error('Usage: npm run test <test-file>')\n   176→    process.exit(1)\n   177→  }\n   178→\n   179→  const envVars = parseEnvFile(resolve('.env'))\n   180→  const neonApiKey = process.env.NEON_API_KEY\n   181→  const projectId = envVars.NEON_PROJECT_ID || process.env.NEON_PROJECT_ID\n   182→\n   183→  if (!neonApiKey) { console.error('Missing NEON_API_KEY env var'); process.exit(1) }\n   184→  if (!process.env.RECORD_REPLAY_API_KEY) { console.error('Missing RECORD_REPLAY_API_KEY env var'); process.exit(1) }\n   185→  if (!projectId) { console.error('Missing NEON_PROJECT_ID in .env'); process.exit(1) }\n   186→\n   187→  // Step 1: Kill stale processes\n   188→  console.log('Killing stale processes...')\n   189→  killStaleProcesses()\n   190→\n   191→  // Step 2: Clean up stale Neon branches\n   192→  console.log('Cleaning up stale Neon branches...')\n   193→  await cleanupStaleBranches(projectId, neonApiKey)\n   194→\n   195→  // Step 3: Create ephemeral Neon branch\n   196→  console.log('Creating ephemeral Neon branch...')\n   197→  const { branchId, databaseUrl } = await createEphemeralBranch(projectId, neonApiKey)\n   198→\n   199→  let playwrightExitCode = 0\n   200→\n   201→  try {\n   202→    // Step 4: Initialize schema, run migrations, seed\n   203→    console.log('Initializing schema...')\n   204→    await initSchema(databaseUrl)\n   205→    await runMigrations(databaseUrl)\n   206→\n   207→    try {\n   208→      const seedModule = await import('./seed-db.ts') as { seed?: (url: string) => Promise<void> }\n   209→      if (seedModule.seed) {\n   210→        console.log('Seeding test data...')\n   211→        await seedModule.seed(databaseUrl)\n   212→      }\n   213→    } catch { /* seed-db not available, skip seeding */ }\n   214→\n   215→    // Step 5: Remove stale recordings\n   216→    console.log('Removing stale recordings...')\n   217→    try { execSync('npx replayio remove --all', { stdio: 'inherit' }) } catch { /* ok */ }\n   218→\n   219→    // Step 6: Run Playwright\n   220→    console.log(`Running Playwright tests: ${testFile}`)\n   221→    try {\n   222→      execSync(`npx playwright test ${testFile} --retries 0`, {\n   223→        stdio: 'inherit',\n   224→        env: { ...process.env, DATABASE_URL: databaseUrl },\n   225→      })\n   226→    } catch (err) {\n   227→      playwrightExitCode = (err as { status?: number }).status || 1\n   228→    }\n   229→\n   230→    // Step 7: On failure, handle recordings\n   231→    if (playwrightExitCode !== 0) {\n   232→      console.log('Tests failed. Processing recordings...')\n   233→      handleFailedRecordings()\n   234→    }\n   235→  } finally {\n   236→    // Step 8: Clean up\n   237→    console.log('Cleaning up...')\n   238→    await deleteBranch(projectId, branchId, neonApiKey)\n   239→    try { execSync('npx replayio remove --all', { stdio: 'inherit' }) } catch { /* ok */ }\n   240→  }\n   241→\n   242→  // Step 9: Exit with Playwright exit code\n   243→  process.exit(playwrightExitCode)\n   244→}\n   245→\n   246→main().catch(err => {\n   247→  console.error('Test script failed:', err)\n   248→  process.exit(1)\n   249→})\n   250→"}]},"parent_tool_use_id":"toolu_019N93oy2EH6Kn9Hhv6K9fxD","session_id":"70075956-0811-4938-aaa4-d7cd4828e07e","uuid":"3d1b39ab-cb90-4872-bfd3-8f66e49905ec"}
