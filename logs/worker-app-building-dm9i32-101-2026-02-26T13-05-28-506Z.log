ities', -2500.00, 'Visa', 'Primary credit card',\n    40→       5000.00, 3500.00, 8500.00, 6000.00,\n    41→       NULL, NULL, NULL, NULL, NULL, 7143.00, NULL),\n    42→      ('Mortgage Loan', '2002', 'liabilities', -78000.00, 'Wells Fargo', 'Home mortgage',\n    43→       0, 0, 0, 0,\n    44→       NULL, NULL, NULL, NULL, 3.5, NULL, 1200.00),\n    45→      ('Car Loan', '2003', 'liabilities', -4700.00, 'Toyota Financial', 'Auto loan',\n    46→       0, 0, 0, 0,\n    47→       NULL, NULL, NULL, NULL, NULL, NULL, NULL),\n    48→\n    49→    -- ---- Equity ----\n    50→      ('Retained Earnings', '3001', 'equity', 50000.00, NULL, 'Accumulated retained earnings',\n    51→       0, 0, 0, 0,\n    52→       NULL, NULL, NULL, NULL, NULL, NULL, NULL),\n    53→      ('Owner''s Equity', '3002', 'equity', 17250.00, NULL, 'Owner equity stake',\n    54→       0, 0, 0, 0,\n    55→       NULL, NULL, NULL, NULL, NULL, NULL, NULL),\n    56→\n    57→    -- ---- Revenue ----\n    58→      ('Service Revenue', '4001', 'revenue', 80000.00, NULL, 'Revenue from services',\n    59→       0, 0, 0, 0,\n    60→       NULL, NULL, NULL, NULL, NULL, NULL, NULL),\n    61→      ('Product Revenue', '4002', 'revenue', 30000.00, NULL, 'Revenue from product sales',\n    62→       0, 0, 0, 0,\n    63→       NULL, NULL, NULL, NULL, NULL, NULL, NULL),\n    64→\n    65→    -- ---- Expenses ----\n    66→      ('Operating Expenses', '5001', 'expenses', 35000.00, NULL, 'General operating expenses',\n    67→       0, 0, 0, 0,\n    68→       NULL, NULL, NULL, NULL, NULL, NULL, NULL),\n    69→      ('Payroll Expenses', '5002', 'expenses', 20500.00, NULL, 'Employee payroll',\n    70→       0, 0, 0, 0,\n    71→       NULL, NULL, NULL, NULL, NULL, NULL, NULL)\n    72→  `;\n    73→\n    74→  // Get account IDs for foreign key references\n    75→  const checkingRows = await sql`SELECT id FROM accounts WHERE name = 'Checking Account'`;\n    76→  const checkingId = checkingRows[0].id;\n    77→  const opexRows = await sql`SELECT id FROM accounts WHERE name = 'Operating Expenses'`;\n    78→  const opexId = opexRows[0].id;\n    79→  const serviceRevRows = await sql`SELECT id FROM accounts WHERE name = 'Service Revenue'`;\n    80→  const serviceRevId = serviceRevRows[0].id;\n    81→\n    82→  // Seed transactions for Checking Account\n    83→  const txn1 = await sql`\n    84→    INSERT INTO transactions (date, description, currency)\n    85→    VALUES ('2023-10-25', 'Grocery Store', 'USD') RETURNING id\n    86→  `;\n    87→  const txn2 = await sql`\n    88→    INSERT INTO transactions (date, description, currency)\n    89→    VALUES ('2023-10-24', 'Salary Deposit', 'USD') RETURNING id\n    90→  `;\n    91→  const txn3 = await sql`\n    92→    INSERT INTO transactions (date, description, currency)\n    93→    VALUES ('2023-10-22', 'Electric Bill', 'USD') RETURNING id\n    94→  `;\n    95→\n    96→  // Seed transaction entries (double-entry)\n    97→  await sql`INSERT INTO transaction_entries (transaction_id, account_id, entry_type, amount)\n    98→    VALUES (${txn1[0].id}, ${checkingId}, 'debit', 125.50)`;\n    99→  await sql`INSERT INTO transaction_entries (transaction_id, account_id, entry_type, amount)\n   100→    VALUES (${txn1[0].id}, ${opexId}, 'credit', 125.50)`;\n   101→\n   102→  await sql`INSERT INTO transaction_entries (transaction_id, account_id, entry_type, amount)\n   103→    VALUES (${txn2[0].id}, ${checkingId}, 'credit', 3200.00)`;\n   104→  await sql`INSERT INTO transaction_entries (transaction_id, account_id, entry_type, amount)\n   105→    VALUES (${txn2[0].id}, ${serviceRevId}, 'debit', 3200.00)`;\n   106→\n   107→  await sql`INSERT INTO transaction_entries (transaction_id, account_id, entry_type, amount)\n   108→    VALUES (${txn3[0].id}, ${checkingId}, 'debit', 85.00)`;\n   109→  await sql`INSERT INTO transaction_entries (transaction_id, account_id, entry_type, amount)\n   110→    VALUES (${txn3[0].id}, ${opexId}, 'credit', 85.00)`;\n   111→\n   112→  // Seed tag