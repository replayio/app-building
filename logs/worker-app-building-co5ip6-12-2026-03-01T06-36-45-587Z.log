/json' },\n    45→    })\n    46→  }\n    47→\n    48→  const sql = neon(process.env.DATABASE_URL!)\n    49→  const payload = body.payload || {}\n    50→  const newStatus = body.status || null\n    51→\n    52→  if (newStatus) {\n    53→    await sql`\n    54→      INSERT INTO containers (container_id, name, status, last_event_at)\n    55→      VALUES (${body.container_id}, ${body.container_id}, ${newStatus}, NOW())\n    56→      ON CONFLICT (container_id)\n    57→      DO UPDATE SET status = ${newStatus}, last_event_at = NOW()\n    58→    `\n    59→  } else {\n    60→    await sql`\n    61→      INSERT INTO containers (container_id, name, status, last_event_at)\n    62→      VALUES (${body.container_id}, ${body.container_id}, 'starting', NOW())\n    63→      ON CONFLICT (container_id)\n    64→      DO UPDATE SET last_event_at = NOW()\n    65→    `\n    66→  }\n    67→\n    68→  await sql`\n    69→    INSERT INTO webhook_events (container_id, event_type, payload)\n    70→    VALUES (${body.container_id}, ${body.event_type}, ${JSON.stringify(payload)})\n    71→  `\n    72→\n    73→  return new Response(JSON.stringify({ success: true }), {\n    74→    status: 200,\n    75→    headers: { 'Content-Type': 'application/json' },\n    76→  })\n    77→}\n    78→"}]},"parent_tool_use_id":"toolu_019ASLF9TmabKQ9ujfhJ37p4","session_id":"08e4eb64-0001-4076-b414-5268e401dc39","uuid":"58d58902-ad65-4832-9560-af597752c949"}
