onse(JSON.stringify({ error: 'Unauthorized' }), {\n    45→      status: 401,\n    46→      headers: { 'Content-Type': 'application/json' },\n    47→    })\n    48→  }\n    49→\n    50→  let body: { prompt?: string }\n    51→  try {\n    52→    body = await request.json()\n    53→  } catch {\n    54→    return new Response(JSON.stringify({ error: 'Invalid JSON body' }), {\n    55→      status: 400,\n    56→      headers: { 'Content-Type': 'application/json' },\n    57→    })\n    58→  }\n    59→\n    60→  const prompt = body.prompt\n    61→  if (!prompt || typeof prompt !== 'string') {\n    62→    return new Response(JSON.stringify({ error: 'Missing or invalid \"prompt\" field' }), {\n    63→      status: 400,\n    64→      headers: { 'Content-Type': 'application/json' },\n    65→    })\n    66→  }\n    67→\n    68→  try {\n    69→    await sql`\n    70→      INSERT INTO settings (key, value)\n    71→      VALUES ('default_prompt', ${prompt})\n    72→      ON CONFLICT (key) DO UPDATE SET value = ${prompt}\n    73→    `\n    74→\n    75→    return new Response(JSON.stringify({ success: true, prompt }), {\n    76→      status: 200,\n    77→      headers: { 'Content-Type': 'application/json' },\n    78→    })\n    79→  } catch (err) {\n    80→    const message = err instanceof Error ? err.message : String(err)\n    81→    return new Response(JSON.stringify({ error: 'Failed to set default prompt', detail: message }), {\n    82→      status: 500,\n    83→      headers: { 'Content-Type': 'application/json' },\n    84→    })\n    85→  }\n    86→}\n    87→"}]},"parent_tool_use_id":"toolu_019ASLF9TmabKQ9ujfhJ37p4","session_id":"08e4eb64-0001-4076-b414-5268e401dc39","uuid":"91966b18-d384-4604-86c6-759a6166fc21"}
