[2026-02-26T14:12:38.586Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01GzHawW1SNTcbvUW4aBg2fH","type":"tool_result","content":"     1→import { createSlice, createAsyncThunk, type PayloadAction } from \"@reduxjs/toolkit\";\n     2→\n     3→export interface Task {\n     4→  id: string;\n     5→  title: string;\n     6→  description: string | null;\n     7→  dueDate: string | null;\n     8→  priority: string;\n     9→  status: string;\n    10→  clientId: string | null;\n    11→  clientName: string | null;\n    12→  dealId: string | null;\n    13→  dealName: string | null;\n    14→  assigneeId: string | null;\n    15→  assigneeName: string | null;\n    16→  assigneeAvatar: string | null;\n    17→  createdAt: string;\n    18→  updatedAt: string;\n    19→}\n    20→\n    21→export interface TaskClient {\n    22→  id: string;\n    23→  name: string;\n    24→}\n    25→\n    26→export interface TaskUser {\n    27→  id: string;\n    28→  name: string;\n    29→  email: string;\n    30→}\n    31→\n    32→export interface TaskDeal {\n    33→  id: string;\n    34→  name: string;\n    35→}\n    36→\n    37→interface TasksState {\n    38→  items: Task[];\n    39→  clients: TaskClient[];\n    40→  users: TaskUser[];\n    41→  clientDeals: TaskDeal[];\n    42→  loading: boolean;\n    43→  error: string | null;\n    44→}\n    45→\n    46→const initialState: TasksState = {\n    47→  items: [],\n    48→  clients: [],\n    49→  users: [],\n    50→  clientDeals: [],\n    51→  loading: false,\n    52→  error: null,\n    53→};\n    54→\n    55→function getAuthHeaders(): Record<string, string> {\n    56→  const token = localStorage.getItem(\"auth_token\");\n    57→  if (token) {\n    58→    return { Authorization: `Bearer ${token}` };\n    59→  }\n    60→  return {};\n    61→}\n    62→\n    63→export const fetchAllTasks = createAsyncThunk(\"tasks/fetchAll\", async () => {\n    64→  const resp = await fetch(\"/.netlify/functions/tasks\");\n    65→  if (!resp.ok) {\n    66→    const data = await resp.json();\n    67→    throw new Error(data.error || \"Failed to fetch tasks\");\n    68→  }\n    69→  return resp.json() as Promise<Task[]>;\n    70→});\n    71→\n    72→export const fetchTaskClients = createAsyncThunk(\"tasks/fetchClients\", async () => {\n    73→  const resp = await fetch(\"/.netlify/functions/clients\");\n    74→  if (!resp.ok) throw new Error(\"Failed to fetch clients\");\n    75→  const data = await resp.json();\n    76→  return (data as { id: string; name: string }[]).map((c) => ({ id: c.id, name: c.name }));\n    77→});\n    78→\n    79→export const fetchTaskUsers = createAsyncThunk(\"tasks/fetchUsers\", async () => {\n    80→  const resp = await fetch(\"/.netlify/functions/users\");\n    81→  if (!resp.ok) throw new Error(\"Failed to fetch users\");\n    82→  return resp.json() as Promise<TaskUser[]>;\n    83→});\n    84→\n    85→export const fetchClientDeals = createAsyncThunk(\"tasks/fetchClientDeals\", async (clientId: string) => {\n    86→  const resp = await fetch(`/.netlify/functions/deals?clientId=${encodeURIComponent(clientId)}`);\n    87→  if (!resp.ok) throw new Error(\"Failed to fetch deals\");\n    88→  const data = await resp.json();\n    89→  return (data as { id: string; name: string }[]).map((d) => ({ id: d.id, name: d.name }));\n    90→});\n    91→\n    92→export const createTask = createAsyncThunk(\n    93→  \"tasks/create\",\n    94→  async (task: {\n    95→    title: string;\n    96→    description?: string;\n    97→    dueDate?: string;\n    98→    priority?: string;\n    99→    clientId?: string;\n   100→    dealId?: string;\n   101→    assigneeId?: string;\n   102→  }) => {\n   103→    const resp = await fetch(\"/.netlify/functions/tasks\", {\n   104→      method: \"POST\",\n   105→      headers: { \"Content-Type\": \"application/json\", ...getAuthHeaders() },\n   106→      body: JSON.stringify(task),\n   107→    });\n   108→    if (!resp.ok) {\n   109→      const data = await resp.json();\n   110→      throw new Error(data.error || \"Failed to create task\");\n   111→    }\n   112→    return resp.json() as Promise<Task>;\n   113→  }\n   114→);\n   115→\n   116→export const updateTaskStatus = createAsyncThunk(\n   117→  \"tasks/updateStatus\",\n   118→  async ({ taskId, status }: { taskId: string; status: string }) => {\n   119→    const resp = await fetch(`/.netlify/functions/tasks/${taskId}`, {\n   120→      method: \"PUT\",\n   121→      headers: { \"Content-Type\": \"application/json\", ...getAuthHeaders() },\n   122→      body: JSON.stringify({ status }),\n   123→    });\n   124→    if (!resp.ok) {\n   125→      const data = await resp.json();\n   126→      throw new Error(data.error || \"Failed to update task\");\n   127→    }\n   128→    return { taskId, status };\n   129→  }\n   130→);\n   131→\n   132→export const deleteTask = createAsyncThunk(\n   133→  \"tasks/delete\",\n   134→  async (taskId: string) => {\n   135→    const resp = await fetch(`/.netlify/functions/tasks/${taskId}`, {\n   136→      method: \"DELETE\",\n   137→      headers: { ...getAuthHeaders() },\n   138→    });\n   139→    if (!resp.ok) {\n   140→      const data = await resp.json();\n   141→      throw new Error(data.error || \"Failed to delete task\");\n   142→    }\n   143→    return taskId;\n   144→  }\n   145→);\n   146→\n   147→const tasksSlice = createSlice({\n   148→  name: \"tasks\",\n   149→  initialState,\n   150→  reducers: {\n   151→    clearClientDeals(state) {\n   152→      state.clientDeals = [];\n   153→    },\n   154→  },\n   155→  extraReducers: (builder) => {\n   156→    builder\n   157→      .addCase(fetchAllTasks.pending, (state) => {\n   158→        if (state.items.length === 0) {\n   159→          state.loading = true;\n   160→        }\n   161→        state.error = null;\n   162→      })\n   163→      .addCase(fetchAllTasks.fulfilled, (state, action: PayloadAction<Task[]>) => {\n   164→        state.loading = false;\n   165→        state.items = action.payload;\n   166→      })\n   167→      .addCase(fetchAllTasks.rejected, (state, action) => {\n   168→        state.loading = false;\n   169→        state.error = action.error.message || \"Failed to fetch tasks\";\n   170→      })\n   171→      .addCase(fetchTaskClients.fulfilled, (state, action: PayloadAction<TaskClient[]>) => {\n   172→        state.clients = action.payload;\n   173→      })\n   174→      .addCase(fetchTaskUsers.fulfilled, (state, action: PayloadAction<TaskUser[]>) => {\n   175→        state.users = action.payload;\n   176→      })\n   177→      .addCase(fetchClientDeals.fulfilled, (state, action: PayloadAction<TaskDeal[]>) => {\n   178→        state.clientDeals = action.payload;\n   179→      })\n   180→      .addCase(createTask.fulfilled, (state, action: PayloadAction<Task>) => {\n   181→        state.items.unshift(action.payload);\n   182→      })\n   183→      .addCase(updateTaskStatus.fulfilled, (state, action: PayloadAction<{ taskId: string; status: string }>) => {\n   184→        const idx = state.items.findIndex((t) => t.id === action.payload.taskId);\n   185→        if (idx !== -1) {\n   186→          state.items[idx].status = action.payload.status;\n   187→        }\n   188→      })\n   189→      .addCase(deleteTask.fulfilled, (state, action: PayloadAction<string>) => {\n   190→        state.items = state.items.filter((t) => t.id !== action.payload);\n   191→      });\n   192→  },\n   193→});\n   194→\n   195→export const { clearClientDeals } = tasksSlice.actions;\n   196→export const tasksReducer = tasksSlice.reducer;\n   197→"}]},"parent_tool_use_id":"toolu_017a8NgwJPrwcehUVigZzVvy","session_id":"6a1f517a-15b0-4062-b01a-12f0190a58f4","uuid":"21b6fd5b-f305-4820-95be-af9d33b14591"}
