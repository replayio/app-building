ity shows matching tasks', async ({ page }) => {\n   247→    await page.goto('/tasks');\n   248→    await page.waitForLoadState('networkidle');\n   249→\n   250→    // Open filter dropdown\n   251→    await page.getByTestId('tasks-filter-button').click();\n   252→    await expect(page.getByTestId('tasks-filter-dropdown')).toBeVisible();\n   253→\n   254→    // Select \"High\" priority filter (this closes the dropdown and triggers a fetch)\n   255→    await selectFilterOption(page, 'tasks-filter-priority', 'high');\n   256→\n   257→    // Wait for filtered results where all cards have high priority badges\n   258→    await expect(async () => {\n   259→      const highBadges = page.locator('[data-testid=\"task-priority-badge-high\"]');\n   260→      const cards = page.locator('[data-testid^=\"task-card-\"]');\n   261→      const cardCount = await cards.count();\n   262→      expect(cardCount).toBeGreaterThan(0);\n   263→      const badgeCount = await highBadges.count();\n   264→      expect(badgeCount).toBe(cardCount);\n   265→    }).toPass({ timeout: 10000 });\n   266→  });\n   267→\n   268→  test('TLP-FLT-03: Text filter searches task titles', async ({ page }) => {\n   269→    await page.goto('/tasks');\n   270→    await page.waitForLoadState('networkidle');\n   271→\n   272→    // Get the title of the first task to use as a search term\n   273→    const firstTitle = page.locator('[data-testid^=\"task-title-\"]').first();\n   274→    const isVisible = await firstTitle.isVisible().catch(() => false);\n   275→\n   276→    if (isVisible) {\n   277→      const titleText = await firstTitle.textContent();\n   278→      // Use a substring to search\n   279→      const searchTerm = titleText!.substring(0, Math.min(5, titleText!.length));\n   280→\n   281→      await page.getByTestId('tasks-filter-search').fill(searchTerm);\n   282→      // Wait for debounce\n   283→      await page.waitForTimeout(500);\n   284→      await page.waitForLoadState('networkidle');\n   285→\n   2