itForLoadState('networkidle');\n    37→    await expect(page).toHaveURL(/\\/clients/);\n    38→\n    39→    // Click \"Deals\" → navigates to /deals\n    40→    await page.getByTestId('sidebar-nav-deals').click();\n    41→    await page.waitForLoadState('networkidle');\n    42→    await expect(page).toHaveURL(/\\/deals/);\n    43→\n    44→    // Click \"Team\" → navigates to /users\n    45→    await page.getByTestId('sidebar-nav-team').click();\n    46→    await page.waitForLoadState('networkidle');\n    47→    await expect(page).toHaveURL(/\\/users/);\n    48→\n    49→    // Click \"Settings\" → navigates to /settings\n    50→    await page.getByTestId('sidebar-nav-settings').click();\n    51→    await page.waitForLoadState('networkidle');\n    52→    await expect(page).toHaveURL(/\\/settings/);\n    53→\n    54→    // Click \"Tasks\" → navigates back to /tasks\n    55→    await page.getByTestId('sidebar-nav-tasks').click();\n    56→    await page.waitForLoadState('networkidle');\n    57→    await expect(page).toHaveURL(/\\/tasks/);\n    58→  });\n    59→});\n    60→\n    61→test.describe('TasksListPage - PageHeader (TLP-HDR)', () => {\n    62→  test('TLP-HDR-01: Page header shows title and New Task button', async ({ page }) => {\n    63→    await page.goto('/tasks');\n    64→    await page.waitForLoadState('networkidle');\n    65→\n    66→    // \"Upcoming Tasks\" title displayed\n    67→    await expect(page.getByRole('heading', { name: 'Upcoming Tasks' })).toBeVisible();\n    68→\n    69→    // \"New Task\" button visible and accent colored\n    70→    const newTaskBtn = page.getByTestId('new-task-button');\n    71→    await expect(newTaskBtn).toBeVisible();\n    72→    await expect(newTaskBtn).toContainText('New Task');\n    73→    await expect(newTaskBtn).toHaveClass(/bg-accent/);\n    74→  });\n    75→\n    76→  test('TLP-HDR-02: New Task button opens task creation modal', async ({ page }) => {\n    77→    await page.goto('/tasks');\n    78→    await page.waitForLoadState('networkidle');\n    79→\n    80→    await page.getByTestId('new-task-button').click();\n    81→\n    82→    const modal = page.getByTestId('create-task-modal');\n    83→    await expect(modal).toBeVisible();\n    84→\n    85→    // Modal has required fields\n    86→    await expect(modal.getByText('Title *')).toBeVisible();\n    87→    await expect(modal.getByText('Description')).toBeVisible();\n    88→    await expect(modal.getByText('Due Date')).toBeVisible();\n    89→    await expect(modal.getByText('Priority')).toBeVisible();\n    90→    await expect(page.getByTestId('create-task-assignee-name')).toBeVisible();\n    91→    await expect(modal.getByTestId('create-task-client')).toBeVisible();\n    92→\n    93→    // Save and Cancel buttons\n    94→    await expect(page.getByTestId('create-task-save')).toBeVisible();\n    95→    await expect(page.getByTestId('create-task-cancel')).toBeVisible();\n    96→  });\n    97→\n    98→  test('TLP-HDR-03: Creating a task persists and appears in list', async ({ page }) => {\n    99→    await page.goto('/tasks');\n   100→    await page.waitForLoadState('networkidle');\n   101→\n   102→    // Open create task modal\n   103→    await page.getByTestId('new-task-button').click();\n   104→    const modal = page.getByTestId('create-task-modal');\n   105→    await expect(modal).toBeVisible();\n   106→\n   107→    // Fill in task data\n   108→    const taskTitle = `Test Task ${Date.now()}`;\n   109→    await page.getByTestId('create-task-title').fill(taskTitle);\n   110→    await selectFilterOption(page, 'create-task-priority', 'high');\n   111→    // Select assignee from dropdown\n   112→    await page.getByTestId('create-task-assignee-name-trigger').click();\n   113→    const assigneeMenu = page.getByTestId('create-task-assignee-name-menu');\n   114→    await expect(assigneeMenu).toBeVisible();\n   115→    // Find \"Sarah J.\" option or pick any user\n   116→    const sarahOption = assigneeMenu.locator('button', { has