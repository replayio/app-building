nown) {\n   188→      const err = e as { stdout?: string; stderr?: string };\n   189→      appendLog('Migration error:\\n' + (err.stdout || '') + (err.stderr || '') + '\\n');\n   190→      console.log('Deploy failed (migrations) — see logs/deploy.log');\n   191→      process.exit(1);\n   192→    }\n   193→  }\n   194→\n   195→  // -------------------------------------------------------------------------\n   196→  // 4. Netlify site setup\n   197→  // -------------------------------------------------------------------------\n   198→  if (!env.NETLIFY_SITE_ID) {\n   199→    appendLog('=== Creating Netlify site ===\\n');\n   200→    try {\n   201→      const resp = await fetch(`https://api.netlify.com/api/v1/${netlifySlug}/sites`, {\n   202→        method: 'POST',\n   203→        headers: {\n   204→          'Authorization': `Bearer ${netlifyToken}`,\n   205→          'Content-Type': 'application/json',\n   206→        },\n   207→        body: JSON.stringify({}),\n   208→      });\n   209→      if (!resp.ok) {\n   210→        const body = await resp.text();\n   211→        appendLog(`Netlify API error: ${resp.status} ${body}\\n`);\n   212→        console.log('Deploy failed (netlify) — see logs/deploy.log');\n   213→        process.exit(1);\n   214→      }\n   215→      const site = await resp.json() as { id: string };\n   216→      writeEnvValue('NETLIFY_SITE_ID', site.id);\n   217→      appendLog(`Created Netlify site ${site.id}\\n`);\n   218→    } catch (err) {\n   219→      appendLog(`Netlify API error: ${String(err)}\\n`);\n   220→      console.log('Deploy failed (netlify) — see logs/deploy.log');\n   221→      process.exit(1);\n   222→    }\n   223→  }\n   224→\n   225→  env = readEnvFile();\n   226→  const siteId = env.NETLIFY_SITE_ID;\n   227→\n   228→  // -------------------------------------------------------------------------\n   229→  // 5. Build\n   230→  // -------------------------------------------------------------------------\n   231→  appendLog('=== B