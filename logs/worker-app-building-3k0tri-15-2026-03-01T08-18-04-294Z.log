test <testFile>');\n    50→  process.exit(1);\n    51→}\n    52→\n    53→const runId = `test-worker-${Date.now()}`;\n    54→\n    55→// Neon API helpers\n    56→async function neonApi(path: string, options: RequestInit = {}) {\n    57→  const res = await fetch(`https://console.neon.tech/api/v2${path}`, {\n    58→    ...options,\n    59→    headers: {\n    60→      'Authorization': `Bearer ${NEON_API_KEY}`,\n    61→      'Content-Type': 'application/json',\n    62→      ...options.headers,\n    63→    },\n    64→  });\n    65→  if (!res.ok) {\n    66→    const text = await res.text();\n    67→    throw new Error(`Neon API ${path} failed (${res.status}): ${text}`);\n    68→  }\n    69→  return res.json();\n    70→}\n    71→\n    72→async function listBranches(): Promise<{ branches: Array<{ id: string; name: string }> }> {\n    73→  return neonApi(`/projects/${NEON_PROJECT_ID}/branches`);\n    74→}\n    75→\n    76→async function deleteBranch(branchId: string) {\n    77→  return neonApi(`/projects/${NEON_PROJECT_ID}/branches/${branchId}`, { method: 'DELETE' });\n    78→}\n    79→\n    80→async function createBranch(name: string): Promise<{\n    81→  branch: { id: string };\n    82→  connection_uris: Array<{ connection_uri: string }>;\n    83→}> {\n    84→  return neonApi(`/projects/${NEON_PROJECT_ID}/branches`, {\n    85→    method: 'POST',\n    86→    body: JSON.stringify({\n    87→      branch: { name },\n    88→      endpoints: [{ type: 'read_write' }],\n    89→    }),\n    90→  });\n    91→}\n    92→\n    93→function getNextLogNumber(): number {\n    94→  mkdirSync('logs', { recursive: true });\n    95→  const files = readdirSync('logs').filter(f => /^test-run-\\d+\\.log$/.test(f));\n    96→  const nums = files.map(f => parseInt(f.match(/test-run-(\\d+)/)?.[1] || '0'));\n    97→  return nums.length > 0 ? Math.max(...nums) + 1 : 1;\n    98→}\n    99→\n   100→// Parse recordings.log\n   101→interface RecordingEntry {\n   102→  id: string;\n   103→  kind: string;\n   104→  timestamp?: number;\n   105→  metadata?: {\n   106→    test?: {\n   107→      result?: string;\n   108→      title?: string;\n   109→      file?: string;\n   110→    };\n   111→    uri?: string;\n   112→  };\n   113→  buildId?: string;\n   114→  duration?: number;\n   115→  recordingStatus?: string;\n   116→}\n   117→\n   118→function parseRecordingsLog(): Map<string, RecordingEntry> {\n   119→  const recordings = new Map<string, RecordingEntry>();\n   120→  try {\n   121→    const logPath = `${process.env.HOME}/.replay/recordings.log`;\n   122→    const content = readFileSync(logPath, 'utf-8');\n   123→    for (const line of content.split('\\n')) {\n   124→      if (!line.trim()) continue;\n   125→      try {\n   126→        const entry = JSON.parse(line);\n   127→        if (entry.kind === 'createRecording' && entry.id) {\n   128→          recordings.set(entry.id, { ...entry, metadata: {} });\n   129→        } else if (entry.kind === 'addMetadata' && entry.id) {\n   130→          const existing = recordings.get(entry.id);\n   131→          if (existing) {\n   132→            existing.metadata = { ...existing.metadata, ...entry.metadata };\n   133→          }\n   134→        } else if (entry.kind === 'writeFinished' && entry.id) {\n   135→          const existing = recordings.get(entry.id);\n   136→          if (existing) {\n   137→            existing.recordingStatus = 'finished';\n   138→            if (entry.duration) existing.duration = entry.duration;\n   139→          }\n   140→        }\n   141→      } catch {\n   142→        // skip malformed lines\n   143→      }\n   144→    }\n   145→  } catch {\n   146→    // recordings.log may not exist\n   147→  }\n   148→  return recordings;\n   149→}\n   150→\n   151→async function main() {\n   152→  const logNum = getNextLogNumber();\n   153→  const logFile = `logs/test-run-${logNum}.log`;\n   154→\n   155→  // 1. Kill stale processes\n   156→  try { execSync('pkill -f \"netlify\" 2>/dev/null || true', { stdio: 'ignore' }); } catch {}\n   157→  try { execSync('pkill -f \"vite\" 2>/dev/null || true', { stdio: 'ignore' }); } catch {}\n   158→\n   159→  // Wait for processes to die\n   160→  await new Promise(r => setTimeout(r, 1000));\n   161→\n   162→  // 2. Clean up stale Neon branches\n   163→  const NUM_WORKERS = 2;\n   164→  const workerBranches: { id: string; dbUrl: string }[] = [];\n   165→  try {\n   166→    const { branches } = await listBranches();\n   167→    for (const b of branches) {\n   168→      if (b.name.startsWith('test-worker-')) {\n   169→        try {\n   170→          await deleteBranch(b.id);\n   171→        } catch {\n   172→          // ignore cleanup failures\n   173→        }\n   174→      }\n   175→    }\n   176→  } catch (e) {\n   177→    console.error('Warning: failed to clean stale branches:', e);\n   178→  }\n   179→\n   180→  // 3. Create per-worker ephemeral Neon branches\n   181→  try {\n   182→    for (let i = 0; i < NUM_WORKERS; i++) {\n   183→      const result = await createBranch(`${runId}-w${i}`);\n   184→      const dbUrl = result.connection_uris[0].connection_uri;\n   185→      workerBranches.push({ id: result.branch.id, dbUrl });\n   186→      await initSchema(dbUrl);\n   187→    }\n   188→  } catch (e) {\n   189→    console.error('Failed to create Neon branches:', e);\n   190→    // Clean up any branches we already created\n   191→    for (const b of workerBranches) {\n   192→      try { await deleteBranch(b.id); } catch {}\n   193→    }\n   194→    process.exit(1);\n   195→  }\n   196→\n   197→  let playwrightExitCode = 1;\n   198→\n   199→  try {\n   200→    // 4. Remove stale recordings\n   201→    try { execSync('npx replayio remove --all', { stdio: 'ignore' }); } catch {}\n   202→\n   203→    // 5. Build per-worker DATABASE_URL env vars\n   204→    const workerEnv: Record<string, string> = {\n   205→      ...process.env as Record<string, string>,\n   206→      DATABASE_URL: workerBranches[0].dbUrl,\n   207→    };\n   208→    for (let i = 0; i < NUM_WORKERS; i++) {\n   209→      workerEnv[`DATABASE_URL_W${i}`] = workerBranches[i].dbUrl;\n   210→    }\n   211→\n   212→    // 6. Run Playwright (webServer managed by Playwright config; per-worker DB via cookies)\n   213→    const result = spawnSync(\n   214→      'npx',\n   215→      ['playwright', 'test', testFile, '--retries', '0'],\n   216→      {\n   217→        env: workerEnv,\n   218→        stdio: ['ignore', 'pipe', 'pipe'],\n   219→        timeout: 1200000, // 20 minute overall timeout\n   220→      }\n   221→    );\n   222→\n   223→    const stdout = result.stdout?.toString() || '';\n   224→    const stderr = result.stderr?.toString() || '';\n   225→    const logContent = `=== PLAYWRIGHT OUTPUT ===\\n${stdout}\\n=== STDERR ===\\n${stderr}\\n`;\n   226→    writeFileSync(logFile, logContent);\n   227→\n   228→    playwrightExitCode = result.status ?? 1;\n   229→\n   230→    // 7. Parse results\n   231→    let passed = 0;\n   232→    let failed = 0;\n   233→    let skipped = 0;\n   234→    try {\n   235→      const results = JSON.parse(readFileSync('test-results/results.json', 'utf-8'));\n   236→      for (const suite of results.suites || []) {\n   237→        for (const spec of suite.specs || []) {\n   238→          for (const test of spec.tests || []) {\n   239→            if (test.status === 'expected') passed++;\n   240→            else if (test.status === 'unexpected') failed++;\n   241→            else if (test.status === 'skipped') skipped++;\n   242→          }\n   243→        }\n   244→      }\n   245→    } catch {\n   246→      // If JSON results not available, try to parse from stdout\n   247→      const passMatch = stdout.match(/(\\d+) passed/);\n   248→      const failMatch = stdout.match(/(\\d+) failed/);\n   249→      if (passMatch) passed = parseInt(passMatch[1]);\n   250→      if (failMatch) failed = parseInt(failMatch[1]);\n   251→    }\n   252→\n   253→    // 8. On failure, upload recordings\n   254→    let uploadedId = '';\n   255→    if (playwrightExitCode !== 0) {\n   256→      const recordings = parseRecordingsLog();\n   257→      const metadataEntries: string[] = [];\n   258→\n   259→      // Find recordings with test metadata\n   260→      let bestRecording: { id: string; duration: number } | null = null;\n   261→      for (const [id, rec] of recordings) {\n   262→        if (rec.metadata?.test) {\n   263→          metadataEntries.push(JSON.stringify({\n   264→            id,\n   265→            testResult: rec.metadata.test.result,\n   266→            testTitle: rec.metadata.test.title,\n   267→            specFile: rec.metadata.test.file,\n   268→          }));\n   269→        }\n   270→        if (\n   271→          rec.recordingStatus === 'finished' &&\n   272→          (rec.metadata?.test?.result === 'failed' || rec.metadata?.test?.result === 'timedOut') &&\n   273→          (rec.duration || 0) > (bestRecording?.duration || 0)\n   274→        ) {\n   275→          bestRecording = { id, duration: rec.duration || 0 };\n   276→        }\n   277→      }\n   278→\n   279→      // Append metadata block to log\n   280→      if (metadataEntries.length > 0) {\n   281→        const metaBlock = `\\n=== REPLAY RECORDINGS METADATA ===\\n${metadataEntries.join('\\n')}\\n=== END REPLAY RECORDINGS METADATA ===\\n`;\n   282→        writeFileSync(logFile, readFileSync(logFile, 'utf-8') + metaBlock);\n   283→      }\n   284→\n   285→      // Upload the best recording\n   286→      if (bestRecording) {\n   287→        try {\n   288→          const uploadResult = spawnSync('npx', ['replayio', 'upload', bestRecording.id], {\n   289→            env: process.env,\n   290→            stdio: ['ignore', 'pipe', 'pipe'],\n   291→            timeout: 120000,\n   292→          });\n   293→          const uploadOut = uploadResult.stdout?.toString() || '';\n   294→          if (uploadResult.status === 0) {\n   295→            uploadedId = bestRecording.id;\n   296→            writeFileSync(logFile, readFileSync(logFile, 'utf-8') + `\\nREPLAY UPLOADED: ${uploadedId}\\n`);\n   297→          } else {\n   298→            writeFileSync(logFile, readFileSync(logFile, 'utf-8') + `\\nREPLAY UPLOAD FAILED (exit ${uploadResult.status}): ${uploadOut}\\n`);\n   299→          }\n   300→        } catch (e) {\n   301→          writeFileSync(logFile, readFileSync(logFile, 'utf-8') + `\\nREPLAY UPLOAD ERROR: ${e}\\n`);\n   302→        }\n   303→      } else {\n   304→        // Try listing and uploading any available recording\n   305→        try {\n   306→          const listResult = spawnSync('npx', ['replayio', 'list', '--json'], {\n   307→            env: process.env,\n   308→            stdio: ['ignore', 'pipe', 'pipe'],\n   309→            timeout: 30000,\n   310→          });\n   311→          const listOut = listResult.stdout?.toString() || '';\n   312→          writeFileSync(logFile, readFileSync(logFile, 'utf-8') + `\\n=== REPLAY LIST ===\\n${listOut}\\n`);\n   313→\n   314→          // Try to parse and upload the first available recording\n   315→          try {\n   316→            const recs = JSON.parse(listOut);\n   317→            if (Array.isArray(recs) && recs.length > 0) {\n   318→              const rec = recs[0];\n   319→              const uploadResult = spawnSync('npx', ['replayio', 'upload', rec.id], {\n   320→                env: process.env,\n   321→                stdio: ['ignore', 'pipe', 'pipe'],\n   322→                timeout: 120000,\n   323→              });\n   324→              if (uploadResult.status === 0) {\n   325→                uploadedId = rec.id;\n   326→                writeFileSync(logFile, readFileSync(logFile, 'utf-8') + `\\nREPLAY UPLOADED: ${uploadedId}\\n`);\n   327→              }\n   328→            }\n   329→          } catch {}\n   330→        } catch {}\n   331→      }\n   332→    }\n   333→\n   334→    // 10. Print summary\n   335→    const parts = [];\n   336→    if (passed > 0) parts.push(`${passed} passed`);\n   337→    if (failed > 0) parts.push(`${failed} failed`);\n   338→    if (skipped > 0) parts.push(`${skipped} skipped`);\n   339→    let summary = parts.join(', ') || '0 passed';\n   340→    if (uploadedId) summary += ` (recording: ${uploadedId})`;\n   341→    summary += ` — see ${logFile}`;\n   342→    console.log(summary);\n   343→\n   344→  } finally {\n   345→    // 9. Clean up branches\n   346→    for (const b of workerBranches) {\n   347→      try { await deleteBranch(b.id); } catch {}\n   348→    }\n   349→    // Note: recordings are NOT removed here so they can be manually uploaded for debugging\n   350→  }\n   351→\n   352→  process.exit(playwrightExitCode);\n   353→}\n   354→\n   355→main().catch((e) => {\n   356→  console.error('Test script error:', e);\n   357→  process.exit(1);\n   358→});\n   359→"}]},"parent_tool_use_id":"toolu_0157HNNFRCWHoJvSRsFHpmFR","session_id":"eb77f299-fcce-4b5c-a497-f9e13804a6f0","uuid":"b4dbfdcb-5dbe-4d0e-8791-281f450bba19"}
