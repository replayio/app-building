[2026-02-28T20:50:45.895Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01PevA8hQTZzSE6TUHLfBEcB","type":"tool_result","content":"     1→import { execSync, spawnSync } from 'child_process'\n     2→import { readFileSync, writeFileSync, mkdirSync, readdirSync, existsSync } from 'fs'\n     3→import { join } from 'path'\n     4→import { randomUUID } from 'crypto'\n     5→import { initSchema } from './schema.js'\n     6→import { runMigrations } from './migrate.js'\n     7→import { seedTestData } from './seed-db.js'\n     8→\n     9→const appDir = join(import.meta.dirname, '..')\n    10→const logDir = join(appDir, 'logs')\n    11→mkdirSync(logDir, { recursive: true })\n    12→\n    13→const testFile = process.argv[2]\n    14→if (!testFile) {\n    15→  console.error('Usage: npm run test <test-file>')\n    16→  process.exit(1)\n    17→}\n    18→\n    19→// Read .env if present\n    20→const envPath = join(appDir, '.env')\n    21→if (existsSync(envPath)) {\n    22→  const envContent = readFileSync(envPath, 'utf-8')\n    23→  for (const line of envContent.split('\\n')) {\n    24→    const trimmed = line.trim()\n    25→    if (!trimmed || trimmed.startsWith('#')) continue\n    26→    const eqIdx = trimmed.indexOf('=')\n    27→    if (eqIdx === -1) continue\n    28→    const key = trimmed.slice(0, eqIdx).trim()\n    29→    const val = trimmed.slice(eqIdx + 1).trim()\n    30→    if (!process.env[key]) {\n    31→      process.env[key] = val\n    32→    }\n    33→  }\n    34→}\n    35→\n    36→const NEON_API_KEY = process.env.NEON_API_KEY\n    37→const NEON_PROJECT_ID = process.env.NEON_PROJECT_ID\n    38→const DATABASE_URL = process.env.DATABASE_URL\n    39→const REPLAY_CLI = process.env.REPLAY_CLI || 'replayio'\n    40→\n    41→if (!NEON_API_KEY) { console.error('Missing NEON_API_KEY'); process.exit(1) }\n    42→if (!NEON_PROJECT_ID) { console.error('Missing NEON_PROJECT_ID'); process.exit(1) }\n    43→if (!DATABASE_URL) { console.error('Missing DATABASE_URL'); process.exit(1) }\n    44→\n    45→const runId = randomUUID().slice(0, 8)\n    46→const branchName = `test-worker-${runId}`\n    47→\n    48→// Determine log file number\n    49→const existingLogs = readdirSync(logDir).filter(f => /^test-run-\\d+\\.log$/.test(f))\n    50→const maxNum = existingLogs.reduce((max, f) => {\n    51→  const n = parseInt(f.match(/test-run-(\\d+)\\.log/)?.[1] || '0', 10)\n    52→  return n > max ? n : max\n    53→}, 0)\n    54→const logNum = maxNum + 1\n    55→const logFile = join(logDir, `test-run-${logNum}.log`)\n    56→\n    57→let log = ''\n    58→function appendLog(text: string) {\n    59→  log += text + '\\n'\n    60→}\n    61→\n    62→const neonHeaders = {\n    63→  'Authorization': `Bearer ${NEON_API_KEY}`,\n    64→  'Content-Type': 'application/json',\n    65→  'Accept': 'application/json',\n    66→}\n    67→\n    68→async function neonApi(path: string, options: RequestInit = {}): Promise<Response> {\n    69→  return fetch(`https://console.neon.tech/api/v2${path}`, {\n    70→    ...options,\n    71→    headers: { ...neonHeaders, ...options.headers as Record<string, string> },\n    72→  })\n    73→}\n    74→\n    75→function killStaleProcesses() {\n    76→  try { execSync('pkill -f \"netlify dev\"', { stdio: 'ignore' }) } catch { /* no-op */ }\n    77→  try { execSync('pkill -f \"vite\"', { stdio: 'ignore' }) } catch { /* no-op */ }\n    78→}\n    79→\n    80→async function cleanupStaleBranches() {\n    81→  const res = await neonApi(`/projects/${NEON_PROJECT_ID}/branches`)\n    82→  if (!res.ok) return\n    83→  const data = await res.json() as { branches: Array<{ id: string; name: string }> }\n    84→  for (const branch of data.branches) {\n    85→    if (branch.name.startsWith('test-worker-')) {\n    86→      await neonApi(`/projects/${NEON_PROJECT_ID}/branches/${branch.id}`, { method: 'DELETE' })\n    87→    }\n    88→  }\n    89→}\n    90→\n    91→async function createBranch(): Promise<{ branchId: string; host: string }> {\n    92→  const res = await neonApi(`/projects/${NEON_PROJECT_ID}/branches`, {\n    93→    method: 'POST',\n    94→    body: JSON.stringify({\n    95→      branch: { name: branchName },\n    96→      endpoints: [{ type: 'read_write' }],\n    97→    }),\n    98→  })\n    99→  if (!res.ok) {\n   100→    const text = await res.text()\n   101→    throw new Error(`Failed to create Neon branch: ${res.status} ${text}`)\n   102→  }\n   103→  const data = await res.json() as {\n   104→    branch: { id: string }\n   105→    endpoints: Array<{ host: string }>\n   106→  }\n   107→  return { branchId: data.branch.id, host: data.endpoints[0].host }\n   108→}\n   109→\n   110→function buildDatabaseUrl(host: string): string {\n   111→  const url = new URL(DATABASE_URL!)\n   112→  url.hostname = host\n   113→  return url.toString()\n   114→}\n   115→\n   116→async function deleteBranch(branchId: string) {\n   117→  await neonApi(`/projects/${NEON_PROJECT_ID}/branches/${branchId}`, { method: 'DELETE' })\n   118→}\n   119→\n   120→function removeRecordings() {\n   121→  try {\n   122→    execSync(`npx ${REPLAY_CLI} remove --all`, { cwd: appDir, stdio: 'ignore' })\n   123→  } catch { /* no-op */ }\n   124→}\n   125→\n   126→interface RecordingsLogEntry {\n   127→  kind: string\n   128→  id?: string\n   129→  metadata?: {\n   130→    test?: {\n   131→      result?: string\n   132→      title?: string\n   133→      path?: string[]\n   134→    }\n   135→  }\n   136→  recordingId?: string\n   137→  duration?: number\n   138→}\n   139→\n   140→function parseRecordingsLog(): RecordingsLogEntry[] {\n   141→  const logPath = join(process.env.HOME || '~', '.replay', 'recordings.log')\n   142→  if (!existsSync(logPath)) return []\n   143→  const content = readFileSync(logPath, 'utf-8')\n   144→  const entries: RecordingsLogEntry[] = []\n   145→  for (const line of content.split('\\n')) {\n   146→    if (!line.trim()) continue\n   147→    try {\n   148→      entries.push(JSON.parse(line))\n   149→    } catch { /* skip malformed lines */ }\n   150→  }\n   151→  return entries\n   152→}\n   153→\n   154→interface RecordingInfo {\n   155→  id: string\n   156→  testResult: string\n   157→  testTitle: string\n   158→  specFile: string\n   159→  duration: number\n   160→}\n   161→\n   162→function extractRecordingMetadata(entries: RecordingsLogEntry[]): RecordingInfo[] {\n   163→  const recordingMap = new Map<string, RecordingInfo>()\n   164→\n   165→  // First pass: collect createRecording entries\n   166→  for (const entry of entries) {\n   167→    if (entry.kind === 'createRecording' && entry.id) {\n   168→      recordingMap.set(entry.id, {\n   169→        id: entry.id,\n   170→        testResult: '',\n   171→        testTitle: '',\n   172→        specFile: '',\n   173→        duration: 0,\n   174→      })\n   175→    }\n   176→  }\n   177→\n   178→  // Second pass: enrich with metadata\n   179→  for (const entry of entries) {\n   180→    if (entry.kind === 'addMetadata' && entry.id && entry.metadata?.test) {\n   181→      const rec = recordingMap.get(entry.id)\n   182→      if (rec) {\n   183→        rec.testResult = entry.metadata.test.result || ''\n   184→        rec.testTitle = entry.metadata.test.title || ''\n   185→        rec.specFile = entry.metadata.test.path?.join('/') || ''\n   186→      }\n   187→    }\n   188→    if (entry.kind === 'writeFinished' && entry.id) {\n   189→      const rec = recordingMap.get(entry.id)\n   190→      if (rec && entry.duration) {\n   191→        rec.duration = entry.duration\n   192→      }\n   193→    }\n   194→  }\n   195→\n   196→  return [...recordingMap.values()].filter(r => r.testResult)\n   197→}\n   198→\n   199→interface PlaywrightResults {\n   200→  passed: number\n   201→  failed: number\n   202→  skipped: number\n   203→}\n   204→\n   205→function parsePlaywrightResults(): PlaywrightResults {\n   206→  const resultsPath = join(appDir, 'test-results', 'results.json')\n   207→  if (!existsSync(resultsPath)) return { passed: 0, failed: 0, skipped: 0 }\n   208→  const data = JSON.parse(readFileSync(resultsPath, 'utf-8')) as {\n   209→    suites?: Array<{\n   210→      specs?: Array<{\n   211→        tests?: Array<{ results?: Array<{ status?: string }> }>\n   212→      }>\n   213→    }>\n   214→  }\n   215→  let passed = 0, failed = 0, skipped = 0\n   216→  for (const suite of data.suites || []) {\n   217→    for (const spec of suite.specs || []) {\n   218→      for (const test of spec.tests || []) {\n   219→        for (const result of test.results || []) {\n   220→          if (result.status === 'passed') passed++\n   221→          else if (result.status === 'skipped') skipped++\n   222→          else failed++\n   223→        }\n   224→      }\n   225→    }\n   226→  }\n   227→  return { passed, failed, skipped }\n   228→}\n   229→\n   230→async function main() {\n   231→  let branchId = ''\n   232→  let playwrightExitCode = 1\n   233→\n   234→  try {\n   235→    // Step 1: Kill stale processes\n   236→    killStaleProcesses()\n   237→\n   238→    // Step 2: Clean up stale Neon branches\n   239→    await cleanupStaleBranches()\n   240→\n   241→    // Step 3: Create ephemeral Neon branch\n   242→    appendLog(`Creating Neon branch: ${branchName}`)\n   243→    const branch = await createBranch()\n   244→    branchId = branch.branchId\n   245→    const branchDatabaseUrl = buildDatabaseUrl(branch.host)\n   246→    appendLog(`Branch created: ${branchId} (host: ${branch.host})`)\n   247→\n   248→    // Step 4: Initialize schema, run migrations, seed data\n   249→    appendLog('Initializing schema...')\n   250→    await initSchema(branchDatabaseUrl)\n   251→    appendLog('Running migrations...')\n   252→    await runMigrations(branchDatabaseUrl)\n   253→    appendLog('Seeding test data...')\n   254→    await seedTestData(branchDatabaseUrl)\n   255→    appendLog('Database ready.')\n   256→\n   257→    // Step 5: Remove stale recordings\n   258→    removeRecordings()\n   259→\n   260→    // Step 6: Run Playwright\n   261→    appendLog(`Running: npx playwright test ${testFile} --retries 0`)\n   262→    const pw = spawnSync('npx', ['playwright', 'test', testFile, '--retries', '0'], {\n   263→      cwd: appDir,\n   264→      env: { ...process.env, DATABASE_URL: branchDatabaseUrl },\n   265→      stdio: ['ignore', 'pipe', 'pipe'],\n   266→      encoding: 'utf-8',\n   267→      timeout: 300_000,\n   268→    })\n   269→    appendLog('=== PLAYWRIGHT STDOUT ===')\n   270→    appendLog(pw.stdout || '')\n   271→    appendLog('=== PLAYWRIGHT STDERR ===')\n   272→    appendLog(pw.stderr || '')\n   273→    playwrightExitCode = pw.status ?? 1\n   274→\n   275→    // Step 7: Parse results\n   276→    const results = parsePlaywrightResults()\n   277→    appendLog(`Results: ${results.passed} passed, ${results.failed} failed, ${results.skipped} skipped`)\n   278→\n   279→    // Step 8: On failure — handle recordings\n   280→    let uploadedId = ''\n   281→    if (playwrightExitCode !== 0) {\n   282→      const entries = parseRecordingsLog()\n   283→      const recordings = extractRecordingMetadata(entries)\n   284→\n   285→      if (recordings.length > 0) {\n   286→        appendLog('=== REPLAY RECORDINGS METADATA ===')\n   287→        for (const rec of recordings) {\n   288→          appendLog(JSON.stringify(rec))\n   289→        }\n   290→        appendLog('=== END REPLAY RECORDINGS METADATA ===')\n   291→\n   292→        // Pick longest-duration failed recording\n   293→        const failedRecs = recordings.filter(\n   294→          r => r.testResult === 'failed' || r.testResult === 'timedOut'\n   295→        )\n   296→        if (failedRecs.length > 0) {\n   297→          const best = failedRecs.reduce((a, b) => a.duration > b.duration ? a : b)\n   298→          appendLog(`Uploading recording: ${best.id}`)\n   299→          const upload = spawnSync('npx', [REPLAY_CLI, 'upload', best.id], {\n   300→            cwd: appDir,\n   301→            env: process.env,\n   302→            stdio: ['ignore', 'pipe', 'pipe'],\n   303→            encoding: 'utf-8',\n   304→            timeout: 120_000,\n   305→          })\n   306→          if (upload.status === 0) {\n   307→            // Extract the recording ID from upload output\n   308→            const match = (upload.stdout || '').match(/([a-f0-9-]{36})/)\n   309→            uploadedId = match ? match[1] : best.id\n   310→            appendLog(`REPLAY UPLOADED: ${uploadedId}`)\n   311→          } else {\n   312→            appendLog(`Upload failed with exit code: ${upload.status}`)\n   313→            appendLog(upload.stdout || '')\n   314→            appendLog(upload.stderr || '')\n   315→          }\n   316→        }\n   317→      }\n   318→    }\n   319→\n   320→    // Step 10: Print summary\n   321→    const parts: string[] = []\n   322→    if (results.passed > 0) parts.push(`${results.passed} passed`)\n   323→    if (results.failed > 0) parts.push(`${results.failed} failed`)\n   324→    if (results.skipped > 0) parts.push(`${results.skipped} skipped`)\n   325→    let summary = parts.join(', ') || '0 tests'\n   326→    if (uploadedId) summary += ` (recording: ${uploadedId})`\n   327→    if (playwrightExitCode !== 0) summary += ` — see logs/test-run-${logNum}.log`\n   328→    console.log(summary)\n   329→  } catch (err) {\n   330→    appendLog(`Fatal error: ${err}`)\n   331→    console.error(`test failed — see logs/test-run-${logNum}.log`)\n   332→    playwrightExitCode = 1\n   333→  } finally {\n   334→    // Step 9: Clean up\n   335→    if (branchId) {\n   336→      try { await deleteBranch(branchId) } catch { /* no-op */ }\n   337→    }\n   338→    removeRecordings()\n   339→    writeFileSync(logFile, log)\n   340→  }\n   341→\n   342→  process.exit(playwrightExitCode)\n   343→}\n   344→\n   345→main()\n   346→"}]},"parent_tool_use_id":"toolu_0153ryt5N326wat4GaUb78y4","session_id":"496d909c-d711-4bfc-ba3c-10194d61b39a","uuid":"5830bb65-7068-4e69-a269-140490dc53