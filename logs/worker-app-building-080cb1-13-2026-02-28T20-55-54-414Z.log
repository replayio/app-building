neon(process.env.DATABASE_URL!)\n    55→  const body = await request.json() as { name: string; description: string; requirements: string }\n    56→  const { name, description, requirements } = body\n    57→\n    58→  const assessment = assessRequest(name, description, requirements)\n    59→\n    60→  if (!assessment.accepted) {\n    61→    await sql`\n    62→      INSERT INTO app_requests (name, description, requirements, status, rejection_reason)\n    63→      VALUES (${name}, ${description}, ${requirements || null}, 'rejected', ${assessment.reason})\n    64→    `\n    65→    return new Response(\n    66→      JSON.stringify({\n    67→        result: 'rejected',\n    68→        rejectionReason: assessment.reason,\n    69→      }),\n    70→      { headers: { 'Content-Type': 'application/json' } },\n    71→    )\n    72→  }\n    73→\n    74→  const appId = name\n    75→    .toLowerCase()\n    76→    .replace(/[^a-z0-9]+/g, '-')\n    77→    .replace(/^-|-$/g, '')\n    78→\n    79→  await sql`\n    80→    INSERT INTO apps (id, name, description, status, progress, model, deployment_url, source_url)\n    81→    VALUES (${appId}, ${name}, ${description}, 'queued', 0, NULL, NULL, NULL)\n    82→  `\n    83→\n    84→  await sql`\n    85→    INSERT INTO app_requests (name, description, requirements, status, app_id)\n    86→    VALUES (${name}, ${description}, ${requirements || null}, 'accepted', ${appId})\n    87→  `\n    88→\n    89→  return new Response(\n    90→    JSON.stringify({\n    91→      result: 'accepted',\n    92→      appId,\n    93→    }),\n    94→    { headers: { 'Content-Type': 'application/json' } },\n    95→  )\n    96→}\n    97→"}]},"parent_tool_use_id":"toolu_0153ryt5N326wat4GaUb78y4","session_id":"496d909c-d711-4bfc-ba3c-10194d61b39a","uuid":"421986db-be36-4fda-aff3-1f57fe2c2eb7"}
