[2026-02-26T05:20:44.288Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01JottJhB7LNhz8vi9DmAGvH","type":"tool_result","content":"     1→import { execSync } from \"child_process\";\n     2→import { readFileSync, existsSync } from \"fs\";\n     3→import path from \"path\";\n     4→import { initSchema } from \"./schema.js\";\n     5→\n     6→const appDir = path.resolve(import.meta.dirname, \"..\");\n     7→\n     8→// ---------------------------------------------------------------------------\n     9→// Helpers\n    10→// ---------------------------------------------------------------------------\n    11→\n    12→function env(key: string): string {\n    13→  const val = process.env[key];\n    14→  if (!val) {\n    15→    console.error(`Missing required environment variable: ${key}`);\n    16→    process.exit(1);\n    17→  }\n    18→  return val;\n    19→}\n    20→\n    21→function readEnvFile(): Record<string, string> {\n    22→  const envPath = path.join(appDir, \".env\");\n    23→  if (!existsSync(envPath)) return {};\n    24→  const lines = readFileSync(envPath, \"utf-8\").split(\"\\n\");\n    25→  const result: Record<string, string> = {};\n    26→  for (const line of lines) {\n    27→    const trimmed = line.trim();\n    28→    if (!trimmed || trimmed.startsWith(\"#\")) continue;\n    29→    const idx = trimmed.indexOf(\"=\");\n    30→    if (idx === -1) continue;\n    31→    result[trimmed.slice(0, idx)] = trimmed.slice(idx + 1);\n    32→  }\n    33→  return result;\n    34→}\n    35→\n    36→function tryExec(cmd: string, opts?: { cwd?: string; encoding?: BufferEncoding }) {\n    37→  try {\n    38→    return execSync(cmd, { encoding: \"utf-8\", ...opts });\n    39→  } catch {\n    40→    return null;\n    41→  }\n    42→}\n    43→\n    44→// ---------------------------------------------------------------------------\n    45→// Neon branch helpers\n    46→// ---------------------------------------------------------------------------\n    47→\n    48→async function neonFetch(apiPath: string, options: RequestInit = {}): Promise<Response> {\n    49→  const neonApiKey = env(\"NEON_API_KEY\");\n    50→  return fetch(`https://console.neon.tech/api/v2${apiPath}`, {\n    51→    ...options,\n    52→    headers: {\n    53→      Authorization: `Bearer ${neonApiKey}`,\n    54→      \"Content-Type\": \"application/json\",\n    55→      ...options.headers,\n    56→    },\n    57→  });\n    58→}\n    59→\n    60→interface NeonBranch {\n    61→  id: string;\n    62→  name: string;\n    63→}\n    64→\n    65→async function listBranches(projectId: string): Promise<NeonBranch[]> {\n    66→  const res = await neonFetch(`/projects/${projectId}/branches`);\n    67→  if (!res.ok) {\n    68→    console.error(\"Failed to list Neon branches:\", await res.text());\n    69→    return [];\n    70→  }\n    71→  const data = await res.json();\n    72→  return data.branches;\n    73→}\n    74→\n    75→async function deleteBranch(projectId: string, branchId: string) {\n    76→  const res = await neonFetch(`/projects/${projectId}/branches/${branchId}`, {\n    77→    method: \"DELETE\",\n    78→  });\n    79→  if (!res.ok) {\n    80→    console.error(`Failed to delete branch ${branchId}:`, await res.text());\n    81→  }\n    82→}\n    83→\n    84→interface CreatedBranch {\n    85→  branchId: string;\n    86→  connectionUri: string;\n    87→}\n    88→\n    89→async function createBranch(projectId: string, branchName: string): Promise<CreatedBranch> {\n    90→  const res = await neonFetch(`/projects/${projectId}/branches`, {\n    91→    method: \"POST\",\n    92→    body: JSON.stringify({\n    93→      branch: { name: branchName },\n    94→      endpoints: [{ type: \"read_write\" }],\n    95→    }),\n    96→  });\n    97→  if (!res.ok) {\n    98→    console.error(\"Failed to create Neon branch:\", await res.text());\n    99→    process.exit(1);\n   100→  }\n   101→  const data = await res.json();\n   102→  return {\n   103→    branchId: data.branch.id,\n   104→    connectionUri: data.connection_uris[0].connection_uri,\n   105→  };\n   106→}\n   107→\n   108→// ---------------------------------------------------------------------------\n   109→// Replay recording helpers\n   110→// ---------------------------------------------------------------------------\n   111→\n   112→interface RecordingEntry {\n   113→  kind: string;\n   114→  id?: string;\n   115→  metadata?: Record<string, unknown>;\n   116→  timestamp?: string;\n   117→  duration?: number;\n   118→}\n   119→\n   120→function parseRecordingsLog(): RecordingEntry[] {\n   121→  const logPath = path.join(process.env.HOME || \"~\", \".replay\", \"recordings.log\");\n   122→  if (!existsSync(logPath)) return [];\n   123→  const entries: RecordingEntry[] = [];\n   124→  for (const line of readFileSync(logPath, \"utf-8\").split(\"\\n\")) {\n   125→    const trimmed = line.trim();\n   126→    if (!trimmed) continue;\n   127→    try {\n   128→      entries.push(JSON.parse(trimmed));\n   129→    } catch {\n   130→      // skip malformed lines\n   131→    }\n   132→  }\n   133→  return entries;\n   134→}\n   135→\n   136→interface RecordingInfo {\n   137→  id: string;\n   138→  testResult?: string;\n   139→  testTitle?: string;\n   140→  specFile?: string;\n   141→  duration: number;\n   142→  finished: boolean;\n   143→}\n   144→\n   145→function buildRecordingMap(entries: RecordingEntry[]): Map<string, RecordingInfo> {\n   146→  const recordings = new Map<string, RecordingInfo>();\n   147→\n   148→  for (const entry of entries) {\n   149→    if (entry.kind === \"createRecording\" && entry.id) {\n   150→      recordings.set(entry.id, {\n   151→        id: entry.id,\n   152→        duration: 0,\n   153→        finished: false,\n   154→      });\n   155→    }\n   156→    if (entry.kind === \"addMetadata\" && entry.id) {\n   157→      const rec = recordings.get(entry.id);\n   158→      if (rec && entry.metadata) {\n   159→        const test = entry.metadata.test as\n   160→          | { result?: string; title?: string; path?: string[] }\n   161→          | undefined;\n   162→        if (test) {\n   163→          rec.testResult = test.result;\n   164→          rec.testTitle = test.title;\n   165→          if (test.path) rec.specFile = test.path.join(\"/\");\n   166→        }\n   167→      }\n   168→    }\n   169→    if (entry.kind === \"writeFinished\" && entry.id) {\n   170→      const rec = recordings.get(entry.id);\n   171→      if (rec) {\n   172→        rec.finished = true;\n   173→        if (entry.duration != null) rec.duration = entry.duration;\n   174→      }\n   175→    }\n   176→  }\n   177→\n   178→  return recordings;\n   179→}\n   180→\n   181→// ---------------------------------------------------------------------------\n   182→// Main\n   183→// ---------------------------------------------------------------------------\n   184→\n   185→async function main() {\n   186→  const testFile = process.argv[2];\n   187→  if (!testFile) {\n   188→    console.error(\"Usage: npm run test <test-file>\");\n   189→    console.error(\"Example: npm run test tests/auth.spec.ts\");\n   190→    process.exit(1);\n   191→  }\n   192→\n   193→  const neonApiKey = env(\"NEON_API_KEY\");\n   194→  void neonApiKey; // used via neonFetch\n   195→\n   196→  const replayCli = process.env.REPLAY_CLI || \"replayio\";\n   197→\n   198→  const envVals = readEnvFile();\n   199→  const neonProjectId = envVals[\"NEON_PROJECT_ID\"] || env(\"NEON_PROJECT_ID\");\n   200→\n   201→  // 1. Kill stale processes\n   202→  console.log(\"\\n--- Killing stale processes ---\");\n   203→  tryExec(\"pkill -f 'netlify dev' || true\", { cwd: appDir });\n   204→  tryExec(\"pkill -f 'vite' || true\", { cwd: appDir });\n   205→\n   206→  // 2. Clean up stale Neon branches\n   207→  console.log(\"\\n--- Cleaning up stale test branches ---\");\n   208→  const branches = await listBranches(neonProjectId);\n   209→  for (const branch of branches) {\n   210→    if (branch.name.startsWith(\"test-worker-\")) {\n   211→      console.log(`Deleting stale branch: ${branch.name}`);\n   212→      await deleteBranch(neonProjectId, branch.id);\n   213→    }\n   214→  }\n   215→\n   216→  // 3. Create ephemeral Neon branch\n   217→  const runId = `${Date.now()}-${Math.random().toString(36).slice(2, 8)}`;\n   218→  const branchName = `test-worker-${runId}`;\n   219→  console.log(`\\n--- Creating ephemeral branch: ${branchName} ---`);\n   220→  const { branchId, connectionUri } = await createBranch(neonProjectId, branchName);\n   221→  console.log(`Branch created: ${branchId}`);\n   222→\n   223→  let playwrightExitCode = 1;\n   224→\n   225→  try {\n   226→    // 4. Initialize and seed\n   227→    console.log(\"\\n--- Initializing schema on test branch ---\");\n   228→    await initSchema(connectionUri);\n   229→    console.log(\"Schema initialized.\");\n   230→\n   231→    // Run migrations if migrate-db.ts exists\n   232→    const migrateScript = path.join(appDir, \"scripts\", \"migrate-db.ts\");\n   233→    if (existsSync(migrateScript)) {\n   234→      console.log(\"\\n--- Running migrations ---\");\n   235→      execSync(`npx tsx scripts/migrate-db.ts \"${connectionUri}\"`, {\n   236→        stdio: \"inherit\",\n   237→        cwd: appDir,\n   238→      });\n   239→    }\n   240→\n   241→    // Seed test data if seed-db.ts exists\n   242→    const seedScript = path.join(appDir, \"scripts\", \"seed-db.ts\");\n   243→    if (existsSync(seedScript)) {\n   244→      console.log(\"\\n--- Seeding test data ---\");\n   245→      execSync(`npx tsx scripts/seed-db.ts \"${connectionUri}\"`, {\n   246→        stdio: \"inherit\",\n   247→        cwd: appDir,\n   248→      });\n   249→    }\n   250→\n   251→    // 5. Remove stale recordings\n   252→    console.log(\"\\n--- Removing stale recordings ---\");\n   253→    tryExec(`npx ${replayCli} remove --all`, { cwd: appDir });\n   254→\n   255→    // 6. Run Playwright\n   256→    console.log(`\\n--- Running Playwright: ${testFile} ---`);\n   257→    try {\n   258→      execSync(`npx playwright test ${testFile} --retries 0`, {\n   259→        stdio: \"inherit\",\n   260→        cwd: appDir,\n   261→        env: { ...process.env, DATABASE_URL: connectionUri },\n   262→      });\n   263→      playwrightExitCode = 0;\n   264→    } catch (err: unknown) {\n   265→      const exitErr = err as { status?: number };\n   266→      playwrightExitCode = exitErr.status ?? 1;\n   267→      console.error(`\\nPlaywright exited with code ${playwrightExitCode}`);\n   268→    }\n   269→\n   270→    // 7. On failure — parse recordings and upload\n   271→    if (playwrightExitCode !== 0) {\n   272→      const entries = parseRecordingsLog();\n   273→      const recordings = buildRecordingMap(entries);\n   274→\n   275→      // Log metadata for all recordings with test metadata\n   276→      const withTestMeta = [...recordings.values()].filter((r) => r.testResult);\n   277→      if (withTestMeta.length > 0) {\n   278→        console.log(\"\\n=== REPLAY RECORDINGS METADATA ===\");\n   279→        for (const rec of withTestMeta) {\n   280→          console.log(\n   281→            JSON.stringify({\n   282→              id: rec.id,\n   283→              testResult: rec.testResult,\n   284→              testTitle: rec.testTitle,\n   285→              specFile: rec.specFile,\n   286→            })\n   287→          );\n   288→        }\n   289→        console.log(\"=== END REPLAY RECORDINGS METADATA ===\");\n   290→      }\n   291→\n   292→      // Upload one failed recording with longest duration\n   293→      const failedRecordings = [...recordings.values()].filter(\n   294→        (r) =>\n   295→          r.finished &&\n   296→          (r.testResult === \"failed\" || r.testResult === \"timedOut\")\n   297→      );\n   298→      failedRecordings.sort((a, b) => b.duration - a.duration);\n   299→\n   300→      if (failedRecordings.length > 0) {\n   301→        const toUpload = failedRecordings[0];\n   302→        console.log(`\\nUploading recording ${toUpload.id} (duration: ${toUpload.duration})...`);\n   303→        try {\n   304→          execSync(`npx ${replayCli} upload ${toUpload.id}`, {\n   305→            stdio: \"inherit\",\n   306→            cwd: appDir,\n   307→          });\n   308→          console.log(`REPLAY UPLOADED: ${toUpload.id}`);\n   309→        } catch (uploadErr: unknown) {\n   310→          const exitErr = uploadErr as { status?: number };\n   311→          console.error(`Upload failed with exit code ${exitErr.status ?? \"unknown\"}`);\n   312→        }\n   313→      }\n   314→    }\n   315→  } finally {\n   316→    // 8. Clean up\n   317→    console.log(\"\\n--- Cleaning up ---\");\n   318→    console.log(`Deleting ephemeral branch: ${branchName}`);\n   319→    await deleteBranch(neonProjectId, branchId);\n   320→    tryExec(`npx ${replayCli} remove --all`, { cwd: appDir });\n   321→  }\n   322→\n   323→  // 9. Exit with original Playwright exit code\n   324→  process.exit(playwrightExitCode);\n   325→}\n   326→\n   327→main().catch((err) => {\n   328→  console.error(\"Test script failed:\", err);\n   329→  process.exit(1);\n   330→});\n   331→"}]},"parent_tool_use_id":"toolu_01GvYtMcL5zMqCtwNmjhRFcN","session_id":"6dac4388-c7a0-4420-b6ba-7d3c11c960f5","uuid":"bcaa077e-b667-41de-8541-7deb9fe2b018"}
