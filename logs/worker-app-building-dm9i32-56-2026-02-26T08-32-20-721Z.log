wait goToRun(page, scheduledRunId);\n   215→\n   216→    const productField = page.getByTestId(\"run-info-product\");\n   217→    await expect(productField).toBeVisible();\n   218→    await expect(productField).toHaveText(scheduledRunDetail.product_name);\n   219→  });\n   220→\n   221→  test(\"RUN-HDR-9: Associated Recipe field displays the recipe name\", async ({\n   222→    page,\n   223→  }) => {\n   224→    await goToRun(page, scheduledRunId);\n   225→\n   226→    const recipeField = page.getByTestId(\"run-info-recipe\");\n   227→    await expect(recipeField).toBeVisible();\n   228→\n   229→    // Format: \"RecipeName (ProductName)\"\n   230→    const expected = `${scheduledRunDetail.recipe_name} (${scheduledRunDetail.product_name})`;\n   231→    await expect(recipeField).toHaveText(expected);\n   232→  });\n   233→\n   234→  test(\"RUN-HDR-10: Planned Quantity field displays the quantity with units\", async ({\n   235→    page,\n   236→  }) => {\n   237→    await goToRun(page, scheduledRunId);\n   238→\n   239→    const qtyField = page.getByTestId(\"run-info-quantity\");\n   240→    await expect(qtyField).toBeVisible();\n   241→    await expect(qtyField).toHaveText(\n   242→      `${parseFloat(String(scheduledRunDetail.planned_quantity))} ${scheduledRunDetail.unit}`\n   243→    );\n   244→  });\n   245→\n   246→  test('RUN-HDR-11: Status field displays a \"Scheduled\" badge', async ({\n   247→    page,\n   248→  }) => {\n   249→    await goToRun(page, scheduledRunId);\n   250→\n   251→    const statusField = page.getByTestId(\"run-info-status\");\n   252→    await expect(statusField).toBeVisible();\n   253→\n   254→    const badge = statusField.locator(\".badge\");\n   255→    await expect(badge).toHaveText(\"Scheduled\");\n   256→    await expect(badge).toHaveClass(/badge--scheduled/);\n   257→  });\n   258→\n   259→  test('RUN-HDR-12: Status badge shows \"Confirmed\" styling when run is confirmed', async ({\n   260→    page,\n   261→  }) => {\n   262→    awa