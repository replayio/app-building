ueryOne<NotificationPreferences>(\n    51→      sql,\n    52→      \"SELECT client_updated, deal_created, deal_stage_changed, task_created, task_completed, task_canceled, contact_added, note_added FROM notification_preferences WHERE user_id = $1\",\n    53→      [user.id]\n    54→    );\n    55→\n    56→    if (!row) {\n    57→      // Return defaults (all true)\n    58→      return jsonResponse({\n    59→        clientUpdated: true,\n    60→        dealCreated: true,\n    61→        dealStageChanged: true,\n    62→        taskCreated: true,\n    63→        taskCompleted: true,\n    64→        taskCanceled: true,\n    65→        contactAdded: true,\n    66→        noteAdded: true,\n    67→      });\n    68→    }\n    69→\n    70→    return jsonResponse(mapPreferences(row));\n    71→  }\n    72→\n    73→  // PUT - update preferences\n    74→  if (req.method === \"PUT\") {\n    75→    const body = await req.json() as Record<string, boolean>;\n    76→\n    77→    const clientUpdated = body.clientUpdated ?? true;\n    78→    const dealCreated = body.dealCreated ?? true;\n    79→    const dealStageChanged = body.dealStageChanged ?? true;\n    80→    const taskCreated = body.taskCreated ?? true;\n    81→    const taskCompleted = body.taskCompleted ?? true;\n    82→    const taskCanceled = body.taskCanceled ?? true;\n    83→    const contactAdded = body.contactAdded ?? true;\n    84→    const noteAdded = body.noteAdded ?? true;\n    85→\n    86→    // Upsert\n    87→    const existing = await queryOne<{ id: string }>(\n    88→      sql,\n    89→      \"SELECT id FROM notification_preferences WHERE user_id = $1\",\n    90→      [user.id]\n    91→    );\n    92→\n    93→    if (existing) {\n    94→      await query(\n    95→        sql,\n    96→        `UPDATE notification_preferences SET\n    97→          client_updated = $1, deal_created = $2, deal_stage_changed = $3,\n    98→          task_created = $4, task_completed = $5, task_canceled = $6, contact_added = $7, note_added = $8,\n    99→          updated_at = NOW()\n   100→        WHERE user_id = $9`,\n   101→        [clientUpdated, dealCreated, dealStageChanged, taskCreated, taskCompleted, taskCanceled, contactAdded, noteAdded, user.id]\n   102→      );\n   103→    } else {\n   104→      await query(\n   105→        sql,\n   106→        `INSERT INTO notification_preferences (user_id, client_updated, deal_created, deal_stage_changed, task_created, task_completed, task_canceled, contact_added, note_added)\n   107→        VALUES ($1, $2, $3, $4, $5, $6, $7, $8, $9)`,\n   108→        [user.id, clientUpdated, dealCreated, dealStageChanged, taskCreated, taskCompleted, taskCanceled, contactAdded, noteAdded]\n   109→      );\n   110→    }\n   111→\n   112→    return jsonResponse({\n   113→      clientUpdated,\n   114→      dealCreated,\n   115→      dealStageChanged,\n   116→      taskCreated,\n   117→      taskCompleted,\n   118→      taskCanceled,\n   119→      contactAdded,\n   120→      noteAdded,\n   121→    });\n   122→  }\n   123→\n   124→  return errorResponse(405, \"Method not allowed\");\n   125→}\n   126→\n   127→export default withAuth(handler, { required: true });\n   128→"}]},"parent_tool_use_id":"toolu_01Cfat8HMFVQmHtTwNr2tWrT","session_id":"19df707c-79ad-42dd-9b66-c654e3a22de3","uuid":"30aa6df1-7e23-48e6-a475-c37cd7066d0b"}
