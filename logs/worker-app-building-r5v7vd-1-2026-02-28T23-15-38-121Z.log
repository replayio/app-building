 => {\n    58→        state.loading = false\n    59→        state.containers = action.payload.containers\n    60→        state.webhookEvents = action.payload.webhookEvents\n    61→        state.defaultPrompt = action.payload.defaultPrompt\n    62→      })\n    63→      .addCase(fetchStatus.rejected, (state, action) => {\n    64→        state.loading = false\n    65→        state.error = action.error.message || 'Failed to fetch status'\n    66→      })\n    67→  },\n    68→})\n    69→\n    70→export default statusSlice.reducer\n    71→"}]},"parent_tool_use_id":"toolu_015eYrHhHqfswEYVC2VgPjhN","session_id":"666ff7d9-f28f-415a-80bd-a147548d3a7c","uuid":"8ae152cc-790d-4636-a1f8-b76df506a388"}
