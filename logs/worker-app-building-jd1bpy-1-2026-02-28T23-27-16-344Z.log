[2026-02-28T23:25:40.408Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_019qWY6MWFEYZqcNES4AE96V","type":"tool_result","content":"     1→import { spawnSync } from 'child_process'\n     2→import { readFileSync, writeFileSync, mkdirSync, existsSync } from 'fs'\n     3→import { join } from 'path'\n     4→import { initSchema } from './schema.js'\n     5→import { runMigrations } from './migrate.js'\n     6→\n     7→const appDir = join(import.meta.dirname, '..')\n     8→const logDir = join(appDir, 'logs')\n     9→const logFile = join(logDir, 'deploy.log')\n    10→const envPath = join(appDir, '.env')\n    11→const deploymentPath = join(appDir, 'deployment.txt')\n    12→\n    13→mkdirSync(logDir, { recursive: true })\n    14→\n    15→let log = ''\n    16→function appendLog(text: string) {\n    17→  log += text + '\\n'\n    18→}\n    19→\n    20→// --- .env helpers ---\n    21→\n    22→function readEnvFile(): Record<string, string> {\n    23→  if (!existsSync(envPath)) return {}\n    24→  const content = readFileSync(envPath, 'utf-8')\n    25→  const vars: Record<string, string> = {}\n    26→  for (const line of content.split('\\n')) {\n    27→    const trimmed = line.trim()\n    28→    if (!trimmed || trimmed.startsWith('#')) continue\n    29→    const eqIdx = trimmed.indexOf('=')\n    30→    if (eqIdx === -1) continue\n    31→    vars[trimmed.slice(0, eqIdx).trim()] = trimmed.slice(eqIdx + 1).trim()\n    32→  }\n    33→  return vars\n    34→}\n    35→\n    36→function writeEnvVar(key: string, value: string) {\n    37→  const vars = readEnvFile()\n    38→  if (vars[key]) return // don't clobber existing\n    39→  const line = `${key}=${value}\\n`\n    40→  if (existsSync(envPath)) {\n    41→    const content = readFileSync(envPath, 'utf-8')\n    42→    writeFileSync(envPath, content.endsWith('\\n') ? content + line : content + '\\n' + line)\n    43→  } else {\n    44→    writeFileSync(envPath, line)\n    45→  }\n    46→}\n    47→\n    48→// --- Load .env into process.env ---\n    49→\n    50→function loadEnv() {\n    51→  if (existsSync(envPath)) {\n    52→    const content = readFileSync(envPath, 'utf-8')\n    53→    for (const line of content.split('\\n')) {\n    54→      const trimmed = line.trim()\n    55→      if (!trimmed || trimmed.startsWith('#')) continue\n    56→      const eqIdx = trimmed.indexOf('=')\n    57→      if (eqIdx === -1) continue\n    58→      const key = trimmed.slice(0, eqIdx).trim()\n    59→      const val = trimmed.slice(eqIdx + 1).trim()\n    60→      if (!process.env[key]) {\n    61→        process.env[key] = val\n    62→      }\n    63→    }\n    64→  }\n    65→}\n    66→\n    67→// --- Populate .env from deployment.txt ---\n    68→\n    69→function populateEnvFromDeployment() {\n    70→  if (!existsSync(deploymentPath)) return\n    71→  const content = readFileSync(deploymentPath, 'utf-8')\n    72→  const fields: Record<string, string> = {}\n    73→  for (const line of content.split('\\n')) {\n    74→    const match = line.match(/^(\\w+):\\s*(.+)$/)\n    75→    if (match) {\n    76→      fields[match[1]] = match[2].trim()\n    77→    }\n    78→    // Stop at the first history separator\n    79→    if (line.startsWith('---')) break\n    80→  }\n    81→  if (fields['site_id'] && !readEnvFile()['NETLIFY_SITE_ID']) {\n    82→    writeEnvVar('NETLIFY_SITE_ID', fields['site_id'])\n    83→  }\n    84→  if (fields['neon_project_id'] && !readEnvFile()['NEON_PROJECT_ID']) {\n    85→    writeEnvVar('NEON_PROJECT_ID', fields['neon_project_id'])\n    86→  }\n    87→  if (fields['database_url'] && !readEnvFile()['DATABASE_URL']) {\n    88→    writeEnvVar('DATABASE_URL', fields['database_url'])\n    89→  }\n    90→}\n    91→\n    92→// --- Neon API ---\n    93→\n    94→function getNeonHeaders() {\n    95→  return {\n    96→    'Authorization': `Bearer ${process.env.NEON_API_KEY}`,\n    97→    'Content-Type': 'application/json',\n    98→    'Accept': 'application/json',\n    99→  }\n   100→}\n   101→\n   102→async function neonApi(path: string, options: RequestInit = {}): Promise<Response> {\n   103→  return fetch(`https://console.neon.tech/api/v2${path}`, {\n   104→    ...options,\n   105→    headers: { ...getNeonHeaders(), ...options.headers as Record<string, string> },\n   106→  })\n   107→}\n   108→\n   109→async function createNeonProject(): Promise<{ projectId: string; databaseUrl: string }> {\n   110→  const appName = JSON.parse(readFileSync(join(appDir, 'package.json'), 'utf-8')).name as string\n   111→  const res = await neonApi('/projects', {\n   112→    method: 'POST',\n   113→    body: JSON.stringify({\n   114→      project: { name: appName },\n   115→    }),\n   116→  })\n   117→  if (!res.ok) {\n   118→    const text = await res.text()\n   119→    throw new Error(`Failed to create Neon project: ${res.status} ${text}`)\n   120→  }\n   121→  const data = await res.json() as {\n   122→    project: { id: string }\n   123→    connection_uris: Array<{ connection_uri: string }>\n   124→  }\n   125→  return {\n   126→    projectId: data.project.id,\n   127→    databaseUrl: data.connection_uris[0].connection_uri,\n   128→  }\n   129→}\n   130→\n   131→// --- deployment.txt helpers ---\n   132→\n   133→function updateDeploymentTxt(url: string, siteId: string, neonProjectId: string, databaseUrl: string) {\n   134→  const resourceBlock = [\n   135→    `url: ${url}`,\n   136→    `site_id: ${siteId}`,\n   137→    `neon_project_id: ${neonProjectId}`,\n   138→    `database_url: ${databaseUrl}`,\n   139→    `deployed_at: ${new Date().toISOString()}`,\n   140→  ].join('\\n')\n   141→\n   142→  let historySection = ''\n   143→  if (existsSync(deploymentPath)) {\n   144→    const existing = readFileSync(deploymentPath, 'utf-8')\n   145→    const historyIdx = existing.indexOf('\\n---')\n   146→    if (historyIdx !== -1) {\n   147→      historySection = existing.slice(historyIdx)\n   148→    }\n   149→  }\n   150→\n   151→  writeFileSync(deploymentPath, resourceBlock + '\\n' + historySection)\n   152→}\n   153→\n   154→// --- Main ---\n   155→\n   156→async function main() {\n   157→  // Step 0: Populate .env from deployment.txt if needed\n   158→  populateEnvFromDeployment()\n   159→  loadEnv()\n   160→\n   161→  // Validate required env vars\n   162→  if (!process.env.NEON_API_KEY) { console.error('Missing NEON_API_KEY'); process.exit(1) }\n   163→  if (!process.env.NETLIFY_AUTH_TOKEN) { console.error('Missing NETLIFY_AUTH_TOKEN'); process.exit(1) }\n   164→  if (!process.env.NETLIFY_ACCOUNT_SLUG) { console.error('Missing NETLIFY_ACCOUNT_SLUG'); process.exit(1) }\n   165→\n   166→  try {\n   167→    // Step 1: Database setup\n   168→    let neonProjectId = process.env.NEON_PROJECT_ID || ''\n   169→    let databaseUrl = process.env.DATABASE_URL || ''\n   170→\n   171→    if (!neonProjectId) {\n   172→      appendLog('Creating new Neon project...')\n   173→      const neon = await createNeonProject()\n   174→      neonProjectId = neon.projectId\n   175→      databaseUrl = neon.databaseUrl\n   176→      writeEnvVar('NEON_PROJECT_ID', neonProjectId)\n   177→      writeEnvVar('DATABASE_URL', databaseUrl)\n   178→      // Reload env so subsequent steps see the new values\n   179→      process.env.NEON_PROJECT_ID = neonProjectId\n   180→      process.env.DATABASE_URL = databaseUrl\n   181→      appendLog(`Neon project created: ${neonProjectId}`)\n   182→    }\n   183→\n   184→    if (!databaseUrl) {\n   185→      console.error('Missing DATABASE_URL — set it in .env or deployment.txt')\n   186→      process.exit(1)\n   187→    }\n   188→\n   189→    // Step 2: Schema sync\n   190→    appendLog('Syncing database schema...')\n   191→    await initSchema(databaseUrl)\n   192→    appendLog('Schema synced.')\n   193→\n   194→    // Step 3: Run migrations\n   195→    appendLog('Running migrations...')\n   196→    await runMigrations(databaseUrl)\n   197→    appendLog('Migrations complete.')\n   198→\n   199→    // Step 4: Netlify site setup\n   200→    let siteId = process.env.NETLIFY_SITE_ID || ''\n   201→\n   202→    if (!siteId) {\n   203→      appendLog('Creating new Netlify site...')\n   204→      const appName = JSON.parse(readFileSync(join(appDir, 'package.json'), 'utf-8')).name as string\n   205→      const siteName = `${appName}-${Date.now()}`\n   206→      const create = spawnSync('npx', [\n   207→        'netlify', 'sites:create',\n   208→        '--account-slug', process.env.NETLIFY_ACCOUNT_SLUG!,\n   209→        '--name', siteName,\n   210→        '--disable-linking',\n   211→      ], {\n   212→        cwd: appDir,\n   213→        env: process.env,\n   214→        stdio: ['ignore', 'pipe', 'pipe'],\n   215→        encoding: 'utf-8',\n   216→        timeout: 60_000,\n   217→      })\n   218→      appendLog('=== NETLIFY SITES:CREATE STDOUT ===')\n   219→      appendLog(create.stdout || '')\n   220→      appendLog('=== NETLIFY SITES:CREATE STDERR ===')\n   221→      appendLog(create.stderr || '')\n   222→\n   223→      if (create.status !== 0) {\n   224→        throw new Error('Failed to create Netlify site')\n   225→      }\n   226→\n   227→      // Parse site ID from output (format: \"Project ID: <id>\" or \"Site ID: <id>\")\n   228→      // eslint-disable-next-line no-control-regex\n   229→      const plain = (create.stdout || '').replace(/\\x1b\\[\\d+m/g, '')\n   230→      const idMatch = plain.match(/(?:Project|Site) ID:\\s*([a-f0-9-]+)/)\n   231→      if (!idMatch) {\n   232→        appendLog('Could not parse site ID from netlify output')\n   233→        throw new Error('Failed to parse Netlify site ID')\n   234→      }\n   235→      siteId = idMatch[1]\n   236→      writeEnvVar('NETLIFY_SITE_ID', siteId)\n   237→      process.env.NETLIFY_SITE_ID = siteId\n   238→      appendLog(`Netlify site created: ${siteId}`)\n   239→    }\n   240→\n   241→    // Step 5: Build\n   242→    appendLog('Building app...')\n   243→    const build = spawnSync('npx', ['vite', 'build'], {\n   244→      cwd: appDir,\n   245→      env: process.env,\n   246→      stdio: ['ignore', 'pipe', 'pipe'],\n   247→      encoding: 'utf-8',\n   248→      timeout: 120_000,\n   249→    })\n   250→    appendLog('=== BUILD STDOUT ===')\n   251→    appendLog(build.stdout || '')\n   252→    appendLog('=== BUILD STDERR ===')\n   253→    appendLog(build.stderr || '')\n   254→\n   255→    if (build.status !== 0) {\n   256→      throw new Error('build')\n   257→    }\n   258→\n   259→    // Step 6: Deploy to Netlify\n   260→    appendLog('Deploying to Netlify...')\n   261→    const deploy = spawnSync('npx', [\n   262→      'netlify', 'deploy', '--prod',\n   263→      '--no-build',\n   264→      '--dir', 'dist',\n   265→      '--functions', './netlify/functions',\n   266→      '--site', siteId,\n   267→    ], {\n   268→      cwd: appDir,\n   269→      env: process.env,\n   270→      stdio: ['ignore', 'pipe', 'pipe'],\n   271→      encoding: 'utf-8',\n   272→      timeout: 180_000,\n   273→    })\n   274→    appendLog('=== DEPLOY STDOUT ===')\n   275→    appendLog(deploy.stdout || '')\n   276→    appendLog('=== DEPLOY STDERR ===')\n   277→    appendLog(deploy.stderr || '')\n   278→\n   279→    if (deploy.status !== 0) {\n   280→      throw new Error('netlify')\n   281→    }\n   282→\n   283→    // Extract deployed URL from output\n   284→    let deployedUrl = ''\n   285→    const urlMatch = (deploy.stdout || '').match(/https:\\/\\/[^\\s]+\\.netlify\\.app[^\\s]*/)\n   286→    if (urlMatch) {\n   287→      deployedUrl = urlMatch[0]\n   288→    }\n   289→\n   290→    // Step 7: Update deployment.txt\n   291→    updateDeploymentTxt(deployedUrl || `https://${siteId}.netlify.app`, siteId, neonProjectId, databaseUrl)\n   292→\n   293→    // Step 8: Print summary\n   294→    console.log(`Deployed to ${deployedUrl || siteId}`)\n   295→  } catch (err) {\n   296→    const step = err instanceof Error && (err.message === 'build' || err.message === 'netlify')\n   297→      ? err.message\n   298→      : 'unknown'\n   299→    appendLog(`Deploy failed: ${err}`)\n   300→    console.log(`Deploy failed (${step}) — see logs/deploy.log`)\n   301→    writeFileSync(logFile, log)\n   302→    process.exit(1)\n   303→  }\n   304→\n   305→  writeFileSync(logFile, log)\n   306→}\n   307→\n   308→main()\n   309→"}]},"parent_tool_use_id":"toolu_0124PX87NmVTMFNuEfZyVdNb","session_id":"681c40a1-193f-4fd4-bf2c-7d9971335ba4","uuid":"908e2f24-8c11-4bbb-b41a-6db1e33538c5"}
