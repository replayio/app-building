[2026-02-20T06:22:37.907Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01URgcFiW6rVFAcCUg11wLNq","type":"tool_result","content":"     1→import { useNavigate } from 'react-router-dom'\n     2→import { Square, CheckSquare } from 'lucide-react'\n     3→import type { Task } from '../../types'\n     4→\n     5→interface TasksSectionProps {\n     6→  tasks: Task[]\n     7→  onToggleTask: (taskId: string, completed: boolean) => void\n     8→}\n     9→\n    10→export function TasksSection({ tasks, onToggleTask }: TasksSectionProps) {\n    11→  const navigate = useNavigate()\n    12→\n    13→  function formatDueDate(dateStr: string | null): string {\n    14→    if (!dateStr) return ''\n    15→    const date = new Date(dateStr)\n    16→    const now = new Date()\n    17→    const today = new Date(now.getFullYear(), now.getMonth(), now.getDate())\n    18→    const tomorrow = new Date(today.getTime() + 86400000)\n    19→    const taskDate = new Date(date.getFullYear(), date.getMonth(), date.getDate())\n    20→\n    21→    if (taskDate.getTime() === today.getTime()) return 'Today'\n    22→    if (taskDate.getTime() === tomorrow.getTime()) return 'Tomorrow'\n    23→\n    24→    const diff = taskDate.getTime() - today.getTime()\n    25→    const days = Math.round(diff / 86400000)\n    26→    if (days > 0 && days <= 7) return 'Next Week'\n    27→    return date.toLocaleDateString()\n    28→  }\n    29→\n    30→  return (\n    31→    <div className=\"border border-border rounded-[6px] p-4 mb-4\" data-testid=\"tasks-section\">\n    32→      <div className=\"flex items-center justify-between mb-3\">\n    33→        <h2 className=\"text-[14px] font-semibold text-text-primary\">Tasks</h2>\n    34→        <span className=\"text-[12px] text-text-muted\" data-testid=\"tasks-unresolved-label\">Unresolved tasks</span>\n    35→      </div>\n    36→\n    37→      {tasks.length === 0 ? (\n    38→        <div className=\"text-[13px] text-text-muted py-2\" data-testid=\"tasks-empty-state\">No unresolved tasks</div>\n    39→      ) : (\n    40→        <div className=\"flex flex-col gap-1\">\n    41→          {tasks.map((task) => (\n    42→            <div\n    43→              key={task.id}\n    44→              data-testid={`task-item-${task.id}`}\n    45→              className=\"flex items-center gap-3 px-3 py-2.5 rounded-[4px] hover:bg-hover transition-colors duration-100 cursor-pointer group\"\n    46→            >\n    47→              <button\n    48→                onClick={(e) => { e.stopPropagation(); onToggleTask(task.id, true) }}\n    49→                data-testid={`task-checkbox-${task.id}`}\n    50→                className=\"flex-shrink-0 text-text-muted hover:text-status-active transition-colors duration-100\"\n    51→              >\n    52→                {task.completed ? (\n    53→                  <CheckSquare size={16} strokeWidth={1.75} />\n    54→                ) : (\n    55→                  <Square size={16} strokeWidth={1.75} />\n    56→                )}\n    57→              </button>\n    58→              <div\n    59→                className=\"flex-1 min-w-0\"\n    60→                onClick={() => navigate(`/tasks/${task.id}`)}\n    61→              >\n    62→                <span className=\"text-[13px] text-text-primary\" data-testid={`task-title-${task.id}`}>{task.title}</span>\n    63→                {task.due_date && (\n    64→                  <span className=\"text-[12px] text-text-muted ml-2\" data-testid={`task-due-date-${task.id}`}>\n    65→                    · Due: {formatDueDate(task.due_date)}\n    66→                  </span>\n    67→                )}\n    68→                {task.deal_name && (\n    69→                  <span className=\"text-[12px] text-text-muted ml-1\" data-testid={`task-deal-link-${task.id}`}>\n    70→                    , Deal: &apos;{task.deal_name}&apos;\n    71→                  </span>\n    72→                )}\n    73→              </div>\n    74→            </div>\n    75→          ))}\n    76→        </div>\n    77→      )}\n    78→    </div>\n    79→  )\n    80→}\n    81→"}]},"parent_tool_use_id":"toolu_01196KGB8bdasTxURJc67Yq9","session_id":"82159325-80a8-4f42-98bc-8cbf54a4e3f1","uuid":"ef2ae85e-019f-4f44-b304-b014ad761966"}
