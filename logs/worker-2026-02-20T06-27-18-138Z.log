ror } = useAppSelector((state) => state.clients)\n    33→\n    34→  const [tasks, setTasks] = useState<Task[]>([])\n    35→  const [deals, setDeals] = useState<Deal[]>([])\n    36→  const [attachments, setAttachments] = useState<Attachment[]>([])\n    37→  const [people, setPeople] = useState<Person[]>([])\n    38→  const [timeline, setTimeline] = useState<TimelineEvent[]>([])\n    39→\n    40→  const [addTaskOpen, setAddTaskOpen] = useState(false)\n    41→  const [addDealOpen, setAddDealOpen] = useState(false)\n    42→  const [addAttachmentOpen, setAddAttachmentOpen] = useState(false)\n    43→  const [addPersonOpen, setAddPersonOpen] = useState(false)\n    44→  const [deleteAttachmentId, setDeleteAttachmentId] = useState<string | null>(null)\n    45→  const [availableUsers, setAvailableUsers] = useState<{ name: string }[]>([])\n    46→\n    47→  useEffect(() => {\n    48→    fetch('/.netlify/functions/users')\n    49→      .then(r => r.json())\n    50→      .then((data: { users: { name: string }[] }) => setAvailableUsers(data.users))\n    51→      .catch(() => {})\n    52→  }, [])\n    53→\n    54→  const loadClientData = useCallback(async () => {\n    55→    if (!clientId) return\n    56→\n    57→    const [tasksRes, dealsRes, attachmentsRes, peopleRes, timelineRes] = await Promise.all([\n    58→      fetch(`/.netlify/functions/client-tasks?clientId=${clientId}`),\n    59→      fetch(`/.netlify/functions/client-deals?clientId=${clientId}`),\n    60→      fetch(`/.netlify/functions/client-attachments?clientId=${clientId}`),\n    61→      fetch(`/.netlify/functions/client-people?clientId=${clientId}`),\n    62→      fetch(`/.netlify/functions/client-timeline?clientId=${clientId}`),\n    63→    ])\n    64→\n    65→    if (tasksRes.ok) {\n    66→      const data = await tasksRes.json() as { tasks: Task[] }\n    67→      setTasks(data.tasks)\n    68→    }\n    69→    if (dealsRes.ok) {\n    70→      const data = await dealsRes.json() as { deals: Deal[] }\n    71→      setDeals(data.deals)\n    72→    }\n    73→    if (attachmentsRes.ok) {\n    74→      const data = await attachmentsRes.json() as { attachments: Attachment[] }\n    75→      setAttachments(data.attachments)\n    76→    }\n    77→    if (peopleRes.ok) {\n    78→      const data = await peopleRes.json() as { people: Person[] }\n    79→      setPeople(data.people)\n    80→    }\n    81→    if (timelineRes.ok) {\n    82→      const data = await timelineRes.json() as { events: TimelineEvent[] }\n    83→      setTimeline(data.events)\n    84→    }\n    85→  }, [clientId])\n    86→\n    87→  useEffect(() => {\n    88→    if (clientId) {\n    89→      dispatch(fetchClient(clientId))\n    90→      loadClientData()\n    91→    }\n    92→  }, [clientId, dispatch, loadClientData])\n    93→\n    94→  function handleUpdateClient(data: Record<string, unknown>) {\n    95→    if (!clientId) return\n    96→    dispatch(updateClient({ clientId, data })).then(() => {\n    97→      loadClientData()\n    98→    })\n    99→  }\n   100→\n   101→  async function handleAddTask(data: {\n   102→    title: string\n   103→    description: string\n   104→    due_date: string\n   105→    priority: string\n   106→    deal_id: string\n   107→  }) {\n   108→    if (!clientId) return\n   109→    const res = await fetch('/.netlify/functions/client-tasks', {\n   110→      method: 'POST',\n   111→      headers: { 'Content-Type': 'application/json' },\n   112→      body: JSON.stringify({\n   113→        ...data,\n   114→        client_id: clientId,\n   115→        deal_id: data.deal_id || null,\n   116→      }),\n   117→    })\n   118→    if (res.ok) {\n   119→      setAddTaskOpen(false)\n   120→      loadClientData()\n   121→    }\n   122→  }\n   123→\n   124→  async function handleAddDeal(data: {\n   125→    name: string\n   126→    value: number\n   127→    stage: string\n   128→    owner: string\n   129→    probability: number\n   130→    expected_close_date: string | null\n   131→  }) {\n   132→    i