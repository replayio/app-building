status}\n    85→              onChange={(v) => setStatus(v as ClientStatus)}\n    86→              options={[\n    87→                { value: 'active', label: 'Active' },\n    88→                { value: 'inactive', label: 'Inactive' },\n    89→                { value: 'prospect', label: 'Prospect' },\n    90→                { value: 'churned', label: 'Churned' },\n    91→              ]}\n    92→            />\n    93→          </div>\n    94→        </div>\n    95→        <div>\n    96→          <label className=\"block text-[12px] font-medium text-text-muted mb-1\">Tags (comma-separated)</label>\n    97→          <input\n    98→            type=\"text\"\n    99→            value={tagsInput}\n   100→            onChange={(e) => setTagsInput(e.target.value)}\n   101→            placeholder=\"e.g. Enterprise, SaaS, VIP\"\n   102→            data-testid=\"client-header-tags-input\"\n   103→            className=\"w-full max-w-[400px] h-[34px] px-3 text-[13px] text-text-primary bg-base border border-border rounded-[5px] placeholder:text-text-disabled focus:outline-none focus:border-accent\"\n   104→          />\n   105→        </div>\n   106→      </div>\n   107→    )\n   108→  }\n   109→\n   110→  return (\n   111→    <div className=\"mb-6\" data-testid=\"client-header\">\n   112→      <div className=\"flex items-start justify-between\">\n   113→        <div>\n   114→          <div className=\"flex items-center gap-3 mb-2\">\n   115→            <h1 className=\"text-[24px] font-semibold text-text-primary\" data-testid=\"client-header-title\">{client.name}</h1>\n   116→            <span className=\"inline-flex items-center px-2 py-0.5 rounded text-[11px] font-medium bg-border/50 text-text-secondary capitalize\" data-testid=\"client-header-type-badge\">\n   117→              {client.type === 'organization' ? 'Organization' : 'Individual'}\n   118→            </span>\n   119→            <StatusDropdown status={client.status} onChange={handleStatusChange} />\n   120→          </div>\n   121→          <div className=\"flex items-center gap-1.5 flex-wrap\" data-testid=\"client-header-tags\">\n   122→            {client.tags.map((tag) => (\n   123→              <TagBadge key={tag} tag={tag} />\n   124→            ))}\n   125→            {client.source_type && (\n   126→              <span className=\"inline-flex items-center gap-1 px-1.5 py-0.5 rounded text-[11px] font-normal text-text-muted bg-border/50\" data-testid=\"client-header-source-badge\">\n   127→                {client.source_type}\n   128→                {client.source_detail && ` (${client.source_detail})`}\n   129→              </span>\n   130→            )}\n   131→          </div>\n   132→        </div>\n   133→        <button\n   134→          onClick={() => setEditing(true)}\n   135→          className=\"inline-flex items-center justify-center w-7 h-7 rounded-[4px] text-text-muted hover:bg-hover transition-colors duration-100\"\n   136→          title=\"Edit client\"\n   137→          data-testid=\"client-header-edit-button\"\n   138→        >\n   139→          <Pencil size={14} strokeWidth={1.75} />\n   140→        </button>\n   141→      </div>\n   142→    </div>\n   143→  )\n   144→}\n   145→\n   146→function StatusDropdown({ status, onChange }: { status: ClientStatus; onChange: (s: ClientStatus) => void }) {\n   147→  const [open, setOpen] = useState(false)\n   148→  const statuses: ClientStatus[] = ['active', 'inactive', 'prospect', 'churned']\n   149→\n   150→  return (\n   151→    <div className=\"relative\" data-testid=\"client-header-status-dropdown\">\n   152→      <button onClick={() => setOpen(!open)} data-testid=\"status-dropdown-button\">\n   153→        <span data-testid=\"client-header-status-badge\"><StatusBadge status={status} /></span>\n   154→      </button>\n   155→      {open && (\n   156→        <>\n   157→          <div className=\"fixed inset-0 z-40\" onClick={() => setOpen(false)} />\n   158→          <div className=\"absolute top-full left