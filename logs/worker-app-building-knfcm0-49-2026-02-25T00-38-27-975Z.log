02→      })\n   103→      .addCase(updateRun.fulfilled, (state, action) => {\n   104→        const idx = state.items.findIndex((r) => r.id === action.payload.id);\n   105→        if (idx !== -1) state.items[idx] = action.payload;\n   106→        if (state.currentRun?.id === action.payload.id) {\n   107→          state.currentRun = action.payload;\n   108→        }\n   109→      });\n   110→  },\n   111→});\n   112→\n   113→export const { clearCurrentRun } = runsSlice.actions;\n   114→export const runsReducer = runsSlice.reducer;\n   115→"}]},"parent_tool_use_id":"toolu_01XW8m1T2kQZvDguxQj1syq9","session_id":"4ad7a60a-f307-4ef7-9ef4-c8396c3e38d1","uuid":"6d957446-8486-4ffc-b0ea-3b88780a4809"}
