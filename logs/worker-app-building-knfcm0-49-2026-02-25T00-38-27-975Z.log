[2026-02-25T00:33:25.519Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01SngMmq3A5HY8rRoT5Hqwyp","type":"tool_result","content":"     1→import { useEffect } from \"react\";\n     2→import { useNavigate } from \"react-router-dom\";\n     3→import { useAppDispatch, useAppSelector } from \"../hooks\";\n     4→import { fetchRunById, clearCurrentRun } from \"../slices/runsSlice\";\n     5→import type { ProductionRun, RunStatus, RunForecast, RecipeMaterial } from \"../types\";\n     6→\n     7→interface RunDetailsPopupProps {\n     8→  run: ProductionRun;\n     9→  onClose: () => void;\n    10→}\n    11→\n    12→function formatPopupDateTime(dateStr: string): string {\n    13→  const d = new Date(dateStr);\n    14→  const months = [\n    15→    \"Jan\", \"Feb\", \"Mar\", \"Apr\", \"May\", \"Jun\",\n    16→    \"Jul\", \"Aug\", \"Sep\", \"Oct\", \"Nov\", \"Dec\",\n    17→  ];\n    18→  const month = months[d.getMonth()];\n    19→  const day = d.getDate();\n    20→  const year = d.getFullYear();\n    21→  const hours = d.getHours();\n    22→  const minutes = String(d.getMinutes()).padStart(2, \"0\");\n    23→  const ampm = hours >= 12 ? \"PM\" : \"AM\";\n    24→  const h12 = hours % 12 || 12;\n    25→  return `${month} ${day}, ${year} ${String(h12).padStart(2, \"0\")}:${minutes} ${ampm}`;\n    26→}\n    27→\n    28→function getStatusBadgeClass(status: RunStatus): string {\n    29→  switch (status) {\n    30→    case \"Scheduled\":\n    31→      return \"badge badge--scheduled\";\n    32→    case \"Confirmed\":\n    33→      return \"badge badge--confirmed\";\n    34→    case \"Cancelled\":\n    35→      return \"badge badge--cancelled\";\n    36→    case \"On Track\":\n    37→      return \"badge badge--on-track\";\n    38→    case \"Material Shortage\":\n    39→      return \"badge badge--material-shortage\";\n    40→    case \"In Progress\":\n    41→      return \"badge badge--in-progress\";\n    42→    case \"Pending Approval\":\n    43→      return \"badge badge--pending-approval\";\n    44→    case \"Completed\":\n    45→      return \"badge badge--confirmed\";\n    46→    default:\n    47→      return \"badge\";\n    48→  }\n    49→}\n    50→\n    51→export function RunDetailsPopup({ run, onClose }: RunDetailsPopupProps) {\n    52→  const navigate = useNavigate();\n    53→  const dispatch = useAppDispatch();\n    54→  const currentRun = useAppSelector((s) => s.runs.currentRun);\n    55→\n    56→  useEffect(() => {\n    57→    dispatch(fetchRunById(run.id));\n    58→    return () => {\n    59→      dispatch(clearCurrentRun());\n    60→    };\n    61→  }, [dispatch, run.id]);\n    62→\n    63→  const detail = currentRun?.id === run.id ? currentRun : null;\n    64→  const product = run.product_name || \"Unknown\";\n    65→  const recipe = run.recipe_name || \"No Recipe\";\n    66→  const title = `Run Details: ${product} (${recipe})`;\n    67→\n    68→  return (\n    69→    <div\n    70→      className=\"run-popup-overlay\"\n    71→      data-testid=\"run-details-popup-overlay\"\n    72→      onClick={onClose}\n    73→    >\n    74→      <div\n    75→        className=\"run-popup-content\"\n    76→        data-testid=\"run-details-popup\"\n    77→        onClick={(e) => e.stopPropagation()}\n    78→      >\n    79→        <h2 className=\"run-popup-title\" data-testid=\"run-popup-title\">\n    80→          {title}\n    81→        </h2>\n    82→\n    83→        <div className=\"run-popup-field\" data-testid=\"run-popup-start\">\n    84→          <span className=\"run-popup-label\">Start:</span>\n    85→          <span className=\"run-popup-value\">{formatPopupDateTime(run.start_date)}</span>\n    86→        </div>\n    87→\n    88→        <div className=\"run-popup-field\" data-testid=\"run-popup-end\">\n    89→          <span className=\"run-popup-label\">End:</span>\n    90→          <span className=\"run-popup-value\">{formatPopupDateTime(run.end_date)}</span>\n    91→        </div>\n    92→\n    93→        <div className=\"run-popup-field\" data-testid=\"run-popup-quantity\">\n    94→          <span className=\"run-popup-label\">Quantity:</span>\n    95→          <span className=\"run-popup-value\">\n    96→            {run.planned_quantity} {run.unit}\n    97→          </span>\n    98→        </div>\n    99→\n   100→        <div className=\"run-popup-field\" data-testid=\"run-popup-status\">\n   101→          <span className=\"run-popup-label\">Status:</span>\n   102→          <span className=\"run-popup-value\">\n   103→            <span className={getStatusBadgeClass(run.status)}>{run.status}</span>\n   104→          </span>\n   105→        </div>\n   106→\n   107→        <div className=\"run-popup-materials\" data-testid=\"run-popup-materials\">\n   108→          <div className=\"run-popup-materials-title\">Materials:</div>\n   109→          {detail && detail.forecasts.length > 0 ? (\n   110→            detail.forecasts.map((f: RunForecast) => {\n   111→              const isOk = f.forecast_available >= f.required_amount;\n   112→              return (\n   113→                <div\n   114→                  key={f.id}\n   115→                  className=\"run-popup-material-item\"\n   116→                  data-testid={`run-popup-material-${f.material_name.toLowerCase().replace(/\\s+/g, \"-\")}`}\n   117→                >\n   118→                  <span className=\"run-popup-material-name\">\n   119→                    {f.material_name} ({f.required_amount} {f.unit})\n   120→                  </span>\n   121→                  <span\n   122→                    className={`run-popup-material-status ${isOk ? \"run-popup-material-status--ok\" : \"run-popup-material-status--shortage\"}`}\n   123→                  >\n   124→                    {isOk ? \"OK for all\" : \"Shortage\"}\n   125→                  </span>\n   126→                </div>\n   127→              );\n   128→            })\n   129→          ) : detail && detail.materials.length > 0 ? (\n   130→   