[2026-02-24T03:35:55.182Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01JhBxXviPzVGG8P5u4ufcRQ","type":"tool_result","content":"     1→import { execSync } from 'child_process'\n     2→import { readFileSync, existsSync } from 'fs'\n     3→import { resolve } from 'path'\n     4→import { initSchema } from './schema.js'\n     5→\n     6→const APP_DIR = resolve(import.meta.dirname, '..')\n     7→\n     8→// ---------------------------------------------------------------------------\n     9→// Helpers\n    10→// ---------------------------------------------------------------------------\n    11→\n    12→function loadEnv(): Record<string, string> {\n    13→  const envPath = resolve(APP_DIR, '.env')\n    14→  const vars: Record<string, string> = {}\n    15→  if (existsSync(envPath)) {\n    16→    const lines = readFileSync(envPath, 'utf-8').split('\\n')\n    17→    for (const line of lines) {\n    18→      const trimmed = line.trim()\n    19→      if (!trimmed || trimmed.startsWith('#')) continue\n    20→      const eqIdx = trimmed.indexOf('=')\n    21→      if (eqIdx === -1) continue\n    22→      const key = trimmed.slice(0, eqIdx).trim()\n    23→      const val = trimmed.slice(eqIdx + 1).trim().replace(/^[\"']|[\"']$/g, '')\n    24→      vars[key] = val\n    25→    }\n    26→  }\n    27→  return vars\n    28→}\n    29→\n    30→function required(name: string): string {\n    31→  const val = process.env[name]\n    32→  if (!val) {\n    33→    console.error(`Missing required environment variable: ${name}`)\n    34→    process.exit(1)\n    35→  }\n    36→  return val\n    37→}\n    38→\n    39→function killStaleProcesses(): void {\n    40→  for (const pattern of ['netlify', 'vite']) {\n    41→    try {\n    42→      execSync(`pkill -f \"${pattern}\"`, { stdio: 'ignore' })\n    43→    } catch {\n    44→      // no matching processes — fine\n    45→    }\n    46→  }\n    47→  // small grace period for ports to free up\n    48→  execSync('sleep 1', { stdio: 'ignore' })\n    49→}\n    50→\n    51→// ---------------------------------------------------------------------------\n    52→// Neon branch helpers\n    53→// ---------------------------------------------------------------------------\n    54→\n    55→const NEON_API = 'https://console.neon.tech/api/v2'\n    56→\n    57→interface NeonBranch {\n    58→  id: string\n    59→  name: string\n    60→}\n    61→\n    62→interface NeonEndpoint {\n    63→  host: string\n    64→}\n    65→\n    66→async function neonFetch(path: string, opts: RequestInit = {}): Promise<Response> {\n    67→  const apiKey = required('NEON_API_KEY')\n    68→  const res = await fetch(`${NEON_API}${path}`, {\n    69→    ...opts,\n    70→    headers: {\n    71→      'Content-Type': 'application/json',\n    72→      Authorization: `Bearer ${apiKey}`,\n    73→      ...(opts.headers as Record<string, string> | undefined),\n    74→    },\n    75→  })\n    76→  if (!res.ok) {\n    77→    const text = await res.text()\n    78→    throw new Error(`Neon API ${opts.method ?? 'GET'} ${path} failed (${res.status}): ${text}`)\n    79→  }\n    80→  return res\n    81→}\n    82→\n    83→async function listBranches(projectId: string): Promise<NeonBranch[]> {\n    84→  const res = await neonFetch(`/projects/${projectId}/branches`)\n    85→  const data = (await res.json()) as { branches: NeonBranch[] }\n    86→  return data.branches\n    87→}\n    88→\n    89→async function deleteBranch(projectId: string, branchId: string): Promise<void> {\n    90→  await neonFetch(`/projects/${projectId}/branches/${branchId}`, { method: 'DELETE' })\n    91→}\n    92→\n    93→async function createBranch(\n    94→  projectId: string,\n    95→  branchName: string,\n    96→): Promise<{ branchId: string; host: string }> {\n    97→  const res = await neonFetch(`/projects/${projectId}/branches`, {\n    98→    method: 'POST',\n    99→    body: JSON.stringify({\n   100→      branch: { name: branchName },\n   101→      endpoints: [{ type: 'read_write' }],\n   102→    }),\n   103→  })\n   104→  const data = (await res.json()) as {\n   105→    branch: { id: string }\n   106→    endpoints: NeonEndpoint[]\n   107→  }\n   108→  return { branchId: data.branch.id, host: data.endpoints[0].host }\n   109→}\n   110→\n   111→function buildDatabaseUrl(templateUrl: string, newHost: string): string {\n   112→  // Replace the host portion of the template DATABASE_URL with the new branch's host\n   113→  const url = new URL(templateUrl)\n   114→  url.hostname = newHost\n   115→  return url.toString()\n   116→}\n   117→\n   118→// ---------------------------------------------------------------------------\n   119→// Replay recordings.log parser\n   120→// ---------------------------------------------------------------------------\n   121→\n   122→interface RecordingEntry {\n   123→  id: string\n   124→  kind: string\n   125→  timestamp?: string\n   126→  metadata?: Record<string, unknown>\n   127→  duration?: number\n   128→}\n   129→\n   130→interface ParsedRecording {\n   131→  id: string\n   132→  testResult?: string\n   133→  testTitle?: string\n   134→  specFile?: string\n   135→  duration: number\n   136→  writeFinished: boolean\n   137→}\n   138→\n   139→function parseRecordingsLog(): ParsedRecording[] {\n   140→  const logPath = resolve(process.env.HOME ?? '~', '.replay/recordings.log')\n   141→  if (!existsSync(logPath)) return []\n   142→\n   143→  const content = readFileSync(logPath, 'utf-8')\n   144→  const lines = content.split('\\n').filter((l) => l.trim())\n   145→\n   146→  const recordings = new Map<string, ParsedRecording>()\n   147→\n   148→  for (const line of lines) {\n   149→    let entry: RecordingEntry\n   150→    try {\n   151→      entry = JSON.parse(line) as RecordingEntry\n   152→    } catch {\n   153→      continue\n   154→    }\n   155→\n   156→    if (entry.kind === 'createRecording' && entry.id) {\n   157→      recordings.set(entry.id, {\n   158→        id: entry.id,\n   159→        duration: 0,\n   160→        writeFinished: false,\n   161→      })\n   162→    }\n   163→\n   164→    if (entry.kind === 'addMetadata' && entry.id) {\n   165→      const rec = recordings.get(entry.id)\n   166→      if (rec && entry.metadata) {\n   167→        const test = entry.metadata.test as\n   168→          | { result?: string; title?: string; path?: string[] }\n   169→          | undefined\n   170→        if (test) {\n   171→          rec.testResult = test.result\n   172→          rec.testTitle = test.title\n   173→          if (test.path) rec.specFile = test.path[0]\n   174→        }\n   175→      }\n   176→    }\n   177→\n   178→    if (entry.kind === 'writeFinished' && entry.id) {\n   179→      const rec = recordings.get(entry.id)\n   180→      if (rec) {\n   181→        rec.writeFinished = true\n   182→        if (entry.duration != null) rec.duration = entry.duration\n   183→      }\n   184→    }\n   185→  }\n   186→\n   187→  return Array.from(recordings.values())\n   188→}\n   189→\n   190→// ---------------------------------------------------------------------------\n   191→// Main\n   192→// ---------------------------------------------------------------------------\n   193→\n   194→async function main(): Promise<void> {\n   195→  const testFile = process.argv[2]\n   196→  if (!testFile) {\n   197→    console.error('Usage: tsx scripts/test.ts <testFile>')\n   198→    console.error('Example: npm run test tests/auth.spec.ts')\n   199→    process.exit(1)\n   200→  }\n   201→\n   202→  // Load .env into process.env so child processes inherit them\n   203→  const envVars = loadEnv()\n   204→  for (const [k, v] of Object.entries(envVars)) {\n   205→    if (!process.env[k]) process.env[k] = v\n   206→  }\n   207→\n   208→  const neonProjectId = required('NEON_PROJECT_ID')\n   209→  const templateDbUrl = required('DATABASE_URL')\n   210→  required('NEON_API_KEY')\n   211→\n   212→  // 1. Kill stale processes\n   213→  console.log('Killing stale processes...')\n   214→  killStaleProcesses()\n   215→\n   216→  // 2. Clean up stale Neon branches\n   217→  console.log('Cleaning up stale Neon branches...')\n   218→  const branches = await listBranches(neonProjectId)\n   219→  for (const branch of branches) {\n   220→    if (branch.name.startsWith('test-worker-')) {\n   221→      console.log(`  Deleting stale branch: ${branch.name}`)\n   222→      await deleteBranch(neonProjectId, branch.id)\n   223→    }\n   224→  }\n   225→\n   226→  // 3. Create ephemeral Neon branch\n   227→  const runId = `${Date.now()}-${Math.random().toString(36).slice(2, 8)}`\n   228→  const branchName = `test-worker-${runId}`\n   229→  console.log(`Creating ephemeral branch: ${branchName}`)\n   230→  const { branchId, host } = await createBranch(neonProjectId, branchName)\n   231→  const databaseUrl = buildDatabaseUrl(templateDbUrl, host)\n   232→\n   233→  let playwrightExitCode = 1\n   234→\n   235→  try {\n   236→    // 4. Initialize schema and seed\n   237→    console.log('Initializing schema...')\n   238→    await initSchema(databaseUrl)\n   239→\n   240→    // Seed test data if seed module exists\n   241→    const seedPath = resolve(APP_DIR, 'scripts/seed-db.ts')\n   242→    if (existsSync(seedPath)) {\n   243→      console.log('Seeding test data...')\n   244→      execSync(`npx tsx scripts/seed-db.ts`, {\n   245→        stdio: 'inherit',\n   246→        cwd: APP_DIR,\n   247→        env: { ...process.env, DATABASE_URL: databaseUrl },\n   248→      })\n   249→    }\n   250→\n   251→    // 5. Remove stale recordings\n   252→    console.log('Removing stale recordings...')\n   253→    try {\n   254→      execSync('npx replayio remove --all', { stdio: 'inherit', cwd: APP_DIR })\n   255→    } catch {\n   256→      // may fail if no recordings exist\n   257→    }\n   258→\n   259→    // 6. Run Playwright\n   260→    console.log(`\\nRunning tests: ${testFile}\\n`)\n   261→    try {\n   262→      execSync(`npx playwright test ${testFile} --retries 0`, {\n   263→        stdio: 'inherit',\n   264→        cwd: APP_DIR,\n   265→        env: { ...process.env, DATABASE_URL: databaseUrl },\n   266→      })\n   267→      playwrightExitCode = 0\n   268→    } catch (err) {\n   269→      const error = err as { status?: number }\n   270→      playwrightExitCode = error.status ?? 1\n   271→    }\n   272→\n   273→    // 7. On failure — parse recordings and upload\n   274→    if (playwrightExitCode !== 0) {\n   275→      console.log('\\nTests failed. Processing recordings...\\n')\n   276→\n   277→      const recordings = parseRecordingsLog()\n   278→\n   279→      // Log metadata block\n   280→      const recordingsWithMeta = recordings.filter((r) => r.testResult)\n   281→      if (recordingsWithMeta.length > 0) {\n   282→        console.log('=== REPLAY RECORDINGS METADATA ===')\n   283→        for (const rec of recordingsWithMeta) {\n   284→          console.log(\n   285→            JSON.stringify({\n   286→              id: rec.id,\n   287→              testResult: rec.testResult,\n   288→              testTitle: rec.testTitle,\n   289→              specFile: rec.specFile,\n   290→              duration: rec.duration,\n   291→            }),\n   292→          )\n   293→        }\n   294→        console.log('=== END REPLAY RECORDINGS METADATA ===')\n   295→      }\n   296→\n   297→      // Upload exactly ONE recording: failed/timedOut with longest duration\n   298→      const failedRecordings = recordings.filter(\n   299→        (r) =>\n   300→          r.writeFinished &&\n   301→          (r.testResult === 'failed' || r.testResult === 'timedOut'),\n   302→      )\n   303→      failedRecordings.sort((a, b) => b.duration - a.duration)\n   304→\n   305→      if (failedRecordings.length > 0) {\n   306→        const toUpload = failedRecordings[0]\n   307→        console.log(\n   308→          `\\nUploading recording ${toUpload.id} (${toUpload.testTitle}, duration: ${toUpload.duration}ms)...`,\n   309→        )\n   310→       