  49→          setSearchText(e.target.value);\n    50→          setShowDropdown(true);\n    51→        }}\n    52→        onFocus={() => {\n    53→          setSearchText(\"\");\n    54→          setShowDropdown(true);\n    55→        }}\n    56→        onBlur={() => setTimeout(() => setShowDropdown(false), 150)}\n    57→      />\n    58→      {showDropdown && filtered.length > 0 && (\n    59→        <div className=\"searchable-select-dropdown\">\n    60→          {filtered.map((o) => (\n    61→            <button\n    62→              key={o.id}\n    63→              className=\"searchable-select-option\"\n    64→              onMouseDown={() => {\n    65→                onChange(o.id);\n    66→                setShowDropdown(false);\n    67→              }}\n    68→            >\n    69→              {o.label}\n    70→            </button>\n    71→          ))}\n    72→        </div>\n    73→      )}\n    74→    </div>\n    75→  );\n    76→}\n    77→"}]},"parent_tool_use_id":"toolu_01SNKdbrL9pjPWLdkqqQqphW","session_id":"29f9c70d-be34-45a1-a8d4-b264bbe3f0fb","uuid":"3f992afc-b7e8-409c-93db-69614b38f53f"}
