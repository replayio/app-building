[2026-02-24T01:05:55.731Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01GaPUohkEkG1KJcABHZi4kM","type":"tool_result","content":"     1→import type { Context } from '@netlify/functions'\n     2→import { neon } from '@neondatabase/serverless'\n     3→\n     4→const ITEMS_PER_PAGE = 50\n     5→\n     6→export default async function handler(req: Request, _context: Context) {\n     7→  const databaseUrl = process.env.DATABASE_URL\n     8→  if (!databaseUrl) {\n     9→    return Response.json({ error: 'Database not configured' }, { status: 500 })\n    10→  }\n    11→\n    12→  const sql = neon(databaseUrl)\n    13→  const url = new URL(req.url)\n    14→  const segments = url.pathname.split('/').filter(Boolean)\n    15→  const resourceId = segments[3] || null\n    16→\n    17→  // GET requests\n    18→  if (req.method === 'GET') {\n    19→    // CSV export\n    20→    if (url.searchParams.get('export') === 'csv') {\n    21→      const rows = await sql`\n    22→        SELECT i.name, i.title, i.email, i.phone, i.location,\n    23→               COALESCE(\n    24→                 (SELECT STRING_AGG(c.name, ', ' ORDER BY c.name)\n    25→                  FROM client_individuals ci\n    26→                  JOIN clients c ON ci.client_id = c.id\n    27→                  WHERE ci.individual_id = i.id),\n    28→                 ''\n    29→               ) as client_name\n    30→        FROM individuals i\n    31→        ORDER BY i.name ASC\n    32→      `\n    33→      const header = 'Name,Title,Email,Phone,Location,Client Name'\n    34→      const csvRows = rows.map((r: Record<string, unknown>) => {\n    35→        const fields = [r.name, r.title || '', r.email || '', r.phone || '', r.location || '', r.client_name || '']\n    36→        return fields.map((f) => `\"${String(f).replace(/\"/g, '\"\"')}\"`).join(',')\n    37→      })\n    38→      const csv = [header, ...csvRows].join('\\n')\n    39→      return new Response(csv, {\n    40→        status: 200,\n    41→        headers: {\n    42→          'Content-Type': 'text/csv',\n    43→          'Content-Disposition': 'attachment; filename=\"contacts.csv\"',\n    44→        },\n    45→      })\n    46→    }\n    47→\n    48→    // Single individual by ID\n    49→    if (resourceId) {\n    50→      const rows = await sql`\n    51→        SELECT i.*,\n    52→               COALESCE(\n    53→                 (SELECT JSON_AGG(JSON_BUILD_OBJECT('id', c.id, 'name', c.name))\n    54→                  FROM client_individuals ci\n    55→                  JOIN clients c ON ci.client_id = c.id\n    56→                  WHERE ci.individual_id = i.id),\n    57→                 '[]'::json\n    58→               ) as clients\n    59→        FROM individuals i\n    60→        WHERE i.id = ${resourceId}\n    61→      `\n    62→      if (rows.length === 0) {\n    63→        return Response.json({ error: 'Individual not found' }, { status: 404 })\n    64→      }\n    65→      return Response.json(rows[0])\n    66→    }\n    67→\n    68→    // List individuals with search and pagination\n    69→    const search = url.searchParams.get('search') || ''\n    70→    const page = Math.max(1, parseInt(url.searchParams.get('page') || '1', 10))\n    71→    const offset = (page - 1) * ITEMS_PER_PAGE\n    72→\n    73→    const countResult = await sql`\n    74→      SELECT COUNT(*) as count\n    75→      FROM individuals i\n    76→      WHERE (${search} = '' OR\n    77→             i.name ILIKE ${'%' + search + '%'} OR\n    78→             i.email ILIKE ${'%' + search + '%'} OR\n    79→             i.title ILIKE ${'%' + search + '%'} OR\n    80→             i.phone ILIKE ${'%' + search + '%'} OR\n    81→             i.location ILIKE ${'%' + search + '%'})\n    82→    `\n    83→    const total = parseInt(String(countResult[0].count), 10)\n    84→\n    85→    const rows = await sql`\n    86→      SELECT i.*,\n    87→             COALESCE(\n    88→               (SELECT ARRAY_AGG(c.name ORDER BY c.name)\n    89→                FROM client_individuals ci\n    90→                JOIN clients c ON ci.client_id = c.id\n    91→                WHERE ci.individual_id = i.id),\n    92→               '{}'\n    93→             ) as client_names\n    94→      FROM individuals i\n    95→      WHERE (${search} = '' OR\n    96→             i.name ILIKE ${'%' + search + '%'} OR\n    97→             i.email ILIKE ${'%' + search + '%'} OR\n    98→             i.title ILIKE ${'%' + search + '%'} OR\n    99→             i.phone ILIKE ${'%' + search + '%'} OR\n   100→             i.location ILIKE ${'%' + search + '%'})\n   101→      ORDER BY i.name ASC\n   102→      LIMIT ${ITEMS_PER_PAGE} OFFSET ${offset}\n   103→    `\n   104→\n   105→    return Response.json({ individuals: rows, total, page, perPage: ITEMS_PER_PAGE })\n   106→  }\n   107→\n   108→  // POST - create or import\n   109→  if (req.method === 'POST') {\n   110→    if (resourceId === 'import') {\n   111→      const body = await req.json()\n   112→      const { contacts: contactRows } = body as { contacts: Array<Record<string, string>> }\n   113→      if (!Array.isArray(contactRows)) {\n   114→        return Response.json({ error: 'contacts array required' }, { status: 400 })\n   115→      }\n   116→\n   117→      const errors: Array<{ row: number; error: string }> = []\n   118→      let imported = 0\n   119→\n   120→      for (let i = 0; i < contactRows.length; i++) {\n   121→        const row = contactRows[i]\n   122→        const name = (row.Name || row.name || '').trim()\n   123→        if (!name) {\n   124→          errors.push({ row: i + 1, error: 'Name is required' })\n   125→          continue\n   126→        }\n   127→\n   128→        const title = (row.Title || row.title || '').trim() || null\n   129→        const email = (row.Email || row.email || '').trim() || null\n   130→        const phone = (row.Phone || row.phone || '').trim() || null\n   131→        const location = (row.Location || row.location || '').trim() || null\n   132→\n   133→        const result = await sql`\n   134→          INSERT INTO individuals (name, title, email, phone, location)\n   135→          VALUES (${name}, ${title}, ${email}, ${phone}, ${location})\n   136→          RETURNING id\n   137→        `\n   138→        const individualId = result[0].id\n   139→\n   140→        // Link to client if provided\n   141→        const clientName = (row['Client Name'] || row.client_name || '').trim()\n   142→        if (clientName) {\n   143→          const clients = await sql`SELECT id FROM clients WHERE LOWER(name) = LOWER(${clientName}) LIMIT 1`\n   144→          if (clients.length > 0) {\n   145→            await sql`\n   146→              INSERT INTO client_individuals (client_id, individual_id)\n   147→              VALUES (${clients[0].id}, ${individualId})\n   148→              ON CONFLICT DO NOTHING\n   149→            `\n   150→            // Create timeline event for the client\n   151→            await sql`\n   152→              INSERT INTO timeline_events (client_id, individual_id, event_type, description, actor_name)\n   153→              VALUES (${clients[0].id}, ${individualId}, 'contact_added', ${'Contact added: ' + name}, 'System')\n   154→            `\n   155→          }\n   156→        }\n   157→\n   158→        imported++\n   159→      }\n   160→\n   161→      return Response.json({ imported, errors, total: contactRows.length })\n   162→    }\n   163→\n   164→    // Single create\n   165→    const body = await req.json()\n   166→    const { name, title, email, phone, location, client_id } = body as {\n   167→      name?: string\n   168→      title?: string\n   169→      email?: string\n   170→      phone?: string\n   171→      location?: string\n   172→      client_id?: string\n   173→    }\n   174→\n   175→    if (!name?.trim()) {\n   176→      return Response.json({ error: 'Name is required' }, { status: 400 })\n   177→    }\n   178→\n   179→    const rows = await sql`\n   180→      INSERT INTO individuals (name, title, email, phone, location)\n   181→      VALUES (\n   182→        ${name.trim()},\n   183→        ${title?.trim() || null},\n   184→        ${email?.trim() || null},\n   185→        ${phone?.trim() || null},\n   186→        ${location?.trim() || null}\n   187→      )\n   188→      RETURNING *\n   189→    `\n   190→\n   191→    const individual = rows[0]\n   192→\n   193→    // Link to client if provided\n   194→    if (client_id) {\n   195→      await sql`\n   196→        INSERT INTO client_individuals (client_id, individual_id)\n   197→        VALUES (${client_id}, ${individual.id})\n   198→        ON CONFLICT DO NOTHING\n   199→      `\n   200→      // Create timeline event\n   201→      await sql`\n   202→        INSERT INTO timeline_events (client_id, individual_id, event_type, description, actor_name)\n   203→        VALUES (${client_id}, ${individual.id}, 'contact_added', ${'Contact added: ' + name.trim()}, 'System')\n   204→      `\n   205→    }\n   206→\n   207→    // Return with client_names enriched\n   208→    const full = await sql`\n   209→      SELECT i.*,\n   210→             COALESCE(\n   211→               (SELECT ARRAY_AGG(c.name ORDER BY c.name)\n   212→                FROM client_individuals ci\n   213→                JOIN clients c ON ci.client_id = c.id\n   214→                WHERE ci.individual_id = i.id),\n   215→               '{}'\n   216→             ) as client_names\n   217→      FROM individuals i\n   218→      WHERE i.id = ${individual.id}\n   219→    `\n   220→\n   221→    return Response.json(full[0], { status: 201 })\n   222→  }\n   223→\n   224→  // PUT - update\n   225→  if (req.method === 'PUT') {\n   226→    if (!resourceId) {\n   227→      return Response.json({ error: 'Individual ID required' }, { status: 400 })\n   228→    }\n   229→\n   230→    const body = await req.json()\n   231→    const { name, title, email, phone, location } = body as {\n   232→      name?: string\n   233→      title?: string\n   234→      email?: string\n   235→      phone?: string\n   236→      location?: string\n   237→    }\n   238→\n   239→    const current = await sql`SELECT * FROM individuals WHERE id = ${resourceId}`\n   240→    if (current.length === 0) {\n   241→      return Response.json({ error: 'Individual not found' }, { status: 404 })\n   242→    }\n   243→\n   244→    const rows = await sql`\n   245→      UPDATE individuals SET\n   246→        name = ${name?.trim() || current[0].name},\n   247→        title = ${title !== undefined ? (title?.trim() || null) : current[0].title},\n   248→        email = ${email !== undefined ? (email?.trim() || null) : current[0].email},\n   249→        phone = ${phone !== undefined ? (phone?.trim() || null) : current[0].phone},\n   250→        location = ${location !== undefined ? (location?.trim() || null) : current[0].location},\n   251→        updated_at = NOW()\n   252→      WHERE id = ${resourceId}\n   253→      RETURNING *\n   254→    `\n   255→\n   256→    return Response.json(rows[0])\n   257→  }\n   258→\n   259→  // DELETE\n   260→  if (req.method === 'DELETE') {\n   261→    if (!resourceId) {\n   262→      return Response.json({ error: 'Individual ID required' }, { status: 400 })\n   263→    }\n   264→\n   265→    const existing = await sql`SELECT * FROM individuals WHERE id = ${resourceId}`\n   266→    if (existing.length === 0) {\n   267→      return Response.json({ error: 'Individual not found' }, { status: 404 })\n   268→    }\n   269→\n   270→    await sql`DELETE FROM individuals WHERE id = ${resourceId}`\n   271→\n   272→    return Response.json({ success: true })\n   273→  }\n   274→\n   275→  return Response.json({ error: 'Method not allowed' }, { status: 405 })\n   276→}\n   277→"}]},"parent_tool_use_id":"toolu_017BCNJwnBLTU5eecjitSNZi","session_id":"dd5e9048-3f3e-408e-818d-bb89ec672583","uuid":"c1a76eab-b0c0-451c-8d09-695804e41d2a"}
