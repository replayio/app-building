      .addCase(fetchUsers.pending, (state) => {\n    64→        if (state.items.length === 0) state.loading = true\n    65→        state.error = null\n    66→      })\n    67→      .addCase(fetchUsers.fulfilled, (state, action) => {\n    68→        state.loading = false\n    69→        state.items = action.payload.users\n    70→      })\n    71→      .addCase(fetchUsers.rejected, (state, action) => {\n    72→        state.loading = false\n    73→        state.error = action.payload as string\n    74→      })\n    75→      .addCase(fetchUser.pending, (state) => {\n    76→        if (!state.current) state.loading = true\n    77→        state.error = null\n    78→      })\n    79→      .addCase(fetchUser.fulfilled, (state, action) => {\n    80→        state.loading = false\n    81→        state.current = action.payload\n    82→      })\n    83→      .addCase(fetchUser.rejected, (state, action) => {\n    84→        state.loading = false\n    85→        state.error = action.payload as string\n    86→      })\n    87→  },\n    88→})\n    89→\n    90→export const { clearCurrent } = usersSlice.actions\n    91→export default usersSlice.reducer\n    92→"}]},"parent_tool_use_id":"toolu_017BCNJwnBLTU5eecjitSNZi","session_id":"dd5e9048-3f3e-408e-818d-bb89ec672583","uuid":"b93825fe-b656-47c8-b53c-8a89798d01cf"}
