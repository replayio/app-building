[2026-02-24T01:06:04.049Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01JApqbGds2FP91jT9WCbzix","type":"tool_result","content":"     1→import type { Context } from '@netlify/functions'\n     2→import { neon } from '@neondatabase/serverless'\n     3→import jwt from 'jsonwebtoken'\n     4→import { createHash, randomBytes } from 'crypto'\n     5→\n     6→const JWT_SECRET = process.env.JWT_SECRET || 'dev-secret-key'\n     7→\n     8→function hashPassword(password: string): string {\n     9→  return createHash('sha256').update(password).digest('hex')\n    10→}\n    11→\n    12→function verifyPassword(password: string, hash: string): boolean {\n    13→  return hashPassword(password) === hash\n    14→}\n    15→\n    16→function generateToken(userId: string, email: string): string {\n    17→  return jwt.sign({ userId, email }, JWT_SECRET, { expiresIn: '7d' })\n    18→}\n    19→\n    20→function verifyToken(token: string): { userId: string; email: string } | null {\n    21→  try {\n    22→    return jwt.verify(token, JWT_SECRET) as { userId: string; email: string }\n    23→  } catch {\n    24→    return null\n    25→  }\n    26→}\n    27→\n    28→export default async function handler(req: Request, context: Context) {\n    29→  const databaseUrl = process.env.DATABASE_URL\n    30→  if (!databaseUrl) {\n    31→    return new Response(JSON.stringify({ error: 'Database not configured' }), {\n    32→      status: 500,\n    33→      headers: { 'Content-Type': 'application/json' },\n    34→    })\n    35→  }\n    36→\n    37→  const sql = neon(databaseUrl)\n    38→  const url = new URL(req.url)\n    39→\n    40→  // GET /auth?action=me - Get current user from token\n    41→  if (req.method === 'GET') {\n    42→    const action = url.searchParams.get('action')\n    43→    if (action === 'me') {\n    44→      const authHeader = req.headers.get('Authorization')\n    45→      if (!authHeader?.startsWith('Bearer ')) {\n    46→        return new Response(JSON.stringify({ error: 'No token provided' }), {\n    47→          status: 401,\n    48→          headers: { 'Content-Type': 'application/json' },\n    49→        })\n    50→      }\n    51→\n    52→      const token = authHeader.slice(7)\n    53→      const decoded = verifyToken(token)\n    54→      if (!decoded) {\n    55→        return new Response(JSON.stringify({ error: 'Invalid token' }), {\n    56→          status: 401,\n    57→          headers: { 'Content-Type': 'application/json' },\n    58→        })\n    59→      }\n    60→\n    61→      const rows = await sql`SELECT id, email, name FROM users WHERE id = ${decoded.userId}`\n    62→      if (rows.length === 0) {\n    63→        return new Response(JSON.stringify({ error: 'User not found' }), {\n    64→          status: 401,\n    65→          headers: { 'Content-Type': 'application/json' },\n    66→        })\n    67→      }\n    68→\n    69→      return new Response(JSON.stringify({ user: rows[0] }), {\n    70→        status: 200,\n    71→        headers: { 'Content-Type': 'application/json' },\n    72→      })\n    73→    }\n    74→\n    75→    return new Response(JSON.stringify({ error: 'Unknown action' }), {\n    76→      status: 400,\n    77→      headers: { 'Content-Type': 'application/json' },\n    78→    })\n    79→  }\n    80→\n    81→  // POST /auth - Sign in, sign up, forgot password, etc.\n    82→  if (req.method === 'POST') {\n    83→    const body = await req.json()\n    84→    const { action, email, password, name } = body as {\n    85→      action: string\n    86→      email?: string\n    87→      password?: string\n    88→      name?: string\n    89→    }\n    90→\n    91→    if (action === 'signin') {\n    92→      if (!email || !password) {\n    93→        return new Response(JSON.stringify({ error: 'Email and password required' }), {\n    94→          status: 400,\n    95→          headers: { 'Content-Type': 'application/json' },\n    96→        })\n    97→      }\n    98→\n    99→      const rows = await sql`SELECT id, email, name, password_hash FROM users WHERE email = ${email}`\n   100→      if (rows.length === 0) {\n   101→        return new Response(JSON.stringify({ error: 'Invalid email or password' }), {\n   102→          status: 401,\n   103→          headers: { 'Content-Type': 'application/json' },\n   104→        })\n   105→      }\n   106→\n   107→      const user = rows[0]\n   108→      if (!verifyPassword(password, user.password_hash)) {\n   109→        return new Response(JSON.stringify({ error: 'Invalid email or password' }), {\n   110→          status: 401,\n   111→          headers: { 'Content-Type': 'application/json' },\n   112→        })\n   113→      }\n   114→\n   115→      const token = generateToken(user.id, user.email)\n   116→      return new Response(\n   117→        JSON.stringify({\n   118→          user: { id: user.id, email: user.email, name: user.name },\n   119→          token,\n   120→        }),\n   121→        { status: 200, headers: { 'Content-Type': 'application/json' } },\n   122→      )\n   123→    }\n   124→\n   125→    if (action === 'signup') {\n   126→      if (!email || !password) {\n   127→        return new Response(JSON.stringify({ error: 'Email and password required' }), {\n   128→          status: 400,\n   129→          headers: { 'Content-Type': 'application/json' },\n   130→        })\n   131→      }\n   132→\n   133→      // Check if user already exists\n   134→      const existing = await sql`SELECT id FROM users WHERE email = ${email}`\n   135→      if (existing.length > 0) {\n   136→        return new Response(JSON.stringify({ error: 'Email already in use' }), {\n   137→          status: 409,\n   138→          headers: { 'Content-Type': 'application/json' },\n   139→        })\n   140→      }\n   141→\n   142→      const passwordHash = hashPassword(password)\n   143→      const displayName = name || email.split('@')[0]\n   144→\n   145→      const inserted = await sql`\n   146→        INSERT IN