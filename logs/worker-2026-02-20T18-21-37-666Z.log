  87→\n    88→  // POST ?action=signup\n    89→  if (action === 'signup') {\n    90→    const existing = await sql`SELECT id FROM users WHERE email = ${email}`\n    91→    if (existing.length > 0) {\n    92→      return new Response(JSON.stringify({ error: 'An account with this email already exists' }), { status: 409, headers })\n    93→    }\n    94→\n    95→    const passwordHash = hashPassword(password)\n    96→    const name = email.split('@')[0] || 'User'\n    97→    const authUserId = crypto.randomUUID()\n    98→\n    99→    const rows = await sql`\n   100→      INSERT INTO users (auth_user_id, email, name, password_hash, provider)\n   101→      VALUES (${authUserId}::uuid, ${email}, ${name}, ${passwordHash}, 'email')\n   102→      RETURNING id, auth_user_id, email, name, avatar_url\n   103→    `\n   104→    const user = rows[0]\n   105→    const token = await generateToken(user.auth_user_id as string, user.email as string, user.name as string)\n   106→\n   107→    return new Response(JSON.stringify({\n   108→      access_token: token,\n   109→      user: { id: user.id, email: user.email, name: user.name, avatar_url: user.avatar_url },\n   110→    }), { status: 201, headers })\n   111→  }\n   112→\n   113→  // POST ?action=login\n   114→  if (action === 'login') {\n   115→    const rows = await sql`\n   116→      SELECT id, auth_user_id, email, name, avatar_url, password_hash\n   117→      FROM users WHERE email = ${email}\n   118→    `\n   119→    if (rows.length === 0 || !rows[0].password_hash) {\n   120→      return new Response(JSON.stringify({ error: 'Invalid email or password' }), { status: 401, headers })\n   121→    }\n   122→\n   123→    const user = rows[0]\n   124→    if (!verifyPassword(password, user.password_hash as string)) {\n   125→      return new Response(JSON.stringify({ error: 'Invalid email or password' }), { status: 401, headers })\n   126→    }\n   127→\n   128→    const token = await generateToken(user.auth_user_id as string, user.emai