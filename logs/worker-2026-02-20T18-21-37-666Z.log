=> {\n    57→        removeToken()\n    58→      })\n    59→      .finally(() => {\n    60→        setLoading(false)\n    61→      })\n    62→  }, [])\n    63→\n    64→  const signUp = useCallback(async (email: string, password: string): Promise<AuthResult> => {\n    65→    try {\n    66→      const res = await fetch('/.netlify/functions/auth?action=signup', {\n    67→        method: 'POST',\n    68→        headers: { 'Content-Type': 'application/json' },\n    69→        body: JSON.stringify({ email, password }),\n    70→      })\n    71→      const data = await res.json()\n    72→      if (!res.ok) {\n    73→        return { error: data.error || 'Sign up failed' }\n    74→      }\n    75→      setToken(data.access_token)\n    76→      setUser(data.user)\n    77→      return { error: null }\n    78→    } catch {\n    79→      return { error: 'Sign up failed' }\n    80→    }\n    81→  }, [])\n    82→\n    83→  const signIn = useCallback(async (email: string, password: string): Promise<AuthResult> => {\n    84→    try {\n    85→      const res = await fetch('/.netlify/functions/auth?action=login', {\n    86→        method: 'POST',\n    87→        headers: { 'Content-Type': 'application/json' },\n    88→        body: JSON.stringify({ email, password }),\n    89→      })\n    90→      const data = await res.json()\n    91→      if (!res.ok) {\n    92→        return { error: data.error || 'Sign in failed' }\n    93→      }\n    94→      setToken(data.access_token)\n    95→      setUser(data.user)\n    96→      return { error: null }\n    97→    } catch {\n    98→      return { error: 'Sign in failed' }\n    99→    }\n   100→  }, [])\n   101→\n   102→  const signOut = useCallback(() => {\n   103→    removeToken()\n   104→    setUser(null)\n   105→  }, [])\n   106→\n   107→  return (\n   108→    <AuthContext.Provider\n   109→      value={{\n   110→        isLoggedIn: !!user,\n   111→        loading,\n   112→        user,\n   113→        signIn,\n   114→        signUp,\n   115→        signOut,\n   116→      }}\n   117→    >\n   118→      {children}\n   119→    </AuthContext.Provider>\n   120→  )\n   121→}\n   122→\n   123→export function useAuth() {\n   124→  return useContext(AuthContext)\n   125→}\n   126→"}]},"parent_tool_use_id":"toolu_01S9sPGMEP678YtUR1cJ2Urc","session_id":"bffa6e68-a572-4c93-ba6b-39e153c0d749","uuid":"4b206c9b-e3ff-46db-adb7-559008d275c8"}
