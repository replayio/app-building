[2026-02-20T13:18:15.146Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01P5eDv3FrxFnLD5YB18rWaN","type":"tool_result","content":"     1→import { useState, useEffect, useCallback } from 'react'\n     2→import { SettingsPageHeader } from '../components/settings/SettingsPageHeader'\n     3→import { ImportExportSection } from '../components/settings/ImportExportSection'\n     4→import { WebhookSection } from '../components/settings/WebhookSection'\n     5→import { WebhookModal } from '../components/settings/WebhookModal'\n     6→import { ImportDialog } from '../components/shared/ImportDialog'\n     7→import { ConfirmDialog } from '../components/shared/ConfirmDialog'\n     8→\n     9→interface Webhook {\n    10→  id: string\n    11→  name: string\n    12→  url: string\n    13→  events: string[]\n    14→  enabled: boolean\n    15→  created_at: string\n    16→}\n    17→\n    18→export function SettingsPage() {\n    19→  const [webhooks, setWebhooks] = useState<Webhook[]>([])\n    20→  const [availableEvents, setAvailableEvents] = useState<string[]>([])\n    21→  const [webhookModalOpen, setWebhookModalOpen] = useState(false)\n    22→  const [editingWebhook, setEditingWebhook] = useState<Webhook | null>(null)\n    23→  const [deleteConfirm, setDeleteConfirm] = useState<string | null>(null)\n    24→  const [importType, setImportType] = useState<string | null>(null)\n    25→\n    26→  const loadWebhooks = useCallback(async () => {\n    27→    try {\n    28→      const res = await fetch('/.netlify/functions/webhooks')\n    29→      const data = await res.json() as { webhooks: Webhook[]; availableEvents: string[] }\n    30→      setWebhooks(data.webhooks)\n    31→      setAvailableEvents(data.availableEvents)\n    32→    } catch {\n    33→      // ignore\n    34→    }\n    35→  }, [])\n    36→\n    37→  useEffect(() => {\n    38→    loadWebhooks()\n    39→  }, [loadWebhooks])\n    40→\n    41→  async function handleSaveWebhook(data: { name: string; url: string; events: string[]; enabled: boolean }) {\n    42→    try {\n    43→      if (editingWebhook) {\n    44→        await fetch(`/.netlify/functions/webhooks/${editingWebhook.id}`, {\n    45→          method: 'PUT',\n    46→          headers: { 'Content-Type': 'application/json' },\n    47→          body: JSON.stringify(data),\n    48→        })\n    49→      } else {\n    50→        await fetch('/.netlify/functions/webhooks', {\n    51→          method: 'POST',\n    52→          headers: { 'Content-Type': 'application/json' },\n    53→          body: JSON.stringify(data),\n    54→        })\n    55→      }\n    56→      setWebhookModalOpen(false)\n    57→      setEditingWebhook(null)\n    58→      loadWebhooks()\n    59→    } catch {\n    60→      // ignore\n    61→    }\n    62→  }\n    63→\n    64→  async function handleDeleteWebhook() {\n    65→    if (!deleteConfirm) return\n    66→    try {\n    67→      await fetch(`/.netlify/functions/webhooks/${deleteConfirm}`, { method: 'DELETE' })\n    68→      setDeleteConfirm(null)\n    69→      loadWebhooks()\n    70→    } catch {\n    71→      // ignore\n    72→    }\n    73→  }\n    74→\n    75→  async function handleToggleWebhook(id: string, enabled: boolean) {\n    76→    try {\n    77→      await fetch(`/.netlify/functions/webhooks/${id}`, {\n    78→        method: 'PUT',\n    79→        headers: { 'Content-Type': 'application/json' },\n    80→        body: JSON.stringify({ enabled }),\n    81→      })\n    82→      loadWebhooks()\n    83→    } catch {\n    84→      // ignore\n    85→    }\n    86→  }\n    87→\n    88→  function downloadCSV(filename: string, headers: string[], rows: string[][]) {\n    89→    const csv = [headers.join(','), ...rows.map(r => r.map(c => `\"${(c ?? '').replace(/\"/g, '\"\"')}\"`).join(','))].join('\\n')\n    90→    const blob = new Blob([csv], { type: 'text/csv' })\n    91→    const url = URL.createObjectURL(blob)\n    92→    const a = document.createElement('a')\n    93→    a.