   72→\n    73→    // Priority badge visible\n    74→    await expect(page.locator('[data-testid^=\"task-priority-badge-\"]')).toBeVisible();\n    75→\n    76→    // Description visible (if present on the task)\n    77→    const descriptionEl = page.getByTestId('task-detail-description');\n    78→    const descriptionCount = await descriptionEl.count();\n    79→    if (descriptionCount > 0) {\n    80→      await expect(descriptionEl).toBeVisible();\n    81→    }\n    82→\n    83→    // Status visible\n    84→    await expect(page.getByTestId('task-detail-status')).toBeVisible();\n    85→    const status = await page.getByTestId('task-detail-status').textContent();\n    86→    expect(['Open', 'Completed']).toContain(status!.trim());\n    87→\n    88→    // Due date visible\n    89→    await expect(page.getByTestId('task-detail-due-date')).toBeVisible();\n    90→\n    91→    // Assignee visible\n    92→    await expect(page.getByTestId('task-detail-assignee')).toBeVisible();\n    93→\n    94→    // Client name visible\n    95→    await expect(page.getByTestId('task-detail-client')).toBeVisible();\n    96→\n    97→    // Notes section visible\n    98→    await expect(page.getByTestId('task-notes-section')).toBeVisible();\n    99→  });\n   100→\n   101→  test('TDP-HDR-02: Mark Complete changes task status', async ({ page }) => {\n   102→    // Create a task with a client association for timeline verification\n   103→    await page.goto('/tasks');\n   104→    await page.waitForLoadState('networkidle');\n   105→\n   106→    await page.getByTestId('new-task-button').click();\n   107→    const modal = page.getByTestId('create-task-modal');\n   108→    await expect(modal).toBeVisible();\n   109→\n   110→    const taskTitle = `CompleteTest_${Date.now()}`;\n   111→    await page.getByTestId('create-task-title').fill(taskTitle);\n   112→\n   113→    // Select a client so the timeline entry can be verified\n   114→    await page.getByTestId('create-task-client-trigger').click();