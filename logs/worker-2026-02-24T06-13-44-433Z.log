e(state, action: PayloadAction<string>) {\n   130→      state.filterType = action.payload\n   131→      state.filterText = ''\n   132→    },\n   133→    setFilterText(state, action: PayloadAction<string>) {\n   134→      state.filterText = action.payload\n   135→    },\n   136→  },\n   137→  extraReducers: (builder) => {\n   138→    builder\n   139→      .addCase(fetchTasks.pending, (state) => {\n   140→        if (state.items.length === 0) {\n   141→          state.loading = true\n   142→        }\n   143→        state.error = null\n   144→      })\n   145→      .addCase(fetchTasks.fulfilled, (state, action) => {\n   146→        state.loading = false\n   147→        state.items = action.payload.tasks\n   148→        state.clients = action.payload.clients\n   149→        state.deals = action.payload.deals\n   150→        state.assignees = action.payload.assignees\n   151→      })\n   152→      .addCase(fetchTasks.rejected, (state, action) => {\n   153→        state.loading = false\n   154→        state.error = action.error.message || 'Failed to fetch tasks'\n   155→      })\n   156→      .addCase(deleteTask.fulfilled, (state, action) => {\n   157→        state.items = state.items.filter(t => t.id !== action.payload)\n   158→      })\n   159→      .addCase(updateTask.fulfilled, (state, action) => {\n   160→        const updated = action.payload as TaskRow\n   161→        if (updated.completed) {\n   162→          state.items = state.items.filter(t => t.id !== updated.id)\n   163→        } else {\n   164→          const idx = state.items.findIndex(t => t.id === updated.id)\n   165→          if (idx !== -1) {\n   166→            state.items[idx] = updated\n   167→          }\n   168→        }\n   169→      })\n   170→  },\n   171→})\n   172→\n   173→export const { setFilterType, setFilterText } = tasksSlice.actions\n   174→export default tasksSlice.reducer\n   175→"}]},"parent_tool_use_id":"toolu_01Sg16Huuj1UE9JDoShU3MBP","session_id":"1de25650-0916-49a5-a5f2-6a397262e