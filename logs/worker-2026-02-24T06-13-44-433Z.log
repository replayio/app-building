r: pointer;\n  transition: background 0.1s ease;\n  white-space: nowrap;\n}\n\n.btn-ghost:hover {\n  background: var(--color-bg-hover);\n  color: var(--color-text-primary);\n}\n\n.btn-primary {\n  display: inline-flex;\n  align-items: center;\n  gap: 6px;\n  height: 28px;\n  padding: 0 12px;\n  border: none;\n  border-radius: 5px;\n  background: var(--color-accent);\n  color: white;\n  font-size: 13px;\n  font-weight: 500;\n  cursor: pointer;\n  transition: opacity 0.1s ease;\n  white-space: nowrap;\n}\n\n.btn-primary:hover {\n  opacity: 0.9;\n}\n\n.btn-primary:disabled {\n  opacity: 0.6;\n  cursor: default;\n}\n\n.btn-secondary {\n  display: inline-flex;\n  align-items: center;\n  gap: 6px;\n  height: 28px;\n  padding: 0 12px;\n  border: 1px solid var(--color-border-default);\n  border-radius: 5px;\n  background: var(--color-bg-base);\n  color: var(--color-text-secondary);\n  font-size: 13px;\n  cursor: pointer;\n  transition: background 0.1s ease;\n  white-space: nowrap;\n}\n\n.btn-secondary:hover {\n  background: var(--color-bg-hover);\n}\n\n.btn-danger {\n  display: inline-flex;\n  align-items: center;\n  gap: 6px;\n  height: 28px;\n  padding: 0 12px;\n  border: none;\n  border-radius: 5px;\n  background: var(--color-status-churned);\n  color: white;\n  font-size: 13px;\n  font-weight: 500;\n  cursor: pointer;\n  transition: opacity 0.1s ease;\n  white-space: nowrap;\n}\n\n.btn-danger:hover {\n  opacity: 0.9;\n}\n\n/* ===== Table ===== */\n.clients-table {\n  display: grid;\n  grid-template-columns: 1fr;\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-md);\n}\n\n.clients-table-header {\n  display: grid;\n  grid-template-columns: 2fr 1fr 1fr 1.5fr 1.5fr 1fr 1.5fr 44px;\n  align-items: center;\n  height: 36px;\n  padding: 0 12px;\n  background: var(--color-bg-sidebar);\n  border-bottom: 1px solid var(--color-border-default);\n  font-size: 12px;\n  font-weight: 500;\n  color: var(--color-text-muted);\n}\n\n.clients-table-row {\n  display: grid;\n  grid-template-columns: 2fr 1fr 1fr 1.5fr 1.5fr 1fr 1.5fr 44px;\n  align-items: center;\n  height: 44px;\n  padding: 0 12px;\n  border-bottom: 1px solid var(--color-border-light);\n  transition: background 0.1s ease;\n  cursor: default;\n}\n\n.clients-table-row:last-child {\n  border-bottom: none;\n}\n\n.clients-table-row:hover {\n  background: var(--color-bg-hover);\n}\n\n.table-cell {\n  padding: 0 8px;\n  overflow: hidden;\n  text-overflow: ellipsis;\n  white-space: nowrap;\n  font-size: 13px;\n  color: var(--color-text-secondary);\n}\n\n.table-cell-action {\n  padding: 0;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  position: relative;\n  z-index: 1;\n  overflow: visible;\n}\n\n.table-cell-tags {\n  display: flex;\n  gap: 4px;\n  overflow: hidden;\n}\n\n.table-cell-contact,\n.table-cell-task {\n  font-size: 12px;\n  color: var(--color-text-muted);\n}\n\n.table-cell-type {\n  font-size: 12px;\n}\n\n.client-name-link {\n  border: none;\n  background: none;\n  color: var(--color-text-primary);\n  font-size: 13px;\n  font-weight: 500;\n  cursor: pointer;\n  padding: 0;\n  text-decoration: none;\n  transition: color 0.1s ease;\n}\n\n.client-name-link:hover {\n  color: var(--color-accent);\n}\n\n/* ===== Status Badges ===== */\n.status-badge {\n  display: inline-flex;\n  align-items: center;\n  height: 20px;\n  padding: 0 8px;\n  border-radius: 10px;\n  font-size: 11px;\n  font-weight: 500;\n  white-space: nowrap;\n}\n\n.status-active {\n  color: var(--color-status-active);\n  background: var(--color-status-active-bg);\n}\n\n.status-inactive {\n  color: var(--color-status-inactive);\n  background: var(--color-status-inactive-bg);\n}\n\n.status-prospect {\n  color: var(--color-status-prospect);\n  background: var(--color-status-prospect-bg);\n}\n\n.status-churned {\n  color: var(--color-status-churned);\n  background: var(--color-status-churned-bg);\n}\n\n/* ===== Tag Chips ===== */\n.tag-chip {\n  display: inline-flex;\n  align-items: center;\n  height: 20px;\n  padding: 0 8px;\n  border-radius: 10px;\n  font-size: 11px;\n  color: var(--color-accent-muted);\n  background: #eef0ff;\n  white-space: nowrap;\n}\n\n/* ===== Row Actions ===== */\n.row-action-wrapper {\n  position: relative;\n}\n\n.row-action-trigger {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 28px;\n  height: 28px;\n  border: none;\n  border-radius: var(--radius-sm);\n  background: transparent;\n  color: var(--color-text-muted);\n  cursor: pointer;\n  transition: background 0.1s ease;\n}\n\n.row-action-trigger:hover {\n  background: var(--color-bg-hover);\n  color: var(--color-text-primary);\n}\n\n.row-action-menu {\n  position: absolute;\n  top: calc(100% + 2px);\n  right: 0;\n  min-width: 160px;\n  background: var(--color-bg-base);\n  border-radius: var(--radius-md);\n  box-shadow: var(--shadow-md);\n  padding: 4px;\n  z-index: 40;\n}\n\n.row-action-item {\n  display: flex;\n  align-items: center;\n  gap: 8px;\n  width: 100%;\n  padding: 6px 8px;\n  border: none;\n  border-radius: var(--radius-sm);\n  background: transparent;\n  color: var(--color-text-secondary);\n  font-size: 13px;\n  text-align: left;\n  cursor: pointer;\n  transition: background 0.1s ease;\n}\n\n.row-action-item:hover {\n  background: var(--color-bg-hover);\n}\n\n.row-action-item.danger {\n  color: var(--color-status-churned);\n}\n\n.row-action-item.danger:hover {\n  background: var(--color-status-churned-bg);\n}\n\n/* ===== Empty State ===== */\n.clients-table-empty {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: center;\n  padding: 48px 24px;\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-md);\n  text-align: center;\n}\n\n.empty-title {\n  font-size: 16px;\n  font-weight: 500;\n  color: var(--color-text-primary);\n  margin-bottom: 4px;\n}\n\n.empty-subtitle {\n  font-size: 13px;\n  color: var(--color-text-muted);\n}\n\n/* ===== Pagination