    33→  author: string\n    34→  version: number\n    35→  created_at: string\n    36→}\n    37→\n    38→export function DealDetailPage() {\n    39→  const { dealId } = useParams()\n    40→  const navigate = useNavigate()\n    41→  const dispatch = useAppDispatch()\n    42→  const { currentDeal, loading } = useAppSelector((state) => state.deals)\n    43→\n    44→  const [history, setHistory] = useState<DealHistory[]>([])\n    45→  const [writeups, setWriteups] = useState<Writeup[]>([])\n    46→  const [tasks, setTasks] = useState<Task[]>([])\n    47→  const [attachments, setAttachments] = useState<Attachment[]>([])\n    48→  const [contacts, setContacts] = useState<DealContact[]>([])\n    49→\n    50→  const [addWriteupOpen, setAddWriteupOpen] = useState(false)\n    51→  const [addTaskOpen, setAddTaskOpen] = useState(false)\n    52→  const [uploadOpen, setUploadOpen] = useState(false)\n    53→  const [deleteAttachmentId, setDeleteAttachmentId] = useState<string | null>(null)\n    54→  const [versionModalOpen, setVersionModalOpen] = useState(false)\n    55→  const [versions, setVersions] = useState<VersionEntry[]>([])\n    56→  const [availableUsers, setAvailableUsers] = useState<{ name: string }[]>([])\n    57→  const [availableClients, setAvailableClients] = useState<{ id: string; name: string }[]>([])\n    58→\n    59→  useEffect(() => {\n    60→    fetch('/.netlify/functions/users')\n    61→      .then(r => r.json())\n    62→      .then((data: { users: { name: string }[] }) => setAvailableUsers(data.users))\n    63→      .catch(() => {})\n    64→    fetch('/.netlify/functions/clients?pageSize=500')\n    65→      .then(r => r.json())\n    66→      .then((data: { clients: { id: string; name: string }[] }) => setAvailableClients(data.clients.map(c => ({ id: c.id, name: c.name }))))\n    67→      .catch(() => {})\n    68→  }, [])\n    69→\n    70→  const loadDetailData = useCallback(async () => {\n    71→    if (!dealId) return\n    72→    const res = await fetch(`/