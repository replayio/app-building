ta = await res.json()\n    54→        return rejectWithValue(data.error || 'Sign up failed')\n    55→      }\n    56→      return await res.json()\n    57→    } catch {\n    58→      return rejectWithValue('Network error')\n    59→    }\n    60→  },\n    61→)\n    62→\n    63→export const loadSession = createAsyncThunk(\n    64→  'auth/loadSession',\n    65→  async (_, { getState, rejectWithValue }) => {\n    66→    const state = getState() as { auth: AuthState }\n    67→    const token = state.auth.token\n    68→    if (!token) return rejectWithValue('No token')\n    69→    try {\n    70→      const res = await fetch('/.netlify/functions/auth?action=me', {\n    71→        headers: { Authorization: `Bearer ${token}` },\n    72→      })\n    73→      if (!res.ok) return rejectWithValue('Invalid session')\n    74→      return await res.json()\n    75→    } catch {\n    76→      return rejectWithValue('Network error')\n    77→    }\n    78→  },\n    79→)\n    80→\n    81→const authSlice = createSlice({\n    82→  name: 'auth',\n    83→  initialState,\n    84→  reducers: {\n    85→    signOut(state) {\n    86→      state.user = null\n    87→      state.token = null\n    88→      state.error = null\n    89→      localStorage.removeItem('token')\n    90→    },\n    91→    clearError(state) {\n    92→      state.error = null\n    93→    },\n    94→    setSession(state, action: { payload: { user: User; token: string } }) {\n    95→      state.user = action.payload.user\n    96→      state.token = action.payload.token\n    97→      localStorage.setItem('token', action.payload.token)\n    98→    },\n    99→  },\n   100→  extraReducers: (builder) => {\n   101→    builder\n   102→      .addCase(signIn.pending, (state) => {\n   103→        state.loading = true\n   104→        state.error = null\n   105→      })\n   106→      .addCase(signIn.fulfilled, (state, action) => {\n   107→        state.loading = false\n   108→        state.user = action.payload.user\n   109→        state.