nst dateOnly = new Date(d.getFullYear(), d.getMonth(), d.getDate())\n    55→  const timeStr = d.toLocaleTimeString('en-US', { hour: 'numeric', minute: '2-digit' })\n    56→\n    57→  if (dateOnly.getTime() === today.getTime()) {\n    58→    return `Today, ${timeStr}`\n    59→  }\n    60→  if (dateOnly.getTime() === tomorrow.getTime()) {\n    61→    return `Tomorrow, ${timeStr}`\n    62→  }\n    63→  return new Intl.DateTimeFormat('en-US', {\n    64→    month: 'short',\n    65→    day: 'numeric',\n    66→    year: 'numeric',\n    67→  }).format(d)\n    68→}\n    69→\n    70→export function getAuthHeader(): Record<string, string> {\n    71→  const token = localStorage.getItem('token')\n    72→  return token ? { Authorization: `Bearer ${token}` } : {}\n    73→}\n    74→"}]},"parent_tool_use_id":"toolu_01JZahsYTLEMAqtAKsz8Neyx","session_id":"a1281933-7201-4a5b-a8d3-b01fd73f578c","uuid":"7012bc54-9e31-4493-82e6-92792368e82d"}
