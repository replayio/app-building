[2026-02-20T21:53:43.264Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01NPodETbaA6Vz8bhr4VGpWi","type":"tool_result","content":"     1→import { useState } from 'react'\n     2→import { useNavigate } from 'react-router-dom'\n     3→import type { Deal, DealStage } from '../../types'\n     4→import { DealStatusBadge } from './DealStatusBadge'\n     5→\n     6→interface DealsPipelineViewProps {\n     7→  deals: Deal[]\n     8→  onStageChange?: (dealId: string, newStage: DealStage) => void\n     9→}\n    10→\n    11→const PIPELINE_STAGES: { key: DealStage; label: string; color: string }[] = [\n    12→  { key: 'lead', label: 'Lead', color: 'border-t-gray-400' },\n    13→  { key: 'qualification', label: 'Qualification', color: 'border-t-accent-blue' },\n    14→  { key: 'discovery', label: 'Discovery', color: 'border-t-accent-purple' },\n    15→  { key: 'proposal', label: 'Proposal', color: 'border-t-accent' },\n    16→  { key: 'negotiation', label: 'Negotiation', color: 'border-t-yellow-500' },\n    17→  { key: 'closed_won', label: 'Closed Won', color: 'border-t-status-active' },\n    18→]\n    19→\n    20→function formatCurrency(value: number): string {\n    21→  if (value >= 1_000_000) {\n    22→    return `$${(value / 1_000_000).toFixed(1)}M`\n    23→  }\n    24→  if (value >= 1_000) {\n    25→    return `$${(value / 1_000).toFixed(0)}k`\n    26→  }\n    27→  return `$${value.toLocaleString()}`\n    28→}\n    29→\n    30→function DealCard({ deal, onDragStart }: { deal: Deal; onDragStart: (e: React.DragEvent, dealId: string) => void }) {\n    31→  const navigate = useNavigate()\n    32→\n    33→  return (\n    34→    <div\n    35→      data-testid={`pipeline-deal-card-${deal.id}`}\n    36→      draggable\n    37→      onDragStart={(e) => onDragStart(e, deal.id)}\n    38→      onClick={() => navigate(`/deals/${deal.id}`)}\n    39→      className=\"p-3 bg-surface border border-border rounded-[6px] cursor-grab hover:shadow-[var(--shadow-elevation-1)] transition-shadow duration-150 active:cursor-grabbing\"\n    40→    >\n    41→      <p className=\"text-[13px] font-medium text-text-primary mb-1 truncate\">{deal.name}</p>\n    42→      <p className=\"text-[12px] text-text-muted mb-2 truncate\">{deal.client_name}</p>\n    43→      <div className=\"flex items-center justify-between\">\n    44→        <span className=\"text-[13px] font-medium text-text-primary\">{formatCurrency(deal.value)}</span>\n    45→        <DealStatusBadge status={deal.status} />\n    46→      </div>\n    47→    </div>\n    48→  )\n    49→}\n    50→\n    51→export function DealsPipelineView({ deals, onStageChange }: DealsPipelineViewProps) {\n    52→  const [dropTarget, setDropTarget] = useState<DealStage | null>(null)\n    53→\n    54→  const dealsByStage = PIPELINE_STAGES.map((stage) => ({\n    55→    ...stage,\n    56→    deals: deals.filter((d) => d.stage === stage.key),\n    57→    totalValue: deals\n    58→      .filter((d) => d.stage === stage.key)\n    59→      .reduce((sum, d) => sum + Number(d.value), 0),\n    60→  }))\n    61→\n    62→  function handleDragStart(e: React.DragEvent, dealId: string) {\n    63→    e.dataTransfer.setData('text/plain', dealId)\n    64→    e.dataTransfer.effectAllowed = 'move'\n    65→  }\n    66→\n    67→  function handleDragOver(e: React.DragEvent, stage: DealStage) {\n    68→    e.preventDefault()\n    69→    e.dataTransfer.dropEffect = 'move'\n    70→    setDropTarget(stage)\n    71→  }\n    72→\n    73→  function handleDragLeave() {\n    74→    setDropTarget(null)\n    75→  }\n    76→\n    77→  function handleDrop(e: React.DragEvent, stage: DealStage) {\n    78→    e.preventDefault()\n    79→    setDropTarget(null)\n    80→    const dealId = e.dataTransfer.getData('text/plain')\n    81→    if (dealId && onStageChange) {\n    82→      const deal = deals.find((d) => d.id === dealId)\n    83→      if (deal && deal.stage !== stage) {\n    84→        onStageChange(dealId, stage)\n    85→      }\n    86→    }\n    87→  }\n    88→\n    89→  return (\n    90→    <div className=\"overflow-x-auto\">\n    91→    <div className=\"grid grid-cols-6 gap-3 min-h-[400px] min-w-[720px]\" data-testid=\"deals-pipeline-view\">\n    92→      {dealsByStage.map((column) => (\n    93→        <div\n    94→          key={column.key}\n    95→          data-testid={`pipeline-column-${column.key}`}\n    96→          onDragOver={(e) => handleDragOver(e, column.key)}\n    97→          onDragLeave={handleDragLeave}\n    98→          onDrop={(e) => handleDrop(e, column.key)}\n    99→          className={`flex flex-col border-t-2 ${column.color} rounded-t-none transition-colors duration-150 ${\n   100→            dropTarget === column.key ? 'bg-accent/5' : ''\n   101→          }`}\n   102→        >\n   103→          <div className=\"px-2 py-2 mb-2\">\n   104→            <div className=\"flex items-center justify-between mb-0.5\">\n   105→              <span className=\"text-[13px] font-medium text-text-primary\">{column.label}</span>\n   106→              <span className=\"text-[11px] text-text-muted bg-hover rounded-full px-1.5 py-0.5\">\n   107→                {column.deals.length}\n   108→              </span>\n   109→            </div>\n   110→            <p className=\"text-[12px] text-text-muted\">{formatCurrency(column.totalValue)}</p>\n   111→          </div>\n   112→          <div className=\"flex flex-col gap-2 px-1 pb-2 flex-1 overflow-y-auto\">\n   113→            {column.deals.map((deal) => (\n   114→              <DealCard key={deal.id} deal={deal} onDragStart={handleDragStart} />\n   115→            ))}\n   116→          </div>\n   117→        </div>\n   118→      ))}\n   119→    </div>\n   120→    </div>\n   121→  )\n   122→}\n   123→"}]},"parent_tool_use_id":"toolu_01TF6qW9im5S8fsi2DQ8r3Lh","session_id":"7b907b2c-9f55-40b8-8a2b-1391eda41c21","uuid":"e08d1a4c-a459-466c-837c-53b1fbcfe6ef"}
