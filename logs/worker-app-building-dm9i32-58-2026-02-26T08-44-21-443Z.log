  493→  });\n   494→\n   495→  test(\"RUN-ACT-7: Cancel Run confirmation can be dismissed\", async ({\n   496→    page,\n   497→  }) => {\n   498→    await goToRun(page, scheduledRunId);\n   499→\n   500→    // Click Cancel Run\n   501→    await page.getByTestId(\"cancel-run-btn\").click();\n   502→\n   503→    // Dialog appears\n   504→    await expect(page.getByTestId(\"confirm-dialog\")).toBeVisible();\n   505→\n   506→    // Dismiss the dialog\n   507→    await page.getByTestId(\"confirm-dialog-cancel\").click();\n   508→\n   509→    // Dialog closes\n   510→    await expect(page.getByTestId(\"confirm-dialog\")).toBeHidden();\n   511→\n   512→    // Status remains Scheduled\n   513→    await expect(\n   514→      page.getByTestId(\"run-info-status\").locator(\".badge\")\n   515→    ).toHaveText(\"Scheduled\");\n   516→  });\n   517→\n   518→  test(\"RUN-ACT-8: Action buttons are hidden or disabled for a Cancelled run\", async ({\n   519→    page,\n   520→  }) => {\n   521→    await page.goto(`/runs/${cancelledRunId}`);\n   522→    await expect(page.getByTestId(\"run-details-page\")).toBeVisible({\n   523→      timeout: 30000,\n   524→    });\n   525→\n   526→    // Wait for data to load (status badge shows Cancelled)\n   527→    await expect(\n   528→      page.getByTestId(\"run-info-status\").locator(\".badge\")\n   529→    ).toHaveText(\"Cancelled\", { timeout: 30000 });\n   530→\n   531→    // All action buttons should be hidden\n   532→    await expect(page.getByTestId(\"adjust-quantities-btn\")).toHaveCount(0);\n   533→    await expect(page.getByTestId(\"confirm-run-btn\")).toHaveCount(0);\n   534→    await expect(page.getByTestId(\"cancel-run-btn\")).toHaveCount(0);\n   535→  });\n   536→\n   537→  test(\"RUN-ACT-9: Action buttons are hidden or disabled for a Confirmed run except Cancel\", async ({\n   538→    page,\n   539→  }) => {\n   540→    await goToRun(page, confirmedRunId);\n   541→\n   542→    // Adjust Quantities and Confirm Run should be hidden\