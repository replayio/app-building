  106→    onClose();\n   107→  };\n   108→\n   109→  return (\n   110→    <div\n   111→      className=\"modal-overlay\"\n   112→      onClick={onClose}\n   113→      data-testid=\"adjust-quantity-modal\"\n   114→    >\n   115→      <div className=\"modal-content\" onClick={(e) => e.stopPropagation()}>\n   116→        <div className=\"modal-header\">\n   117→          <h2 className=\"modal-title\">Adjust Planned Quantity</h2>\n   118→          <button className=\"modal-close-btn\" data-testid=\"adjust-quantity-close\" onClick={onClose}>\n   119→            <svg width=\"16\" height=\"16\" viewBox=\"0 0 16 16\" fill=\"none\">\n   120→              <path\n   121→                d=\"M4 4L12 12M12 4L4 12\"\n   122→                stroke=\"currentColor\"\n   123→                strokeWidth=\"1.5\"\n   124→                strokeLinecap=\"round\"\n   125→              />\n   126→            </svg>\n   127→          </button>\n   128→        </div>\n   129→        <form onSubmit={handleSubmit}>\n   130→          <div className=\"form-group\">\n   131→            <label className=\"form-label\">\n   132→              Planned Quantity ({run.unit})\n   133→            </label>\n   134→            <input\n   135→              data-testid=\"adjust-quantity-input\"\n   136→              className=\"form-input\"\n   137→              type=\"number\"\n   138→              min=\"0\"\n   139→              step=\"any\"\n   140→              value={quantity}\n   141→              onChange={(e) => setQuantity(e.target.value)}\n   142→              required\n   143→            />\n   144→          </div>\n   145→          <div className=\"modal-footer\">\n   146→            <button type=\"button\" className=\"btn-secondary\" data-testid=\"adjust-quantity-cancel\" onClick={onClose}>\n   147→              Cancel\n   148→            </button>\n   149→            <button\n   150→              type=\"submit\"\n   151→              className=\"btn-primary\"\n   152→              disabled={savin