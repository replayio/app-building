st detail = await detailRes.json();\n    55→    if (\n    56→      detail.materials?.length > 0 &&\n    57→      detail.forecasts?.length > 0 &&\n    58→      detail.equipment?.length > 0\n    59→    ) {\n    60→      scheduledRunId = detail.id;\n    61→      scheduledRunDetail = detail;\n    62→      break;\n    63→    }\n    64→  }\n    65→\n    66→  if (!scheduledRunId && runs.length > 0) {\n    67→    const detailRes = await request.get(\n    68→      `/.netlify/functions/runs/${runs[0].id}`\n    69→    );\n    70→    scheduledRunDetail = await detailRes.json();\n    71→    scheduledRunId = scheduledRunDetail.id;\n    72→  }\n    73→\n    74→  if (!scheduledRunId) throw new Error(\"No Scheduled runs found in database\");\n    75→\n    76→  // Create a Confirmed run\n    77→  let res = await request.post(\"/.netlify/functions/runs\", {\n    78→    data: {\n    79→      recipe_id: recipeId,\n    80→      start_date: \"2024-08-01T08:00:00Z\",\n    81→      end_date: \"2024-08-01T17:00:00Z\",\n    82→      planned_quantity: 200,\n    83→      unit: \"Units\",\n    84→      status: \"Confirmed\",\n    85→    },\n    86→  });\n    87→  confirmedRunId = (await res.json()).id;\n    88→\n    89→  // Create a Cancelled run\n    90→  res = await request.post(\"/.netlify/functions/runs\", {\n    91→    data: {\n    92→      recipe_id: recipeId,\n    93→      start_date: \"2024-08-02T08:00:00Z\",\n    94→      end_date: \"2024-08-02T17:00:00Z\",\n    95→      planned_quantity: 150,\n    96→      unit: \"Units\",\n    97→      status: \"Cancelled\",\n    98→    },\n    99→  });\n   100→  cancelledRunId = (await res.json()).id;\n   101→\n   102→  // Create an empty run (no recipe) for empty state tests\n   103→  res = await request.post(\"/.netlify/functions/runs\", {\n   104→    data: {\n   105→      start_date: \"2024-08-03T08:00:00Z\",\n   106→      end_date: \"2024-08-03T17:00:00Z\",\n   107→      planned_quantity: 50,\n   108→      unit: \"Units\",\n   109→    },\n   110→