 = o.supplier_id\n    47→          WHERE o.supplier_id = ${supplierId}::uuid\n    48→          ORDER BY o.order_date DESC\n    49→        `;\n    50→        return new Response(JSON.stringify(rows), {\n    51→          status: 200,\n    52→          headers: { \"Content-Type\": \"application/json\" },\n    53→        });\n    54→      }\n    55→\n    56→      const rows = await sql`\n    57→        SELECT o.*, s.name AS supplier_name\n    58→        FROM orders o\n    59→        LEFT JOIN suppliers s ON s.id = o.supplier_id\n    60→        ORDER BY o.order_date DESC\n    61→      `;\n    62→      return new Response(JSON.stringify(rows), {\n    63→        status: 200,\n    64→        headers: { \"Content-Type\": \"application/json\" },\n    65→      });\n    66→    } catch (err) {\n    67→      console.error(\"Orders GET error:\", err);\n    68→      return new Response(JSON.stringify({ error: \"Internal server error\" }), {\n    69→        status: 500,\n    70→        headers: { \"Content-Type\": \"application/json\" },\n    71→      });\n    72→    }\n    73→  }\n    74→\n    75→  if (req.method === \"POST\") {\n    76→    try {\n    77→      const body = await req.json();\n    78→      const { supplier_id, order_date, expected_delivery, line_items } = body;\n    79→\n    80→      if (!supplier_id) {\n    81→        return new Response(JSON.stringify({ error: \"supplier_id is required\" }), {\n    82→          status: 400,\n    83→          headers: { \"Content-Type\": \"application/json\" },\n    84→        });\n    85→      }\n    86→\n    87→      // Generate a unique order ID (PO-XXXXXX format)\n    88→      const countRows = await sql`SELECT COUNT(*)::int AS cnt FROM orders`;\n    89→      const count = (countRows[0]?.cnt as number) || 0;\n    90→      const generatedOrderId = `PO-${String(count + 1).padStart(6, \"0\")}`;\n    91→\n    92→      // Calculate subtotal from line items\n    93→      const items = Array.isArray(line_items) ? line_items : [];\n    