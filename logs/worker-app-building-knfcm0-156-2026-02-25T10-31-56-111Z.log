tion.css\";\n@import \"@shared/components/ConfirmDialog.css\";\n@import \"@shared/components/DocumentsSection.css\";\n@import \"@shared/components/FilterSelect.css\";\n\n/* App-specific variables */\n:root {\n  /* Supplier status colors */\n  --status-active-bg: #e6ede8;\n  --status-active-text: var(--status-success);\n  --status-inactive-bg: #f0eeea;\n  --status-inactive-text: var(--text-muted);\n  --status-on-hold-bg: #f5f0e3;\n  --status-on-hold-text: var(--status-warning);\n  --status-suspended-bg: #f5ece6;\n  --status-suspended-text: var(--status-error);\n\n  /* Order status colors */\n  --status-pending-bg: #f5f0e3;\n  --status-pending-text: var(--status-warning);\n  --status-in-transit-bg: #e3ecf0;\n  --status-in-transit-text: var(--status-info);\n  --status-fulfilled-bg: #e6ede8;\n  --status-fulfilled-text: var(--status-success);\n  --status-processing-bg: #f5ece6;\n  --status-processing-text: #b07a3e;\n  --status-completed-bg: #e6ede8;\n  --status-completed-text: var(--status-success);\n  --status-cancelled-bg: #f5ece6;\n  --status-cancelled-text: var(--status-error);\n  --status-approved-bg: #e6ede8;\n  --status-approved-text: var(--status-success);\n  --status-shipped-bg: #e3ecf0;\n  --status-shipped-text: var(--status-info);\n  --status-delivered-bg: #f0eeea;\n  --status-delivered-text: var(--text-muted);\n\n  /* Card styles */\n  --card-bg: #ffffff;\n  --card-border: var(--bg-border-color-light);\n}\n\n/* App shell layout (top nav + main content) */\n.app-shell {\n  display: flex;\n  flex-direction: column;\n  min-height: 100vh;\n}\n\n.app-main {\n  flex: 1;\n  min-width: 0;\n  display: flex;\n  flex-direction: column;\n}\n\n/* Page content */\n.page-content {\n  flex: 1;\n  padding: 24px;\n}\n\n@media (max-width: 640px) {\n  .page-content {\n    padding: 12px;\n  }\n}\n\n/* Page header */\n.page-header {\n  display: flex;\n  justify-content: space-between;\n  align-items: flex-start;\n  margin-bottom: 24px;\n}\n\n.page-title {\n  font-size: 24px;\n  font-weight: 600;\n  color: var(--text-primary);\n}\n\n/* Badge styles */\n.badge {\n  display: inline-flex;\n  align-items: center;\n  height: 22px;\n  padding: 0 8px;\n  font-size: 11px;\n  font-weight: 500;\n  border-radius: 11px;\n  white-space: nowrap;\n}\n\n.badge--active {\n  background: var(--status-active-bg);\n  color: var(--status-active-text);\n}\n\n.badge--inactive {\n  background: var(--status-inactive-bg);\n  color: var(--status-inactive-text);\n}\n\n.badge--on-hold {\n  background: var(--status-on-hold-bg);\n  color: var(--status-on-hold-text);\n}\n\n.badge--suspended {\n  background: var(--status-suspended-bg);\n  color: var(--status-suspended-text);\n}\n\n.badge--pending {\n  background: var(--status-pending-bg);\n  color: var(--status-pending-text);\n}\n\n.badge--in-transit {\n  background: var(--status-in-transit-bg);\n  color: var(--status-in-transit-text);\n}\n\n.badge--fulfilled {\n  background: var(--status-fulfilled-bg);\n  color: var(--status-fulfilled-text);\n}\n\n.badge--processing {\n  background: var(--status-processing-bg);\n  color: var(--status-processing-text);\n}\n\n.badge--completed {\n  background: var(--status-completed-bg);\n  color: var(--status-completed-text);\n}\n\n.badge--cancelled {\n  background: var(--status-cancelled-bg);\n  color: var(--status-cancelled-text);\n}\n\n.badge--approved {\n  background: var(--status-approved-bg);\n  color: var(--status-approved-text);\n}\n\n.badge--shipped {\n  background: var(--status-shipped-bg);\n  color: var(--status-shipped-text);\n}\n\n.badge--delivered {\n  background: var(--status-delivered-bg);\n  color: var(--status-delivered-text);\n}\n\n/* Section card */\n.section-card {\n  background: var(--card-bg);\n  border: 1px solid var(--card-border);\n  border-radius: 6px;\n  margin-bottom: 16px;\n}\n\n.section-card-header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 16px 20px;\n  border-bottom: 1px solid var(--bg-border-color-light);\n}\n\n.section-card-title {\n  font-size: 16px;\n  font-weight: 600;\n  color: var(--text-primary);\n}\n\n.section-card-body {\n  padding: 16px 20px;\n}\n\n/* Data table */\n.data-table {\n  width: 100%;\n  border-collapse: collapse;\n}\n\n.data-table th {\n  text-align: left;\n  padding: 10px 12px;\n  font-size: 12px;\n  font-weight: 500;\n  color: var(--text-muted);\n  border-bottom: 1px solid var(--bg-border-color);\n  white-space: nowrap;\n}\n\n.data-table td {\n  padding: 10px 12px;\n  font-size: 13px;\n  color: var(--text-primary);\n  border-bottom: 1px solid var(--bg-border-color-light);\n  vertical-align: middle;\n}\n\n.data-table tbody tr {\n  transition: background-color 0.1s ease;\n}\n\n.data-table tbody tr:hover {\n  background: rgba(50, 45, 35, 0.03);\n}\n\n.data-table tbody tr.clickable {\n  cursor: pointer;\n}\n\n.data-table tbody tr.clickable:hover {\n  background: rgba(50, 45, 35, 0.05);\n}\n\n/* Button styles */\n.btn-primary {\n  display: inline-flex;\n  align-items: center;\n  gap: 6px;\n  height: 32px;\n  padding: 0 14px;\n  font-size: 13px;\n  font-weight: 500;\n  color: var(--text-on-dark);\n  background: var(--accent-primary);\n  border: none;\n  border-radius: 5px;\n  cursor: pointer;\n  transition: background-color 0.15s ease;\n  white-space: nowrap;\n  font-family: var(--font-family);\n}\n\n.btn-primary:hover {\n  background: var(--accent-dark);\n}\n\n.btn-primary:disabled {\n  opacity: 0.5;\n  cursor: default;\n}\n\n.btn-secondary {\n  display: inline-flex;\n  align-items: center;\n  gap: 6px;\n  height: 32px;\n  padding: 0 14px;\n  font-size: 13px;\n  font-weight: 450;\n  color: var(--text-secondary);\n  background: var(--bg-base-color);\n  border: 1px solid var(--bg-border-color);\n  border-radius: 5px;\n  cursor: pointer;\n  transition: background-color 0.1s ease, border-color 0.1s ease;\n  white-space: nowrap;\n  font-family: var(--font-family);\n}\n\n.btn-secondary:hover {\n  background: rgba(50, 45, 35, 0.04);\n  border-color: var(--normal-border-hover);\n}\n\n.btn-ghost {\n  display: inline-flex;\n  align-items: center;\n  gap: 6px;\n  height: 28px;\n  padding: 0 8px;\n  font-size: 13px;\n  font-weight: 400;\n  color: var(--text-secondary);\n  background: transparent;\n  border: none;\n  border-radius: 4px;\n  cursor: pointer;\n  transition: background-color 0.1s ease, color 0.15s ease;\n  font-family: var(--font-family);\n}\n\n.btn-ghost:hover {\n  background: rgba(50, 45, 35, 0.06);\n  color: var(--text-primary);\n}\n\n.btn-danger {\n  display: inline-flex;\n  align-items: center;\n  gap: 6px;\n  height: 32px;\n  padding: 0 14px;\n  font-size: 13px;\n  font-weight: 500;\n  color: var(--text-on-dark);\n  background: var(--status-error);\n  border: none;\n  border-radius: 5px;\n  cursor: pointer;\n  transition: background-color 0.1s ease;\n  white-space: nowrap;\n  font-family: var(--font-family);\n}\n\n.btn-danger:hover {\n  background: #9a4343;\n}\n\n/* Link style */\n.link {\n  color: var(--accent-primary);\n  text-decoration: none;\n  font-size: 13px;\n  font-weight: 450;\n  cursor: pointer;\n  transition: color 0.1s ease;\n}\n\n.link:hover {\n  color: var(--accent-dark);\n}\n\n/* Modal overlay */\n.modal-overlay {\n  position: fixed;\n  inset: 0;\n  background: rgba(0, 0, 0, 0.4);\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  z-index: 1000;\n}\n\n.modal-content {\n  background: var(--card-bg);\n  border-radius: 8px;\n  box-shadow: var(--shadow-elevation-2);\n  width: 90%;\n  max-width: 720px;\n  max-height: 90vh;\n  overflow-y: auto;\n  padding: 24px;\n}\n\n.modal-header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  margin-bottom: 20px;\n}\n\n.modal-title {\n  font-size: 18px;\n  font-weight: 600;\n  color: var(--text-primary);\n}\n\n.modal-close-btn {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 28px;\n  height: 28px;\n  background: transparent;\n  border: none;\n  border-radius: 4px;\n  cursor: pointer;\n  color: var(--text-muted);\n  transition: background-color 0.1s ease;\n}\n\n.modal-close-btn:hover {\n  background: rgba(50, 45, 35, 0.08);\n}\n\n.modal-footer {\n  display: flex;\n  justify-content: flex-end;\n  gap: 8px;\n  margin-top: 20px;\n  padding-top: 16px;\n  border-top: 1px solid var(--bg-border-color-light);\n}\n\n/* Form elements */\n.form-group {\n  margin-bottom: 16px;\n}\n\n.form-label {\n  display: block;\n  font-size: 12px;\n  font-weight: 500;\n  color: var(--text-muted);\n  margin-bottom: 4px;\n}\n\n.form-input {\n  width: 100%;\n  height: 36px;\n  padding: 0 10px;\n  font-size: 13px;\n  font-family: var(--font-family);\n  color: var(--text-primary);\n  background: var(--bg-base-color);\n  border: 1px solid var(--normal-border);\n  border-radius: 4px;\n  transition: border-color 0.15s ease;\n}\n\n.form-input:focus {\n  outline: none;\n  border-color: var(--accent-primary);\n  box-shadow: 0 0 0 2px rgba(77, 124, 94, 0.15);\n}\n\n.form-input::placeholder {\n  color: var(--text-disabled);\n}\n\n.form-textarea {\n  width: 100%;\n  min-height: 80px;\n  padding: 8px 10px;\n  font-size: 13px;\n  font-family: var(--font-family);\n  color: var(--text-primary);\n  background: var(--bg-base-color);\n  border: 1px solid var(--normal-border);\n  border-radius: 4px;\n  resize: vertical;\n  transition: border-color 0.15s ease;\n}\n\n.form-textarea:focus {\n  outline: none;\n  border-color: var(--accent-primary);\n  box-shadow: 0 0 0 2px rgba(77, 124, 94, 0.15);\n}\n\n.form-row {\n  display: flex;\n  gap: 16px;\n}\n\n.form-row > .form-group {\n  flex: 1;\n}\n\n.form-error {\n  font-size: 11px;\n  color: var(--status-error);\n  margin-top: 2px;\n}\n\n/* Empty state */\n.empty-state {\n  text-align: center;\n  padding: 40px 20px;\n  color: var(--text-muted);\n}\n\n.empty-state-message {\n  font-size: 14px;\n  margin-bottom: 12px;\n}\n\n/* Loading state */\n.loading-state {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  padding: 