[2026-02-27T07:29:01.913Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01GHwnccRM4K8fzUZbagVKyx","type":"tool_result","content":"     1→import { getDb, query, jsonResponse, errorResponse } from \"./_db.js\";\n     2→\n     3→export default async function handler(req: Request): Promise<Response> {\n     4→  if (req.method === \"OPTIONS\") {\n     5→    return new Response(null, {\n     6→      status: 204,\n     7→      headers: {\n     8→        \"Access-Control-Allow-Origin\": \"*\",\n     9→        \"Access-Control-Allow-Methods\": \"GET, OPTIONS\",\n    10→        \"Access-Control-Allow-Headers\": \"Content-Type, Authorization\",\n    11→      },\n    12→    });\n    13→  }\n    14→\n    15→  const sql = getDb();\n    16→  const url = new URL(req.url);\n    17→  const segments = url.pathname.split(\"/\").filter(Boolean);\n    18→  const subPath = segments[3] || null;\n    19→\n    20→  // GET /.netlify/functions/users — list all users with stats\n    21→  if (req.method === \"GET\" && !subPath) {\n    22→    const users = await query<{\n    23→      id: string;\n    24→      name: string;\n    25→      email: string;\n    26→      avatar_url: string | null;\n    27→      created_at: string;\n    28→      active_deals: string;\n    29→      open_tasks: string;\n    30→    }>(\n    31→      sql,\n    32→      `SELECT u.id, u.name, u.email, u.avatar_url, u.created_at,\n    33→        COALESCE((SELECT COUNT(*) FILTER (WHERE d.status = 'open') FROM deals d WHERE d.owner_id = u.id), 0)::text AS active_deals,\n    34→        COALESCE((SELECT COUNT(*) FROM tasks t WHERE t.assignee_id = u.id AND t.status IN ('open', 'in_progress')), 0)::text AS open_tasks\n    35→       FROM users u ORDER BY u.name ASC`\n    36→    );\n    37→\n    38→    return jsonResponse(\n    39→      users.map((u) => ({\n    40→        id: u.id,\n    41→        name: u.name,\n    42→        email: u.email,\n    43→        avatarUrl: u.avatar_url,\n    44→        createdAt: u.created_at,\n    45→        activeDeals: parseInt(u.active_deals || \"0\", 10),\n    46→        openTasks: parseInt(u.open_tasks || \"0\", 10),\n    47→      }))\n    48→    );\n    49→  }\n    50→\n    51→  // GET /.netlify/functions/users/<id> — single user with stats, deals, tasks, activity\n    52→  if (req.method === \"GET\" && subPath) {\n    53→    const userRows = await query<{\n    54→      id: string;\n    55→      name: string;\n    56→      email: string;\n    57→      avatar_url: string | null;\n    58→      created_at: string;\n    59→    }>(\n    60→      sql,\n    61→      \"SELECT id, name, email, avatar_url, created_at FROM users WHERE id = $1\",\n    62→      [subPath]\n    63→    );\n    64→\n    65→    if (userRows.length === 0) {\n    66→      return errorResponse(404, \"User not found\");\n    67→    }\n    68→\n    69→    const u = userRows[0];\n    70→\n    71→    const dealStats = await query<{ active_deals: string; total_deals: string }>(\n    72→      sql,\n    73→      `SELECT\n    74→        COUNT(*) FILTER (WHERE status = 'open')::text AS active_deals,\n    75→        COUNT(*)::text AS total_deals\n    76→       FROM deals WHERE owner_id = $1`,\n    77→      [subPath]\n    78→    );\n    79→\n    80→    const taskStats = await query<{ open_tasks: string }>(\n    81→      sql,\n    82→      `SELECT COUNT(*)::text AS open_tasks\n    83→       FROM tasks WHERE assignee_id = $1 AND status IN ('open', 'in_progress')`,\n    84→      [subPath]\n    85→    );\n    86→\n    87→    // Fetch owned deals\n    88→    const ownedDeals = await query<{\n    89→      id: string;\n    90→      name: string;\n    91→      client_id: string;\n    92→      client_name: string | null;\n    93→      value: string | null;\n    94→      stage: string;\n    95→      status: string;\n    96→      created_at: string;\n    97→    }>(\n    98→      sql,\n    99→      `SELECT d.id, d.name, d.client_id, c.name AS client_name, d.value, d.stage, d.status, d.created_at\n   100→       FROM deals d\n   101→       LEFT JOIN clients c ON c.id = d.client_id\n   102→       WHERE d.owner_id = $1\n   103→       ORDER BY d.created_at DESC`,\n   104→      [subPath]\n   105→    );\n   106→\n   107→    // Fetch assigned tasks\n   108→    const assignedTasks = await query<{\n   109→      id: string;\n   110→      title: string;\n   111→      due_date: string | null;\n   112→      priority: string;\n   113→      status: string;\n   114→      client_id: string | null;\n   115→      client_name: string | null;\n   116→      created_at: string;\n   117→    }>(\n   118→      sql,\n   119→      `SELECT t.id, t.title, t.due_date, t.priority, t.status, t.client_id, c.name AS client_name, t.created_at\n   120→       FROM tasks t\n   121→       LEFT JOIN clients c ON c.id = t.client_id\n   122→       WHERE t.assignee_id = $1\n   123→       ORDER BY t.due_date ASC NULLS LAST, t.created_at DESC`,\n   124→      [subPath]\n   125→    );\n   126→\n   127→    // Fetch recent activity: combine timeline events created by this user with deal history and task updates\n   128→    const activity = await query<{\n   129→      id: string;\n   130→      event_type: string;\n   131→      description: string;\n   132→      created_at: string;\n   133→    }>(\n   134→      sql,\n   135→      `(SELECT te.id, te.event_type, te.description, te.created_at\n   136→        FROM timeline_events te\n   137→        WHERE te.created_by = $1\n   138→        ORDER BY te.created_at DESC\n   139→        LIMIT 20)\n   140→       UNION ALL\n   141→       (SELECT dh.id, 'Deal Stage Change' AS event_type,\n   142→        CONCAT('Changed deal stage from ''', dh.old_stage, ''' to ''', dh.new_stage, '''') AS description,\n   143→        dh.changed_at AS created_at\n   144→        FROM deal_history dh\n   145→        JOIN deals d ON d.id = dh.deal_id\n   146→        WHERE d.owner_id = $2 AND dh.changed_by = $3\n   147→        ORDER BY dh.changed_at DESC\n   148→        LIMIT 20)\n   149→       ORDER BY created_at DESC\n   150→       LIMIT 20`,\n   151→      [u.name, subPath, u.name]\n   152→    );\n   153→\n   154→    return jsonResponse({\n   155→      id: u.id,\n   156→      name: u.name,\n   157→      email: u.email,\n   158→      avatarUrl: u.avatar_url,\n   159→      createdAt: u.created_at,\n   160→      activeDeals: parseInt(dealStats[0]?.active_deals || \"0\", 10),\n   161→      totalDeals: parseInt(dealStats[0]?.total_deals || \"0\", 10),\n   162→      openTasks: parseInt(taskStats[0]?.open_tasks || \"0\", 10),\n   163→      deals: ownedDeals.map((d) => ({\n   164→        id: d.id,\n   165→        name: d.name,\n   166→        clientId: d.client_id,\n   167→        clientName: d.client_name,\n   168→        value: d.value ? parseFloat(d.value) : null,\n   169→        stage: d.stage,\n   170→        status: d.status,\n   171→        createdAt: d.created_at,\n   172→      })),\n   173→      tasks: assignedTasks.map((t) => ({\n   174→        id: t.id,\n   175→        title: t.title,\n   176→        dueDate: t.due_date,\n   177→        priority: t.priority,\n   178→        status: t.status,\n   179→        clientId: t.client_id,\n   180→        clientName: t.client_name,\n   181→        createdAt: t.created_at,\n   182→      })),\n   183→      activity: activity.map((a) => ({\n   184→        id: a.id,\n   185→        eventType: a.event_type,\n   186→        description: a.description,\n   187→        createdAt: a.created_at,\n   188→      })),\n   189→    });\n   190→  }\n   191→\n   192→  return errorResponse(405, \"Method not allowed\");\n   193→}\n   194→"}]},"parent_tool_use_id":"toolu_014rFJEPEFsmRE1XepAp9ZML","session_id":"a58d6e82-8d38-4fab-848f-3ed5e19c12ec","uuid":"334b2ad5-1748-4844-9f08-3f77dfdf83b4"}
