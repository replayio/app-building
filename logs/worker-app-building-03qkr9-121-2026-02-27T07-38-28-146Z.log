ct(page.getByLabel(\"Confirm Password\")).toBeVisible();\n    40→  });\n    41→\n    42→  test(\"Submit button is displayed with label\", async ({ page }) => {\n    43→    await page.goto(\"/auth/reset-password?token=any-token\");\n    44→    await expect(page.getByTestId(\"reset-password-form\")).toBeVisible({\n    45→      timeout: 30000,\n    46→    });\n    47→\n    48→    const submitBtn = page.getByTestId(\"reset-password-submit\");\n    49→    await expect(submitBtn).toBeVisible();\n    50→    await expect(submitBtn).toContainText(\"Reset Password\");\n    51→  });\n    52→\n    53→  test(\"Submit with matching passwords succeeds and auto-logs in\", async ({\n    54→    page,\n    55→    request,\n    56→  }) => {\n    57→    test.slow();\n    58→\n    59→    const uniqueEmail = `test-reset-${Date.now()}@example.com`;\n    60→    const originalPassword = \"OldPass123!\";\n    61→    const newPassword = \"NewSecure123!\";\n    62→\n    63→    // 1. Create a user via signup API (IS_TEST=true auto-confirms)\n    64→    await request.post(\"http://localhost:8888/.netlify/functions/auth\", {\n    65→      data: {\n    66→        action: \"signup\",\n    67→        email: uniqueEmail,\n    68→        password: originalPassword,\n    69→      },\n    70→    });\n    71→\n    72→    // 2. Call forgot-password to generate a real reset token in the DB\n    73→    await request.post(\"http://localhost:8888/.netlify/functions/auth\", {\n    74→      data: { action: \"forgot-password\", email: uniqueEmail },\n    75→    });\n    76→\n    77→    // 3. Query the database directly for the generated token\n    78→    const dbUrl = process.env.DATABASE_URL;\n    79→    if (!dbUrl) throw new Error(\"DATABASE_URL not set\");\n    80→    const sql = neon(dbUrl);\n    81→    const rows = await sql(\n    82→      `SELECT et.token FROM email_tokens et\n    83→       JOIN users u ON et.user_id = u.id\n    84→       WHERE u.email = $1 AND et.type = 'reset' AND et.used_at IS NULL\n    85→       ORDER BY et.created_at DESC LIMIT 1`,\n    86→      [uniqueEmail]\n    87→    );\n    88→    const resetToken = rows[0].token as string;\n    89→\n    90→    // 4. Navigate to the reset password page with the real token\n    91→    await page.goto(`/auth/reset-password?token=${resetToken}`);\n    92→    await expect(page.getByTestId(\"reset-password-form\")).toBeVisible({\n    93→      timeout: 30000,\n    94→    });\n    95→\n    96→    // 5. Fill in matching passwords and submit\n    97→    await page.getByTestId(\"reset-password-input\").fill(newPassword);\n    98→    await page.getByTestId(\"reset-password-confirm\").fill(newPassword);\n    99→    await page.getByTestId(\"reset-password-submit\").click();\n   100→\n   101→    // 6. Verify redirect to /clients and authenticated state in sidebar\n   102→    await expect(page).toHaveURL(/\\/clients/, { timeout: 30000 });\n   103→    await expect(page.getByTestId(\"sidebar\")).toBeVisible({ timeout: 30000 });\n   104→    await expect(page.getByTestId(\"sidebar-avatar\")).toBeVisible({\n   105→      timeout: 30000,\n   106→    });\n   107→    await expect(page.getByTestId(\"sidebar-user-name\")).toBeVisible();\n   108→    await expect(page.getByTestId(\"sidebar-signout-btn\")).toBeVisible();\n   109→  });\n   110→\n   111→  test(\"Submit with mismatched passwords shows validation error\", async ({\n   112→    page,\n   113→  }) => {\n   114→    await page.goto(\"/auth/reset-password?token=any-token\");\n   115→    await expect(page.getByTestId(\"reset-password-form\")).toBeVisible({\n   116→      timeout: 30000,\n   117→    });\n   118→\n   119→    await page.getByTestId(\"reset-password-input\").fill(\"Password1!\");\n   120→    await page.getByTestId(\"reset-password-confirm\").fill(\"Password2!\");\n   121→    await page.getByTestId(\"reset-password-submit\").click();\n   122→\n   123→    await expect(page.getByTestId(\"reset-password-error\")).toBeVisible();\n   124→    await expect(page.getByTestId(\"reset-password-error\")).toContainText(\n   125→      \"Passwords do not match\"\n   126→    );\n   127→\n   128→    // Form should remain visible for correction\n   129→    await expect(page.getByTestId(\"reset-password-input\")).toBeVisible();\n   130→    await expect(page.getByTestId(\"reset-password-confirm\")).toBeVisible();\n   131→  });\n   132→\n   133→  test(\"Submit with empty password fields shows validation error\", async ({\n   134→    page,\n   135→  }) => {\n   136→    await page.goto(\"/auth/reset-password?token=any-token\");\n   137→    await expect(page.getByTestId(\"reset-password-form\")).toBeVisible({\n   138→      timeout: 30000,\n   139→    });\n   140→\n   141→    // Click submit without entering any password\n   142→    await page.getByTestId(\"reset-password-submit\").click();\n   143→\n   144→    await expect(page.getByTestId(\"reset-password-error\")).toBeVisible();\n   145→    await expect(page.getByTestId(\"reset-password-error\")).toContainText(\n   146→      \"Password is required\"\n   147→    );\n   148→  });\n   149→\n   150→  test(\"Invalid token shows error message\", async ({ page }) => {\n   151→    await page.goto(\"/auth/reset-password?token=invalid-or-expired-token\");\n   152→    await expect(page.getByTestId(\"reset-password-form\")).toBeVisible({\n   153→      timeout: 30000,\n   154→    });\n   155→\n   156→    // Submit with valid passwords to trigger backend token validation\n   157→    await page.getByTestId(\"reset-password-input\").fill(\"SomePass123!\");\n   158→    await page.getByTestId(\"reset-password-confirm\").fill(\"SomePass123!\");\n   159→    await page.getByTestId(\"reset-password-submit\").click();\n   160→\n   161→    // Backend returns error for invalid token\n   162→    await expect(page.getByTestId(\"reset-password-error\")).toBeVisible({\n   163→      timeout: 30000,\n   164→    });\n   165→    await expect(page.getByTestId(\"reset-password-error\")).toContainText(\n   166→      \"This reset link is invalid or has expired\"\n   167→    );\n   168→\n   169→    // Link to request new reset should appear\n   170→    await expect(page.getByTestId(\"reset-password-forgot-link\")).toBeVisible();\n   171→  });\n   172→\n   173→  test(\"Missing token shows error message\", async ({ page }) => {\n   174→    await page.goto(\"/auth/reset-password\");\n   175→    await expect(page.getByTestId(\"reset-password-form\")).toBeVisible({\n   176→      timeout: 30000,\n   177→    });\n   178→\n   179→    await expect(page.getByTestId(\"reset-password-error\")).toBeVisible();\n   180→    await expect(page.getByTestId(\"reset-password-error\")).toContainText(\n   181→      \"No reset token provided\"\n   182→    );\n   183→\n   184→    // Link to request password reset should be visible\n   185→    await expect(page.getByTestId(\"reset-password-forgot-link\")).toBeVisible();\n   186→  });\n   187→\n   188→  test(\"Submit button shows loading state during request\", async ({\n   189→    page,\n   190→  }) => {\n   191→    await page.goto(\"/auth/reset-password?token=test-loading-token\");\n   192→    await expect(page.getByTestId(\"reset-password-form\")).toBeVisible({\n   193→      timeout: 30000,\n   194→    });\n   195→\n   196→    // Intercept the auth API request and delay it to observe loading state\n   197→    await page.route(\"**/.netlify/functions/auth\", async (route) => {\n   198→      await new Promise((resolve) => setTimeout(resolve, 2000));\n   199→      await route.continue();\n   200→    });\n   201→\n   202→    await page.getByTestId(\"reset-password-input\").fill(\"TestPass123!\");\n   203→    await page.getByTestId(\"reset-password-confirm\").fill(\"TestPass123!\");\n   204→    await page.getByTestId(\"reset-password-submit\").click();\n   205→\n   206→    // Button should show loading state\n   207→    const submitBtn = page.getByTestId(\"reset-password-submit\");\n   208→    await expect(submitBtn).toContainText(\"Resetting...\");\n   209→    await expect(submitBtn).toBeDisabled();\n   210→  });\n   211→\n   212→  test(\"Link to forgot password page is available on error\", async ({\n   213→    page,\n   214→  }) => {\n   215→    await page.goto(\"/auth/reset-password?token=invalid-token\");\n   216→    await expect(page.getByTestId(\"reset-password-form\")).toBeVisible({\n   217→      timeout: 30000,\n   218→    });\n   219→\n   220→    // Submit to trigger invalid token error from backend\n   221→    await page.getByTestId(\"reset-password-input\").fill(\"SomePass123!\");\n   222→    await page.getByTestId(\"reset-password-confirm\").fill(\"SomePass123!\");\n   223→    await page.getByTestId(\"reset-password-submit\").click();\n   224→\n   225→    // Wait for error to appear\n   226→    await expect(page.getByTestId(\"reset-password-error\")).toBeVisible({\n   227→      timeout: 30000,\n   228→    });\n   229→\n   230→    // Forgot password link should be visible with correct text\n   231→    const forgotLink = page.getByTestId(\"reset-password-forgot-link\");\n   232→    await expect(forgotLink).toBeVisible();\n   233→    await expect(forgotLink).toContainText(\"Request a new reset link\");\n   234→\n   235→    // Click and verify navigation to /auth/forgot-password\n   236→    await forgotLink.click();\n   237→    await expect(page).toHaveURL(/\\/auth\\/forgot-password$/);\n   238→    await expect(page.getByTestId(\"forgot-password-page\")).toBeVisible({\n   239→      timeout: 30000,\n   240→    });\n   241→  });\n   242→});\n   243→"}]},"parent_tool_use_id":"toolu_01XE9jSNKHWwkN6bwb6n5Kz1","session_id":"a58d6e82-8d38-4fab-848f-3ed5e19c12ec","uuid":"205f37fe-118d-423c-a639-f92ae00b4d61"}
