",\n    69→  async (userId: string) => {\n    70→    const resp = await fetch(`/.netlify/functions/users/${userId}`);\n    71→    if (!resp.ok) {\n    72→      const data = await resp.json();\n    73→      throw new Error(data.error || \"Failed to fetch user\");\n    74→    }\n    75→    return resp.json() as Promise<UserDetailResponse>;\n    76→  }\n    77→);\n    78→\n    79→const userDetailSlice = createSlice({\n    80→  name: \"userDetail\",\n    81→  initialState,\n    82→  reducers: {\n    83→    clearUserDetail(state) {\n    84→      state.user = null;\n    85→      state.deals = [];\n    86→      state.tasks = [];\n    87→      state.activity = [];\n    88→      state.loading = false;\n    89→      state.error = null;\n    90→    },\n    91→  },\n    92→  extraReducers: (builder) => {\n    93→    builder\n    94→      .addCase(fetchUserDetail.pending, (state) => {\n    95→        if (!state.user) state.loading = true;\n    96→        state.error = null;\n    97→      })\n    98→      .addCase(fetchUserDetail.fulfilled, (state, action: PayloadAction<UserDetailResponse>) => {\n    99→        state.loading = false;\n   100→        const { deals, tasks, activity, ...user } = action.payload;\n   101→        state.user = user;\n   102→        state.deals = deals;\n   103→        state.tasks = tasks;\n   104→        state.activity = activity;\n   105→      })\n   106→      .addCase(fetchUserDetail.rejected, (state, action) => {\n   107→        state.loading = false;\n   108→        state.error = action.error.message || \"Failed to fetch user\";\n   109→      });\n   110→  },\n   111→});\n   112→\n   113→export const { clearUserDetail } = userDetailSlice.actions;\n   114→export const userDetailReducer = userDetailSlice.reducer;\n   115→"}]},"parent_tool_use_id":"toolu_01XE9jSNKHWwkN6bwb6n5Kz1","session_id":"a58d6e82-8d38-4fab-848f-3ed5e19c12ec","uuid":"203f0fb8-76fb-41d9-a2c3-f632dec9acf8"}
