[2026-02-25T01:32:19.517Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01NFrxTTGdaT3G2t5UTQdaVE","type":"tool_result","content":"     1→import { neon } from \"@neondatabase/serverless\";\n     2→import type { Context } from \"@netlify/functions\";\n     3→\n     4→export default async function handler(req: Request, _context: Context) {\n     5→  const dbUrl = process.env.DATABASE_URL;\n     6→  if (!dbUrl) {\n     7→    return new Response(JSON.stringify({ error: \"DATABASE_URL not set\" }), {\n     8→      status: 500,\n     9→      headers: { \"Content-Type\": \"application/json\" },\n    10→    });\n    11→  }\n    12→\n    13→  const sql = neon(dbUrl);\n    14→  const url = new URL(req.url);\n    15→  const segments = url.pathname.split(\"/\").filter(Boolean);\n    16→  const recipeId = segments[3] || null;\n    17→\n    18→  if (req.method === \"GET\") {\n    19→    try {\n    20→      if (recipeId) {\n    21→        const rows = await sql`\n    22→          SELECT id, name, product, version, status, description,\n    23→                 instructions, created_at, updated_at\n    24→          FROM recipes\n    25→          WHERE id = ${recipeId}::uuid\n    26→          LIMIT 1\n    27→        `;\n    28→        if (rows.length === 0) {\n    29→          return new Response(JSON.stringify({ error: \"Recipe not found\" }), {\n    30→            status: 404,\n    31→            headers: { \"Content-Type\": \"application/json\" },\n    32→          });\n    33→        }\n    34→\n    35→        const recipe = rows[0];\n    36→\n    37→        const materials = await sql`\n    38→          SELECT id, recipe_id, material_name, quantity, unit\n    39→          FROM recipe_materials\n    40→          WHERE recipe_id = ${recipeId}::uuid\n    41→          ORDER BY material_name\n    42→        `;\n    43→\n    44→        const products = await sql`\n    45→          SELECT id, recipe_id, product_name, amount\n    46→          FROM recipe_products\n    47→          WHERE recipe_id = ${recipeId}::uuid\n    48→          ORDER BY product_name\n    49→        `;\n    50→\n    51→        const equipment = await sql`\n    52→          SELECT re.id, re.recipe_id, re.equipment_id, e.name AS equipment_name\n    53→          FROM recipe_equipment re\n    54→          JOIN equipment e ON e.id = re.equipment_id\n    55→          WHERE re.recipe_id = ${recipeId}::uuid\n    56→          ORDER BY e.name\n    57→        `;\n    58→\n    59→        return new Response(\n    60→          JSON.stringify({\n    61→            ...recipe,\n    62→            materials,\n    63→            products,\n    64→            equipment,\n    65→          }),\n    66→          {\n    67→            status: 200,\n    68→            headers: { \"Content-Type\": \"application/json\" },\n    69→          }\n    70→        );\n    71→      }\n    72→\n    73→      const rows = await sql`\n    74→        SELECT id, name, product, version, status, description,\n    75→               instructions, created_at, updated_at\n    76→        FROM recipes\n    77→        ORDER BY name\n    78→      `;\n    79→      return new Response(JSON.stringify(rows), {\n    80→        status: 200,\n    81→        headers: { \"Content-Type\": \"application/json\" },\n    82→      });\n    83→    } catch (err) {\n    84→      console.error(\"Recipes GET error:\", err);\n    85→      return new Response(JSON.stringify({ error: \"Internal server error\" }), {\n    86→        status: 500,\n    87→        headers: { \"Content-Type\": \"application/json\" },\n    88→      });\n    89→    }\n    90→  }\n    91→\n    92→  if (req.method === \"POST\") {\n    93→    try {\n    94→      const body = await req.json();\n    95→      const { name, product, version, status, description, instructions } = body;\n    96→\n    97→      if (!name) {\n    98→        return new Response(JSON.stringify({ error: \"name is required\" }), {\n    99→          status: 400,\n   100→          headers: { \"Content-Type\": \"application/json\" },\n   101→        });\n   102→      }\n   103→\n   104→      const rows = await sql`\n   105→        INSERT INTO recipes (name, product, version, status, description, instructions)\n   106→        VALUES (\n   107→          ${name},\n   108→          ${product || \"\"},\n   109→          ${version || \"1.0\"},\n   110→          ${status || \"Draft\"},\n   111→          ${description || \"\"},\n   112→          ${instructions || \"\"}\n   113→        )\n   114→        RETURNING *\n   115→      `;\n   116→      return new Response(JSON.stringify(rows[0]), {\n   117→        status: 201,\n   118→        headers: { \"Content-Type\": \"application/json\" },\n   119→      });\n   120→    } catch (err) {\n   121→      console.error(\"Recipes POST error:\", err);\n   122→      return new Response(JSON.stringify({ error: \"Internal server error\" }), {\n   123→        status: 500,\n   124→        headers: { \"Content-Type\": \"application/json\" },\n   125→      });\n   126→    }\n   127→  }\n   128→\n   129→  if (req.method === \"PUT\") {\n   130→    if (!recipeId) {\n   131→      return new Response(JSON.stringify({ error: \"Recipe ID required\" }), {\n   132→        status: 400,\n   133→        headers: { \"Content-Type\": \"application/json\" },\n   134→      });\n   135→    }\n   136→\n   137→    try {\n   138→      const body = await req.json();\n   139→      const { name, product, version, status, description, instructions, materials, products, equipment } = body;\n   140→\n   141→      const rows = await sql`\n   142→        UPDATE recipes SET\n   143→          name = COALESCE(${name}, name),\n   144→          product = COALESCE(${product}, product),\n   145→          version = COALESCE(${version}, version),\n   146→          status = COALESCE(${status}, status),\n   147→          description = COALESCE(${description}, description),\n   148→          instructions = COALESCE(${instructions}, instructions),\n   149→          updated_at = NOW()\n   150→        WHERE id = ${recipeId}::uuid\n   151→        RETURNING *\n   152→      `;\n   153→      if (rows.length === 0) {\n   154→        return new Response(JSON.stringify({ error: \"Recipe not found\" }), {\n   155→          status: 404,\n   156→          headers: { \"Content-Type\": \"application/json\" },\n   157→        });\n   158→      }\n   159→\n   160→      // If materials were provided, replace them\n   161→      if (materials !== undefined) {\n   162→        await sql`DELETE FROM recipe_materials WHERE recipe_id = ${recipeId}::uuid`;\n   163→        for (const m of materials) {\n   164→          await sql`\n   165→            INSERT INTO recipe_materials (recipe_id, material_name, quantity, unit)\n   166→            VALUES (${recipeId}::uuid, ${m.material_name}, ${m.quantity}, ${m.unit || \"kg\"})\n   167→          `;\n   168→        }\n   169→      }\n   170→\n   171→      // If products were provided, replace them\n   172→      if (products !== undefined) {\n   173→        await sql`DELETE FROM recipe_products WHERE recipe_id = ${recipeId}::uuid`;\n   174→        for (const p of products) {\n   175→          await sql`\n   176→            INSERT INTO recipe_products (recipe_id, product_name, amount)\n   177→            VALUES (${recipeId}::uuid, ${p.product_name}, ${p.amount || \"\"})\n   178→          `;\n   179→        }\n   180→      }\n   181→\n   182→      // If equipment were provided, replace them\n   183→      if (equipment !== undefined) {\n   184→        await sql`DELETE FROM recipe_equipment WHERE recipe_id = ${recipeId}::uuid`;\n   185→        for (const e of equipment) {\n   186→          await sql`\n   187→            INSERT INTO recipe_equipment (recipe_id, equipment_id)\n   188→            VALUES (${recipeId}::uuid, ${e.equipment_id}::uuid)\n   189→          `;\n   190→        }\n   191→      }\n   192→\n   193→      // Re-fetch full recipe with related data\n   194→      const updatedMaterials = await sql`\n   195→        SELECT id, recipe_id, material_name, quantity, unit\n   196→        FROM recipe_materials WHERE recipe_id = ${recipeId}::uuid ORDER BY material_name\n   197→      `;\n   198→      const updatedProducts = await sql`\n   199→        SELECT id, recipe_id, product_name, amount\n   200→        FROM recipe_products WHERE recipe_id = ${recipeId}::uuid ORDER BY product_name\n   201→      `;\n   202→      const updatedEquipment = await sql`\n   203→        SELECT re.id, re.recipe_id, re.equipment_id, e.name AS equipment_name\n   204→        FROM recipe_equipment re\n   205→        JOIN equipment e ON e.id = re.equipment_id\n   206→        WHERE re.recipe_id = ${recipeId}::uuid ORDER BY e.name\n   207→      `;\n   208→\n   209→      return new Response(\n   210→        JSON.stringify({\n   211→          ...rows[0],\n   212→          materials: updatedMaterials,\n   213→          products: updatedProducts,\n   214→          equipment: updatedEquipment,\n   215→        }),\n   216→        {\n   217→          status: 200,\n   218→          headers: { \"Content-Type\": \"application/json\" },\n   219→        }\n   220→      );\n   221→    } catch (err) {\n   222→      console.error(\"Recipes PUT error:\", err);\n   223→      return new Response(JSON.stringify({ error: \"Internal server error\" }), {\n   224→        status: 500,\n   225→        headers: { \"Content-Type\": \"application/json\" },\n   226→      });\n   227→    }\n   228→  }\n   229→\n   230→  if (req.method === \"DELETE\") {\n   231→    if (!recipeId) {\n   232→      return new Response(JSON.stringify({ error: \"Recipe ID required\" }), {\n   233→        status: 400,\n   234→        headers: { \"Content-Type\": \"application/json\" },\n   235→      });\n   236→    }\n   237→\n   238→    try {\n   239→      await sql`DELETE FROM recipes WHERE id = ${recipeId}::uuid`;\n   240→      return new Response(JSON.stringify({ success: true }), {\n   241→        status: 200,\n   242→        headers: { \"Content-Type\": \"application/json\" },\n   243→      });\n   244→    } catch (err) {\n   245→      console.error(\"Recipes DELETE error:\", err);\n   246→      return new Response(JSON.stringify({ error: \"Internal server error\" }), {\n   247→        status: 500,\n   248→        headers: { \"Content-Type\": \"application/json\" },\n   249→      });\n   250→    }\n   251→  }\n   252→\n   253→  return new Response(JSON.stringify({ error: \"Method not allowed\" }), {\n   254→    status: 405,\n   255→    headers: { \"Content-Type\": \"application/json\" },\n   256→  });\n   257→}\n   258→"}]},"parent_tool_use_id":"toolu_017LnrMbVFeaDvUxNLB7mR9i","session_id":"3a6b0167-92fa-4acc-afb9-70cc63a0b3b5","uuid":"fd1fbba2-ec0d-4680-baf7-2055cd97c727"}
