51→          data-testid=\"edit-recipe-btn\"\n    52→          className=\"btn-primary\"\n    53→          onClick={() => setShowEditModal(true)}\n    54→        >\n    55→          Edit Recipe\n    56→        </button>\n    57→      </div>\n    58→\n    59→      {showEditModal && (\n    60→        <EditRecipeModal\n    61→          recipe={recipe}\n    62→          onClose={() => setShowEditModal(false)}\n    63→          onSave={async (data) => {\n    64→            await dispatch(updateRecipe({ id: recipe.id, ...data }));\n    65→            setShowEditModal(false);\n    66→          }}\n    67→        />\n    68→      )}\n    69→    </div>\n    70→  );\n    71→}\n    72→\n    73→interface EditRecipeModalProps {\n    74→  recipe: RecipeWithRelations;\n    75→  onClose: () => void;\n    76→  onSave: (data: Partial<Recipe>) => Promise<void>;\n    77→}\n    78→\n    79→function EditRecipeModal({ recipe, onClose, onSave }: EditRecipeModalProps) {\n    80→  const [name, setName] = useState(recipe.name);\n    81→  const [description, setDescription] = useState(recipe.description);\n    82→  const [product, setProduct] = useState(recipe.product);\n    83→  const [version, setVersion] = useState(recipe.version);\n    84→  const [status, setStatus] = useState(recipe.status);\n    85→  const [saving, setSaving] = useState(false);\n    86→\n    87→  const handleSubmit = async (e: React.FormEvent) => {\n    88→    e.preventDefault();\n    89→    setSaving(true);\n    90→    await onSave({ name, description, product, version, status });\n    91→    setSaving(false);\n    92→  };\n    93→\n    94→  return (\n    95→    <div className=\"modal-overlay\" onClick={onClose} data-testid=\"edit-recipe-modal\">\n    96→      <div className=\"modal-content\" onClick={(e) => e.stopPropagation()}>\n    97→        <div className=\"modal-header\">\n    98→          <h2 className=\"modal-title\">Edit Recipe</h2>\n    99→          <button className=\"modal-close-btn\" onClick={onClose}>\n   