ority-normal: #6b7280;\n  --color-priority-normal-bg: #f3f4f6;\n\n  /* Colors - Deal status */\n  --color-deal-on-track: #22c55e;\n  --color-deal-on-track-bg: #f0fdf4;\n  --color-deal-needs-attention: #f59e0b;\n  --color-deal-needs-attention-bg: #fffbeb;\n  --color-deal-at-risk: #ef4444;\n  --color-deal-at-risk-bg: #fef2f2;\n  --color-deal-won: #7180ff;\n  --color-deal-won-bg: #eef0ff;\n\n  /* Spacing */\n  --sidebar-width: 244px;\n\n  /* Border radius */\n  --radius-sm: 4px;\n  --radius-md: 6px;\n  --radius-lg: 8px;\n\n  /* Shadows */\n  --shadow-sm: 0px 4px 12px rgba(0, 0, 0, 0.1);\n  --shadow-md: 0px 4px 12px rgba(0, 0, 0, 0.1), 0px 0px 0px 1px rgba(0, 0, 0, 0.2);\n}\n\n* {\n  box-sizing: border-box;\n  margin: 0;\n  padding: 0;\n}\n\nbody {\n  font-family: var(--font-sans);\n  font-size: 13px;\n  color: var(--color-text-primary);\n  background: var(--color-bg-base);\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n}\n\n/* Scrollbar styling */\n::-webkit-scrollbar {\n  width: 8px;\n  height: 8px;\n}\n::-webkit-scrollbar-track {\n  background: transparent;\n}\n::-webkit-scrollbar-thumb {\n  background: var(--color-border-default);\n  border-radius: 4px;\n}\n::-webkit-scrollbar-thumb:hover {\n  background: var(--color-text-disabled);\n}\n\n/* ===== Layout ===== */\n.app-layout {\n  display: flex;\n  height: 100vh;\n  overflow: hidden;\n}\n\n.app-main {\n  flex: 1;\n  overflow-y: auto;\n  background: var(--color-bg-base);\n}\n\n.page-content {\n  max-width: 1400px;\n}\n\n.page-header {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  margin-bottom: 20px;\n  gap: 16px;\n}\n\n.page-title {\n  font-size: 20px;\n  font-weight: 600;\n  color: var(--color-text-primary);\n}\n\n/* ===== Search & Filters ===== */\n.clients-filters {\n  display: flex;\n  flex-direction: column;\n  gap: 12px;\n}\n\n.clients-search-wrapper {\n  position: relative;\n  display: flex;\n  align-items: center;\n}\n\n.clients-search-icon {