98→  `\n   199→\n   200→  const csvHeader = 'Client Name,Type,Status,Tags,Source,Primary Contact,Open Deals,Next Task'\n   201→  const csvRows = rows.map((r: Record<string, unknown>) => {\n   202→    const tags = (r.tags as string[]) || []\n   203→    const pc = r.primary_contact as { name: string; role: string } | null\n   204→    const od = r.open_deals as { count: number; totalValue: number } | null\n   205→    const nt = r.next_task as { title: string; dueDate: string } | null\n   206→    return [\n   207→      `\"${String(r.name || '').replace(/\"/g, '\"\"')}\"`,\n   208→      r.type,\n   209→      r.status,\n   210→      `\"${tags.join(', ')}\"`,\n   211→      r.source || '',\n   212→      pc ? `${pc.name} (${pc.role || 'N/A'})` : '',\n   213→      od ? `${od.count} ($${od.totalValue})` : '0',\n   214→      nt ? `${nt.title} - ${nt.dueDate || 'No date'}` : 'N/A',\n   215→    ].join(',')\n   216→  })\n   217→\n   218→  return {\n   219→    statusCode: 200,\n   220→    headers: {\n   221→      'Content-Type': 'text/csv',\n   222→      'Content-Disposition': 'attachment; filename=\"clients.csv\"',\n   223→      ...corsHeaders,\n   224→    },\n   225→    body: [csvHeader, ...csvRows].join('\\n'),\n   226→  }\n   227→}\n   228→\n   229→async function handleCreate(sql: SqlFn, body: string | null) {\n   230→  const data = JSON.parse(body || '{}')\n   231→  const { name, type, status, tags, source } = data\n   232→  if (!name) return json(400, { error: 'Client name is required' })\n   233→\n   234→  const result = await sql`\n   235→    INSERT INTO clients (name, type, status, source)\n   236→    VALUES (${name}, ${type || 'Organization'}, ${status || 'Prospect'}, ${source || null})\n   237→    RETURNING *\n   238→  `\n   239→  const newClient = result[0]\n   240→\n   241→  if (tags && tags.length) {\n   242→    for (const tagName of tags) {\n   243→      await sql`INSERT INTO tags (name) VALUES (${tagName}) ON CONFLICT (name) DO NOTHING`\n   244→      const tagRows =