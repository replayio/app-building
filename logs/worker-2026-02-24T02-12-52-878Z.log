acity: 0.9;\n}\n\n.btn-primary:disabled {\n  opacity: 0.6;\n  cursor: default;\n}\n\n.btn-secondary {\n  display: inline-flex;\n  align-items: center;\n  gap: 6px;\n  height: 28px;\n  padding: 0 12px;\n  border: 1px solid var(--color-border-default);\n  border-radius: 5px;\n  background: var(--color-bg-base);\n  color: var(--color-text-secondary);\n  font-size: 13px;\n  cursor: pointer;\n  transition: background 0.1s ease;\n  white-space: nowrap;\n}\n\n.btn-secondary:hover {\n  background: var(--color-bg-hover);\n}\n\n.btn-danger {\n  display: inline-flex;\n  align-items: center;\n  gap: 6px;\n  height: 28px;\n  padding: 0 12px;\n  border: none;\n  border-radius: 5px;\n  background: var(--color-status-churned);\n  color: white;\n  font-size: 13px;\n  font-weight: 500;\n  cursor: pointer;\n  transition: opacity 0.1s ease;\n  white-space: nowrap;\n}\n\n.btn-danger:hover {\n  opacity: 0.9;\n}\n\n/* ===== Table ===== */\n.clients-table {\n  display: grid;\n  grid-template-columns: 1fr;\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-md);\n}\n\n.clients-table-header {\n  display: grid;\n  grid-template-columns: 2fr 1fr 1fr 1.5fr 1.5fr 1fr 1.5fr 44px;\n  align-items: center;\n  height: 36px;\n  padding: 0 12px;\n  background: var(--color-bg-sidebar);\n  border-bottom: 1px solid var(--color-border-default);\n  font-size: 12px;\n  font-weight: 500;\n  color: var(--color-text-muted);\n}\n\n.clients-table-row {\n  display: grid;\n  grid-template-columns: 2fr 1fr 1fr 1.5fr 1.5fr 1fr 1.5fr 44px;\n  align-items: center;\n  height: 44px;\n  padding: 0 12px;\n  border-bottom: 1px solid var(--color-border-light);\n  transition: background 0.1s ease;\n  cursor: default;\n}\n\n.clients-table-row:last-child {\n  border-bottom: none;\n}\n\n.clients-table-row:hover {\n  background: var(--color-bg-hover);\n}\n\n.table-cell {\n  padding: 0 8px;\n  overflow: hidden;\n  text-overflow: ellipsis;\n  white-space: nowrap;\n  font-size: 13px;\n  color: var(--colo