     AND (${tagFilter} = '' OR EXISTS (\n    98→        SELECT 1 FROM client_tags ct JOIN tags t ON ct.tag_id = t.id\n    99→        WHERE ct.client_id = c.id AND t.name = ${tagFilter}\n   100→      ))\n   101→      AND (${searchPattern} = '' OR c.name ILIKE ${searchPattern}\n   102→        OR EXISTS (SELECT 1 FROM client_tags ct2 JOIN tags t2 ON ct2.tag_id = t2.id\n   103→                   WHERE ct2.client_id = c.id AND t2.name ILIKE ${searchPattern})\n   104→        OR EXISTS (SELECT 1 FROM client_individuals ci JOIN individuals i ON ci.individual_id = i.id\n   105→                   WHERE ci.client_id = c.id AND i.name ILIKE ${searchPattern})\n   106→      )\n   107→  `\n   108→  const total = countResult[0].total\n   109→\n   110→  const rows = await sql`\n   111→    SELECT c.*,\n   112→      (SELECT COALESCE(json_agg(t.name), '[]'::json)\n   113→       FROM client_tags ct JOIN tags t ON ct.tag_id = t.id WHERE ct.client_id = c.id) as tags,\n   114→      (SELECT json_build_object('name', i.name, 'role', COALESCE(ci.role, ''))\n   115→       FROM client_individuals ci JOIN individuals i ON ci.individual_id = i.id\n   116→       WHERE ci.client_id = c.id AND ci.is_primary = true LIMIT 1) as primary_contact,\n   117→      (SELECT json_build_object('count', COUNT(*)::int, 'totalValue', COALESCE(SUM(d.value), 0)::numeric)\n   118→       FROM deals d WHERE d.client_id = c.id AND d.stage NOT IN ('Closed Won', 'Closed Lost')) as open_deals,\n   119→      (SELECT json_build_object('title', tk.title, 'dueDate', tk.due_date)\n   120→       FROM tasks tk WHERE tk.client_id = c.id AND tk.completed = false\n   121→       ORDER BY tk.due_date ASC NULLS LAST LIMIT 1) as next_task\n   122→    FROM clients c\n   123→    WHERE\n   124→      (${statusFilter} = '' OR c.status = ${statusFilter})\n   125→      AND (${sourceFilter} = '' OR c.source = ${sourceFilter})\n   126→      AND (${tagFilter} = '' OR EXISTS (\n   127→        SELECT 1 FROM client_tags ct JOIN tags t ON ct.tag_id =