children: ReactNode }) {\n    55→  const [user, setUser] = useState<AuthUser | null>(null)\n    56→  const [loading, setLoading] = useState(true)\n    57→\n    58→  useEffect(() => {\n    59→    const token = getToken()\n    60→    if (!token) {\n    61→      setLoading(false)\n    62→      return\n    63→    }\n    64→\n    65→    fetch('/.netlify/functions/auth?action=me')\n    66→      .then((res) => {\n    67→        if (res.ok) return res.json()\n    68→        removeToken()\n    69→        return null\n    70→      })\n    71→      .then((data) => {\n    72→        if (data && data.id) {\n    73→          setUser(data)\n    74→        }\n    75→      })\n    76→      .catch(() => {\n    77→        removeToken()\n    78→      })\n    79→      .finally(() => {\n    80→        setLoading(false)\n    81→      })\n    82→  }, [])\n    83→\n    84→  const signUp = useCallback(async (email: string, password: string): Promise<AuthResult> => {\n    85→    try {\n    86→      const res = await fetch('/.netlify/functions/auth?action=signup', {\n    87→        method: 'POST',\n    88→        headers: { 'Content-Type': 'application/json' },\n    89→        body: JSON.stringify({ email, password }),\n    90→      })\n    91→      const text = await res.text()\n    92→      let data: Record<string, unknown>\n    93→      try { data = JSON.parse(text) } catch { data = {} }\n    94→      if (!res.ok) {\n    95→        return { error: (data.error as string) || 'Sign up failed' }\n    96→      }\n    97→      if (data.needsConfirmation) {\n    98→        return { error: null, needsConfirmation: true, message: data.message as string }\n    99→      }\n   100→      setToken(data.access_token as string)\n   101→      setUser(data.user as AuthUser)\n   102→      return { error: null }\n   103→    } catch {\n   104→      return { error: 'Sign up failed' }\n   105→    }\n   106→  }, [])\n   107→\n   108→  const signIn = useCallback(async (email: string, password: string): Promise<AuthResult> => {\n   109→    try {\n   110→      const res = await fetch('/.netlify/functions/auth?action=login', {\n   111→        method: 'POST',\n   112→        headers: { 'Content-Type': 'application/json' },\n   113→        body: JSON.stringify({ email, password }),\n   114→      })\n   115→      const data = await res.json()\n   116→      if (!res.ok) {\n   117→        return { error: data.error || 'Sign in failed' }\n   118→      }\n   119→      setToken(data.access_token)\n   120→      setUser(data.user)\n   121→      return { error: null }\n   122→    } catch {\n   123→      return { error: 'Sign in failed' }\n   124→    }\n   125→  }, [])\n   126→\n   127→  const signOut = useCallback(() => {\n   128→    removeToken()\n   129→    setUser(null)\n   130→  }, [])\n   131→\n   132→  const forgotPassword = useCallback(async (email: string): Promise<ForgotPasswordResult> => {\n   133→    try {\n   134→      const res = await fetch('/.netlify/functions/auth?action=forgot-password', {\n   135→        method: 'POST',\n   136→        headers: { 'Content-Type': 'application/json' },\n   137→        body: JSON.stringify({ email }),\n   138→      })\n   139→      const data = await res.json()\n   140→      if (!res.ok) {\n   141→        return { error: data.error || 'Request failed' }\n   142→      }\n   143→      return { error: null, message: data.message }\n   144→    } catch {\n   145→      return { error: 'Request failed' }\n   146→    }\n   147→  }, [])\n   148→\n   149→  const resetPassword = useCallback(async (token: string, password: string): Promise<ResetPasswordResult> => {\n   150→    try {\n   151→      const res = await fetch('/.netlify/functions/auth?action=reset-password', {\n   152→        method: 'POST',\n   153→        headers: { 'Content-Type': 'application/json' },\n   154→        body: JSON.stringify({ token, password }),\n   155→      })\n   156→      const data = await res.json()\n   157→      if (!res.ok) {\n   158→        return { error: data.error || 'Password reset failed' }\n   159→      }\n   160→      setToken(data.access_token)\n   161→      setUser(data.user)\n   162→      return { error: null }\n   163→    } catch {\n   164→      return { error: 'Password reset failed' }\n   165→    }\n   166→  }, [])\n   167→\n   168→  const confirmEmail = useCallback(async (token: string): Promise<ConfirmEmailResult> => {\n   169→    try {\n   170→      const res = await fetch('/.netlify/functions/auth?action=confirm-email', {\n   171→        method: 'POST',\n   172→        headers: { 'Content-Type': 'application/json' },\n   173→        body: JSON.stringify({ token }),\n   174→      })\n   175→      const data = await res.json()\n   176→      if (!res.ok) {\n   177→        return { error: data.error || 'Email confirmation failed' }\n   178→      }\n   179→      setToken(data.access_token)\n   180→      setUser(data.user)\n   181→      return { error: null }\n   182→    } catch {\n   183→      return { error: 'Email confirmation failed' }\n   184→    }\n   185→  }, [])\n   186→\n   187→  return (\n   188→    <AuthContext.Provider\n   189→      value={{\n   190→        isLoggedIn: !!user,\n   191→        loading,\n   192→        user,\n   193→        signIn,\n   194→        signUp,\n   195→        signOut,\n   196→        forgotPassword,\n   197→        resetPassword,\n   198→        confirmEmail,\n   199→      }}\n   200→    >\n   201→      {children}\n   202→    </AuthContext.Provider>\n   203→  )\n   204→}\n   205→\n   206→export function useAuth() {\n   207→  return useContext(AuthContext)\n   208→}\n   209→"}]},"parent_tool_use_id":"toolu_01LNscEfLaCmeXR4d7Td3TJQ","session_id":"b9cc8440-e040-411a-b77b-3353f7718a6c","uuid":"d68baac9-27de-43f4-8c8b-363a5ff31fc3"}
