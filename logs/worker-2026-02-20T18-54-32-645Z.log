                {isSignUp ? 'Already have an account? Sign in' : \"Don't have an account? Sign up\"}\n   151→                  </button>\n   152→                  <button\n   153→                    type=\"button\"\n   154→                    onClick={() => { setShowAuthForm(false); setAuthError(null) }}\n   155→                    className=\"text-[11px] text-text-muted hover:text-text-secondary transition-colors duration-100\"\n   156→                  >\n   157→                    Cancel\n   158→                  </button>\n   159→                </form>\n   160→              )}\n   161→            </div>\n   162→          ) : (\n   163→            <button\n   164→              data-testid=\"sidebar-sign-in\"\n   165→              onClick={() => { setShowAuthForm(true); setIsSignUp(false); setAuthError(null); setConfirmationMessage(null) }}\n   166→              className=\"flex items-center gap-2 h-7 px-0 rounded-[4px] text-[12px] font-[450] text-text-muted hover:text-text-secondary transition-colors duration-100\"\n   167→            >\n   168→              <LogIn size={14} strokeWidth={1.75} />\n   169→              Sign in\n   170→            </button>\n   171→          )}\n   172→        </div>\n   173→\n   174→        <nav className=\"flex flex-col gap-0.5\">\n   175→          {navItems.map((item) => (\n   176→            <NavLink\n   177→              key={item.label}\n   178→              to={item.to}\n   179→              data-testid={`sidebar-nav-${item.label.toLowerCase()}`}\n   180→              className={({ isActive }) =>\n   181→                `flex items-center gap-2.5 h-7 px-2 rounded-[4px] text-[13px] font-[450] transition-colors duration-100 ${\n   182→                  isActive\n   183→                    ? 'bg-sidebar-active text-text-primary'\n   184→                    : 'text-text-secondary hover:bg-sidebar-hover'\n   185→                }`\n   186→              }\n   187→            >\n   188→              <item.icon size={16} strokeWidt