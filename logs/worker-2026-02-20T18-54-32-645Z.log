n) {\n    88→      return new Response(JSON.stringify({ error: 'Token is required' }), { status: 400, headers })\n    89→    }\n    90→\n    91→    const rows = await sql`\n    92→      SELECT et.id AS token_id, et.user_id, et.expires_at, et.used_at, u.auth_user_id, u.email, u.name, u.avatar_url\n    93→      FROM email_tokens et\n    94→      JOIN users u ON u.id = et.user_id\n    95→      WHERE et.token = ${token} AND et.type = 'confirm'\n    96→    `\n    97→    if (rows.length === 0) {\n    98→      return new Response(JSON.stringify({ error: 'Invalid or expired confirmation link' }), { status: 400, headers })\n    99→    }\n   100→\n   101→    const row = rows[0]\n   102→    if (row.used_at) {\n   103→      return new Response(JSON.stringify({ error: 'This confirmation link has already been used' }), { status: 400, headers })\n   104→    }\n   105→    if (new Date(row.expires_at as string) < new Date()) {\n   106→      return new Response(JSON.stringify({ error: 'This confirmation link has expired' }), { status: 400, headers })\n   107→    }\n   108→\n   109→    await sql`UPDATE email_tokens SET used_at = NOW() WHERE id = ${row.token_id}`\n   110→    await sql`UPDATE users SET email_confirmed = true, updated_at = NOW() WHERE id = ${row.user_id}`\n   111→\n   112→    const jwtToken = await generateToken(row.auth_user_id as string, row.email as string, row.name as string)\n   113→\n   114→    return new Response(JSON.stringify({\n   115→      access_token: jwtToken,\n   116→      user: { id: row.user_id, email: row.email, name: row.name, avatar_url: row.avatar_url },\n   117→    }), { status: 200, headers })\n   118→  }\n   119→\n   120→  // POST ?action=forgot-password — send password reset email\n   121→  if (action === 'forgot-password') {\n   122→    const body = await req.json() as { email?: string }\n   123→    const { email } = body\n   124→    if (!email) {\n   125→      return new Response(JSON.stringify({ error: 'Email is required' }), { status: 400, he