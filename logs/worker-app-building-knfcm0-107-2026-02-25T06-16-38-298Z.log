ON.stringify(data),\n    52→    });\n    53→    if (!res.ok) throw new Error(\"Failed to create report\");\n    54→    return (await res.json()) as Report;\n    55→  }\n    56→);\n    57→\n    58→export const deleteReport = createAsyncThunk(\n    59→  \"reports/deleteReport\",\n    60→  async (id: string) => {\n    61→    const res = await fetch(`/.netlify/functions/reports/${id}`, {\n    62→      method: \"DELETE\",\n    63→    });\n    64→    if (!res.ok) throw new Error(\"Failed to delete report\");\n    65→    return id;\n    66→  }\n    67→);\n    68→\n    69→const reportsSlice = createSlice({\n    70→  name: \"reports\",\n    71→  initialState,\n    72→  reducers: {\n    73→    clearCurrentReport(state) {\n    74→      state.currentReport = null;\n    75→      state.currentReportItems = [];\n    76→    },\n    77→  },\n    78→  extraReducers: (builder) => {\n    79→    builder\n    80→      .addCase(fetchReports.pending, (state) => {\n    81→        if (state.items.length === 0) state.loading = true;\n    82→        state.error = null;\n    83→      })\n    84→      .addCase(fetchReports.fulfilled, (state, action) => {\n    85→        state.items = action.payload;\n    86→        state.loading = false;\n    87→      })\n    88→      .addCase(fetchReports.rejected, (state, action) => {\n    89→        state.loading = false;\n    90→        state.error = action.error.message ?? \"Failed to fetch reports\";\n    91→      })\n    92→      .addCase(fetchReportById.pending, (state) => {\n    93→        if (!state.currentReport) state.loading = true;\n    94→        state.error = null;\n    95→      })\n    96→      .addCase(fetchReportById.fulfilled, (state, action) => {\n    97→        state.currentReport = action.payload.report;\n    98→        state.currentReportItems = action.payload.items;\n    99→        state.loading = false;\n   100→      })\n   101→      .addCase(fetchReportById.rejected, (state, action) => {\n   102→        state.loading = false;\n   103→        state.error = action.error.message ?? \"Failed to fetch report\";\n   104→      })\n   105→      .addCase(createReport.fulfilled, (state, action) => {\n   106→        state.items.unshift(action.payload);\n   107→      })\n   108→      .addCase(deleteReport.fulfilled, (state, action) => {\n   109→        state.items = state.items.filter((r) => r.id !== action.payload);\n   110→      });\n   111→  },\n   112→});\n   113→\n   114→export const { clearCurrentReport } = reportsSlice.actions;\n   115→export const reportsReducer = reportsSlice.reducer;\n   116→"}]},"parent_tool_use_id":"toolu_015hKqMxgaYDn8wd2pJSRC6W","session_id":"18f1644b-6690-4259-9ae0-df6e454090df","uuid":"cbefee9d-5d93-4654-ac93-c7abae039042"}
