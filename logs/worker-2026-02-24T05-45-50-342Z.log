-status-inactive: #6b7280;\n  --color-status-inactive-bg: #f3f4f6;\n  --color-status-prospect: #f59e0b;\n  --color-status-prospect-bg: #fffbeb;\n  --color-status-churned: #ef4444;\n  --color-status-churned-bg: #fef2f2;\n\n  /* Colors - Priority badges */\n  --color-priority-high: #ef4444;\n  --color-priority-high-bg: #fef2f2;\n  --color-priority-medium: #f59e0b;\n  --color-priority-medium-bg: #fffbeb;\n  --color-priority-low: #22c55e;\n  --color-priority-low-bg: #f0fdf4;\n  --color-priority-normal: #6b7280;\n  --color-priority-normal-bg: #f3f4f6;\n\n  /* Colors - Deal status */\n  --color-deal-on-track: #22c55e;\n  --color-deal-on-track-bg: #f0fdf4;\n  --color-deal-needs-attention: #f59e0b;\n  --color-deal-needs-attention-bg: #fffbeb;\n  --color-deal-at-risk: #ef4444;\n  --color-deal-at-risk-bg: #fef2f2;\n  --color-deal-won: #7180ff;\n  --color-deal-won-bg: #eef0ff;\n\n  /* Spacing */\n  --sidebar-width: 244px;\n\n  /* Border radius */\n  --radius-sm: 4px;\n  --radius-md: 6px;\n  --radius-lg: 8px;\n\n  /* Shadows */\n  --shadow-sm: 0px 4px 12px rgba(0, 0, 0, 0.1);\n  --shadow-md: 0px 4px 12px rgba(0, 0, 0, 0.1), 0px 0px 0px 1px rgba(0, 0, 0, 0.2);\n}\n\n* {\n  box-sizing: border-box;\n  margin: 0;\n  padding: 0;\n}\n\nbody {\n  font-family: var(--font-sans);\n  font-size: 13px;\n  color: var(--color-text-primary);\n  background: var(--color-bg-base);\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n}\n\n/* Scrollbar styling */\n::-webkit-scrollbar {\n  width: 8px;\n  height: 8px;\n}\n::-webkit-scrollbar-track {\n  background: transparent;\n}\n::-webkit-scrollbar-thumb {\n  background: var(--color-border-default);\n  border-radius: 4px;\n}\n::-webkit-scrollbar-thumb:hover {\n  background: var(--color-text-disabled);\n}\n\n/* ===== Layout ===== */\n.app-layout {\n  display: flex;\n  height: 100vh;\n  overflow: hidden;\n}\n\n.app-main {\n  flex: 1;\n  overflow-y: auto;\n  background: var(--color-bg-base);\n}\n\n.page-content {\n  max-width: 1400px;\n}\n\n.page-header {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  margin-bottom: 20px;\n  gap: 16px;\n}\n\n.page-title {\n  font-size: 20px;\n  font-weight: 600;\n  color: var(--color-text-primary);\n}\n\n/* ===== Search & Filters ===== */\n.clients-filters {\n  display: flex;\n  flex-direction: column;\n  gap: 12px;\n}\n\n.clients-search-wrapper {\n  position: relative;\n  display: flex;\n  align-items: center;\n}\n\n.clients-search-icon {\n  position: absolute;\n  left: 10px;\n  color: var(--color-text-muted);\n  pointer-events: none;\n}\n\n.clients-search-input {\n  width: 100%;\n  height: 36px;\n  padding: 0 32px 0 34px;\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-sm);\n  background: var(--color-bg-base);\n  font-size: 13px;\n  color: var(--color-text-primary);\n  outline: none;\n  transition: border-color 0.15s ease;\n}\n\n.clients-search-input::placeholder {\n  color: var(--color-text-disabled);\n}\n\n.clients-search-input:focus {\n  border-color: var(--color-accent);\n}\n\n.clients-search-clear {\n  position: absolute;\n  right: 8px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 20px;\n  height: 20px;\n  border: none;\n  border-radius: 50%;\n  background: var(--color-bg-hover);\n  color: var(--color-text-muted);\n  cursor: pointer;\n}\n\n.clients-filter-row {\n  display: flex;\n  gap: 8px;\n  flex-wrap: wrap;\n}\n\n.filter-dropdown-wrapper {\n  position: relative;\n}\n\n.filter-trigger {\n  display: inline-flex;\n  align-items: center;\n  gap: 4px;\n  height: 28px;\n  padding: 0 8px;\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-sm);\n  background: var(--color-bg-base);\n  color: var(--color-text-secondary);\n  font-size: 12px;\n  cursor: pointer;\n  transition: background 0.1s ease;\n  white-space: nowrap;\n}\n\n.filter-trigger:hover {\n  background: var(--color-bg-hover);\n}\n\n.filter-dropdown-menu {\n  position: absolute;\n  top: calc(100% + 4px);\n  left: 0;\n  min-width: 160px;\n  background: var(--color-bg-base);\n  border-radius: var(--radius-md);\n  box-shadow: var(--shadow-md);\n  padding: 4px;\n  z-index: 40;\n}\n\n.filter-dropdown-option {\n  display: block;\n  width: 100%;\n  padding: 6px 8px;\n  border: none;\n  border-radius: var(--radius-sm);\n  background: transparent;\n  color: var(--color-text-secondary);\n  font-size: 13px;\n  text-align: left;\n  cursor: pointer;\n  transition: background 0.1s ease;\n}\n\n.filter-dropdown-option:hover {\n  background: var(--color-bg-hover);\n}\n\n.filter-dropdown-option.selected {\n  background: var(--color-bg-selected);\n  color: var(--color-text-primary);\n  font-weight: 500;\n}\n\n.filter-section-label {\n  padding: 6px 8px 2px;\n  font-size: 11px;\n  font-weight: 600;\n  text-transform: uppercase;\n  color: var(--color-text-muted);\n  letter-spacing: 0.05em;\n}\n\n.filter-section-divider {\n  height: 1px;\n  background: var(--color-border-default);\n  margin: 4px 0;\n}\n\n.filter-date-range {\n  padding: 4px 8px;\n  display: flex;\n  flex-direction: column;\n  gap: 6px;\n}\n\n.filter-date-label {\n  display: flex;\n  align-items: center;\n  gap: 6px;\n  font-size: 12px;\n  color: var(--color-text-secondary);\n}\n\n.filter-date-input {\n  flex: 1;\n  padding: 4px 6px;\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-sm);\n  background: var(--color-bg-base);\n  color: var(--color-text-primary);\n  font-size: 12px;\n}\n\n/* ===== Actions ===== */\n.clients-actions {\n  display: flex;\n  align-items: center;\n  gap: 8px;\n}\n\n/* ===== Buttons ===== */\n.btn-ghost {\n  display: inline-flex;\n  align-items: center;\n  gap: 6px;\n  height: 28px;\n  padding: 0 8px;\n  border: 1px solid var(--color-border-default);\n  border-radius: 5px;\n  background: transparent;\n  color: var(--color-text-secondary);\n  font-size: 13px;\n  font-weight: 400;\n  cursor: pointer;\n  transition: background 0.1s ease;\n  white-space: nowrap;\n}\n\n.btn-ghost:hover {\n  background: var(--color-bg-hover);\n  color: var(--color-text-primary);\n}\n\n.btn-primary {\n  display: inline-flex;\n  align-items: center;\n  gap: 6px;\n  height: 28px;\n  padding: 0 12px;\n  border: none;\n  border-radius: 5px;\n  background: var(--color-accent);\n  color: white;\n  font-size: 13px;\n  font-weight: 500;\n  cursor: pointer;\n  transition: opacity 0.1s ease;\n  white-space: nowrap;\n}\n\n.btn-primary:hover {\n  opacity: 0.9;\n}\n\n.btn-primary:disabled {\n  opacity: 0.6;\n  cursor: default;\n}\n\n.btn-secondary {\n  display: inline-flex;\n  align-items: center;\n  gap: 6px;\n  height: 28px;\n  padding: 0 12px;\n  border: 1px solid var(--color-border-default);\n  border-radius: 5px;\n  background: var(--color-bg-base);\n  color: var(--color-text-secondary);\n  font-size: 13px;\n  cursor: pointer;\n  transition: background 0.1s ease;\n  white-space: nowrap;\n}\n\n.btn-secondary:hover {\n  background: var(--color-bg-hover);\n}\n\n.btn-danger {\n  display: inline-flex;\n  align-items: center;\n  gap: 6px;\n  height: 28px;\n  padding: 0 12px;\n  border: none;\n  border-radius: 5px;\n  background: var(--color-status-churned);\n  color: white;\n  font-size: 13px;\n  font-weight: 500;\n  cursor: pointer;\n  transition: opacity 0.1s ease;\n  white-space: nowrap;\n}\n\n.btn-danger:hover {\n  opacity: 0.9;\n}\n\n/* ===== Table ===== */\n.clients-table {\n  display: grid;\n  grid-template-columns: 1fr;\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-md);\n}\n\n.clients-table-header {\n  display: grid;\n  grid-template-columns: 2fr 1fr 1fr 1.5fr 1.5fr 1fr 1.5fr 44px;\n  align-items: center;\n  height: 36px;\n  padding: 0 12px;\n  background: var(--color-bg-sidebar);\n  border-bottom: 1px solid var(--color-border-default);\n  font-size: 12px;\n  font-weight: 500;\n  color: var(--color-text-muted);\n}\n\n.clients-table-row {\n  display: grid;\n  grid-template-columns: 2fr 1fr 1fr 1.5fr 1.5fr 1fr 1.5fr 44px;\n  align-items: center;\n  height: 44px;\n  padding: 0 12px;\n  border-bottom: 1px solid var(--color-border-light);\n  transition: background 0.1s ease;\n  cursor: default;\n}\n\n.clients-table-row:last-child {\n  border-bottom: none;\n}\n\n.clients-table-row:hover {\n  background: var(--color-bg-hover);\n}\n\n.table-cell {\n  padding: 0 8px;\n  overflow: hidden;\n  text-overflow: ellipsis;\n  white-space: nowrap;\n  font-size: 13px;\n  color: var(--color-text-secondary);\n}\n\n.table-cell-action {\n  padding: 0;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  position: relative;\n  z-index: 1;\n  overflow: visible;\n}\n\n.table-cell-tags {\n  display: flex;\n  gap: 4px;\n  overflow: hidden;\n}\n\n.table-cell-contact,\n.table-cell-task {\n  font-size: 12px;\n  color: var(--color-text-muted);\n}\n\n.table-cell-type {\n  font-size: 12px;\n}\n\n.client-name-link {\n  border: none;\n  background: none;\n  color: var(--color-text-primary);\n  font-size: 13px;\n  font-weight: 500;\n  cursor: pointer;\n  padding: 0;\n  text-decoration: none;\n  transition: color 0.1s ease;\n}\n\n.client-name-link:hover {\n  color: var(--color-accent);\n}\n\n/* ===== Status Badges ===== */\n.status-badge {\n  display: inline-flex;\n  align-items: center;\n  height: 20px;\n  padding: 0 8px;\n  border-radius: 10px;\n  font-size: 11px;\n  font-weight: 500;\n  white-space: nowrap;\n}\n\n.status-active {\n  color: var(--color-status-active);\n  background: var(--color-status-active-bg);\n}\n\n.status-inactive {\n  color: var(--color-status-inactive);\n  background: var(--color-status-inactive-bg);\n}\n\n.status-prospect {\n  color: var(--color-status-prospect);\n  background: var(--color-status-prospect-bg);\n}\n\n.status-churned {\n  color: var(--color-status-churned);\n  background: var(--color-status-churned-bg);\n}\n\n/* ===== Tag Chips ===== */\n.tag-chip {\n  display: inline-flex;\n  align-items: center;\n  height: 20px;\n  padding: 0 8px;\n  border-radius: 10px;\n  font-size: 11px;\n  color: var(--color-accent-muted);\n  background: #eef0ff;\n  white-space: nowrap;\n}\n\n/* ===== Row Actions ===== */\n.row-action-wrapper {\n  position: relative;\n}\n\n.row-action-trigger {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 28px;\n  height: 28px;\n  border: none;\n  border-radius: var(--radius-sm);\n  background: transparent;\n  color: var(--color-text-muted);\n  cursor: pointer;\n  transition: background 0.1s ease;\n}\n\n.row-action-trigger:hover {\n  background: var(--color-bg-hover);\n  color: var(--color-text-primary);\n}\n\n.row-action-menu {\n  position: absolute;\n  top: calc(100% + 2px);\n  right: 0;\n  min-width: 160px;\n  background: var(--color-bg-base);\n  border-radius: var(--radius-md);\n  box-shadow: var(--shadow-md);\n  padding: 4px;\n  z-index: 40;\n}\n\n.row-action-item {\n  display: flex;\n  align-items: center;\n  gap: 8px;\n  width: 100%;\n  padding: 6px 8px;\n  border: none;\n  border-radius: var(--radius-sm);\n  background: transparent;\n  color: var(--color-text-secondary);\n  font-size: 13px;\n  text-align: left;\n  cursor: pointer;\n  transition: background 0.1s ease;\n}\n\n.row-action-item:hover {\n  background: var(--color-bg-hover);\n}\n\n.row-action-item.danger {\n  color: var(--color-status-churned);\n}\n\n.row-action-item.danger:hover {\n  background: var(--color-status-churned-bg);\n}\n\n/* ===== Empty State ===== */\n.clients-table-empty {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: center;\n  padding: 48px 24px;\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-md);\n  text-align: center;\n}\n\n.empty-title {\n  font-size: 16px;\n  font-weight: 500;\n  color: var(--color-text-primary);\n  margin-bottom: 4px;\n}\n\n.empty-subtitle {\n  font-size: 13px;\n  color: var(--color-text-muted);\n}\n\n/* ===== Pagination ===== */\n.pagination {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  gap: 16px;\n  flex-wrap: wrap;\n}\n\n.pagination-info {\n  font-size: 13px;\n  color: var(--color-text-muted);\n}\n\n.pagination-controls {\n  display: flex;\n  align-items: center;\n  gap: 4px;\n}\n\n.pagination-btn {\n  display: inline-flex;\n  align-items: center;\n  gap: 4px;\n  height: 28px;\n  padding: 0 8px;\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-sm);\n  background: var(--color-bg-base);\n  color: var(--color-text-secondary);\n  font-size: 13px;\n  cursor: pointer;\n  transition: background 0.1s ease;\n}\n\n.pagination-btn:hover:not(:disabled) {\n  background: var(--color-bg-hover);\n}\n\n.pagination-btn:disabled {\n  opacity: 0.5;\n  cursor: default;\n}\n\n.pagination-pages {\n  display: flex;\n  align-items: center;\n  gap: 2px;\n}\n\n.pagination-page {\n  display: inline-flex;\n  align-items: center;\n  justify-content: center;\n  min-width: 28px;\n  height: 28px;\n  padding: 0 6px;\n  border: 1px solid transparent;\n  border-radius: var(--radius-sm);\n  background: transparent;\n  color: var(--color-text-secondary);\n  font-size: 13px;\n  cursor: pointer;\n  transition: background 0.1s ease;\n}\n\n.pagination-page:hover {\n  background: var(--color-bg-hover);\n}\n\n.pagination-page.active {\n  border-color: var(--color-accent);\n  color: var(--color-accent);\n  font-weight: 500;\n}\n\n.pagination-ellipsis {\n  display: inline-flex;\n  align-items: center;\n  justify-content: center;\n  width: 28px;\n  height: 28px;\n  color: var(--color-text-muted);\n  font-size: 13px;\n}\n\n/* ===== Modal ===== */\n.modal-overlay {\n  position: fixed;\n  inset: 0;\n  background: rgba(0, 0, 0, 0.4);\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  z-index: 100;\n}\n\n.modal-content {\n  background: var(--color-bg-base);\n  border-radius: var(--radius-lg);\n  box-shadow: var(--shadow-md);\n  width: 440px;\n  max-height: 90vh;\n  overflow-y: a