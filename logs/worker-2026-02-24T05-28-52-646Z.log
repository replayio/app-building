utton\n                      key={type}\n                      className={`form-dropdown-option ${relationshipType === type ? 'selected' : ''}`}\n                      onClick={() => { setRelationshipType(type); setShowTypeDropdown(false) }}\n                      data-testid=\"relationship-type-option\"\n                    >\n                      {type}\n                    </button>\n                  ))}\n                </div>\n              )}\n            </div>\n            {errors.type && <span className=\"form-error\" data-testid=\"relationship-type-error\">{errors.type}</span>}\n          </div>\n        </div>\n\n        <div className=\"modal-actions\">\n          <button className=\"btn-secondary\" onClick={onClose} data-testid=\"add-relationship-cancel\">Cancel</button>\n          <button className=\"btn-primary\" onClick={handleSubmit} disabled={saving} data-testid=\"add-relationship-save\">\n            {saving ? 'Saving...' : 'Save'}\n          </button>\n        </div>\n      </div>\n    </div>\n  )\n}\n","structuredPatch":[{"oldStart":366,"oldLines":6,"newStart":366,"newLines":7,"lines":["                   <svg width=\"14\" height=\"14\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" strokeWidth=\"2\" className=\"searchable-select-icon\">","                     <circle cx=\"11\" cy=\"11\" r=\"8\"/><line x1=\"21\" y1=\"21\" x2=\"16.65\" y2=\"16.65\"/>","                   </svg>","+                  {/* Test: Add relationship person search shows results after typing */}","                   <input","                     className=\"searchable-select-input\"","                     placeholder=\"Search individuals...\""]}],"userModified":false,"replaceAll":false}}
