yTestId('create-task-modal');\n    40→    await expect(modal).toBeVisible();\n    41→\n    42→    const taskTitle = `CompleteTest_${Date.now()}`;\n    43→    await page.getByTestId('create-task-title').fill(taskTitle);\n    44→    await page.getByTestId('create-task-save').click();\n    45→    await expect(modal).not.toBeVisible();\n    46→    await page.waitForLoadState('networkidle');\n    47→\n    48→    // Find and click the created task\n    49→    await expect(async () => {\n    50→      const card = page.locator(`[data-testid^=\"task-card-\"]`, { hasText: taskTitle });\n    51→      await expect(card).toBeVisible();\n    52→    }).toPass({ timeout: 10000 });\n    53→\n    54→    await page.locator(`[data-testid^=\"task-card-\"]`, { hasText: taskTitle }).click();\n    55→    await page.waitForLoadState('networkidle');\n    56→    await expect(page.getByTestId('task-detail-page')).toBeVisible();\n    57→\n    58→    // Mark complete\n    59→    await page.getByTestId('task-detail-mark-complete').click();\n    60→    await expect(page.getByTestId('confirm-dialog')).toBeVisible();\n    61→    await page.getByTestId('confirm-ok').click();\n    62→    await page.waitForLoadState('networkidle');\n    63→\n    64→    // Status should change to Completed\n    65→    await expect(page.getByTestId('task-detail-status')).toContainText('Completed');\n    66→\n    67→    // Completed date should be shown\n    68→    await expect(page.getByTestId('task-detail-completed-at')).toBeVisible();\n    69→\n    70→    // Mark Complete and Cancel Task buttons should be hidden\n    71→    await expect(page.getByTestId('task-detail-mark-complete')).not.toBeVisible();\n    72→    await expect(page.getByTestId('task-detail-mark-canceled')).not.toBeVisible();\n    73→  });\n    74→\n    75→  test('TDP-HDR-03: Cancel Task deletes task and redirects', async ({ page }) => {\n    76→    // Create a task specifically for this test\n    77→    await page.goto('/tasks');\n    78→    await page.