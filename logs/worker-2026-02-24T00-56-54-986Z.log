    97→            onChange={(e) => setName(e.target.value)}\n    98→            className=\"h-7 px-2 text-[13px] border border-[var(--color-bg-border)] rounded bg-white focus:outline-none focus:border-[var(--color-accent)]\"\n    99→          />\n   100→        )}\n   101→        <input\n   102→          data-testid=\"auth-email-input\"\n   103→          type=\"email\"\n   104→          placeholder=\"Email\"\n   105→          value={email}\n   106→          onChange={(e) => setEmail(e.target.value)}\n   107→          className=\"h-7 px-2 text-[13px] border border-[var(--color-bg-border)] rounded bg-white focus:outline-none focus:border-[var(--color-accent)]\"\n   108→          required\n   109→        />\n   110→        <input\n   111→          data-testid=\"auth-password-input\"\n   112→          type=\"password\"\n   113→          placeholder=\"Password\"\n   114→          value={password}\n   115→          onChange={(e) => setPassword(e.target.value)}\n   116→          className=\"h-7 px-2 text-[13px] border border-[var(--color-bg-border)] rounded bg-white focus:outline-none focus:border-[var(--color-accent)]\"\n   117→          required\n   118→        />\n   119→        <button\n   120→          data-testid=\"auth-submit-button\"\n   121→          type=\"submit\"\n   122→          disabled={loading}\n   123→          className=\"h-7 px-3 text-[13px] font-medium bg-[var(--color-accent)] text-white rounded hover:opacity-90 disabled:opacity-60\"\n   124→        >\n   125→          {loading ? '...' : isSignUp ? 'Sign Up' : 'Sign In'}\n   126→        </button>\n   127→        {error && (\n   128→          <p data-testid=\"auth-error\" className=\"text-[11px] text-red-500\">{error}</p>\n   129→        )}\n   130→        {message && (\n   131→          <p data-testid=\"auth-message\" className=\"text-[11px] text-[var(--color-text-muted)]\">{message}</p>\n   132→        )}\n   133→        <div className=\"flex items-center justify-between\">\n   134→          <button\