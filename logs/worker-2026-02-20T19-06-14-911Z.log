st) ? status : null\n    36→\n    37→    // Count total matching deals\n    38→    const countResult = await sql`\n    39→      SELECT COUNT(*) as count FROM deals d\n    40→      JOIN clients c ON d.client_id = c.id\n    41→      WHERE (${searchPattern}::text IS NULL OR (d.name ILIKE ${searchPattern} OR c.name ILIKE ${searchPattern}))\n    42→      AND (${stageFilter}::text IS NULL OR d.stage = ${stageFilter})\n    43→      AND (${clientFilter}::text IS NULL OR d.client_id = ${clientFilter}::uuid)\n    44→      AND (NOT ${statusIsActive}::boolean OR d.stage NOT IN ('closed_won', 'closed_lost'))\n    45→      AND (NOT ${statusIsWon}::boolean OR d.stage = 'closed_won')\n    46→      AND (NOT ${statusIsLost}::boolean OR d.stage = 'closed_lost')\n    47→      AND (${statusOther}::text IS NULL OR d.status = ${statusOther})\n    48→      AND (${dateFromFilter}::date IS NULL OR d.expected_close_date >= ${dateFromFilter}::date)\n    49→      AND (${dateToFilter}::date IS NULL OR d.expected_close_date <= ${dateToFilter}::date)\n    50→    `\n    51→    const total = parseInt(String(countResult[0].count), 10)\n    52→\n    53→    // Map sort parameter to safe sort key\n    54→    const sortMap: Record<string, string> = {\n    55→      'close_date_asc': 'close_date_asc',\n    56→      'close_date_desc': 'close_date_desc',\n    57→      'name_asc': 'name_asc',\n    58→      'name_desc': 'name_desc',\n    59→      'value_desc': 'value_desc',\n    60→      'value_asc': 'value_asc',\n    61→      'updated_at': 'updated_at',\n    62→    }\n    63→    const sortKey = sortMap[sort] ?? 'close_date_desc'\n    64→\n    65→    // Fetch deals with CASE-based ORDER BY to avoid dynamic SQL\n    66→    const deals = await sql`\n    67→      SELECT d.*, c.name as client_name FROM deals d\n    68→      JOIN clients c ON d.client_id = c.id\n    69→      WHERE (${searchPattern}::text IS NULL OR (d.name ILIKE ${searchPattern} OR c.name ILIKE ${searchPattern}))\n    70→      AND (${stageFilter}::text IS NULL OR d.stage = ${stageFilter})\n    71→      AND (${clientFilter}::text IS NULL OR d.client_id = ${clientFilter}::uuid)\n    72→      AND (NOT ${statusIsActive}::boolean OR d.stage NOT IN ('closed_won', 'closed_lost'))\n    73→      AND (NOT ${statusIsWon}::boolean OR d.stage = 'closed_won')\n    74→      AND (NOT ${statusIsLost}::boolean OR d.stage = 'closed_lost')\n    75→      AND (${statusOther}::text IS NULL OR d.status = ${statusOther})\n    76→      AND (${dateFromFilter}::date IS NULL OR d.expected_close_date >= ${dateFromFilter}::date)\n    77→      AND (${dateToFilter}::date IS NULL OR d.expected_close_date <= ${dateToFilter}::date)\n    78→      ORDER BY\n    79→        CASE WHEN ${sortKey} = 'close_date_asc' THEN d.expected_close_date END ASC NULLS LAST,\n    80→        CASE WHEN ${sortKey} = 'close_date_desc' THEN d.expected_close_date END DESC NULLS LAST,\n    81→        CASE WHEN ${sortKey} = 'name_asc' THEN d.name END ASC,\n    82→        CASE WHEN ${sortKey} = 'name_desc' THEN d.name END DESC,\n    83→        CASE WHEN ${sortKey} = 'value_desc' THEN d.value END DESC,\n    84→        CASE WHEN ${sortKey} = 'value_asc' THEN d.value END ASC,\n    85→        CASE WHEN ${sortKey} = 'updated_at' THEN d.updated_at END DESC,\n    86→        d.expected_close_date DESC NULLS LAST\n    87→      LIMIT ${pageSize} OFFSET ${offset}\n    88→    `\n    89→\n    90→    // Get summary metrics\n    91→    const metrics = await sql`\n    92→      SELECT\n    93→        COUNT(*) FILTER (WHERE status = 'active') as total_active,\n    94→        COALESCE(SUM(value) FILTER (WHERE status = 'active'), 0) as pipeline_value,\n    95→        COUNT(*) FILTER (WHERE stage = 'closed_won' AND updated_at >= date_trunc('quarter', CURRENT_DATE)) as won_count,\n    96→        COALESCE(SUM(value) FILTER (WHERE stage = 'closed_won' AND updated_at >= date_trunc('quarter', CURRENT_DATE)), 0) as won_value,\n    97→        COUNT(*) FILTER (WHERE stage = 'closed_lost' AND updated_at >= date_trunc('quarter', CURRENT_DATE)) as lost_count,\n    98→        COALESCE(SUM(value) FILTER (WHERE stage = 'closed_lost' AND updated_at >= date_trunc('quarter', CURRENT_DATE)), 0) as lost_value\n    99→      FROM deals\n   100→    `\n   101→\n   102→    // Get unique clients for filter dropdown\n   103→    const clients = await sql`\n   104→      SELECT DISTINCT c.id, c.name FROM clients c\n   105→      JOIN deals d ON d.client_id = c.id\n   106→      ORDER BY c.name ASC\n   107→    `\n   108→\n   109→    return Response.json({\n   110→      deals,\n   111→      total,\n   112→      metrics: metrics[0],\n   113→      clients,\n   114→    })\n   115→  }\n   116→\n   117→  // GET /deals/:dealId — get single deal with full details\n   118→  if (req.method === 'GET' && dealId) {\n   119→    const rows = await sql`\n   120→      SELECT d.*, c.name as client_name FROM deals d\n   121→      JOIN clients c ON d.client_id = c.id\n   122→      WHERE d.id = ${dealId}::uuid\n   123→    `\n   124→    if (rows.length === 0) {\n   125→      return new Response(JSON.stringify({ error: 'Deal not found' }), { status: 404 })\n   126→    }\n   127→    return Response.json(rows[0])\n   128→  }\n   129→\n   130→  // POST /deals?action=import (bulk CSV import)\n   131→  if (req.method === 'POST' && !dealId && url.searchParams.get('action') === 'import') {\n   132→    const body = await req.json() as {\n   133→      deals: Array<{\n   134→        name: string\n   135→        client_name?: string\n   136→        value?: string\n   137→        stage?: string\n   138→        owner?: string\n   139→        probability?: string\n   140→        expected_close_date?: string\n   141→        status?: string\n   142→      }>\n   143→    }\n   144→\n   145→    const results = { imported: 0, errors: [] as string[] }\n   146→    const validStages = ['lead', 'qualification', 'discovery', 'proposal', 'negotiation', 'closed_won', 'closed_lost']\n   147→    const validStatuses = ['active', 'on_track', 'at_risk', 'stalled']\n   148→\n   149→    for (let i = 0; i < body.deals.length; i++) {\n   150→      const row = body.deals[i]\n   151→      if (!row.name?.trim()) {\n   152→        results.errors.push(`Row ${i + 1}: Name is required`)\n   153→        continue\n   154→      }\n   155→      if (!row.client_name?.trim()) {\n   156→        results.errors.push(`Row ${i + 1}: Client Name is required`)\n   157→        continue\n   158→      }\n   159→      // Look up client by name\n   160→      const clientRows = await sql`SELECT id FROM clients WHERE LOWER(name) = LOWER(${row.client_name.trim()}) LIMIT 1`\n   161→      if (clientRows.length === 0) {\n   162→        results.errors.push(`Row ${i + 1}: Client \"${row.client_name.trim()}\" not found`)\n   163→        continue\n   164→      }\n   165→      const clientId = clientRows[0].id\n   166→      const stage = row.stage?.trim().toLowerCase() || 'lead'\n   167→      if (!validStages.includes(stage)) {\n   168→        results.errors.push(`Row ${i + 1}: Stage must be one of: ${validStages.join(', ')}`)\n   169→        continue\n   170→      }\n   171→      const status = row.status?.trim().toLowerCase() || 'active'\n   172→      if (!validStatuses.includes(status)) {\n   173→        results.errors.push(`Row ${i + 1}: Status must be one of: ${validStatuses.join(', ')}`)\n   174→        continue\n   175→      }\n   176→      const value = row.value ? parseFloat(row.value) : 0\n   177→      if (isNaN(value)) {\n   178→        results.errors.push(`Row ${i + 1}: Value must be a number`)\n   179→        continue\n   180→      }\n   181→      const probability = row.probability ? parseInt(row.probability) : 0\n   182→      if (isNaN(probability)) {\n   183→        results.errors.push(`Row ${i + 1}: Probability must be a number`)\n   184→        continue\n   185→      }\n   186→      try {\n   187→        await sql`\n   188→          INSERT INTO deals (name, client_id, value, stage, owner, probability, expected_close_date,