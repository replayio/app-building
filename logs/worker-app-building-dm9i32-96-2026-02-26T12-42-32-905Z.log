it sql`\n    48→          SELECT te.*, a.name as account_name\n    49→          FROM transaction_entries te\n    50→          LEFT JOIN accounts a ON a.id = te.account_id\n    51→          WHERE te.transaction_id = ${txn.id}\n    52→          ORDER BY te.created_at\n    53→        `;\n    54→        const tagRows = await sql`\n    55→          SELECT t.id, t.name\n    56→          FROM tags t\n    57→          JOIN transaction_tags tt ON tt.tag_id = t.id\n    58→          WHERE tt.transaction_id = ${txn.id}\n    59→          ORDER BY t.name\n    60→        `;\n    61→        result.push({\n    62→          ...txn,\n    63→          entries: entries.map((e) => ({\n    64→            ...e,\n    65→            amount: parseAmount(e.amount),\n    66→          })),\n    67→          tags: tagRows,\n    68→        });\n    69→      }\n    70→\n    71→      return new Response(JSON.stringify(result), {\n    72→        headers: { \"Content-Type\": \"application/json\" },\n    73→      });\n    74→    }\n    75→\n    76→    // GET /transactions/:id — get single transaction with entries and tags\n    77→    if (req.method === \"GET\" && id) {\n    78→      const rows = await sql`SELECT * FROM transactions WHERE id = ${id}`;\n    79→      if (rows.length === 0) {\n    80→        return new Response(JSON.stringify({ error: \"Not found\" }), {\n    81→          status: 404,\n    82→        });\n    83→      }\n    84→      const txn = rows[0];\n    85→      const entries = await sql`\n    86→        SELECT te.*, a.name as account_name\n    87→        FROM transaction_entries te\n    88→        LEFT JOIN accounts a ON a.id = te.account_id\n    89→        WHERE te.transaction_id = ${id}\n    90→        ORDER BY te.created_at\n    91→      `;\n    92→      const tagRows = await sql`\n    93→        SELECT t.id, t.name\n    94→        FROM tags t\n    95→        JOIN transaction_tags tt ON tt.tag_id = t.id\n    96→        WHERE tt.transaction_id = ${id}\n    97→        ORDER BY t.na