hment; filename=\"tasks.csv\"',\n    40→        },\n    41→      })\n    42→    }\n    43→\n    44→    // Single task by ID\n    45→    if (resourceId) {\n    46→      const rows = await sql`\n    47→        SELECT t.*, c.name as client_name, d.name as deal_name,\n    48→               u.name as assignee, u.avatar_url as assignee_avatar, u.role as assignee_role\n    49→        FROM tasks t\n    50→        LEFT JOIN clients c ON t.client_id = c.id\n    51→        LEFT JOIN deals d ON t.deal_id = d.id\n    52→        LEFT JOIN users u ON t.assignee_id = u.id\n    53→        WHERE t.id = ${resourceId}\n    54→      `\n    55→      if (rows.length === 0) {\n    56→        return Response.json({ error: 'Task not found' }, { status: 404 })\n    57→      }\n    58→      return Response.json(rows[0])\n    59→    }\n    60→\n    61→    // List tasks with filters\n    62→    const search = url.searchParams.get('search') || ''\n    63→    const priority = url.searchParams.get('priority') || ''\n    64→    const status = url.searchParams.get('status') || ''\n    65→    const sortBy = url.searchParams.get('sortBy') || 'due_date'\n    66→    const sortDir = url.searchParams.get('sortDir') || 'asc'\n    67→    const page = Math.max(1, parseInt(url.searchParams.get('page') || '1', 10))\n    68→    const offset = (page - 1) * ITEMS_PER_PAGE\n    69→\n    70→    const allowedSortColumns = ['due_date', 'title', 'priority', 'created_at']\n    71→    const safeSortBy = allowedSortColumns.includes(sortBy) ? sortBy : 'due_date'\n    72→    const safeSortDir = sortDir === 'desc' ? 'desc' : 'asc'\n    73→\n    74→    const countResult = await sql`\n    75→      SELECT COUNT(*) as count\n    76→      FROM tasks t\n    77→      LEFT JOIN clients c ON t.client_id = c.id\n    78→      WHERE (${search} = '' OR t.title ILIKE ${'%' + search + '%'})\n    79→        AND (${priority} = '' OR t.priority = ${priority})\n    80→        AND (${status} = '' OR t.status = ${status})\n    81→    `\n    82→ 