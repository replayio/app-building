→  if (startYear === endYear && start.getMonth() === end.getMonth()) {\n    48→    return `${startMonth} ${startYear}`;\n    49→  }\n    50→  if (startYear === endYear) {\n    51→    return `${startMonth}\\u2013${endMonth} ${startYear}`;\n    52→  }\n    53→  return `${startMonth} ${startYear}\\u2013${endMonth} ${endYear}`;\n    54→}\n    55→\n    56→function formatVariance(value: number): string {\n    57→  if (value >= 0) return `$+${formatCurrency(value).replace(\"$\", \"\")}`;\n    58→  return `$${formatCurrency(value).replace(\"$\", \"\")}`;\n    59→}\n    60→\n    61→function formatVariancePct(value: number): string {\n    62→  if (value >= 0) return `+${value.toFixed(1)}%`;\n    63→  return `${value.toFixed(1)}%`;\n    64→}\n    65→\n    66→function RefreshIcon(): React.ReactElement {\n    67→  return (\n    68→    <svg width=\"14\" height=\"14\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" strokeWidth=\"2\" strokeLinecap=\"round\" strokeLinejoin=\"round\">\n    69→      <path d=\"M21 2v6h-6\" />\n    70→      <path d=\"M3 12a9 9 0 0 1 15-6.7L21 8\" />\n    71→      <path d=\"M3 22v-6h6\" />\n    72→      <path d=\"M21 12a9 9 0 0 1-15 6.7L3 16\" />\n    73→    </svg>\n    74→  );\n    75→}\n    76→\n    77→function ChevronRightIcon(): React.ReactElement {\n    78→  return (\n    79→    <svg width=\"12\" height=\"12\" viewBox=\"0 0 12 12\" fill=\"none\" stroke=\"currentColor\" strokeWidth=\"1.5\" strokeLinecap=\"round\" strokeLinejoin=\"round\">\n    80→      <path d=\"M4.5 2.5L7.5 6L4.5 9.5\" />\n    81→    </svg>\n    82→  );\n    83→}\n    84→\n    85→function buildPreviewData(\n    86→  accounts: { id: string; name: string; category: AccountCategory; balance: number; budget_amount: number; budget_actual: number }[],\n    87→  selectedCategories: AccountCategory[],\n    88→  includeZeroBalance: boolean\n    89→): PreviewRow[] {\n    90→  const filtered = accounts.filter((a) => {\n    91→    if (!selectedCategories.includes(a.category)) return false;\n    92→    if (!includeZeroBalance && a.balance === 0 && a.budget_amount === 0 && a.budget_actual === 0) return false;\n    93→    return true;\n    94→  });\n    95→\n    96→  // Build summary rows\n    97→  const categoryTotals: Record<string, { budget: number; actual: number }> = {};\n    98→  for (const cat of selectedCategories) {\n    99→    categoryTotals[cat] = { budget: 0, actual: 0 };\n   100→  }\n   101→\n   102→  const accountsByCategory: Record<string, typeof filtered> = {};\n   103→  for (const a of filtered) {\n   104→    if (!accountsByCategory[a.category]) accountsByCategory[a.category] = [];\n   105→    accountsByCategory[a.category].push(a);\n   106→    categoryTotals[a.category].budget += a.budget_amount;\n   107→    categoryTotals[a.category].actual += a.budget_actual;\n   108→  }\n   109→\n   110→  const rows: PreviewRow[] = [];\n   111→\n   112→  // Revenue summary\n   113→  const revenueCats = selectedCategories.filter((c) => c === \"revenue\");\n   114→  let totalRevenueBudget = 0;\n   115→  let totalRevenueActual = 0;\n   116→  for (const cat of revenueCats) {\n   117→    totalRevenueBudget += categoryTotals[cat]?.budget ?? 0;\n   118→    totalRevenueActual += categoryTotals[cat]?.actual ?? 0;\n   119→  }\n   120→  if (revenueCats.length > 0) {\n   121→    const variance = totalRevenueActual - totalRevenueBudget;\n   122→    const pct = totalRevenueBudget !== 0 ? (variance / totalRevenueBudget) * 100 : 0;\n   123→    rows.push({\n   124→      id: \"total-revenue\",\n   125→      name: \"Total Revenue\",\n   126→      isSummary: true,\n   127→      isExpandable: false,\n   128→      budget: totalRevenueBudget,\n   129→      actual: totalRevenueActual,\n   130→      variance,\n   131→      variancePct: pct,\n   132→    });\n   133→  }\n   134→\n   135→  // Expenses summary\n   136→  const expenseCats = selectedCategories.filter((c) => c === \"expenses\");\n   137→  let totalExpensesBudget = 0;\n   138→  let totalExpensesActual = 0;\n