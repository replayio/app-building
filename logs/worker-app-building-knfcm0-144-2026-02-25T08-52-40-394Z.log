[2026-02-25T08:46:32.384Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01HRDjeJ5QDhxTY33kB6tZGY","type":"tool_result","content":"     1→import { getDb, query, queryOne, jsonResponse, errorResponse } from \"@shared/backend/db\";\n     2→\n     3→function base64url(str: string): string {\n     4→  return btoa(str).replace(/\\+/g, \"-\").replace(/\\//g, \"_\").replace(/=+$/, \"\");\n     5→}\n     6→\n     7→function createJwt(payload: Record<string, unknown>): string {\n     8→  const header = base64url(JSON.stringify({ alg: \"none\", typ: \"JWT\" }));\n     9→  const body = base64url(JSON.stringify(payload));\n    10→  return `${header}.${body}.`;\n    11→}\n    12→\n    13→async function hashPassword(password: string): Promise<string> {\n    14→  const encoder = new TextEncoder();\n    15→  const data = encoder.encode(password + \"sales-crm-salt\");\n    16→  const hash = await crypto.subtle.digest(\"SHA-256\", data);\n    17→  return Array.from(new Uint8Array(hash))\n    18→    .map((b) => b.toString(16).padStart(2, \"0\"))\n    19→    .join(\"\");\n    20→}\n    21→\n    22→export default async function handler(req: Request): Promise<Response> {\n    23→  if (req.method === \"OPTIONS\") {\n    24→    return new Response(null, {\n    25→      status: 204,\n    26→      headers: {\n    27→        \"Access-Control-Allow-Origin\": \"*\",\n    28→        \"Access-Control-Allow-Methods\": \"POST, OPTIONS\",\n    29→        \"Access-Control-Allow-Headers\": \"Content-Type, Authorization\",\n    30→      },\n    31→    });\n    32→  }\n    33→\n    34→  if (req.method !== \"POST\") {\n    35→    return errorResponse(405, \"Method not allowed\");\n    36→  }\n    37→\n    38→  const sql = getDb();\n    39→\n    40→  let body: { action?: string; email?: string; password?: string };\n    41→  try {\n    42→    body = await req.json();\n    43→  } catch {\n    44→    return errorResponse(400, \"Invalid JSON body\");\n    45→  }\n    46→\n    47→  const { action, email, password } = body;\n    48→\n    49→  if (!action || !email || !password) {\n    50→    return errorResponse(400, \"Missing required fields: action, email, password\");\n    51→  }\n    52→\n    53→  const isTest = process.env.IS_TEST === \"true\";\n    54→\n    55→  if (action === \"signup\") {\n    56→    const existing = await queryOne<{ id: string }>(sql, \"SELECT id FROM users WHERE email = $1\", [email]);\n    57→    if (existing) {\n    58→      return errorResponse(409, \"An account with this email already exists\");\n    59→    }\n    60→\n    61→    const passwordHash = await hashPassword(password);\n    62→    const name = email.split(\"@\")[0];\n    63→\n    64→    if (isTest) {\n    65→      const rows = await query<{ id: string; name: string; email: string; avatar_url: string | null }>(\n    66→        sql,\n    67→        \"INSERT INTO users (name, email, password_hash, email_confirmed) VALUES ($1, $2, $3, true) RETURNING id, name, email, avatar_url\",\n    68→        [name, email, passwordHash]\n    69→      );\n    70→      const newUser = rows[0];\n    71→      const token = createJwt({ sub: newUser.id, email: newUser.email, name: newUser.name });\n    72→      return jsonResponse({\n    73→        user: { id: newUser.id, name: newUser.name, email: newUser.email, avatarUrl: newUser.avatar_url },\n    74→        token,\n    75→      });\n    76→    }\n    77→\n    78→    const rows = await query<{ id: string; name: string; email: string }>(\n    79→      sql,\n    80→      \"INSERT INTO users (name, email, password_hash, email_confirmed) VALUES ($1, $2, $3, false) RETURNING id, name, email\",\n    81→      [name, email, passwordHash]\n    82→    );\n    83→    const newUser = rows[0];\n    84→\n    85→    const confirmToken = crypto.randomUUID();\n    86→    await query(\n    87→      sql,\n    88→      \"INSERT INTO email_tokens (user_id, token, type, expires_at) VALUES ($1, $2, 'confirmation', NOW() + INTERVAL '24 hours')\",\n    89→      [newUser.id, confirmToken]\n    90→    );\n    91→\n    92→    return jsonResponse({ requiresConfirmation: true, message: \"A confirmation email has been sent.\" });\n    93→  }\n    94→\n    95→  if (action === \"signin\") {\n    96→    const passwordHash = await hashPassword(password);\n    97→    const user = await queryOne<{ id: string; name: string; email: string; avatar_url: string | null; email_confirmed: boolean; password_hash: string }>(\n    98→      sql,\n    99→      \"SELECT id, name, email, avatar_url, email_confirmed, password_hash FROM users WHERE email = $1\",\n   100→      [email]\n   101→    );\n   102→\n   103→    if (!user || user.password_hash !== passwordHash) {\n   104→      return errorResponse(401, \"Invalid email or password\");\n   105→    }\n   106→\n   107→    if (!user.email_confirmed) {\n   108→      return errorResponse(403, \"Please confirm your email before signing in\");\n   109→    }\n   110→\n   111→    const token = createJwt({ sub: user.id, email: user.email, name: user.name });\n   112→    return jsonResponse({\n   113→      user: { id: user.id, name: user.name, email: user.email, avatarUrl: user.avatar_url },\n   114→      token,\n   115→    });\n   116→  }\n   117→\n   118→  return errorResponse(400, \"Invalid action. Use 'signin' or 'signup'.\");\n   119→}\n   120→"}]},"parent_tool_use_id":"toolu_015RqqmZctNDbmVnqmeFazVU","session_id":"ea0d70e7-d981-4aa5-8c55-c72e891cb543","uuid":"b3bc42b0-a79c-4114-af6c-b33367747573"}
