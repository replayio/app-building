 48→            onChange={(e) => setName(e.target.value)}\n    49→            data-testid=\"client-header-name-input\"\n    50→            className=\"text-[24px] font-semibold text-text-primary bg-base border border-border rounded-[5px] px-2 py-1 focus:outline-none focus:border-accent min-w-0 max-w-full\"\n    51→          />\n    52→          <button\n    53→            onClick={handleSave}\n    54→            data-testid=\"client-header-save-button\"\n    55→            className=\"inline-flex items-center justify-center w-7 h-7 rounded-[4px] text-status-active hover:bg-hover transition-colors duration-100\"\n    56→          >\n    57→            <Check size={16} strokeWidth={2} />\n    58→          </button>\n    59→          <button\n    60→            onClick={handleCancel}\n    61→            data-testid=\"client-header-cancel-button\"\n    62→            className=\"inline-flex items-center justify-center w-7 h-7 rounded-[4px] text-text-muted hover:bg-hover transition-colors duration-100\"\n    63→          >\n    64→            <X size={16} strokeWidth={2} />\n    65→          </button>\n    66→        </div>\n    67→        <div className=\"flex flex-wrap items-center gap-3 mb-3\">\n    68→          <div>\n    69→            <label className=\"block text-[12px] font-medium text-text-muted mb-1\">Type</label>\n    70→            <FilterSelect\n    71→              testId=\"client-header-type-select\"\n    72→              value={type}\n    73→              onChange={(v) => setType(v as ClientType)}\n    74→              options={[\n    75→                { value: 'organization', label: 'Organization' },\n    76→                { value: 'individual', label: 'Individual' },\n    77→              ]}\n    78→            />\n    79→          </div>\n    80→          <div>\n    81→            <label className=\"block text-[12px] font-medium text-text-muted mb-1\">Status</label>\n    82→            <FilterSelect\n    83→              testId=\"client-header-status