       fields.push(current.trim())\n    55→        current = ''\n    56→      } else {\n    57→        current += ch\n    58→      }\n    59→    }\n    60→  }\n    61→  fields.push(current.trim())\n    62→  return fields\n    63→}\n    64→\n    65→export default function ImportDialog({ open, onClose, entityName, columns, onImport }: ImportDialogProps) {\n    66→  const [file, setFile] = useState<File | null>(null)\n    67→  const [importing, setImporting] = useState(false)\n    68→  const [result, setResult] = useState<{ imported: number; errors: Array<{ row: number; error: string }> } | null>(null)\n    69→  const fileInputRef = useRef<HTMLInputElement>(null)\n    70→\n    71→  if (!open) return null\n    72→\n    73→  function handleDownloadTemplate() {\n    74→    const header = columns.map((c) => c.name).join(',')\n    75→    const blob = new Blob([header + '\\n'], { type: 'text/csv' })\n    76→    const url = URL.createObjectURL(blob)\n    77→    const a = document.createElement('a')\n    78→    a.href = url\n    79→    a.download = `${entityName.toLowerCase()}_template.csv`\n    80→    a.click()\n    81→    URL.revokeObjectURL(url)\n    82→  }\n    83→\n    84→  async function handleImport() {\n    85→    if (!file) return\n    86→    setImporting(true)\n    87→    try {\n    88→      const text = await file.text()\n    89→      const rows = parseCSV(text)\n    90→      const res = await onImport(rows)\n    91→      setResult(res)\n    92→    } catch {\n    93→      setResult({ imported: 0, errors: [{ row: 0, error: 'Failed to process CSV file' }] })\n    94→    } finally {\n    95→      setImporting(false)\n    96→    }\n    97→  }\n    98→\n    99→  function handleClose() {\n   100→    setFile(null)\n   101→    setResult(null)\n   102→    onClose()\n   103→  }\n   104→\n   105→  return (\n   106→    <div className=\"fixed inset-0 z-50 flex items-center justify-center\" data-testid=\"import-dialog\">\n   107→      <div className=\"absolute inset-0 bg-black/30