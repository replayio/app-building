[2026-02-25T01:47:03.794Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01WjaSfwmTFRjUDUAh9LUnf8","type":"tool_result","content":"     1→import { neon } from \"@neondatabase/serverless\";\n     2→import type { Context } from \"@netlify/functions\";\n     3→\n     4→export default async function handler(req: Request, _context: Context) {\n     5→  const dbUrl = process.env.DATABASE_URL;\n     6→  if (!dbUrl) {\n     7→    return new Response(JSON.stringify({ error: \"DATABASE_URL not set\" }), {\n     8→      status: 500,\n     9→      headers: { \"Content-Type\": \"application/json\" },\n    10→    });\n    11→  }\n    12→\n    13→  const sql = neon(dbUrl);\n    14→  const url = new URL(req.url);\n    15→  const segments = url.pathname.split(\"/\").filter(Boolean);\n    16→  // segments: [\"\", \".netlify\", \"functions\", \"equipment\", \"<id>?\"]\n    17→  const equipmentId = segments[3] || null;\n    18→\n    19→  if (req.method === \"GET\") {\n    20→    try {\n    21→      if (equipmentId) {\n    22→        const rows = await sql`\n    23→          SELECT id, name, description, available_units, status,\n    24→                 model, serial_number, location, manufacturer,\n    25→                 installation_date, photo_url, created_at, updated_at\n    26→          FROM equipment\n    27→          WHERE id = ${equipmentId}::uuid\n    28→          LIMIT 1\n    29→        `;\n    30→        if (rows.length === 0) {\n    31→          return new Response(JSON.stringify({ error: \"Equipment not found\" }), {\n    32→            status: 404,\n    33→            headers: { \"Content-Type\": \"application/json\" },\n    34→          });\n    35→        }\n    36→        return new Response(JSON.stringify(rows[0]), {\n    37→          status: 200,\n    38→          headers: { \"Content-Type\": \"application/json\" },\n    39→        });\n    40→      }\n    41→\n    42→      const rows = await sql`\n    43→        SELECT id, name, description, available_units, status,\n    44→               model, serial_number, location, manufacturer,\n    45→               installation_date, photo_url, created_at, updated_at\n    46→        FROM equipment\n    47→        ORDER BY name\n    48→      `;\n    49→      return new Response(JSON.stringify(rows), {\n    50→        status: 200,\n    51→        headers: { \"Content-Type\": \"application/json\" },\n    52→      });\n    53→    } catch (err) {\n    54→      console.error(\"Equipment GET error:\", err);\n    55→      return new Response(JSON.stringify({ error: \"Internal server error\" }), {\n    56→        status: 500,\n    57→        headers: { \"Content-Type\": \"application/json\" },\n    58→      });\n    59→    }\n    60→  }\n    61→\n    62→  if (req.method === \"POST\") {\n    63→    try {\n    64→      const body = await req.json();\n    65→      const {\n    66→        name,\n    67→        description,\n    68→        available_units,\n    69→        status,\n    70→        model,\n    71→        serial_number,\n    72→        location,\n    73→        manufacturer,\n    74→        installation_date,\n    75→        photo_url,\n    76→      } = body;\n    77→\n    78→      if (!name) {\n    79→        return new Response(JSON.stringify({ error: \"name is required\" }), {\n    80→          status: 400,\n    81→          headers: { \"Content-Type\": \"application/json\" },\n    82→        });\n    83→      }\n    84→\n    85→      const rows = await sql`\n    86→        INSERT INTO equipment (name, description, available_units, status,\n    87→          model, serial_number, location, manufacturer, installation_date, photo_url)\n    88→        VALUES (\n    89→          ${name},\n    90→          ${description || \"\"},\n    91→          ${available_units || 1},\n    92→          ${status || \"Operational\"},\n    93→          ${model || \"\"},\n    94→          ${serial_number || \"\"},\n    95→          ${location || \"\"},\n    96→          ${manufacturer || \"\"},\n    97→          ${installation_date || null},\n    98→          ${photo_url || \"\"}\n    99→        )\n   100→        RETURNING *\n   101→      `;\n   102→      return new Response(JSON.stringify(rows[0]), {\n   103→        status: 201,\n   104→        headers: { \"Content-Type\": \"application/json\" },\n   105→      });\n   106→    } catch (err) {\n   107→      console.error(\"Equipment POST error:\", err);\n   108→      return new Response(JSON.stringify({ error: \"Internal server error\" }), {\n   109→        status: 500,\n   110→        headers: { \"Content-Type\": \"application/json\" },\n   111→      });\n   112→    }\n   113→  }\n   114→\n   115→  if (req.method === \"PUT\") {\n   116→    if (!equipmentId) {\n   117→      return new Response(JSON.stringify({ error: \"Equipment ID required\" }), {\n   118→        status: 400,\n   119→        headers: { \"Content-Type\": \"application/json\" },\n   120→      });\n   121→    }\n   122→\n   123→    try {\n   124→      const body = await req.json();\n   125→      const {\n   126→        name,\n   127→        description,\n   128→        available_units,\n   129→        status,\n   130→        model,\n   131→        serial_number,\n   132→        location,\n   133→        manufacturer,\n   134→        installation_date,\n   135→        photo_url,\n   136→      } = body;\n   137→\n   138→      const rows = await sql`\n   139→        UPDATE equipment SET\n   140→          name = COALESCE(${name}, name),\n   141→          description = COALESCE(${description}, description),\n   142→          available_units = COALESCE(${available_units}, available_units),\n   143→          status = COALESCE(${status}, status),\n   144→          model = COALESCE(${model}, model),\n   145→          serial_number = COALESCE(${serial_number}, serial_number),\n   146→          location = COALESCE(${location}, location),\n   147→          manufacturer = COALESCE(${manufacturer}, manufacturer),\n   148→          installation_date = COALESCE(${installation_date || null}, installation_date),\n   149→          photo_url = COALESCE(${photo_url}, photo_url),\n   150→          updated_at = NOW()\n   151→        WHERE id = ${equipmentId}::uuid\n   152→        RETURNING *\n   153→      `;\n   154→      if (rows.length === 0) {\n   155→        return new Response(JSON.stringify({ error: \"Equipment not found\" }), {\n   156→          status: 404,\n   157→          headers: { \"Content-Type\": \"application/json\" },\n   158→        });\n   159→      }\n   160→      return new Response(JSON.stringify(rows[0]), {\n   161→        status: 200,\n   162→        headers: { \"Content-Type\": \"application/json\" },\n   163→      });\n   164→    } catch (err) {\n   165→      console.error(\"Equipment PUT error:\", err);\n   166→      return new Response(JSON.stringify({ error: \"Internal server error\" }), {\n   167→        status: 500,\n   168→        headers: { \"Content-Type\": \"application/json\" },\n   169→      });\n   170→    }\n   171→  }\n   172→\n   173→  if (req.method === \"DELETE\") {\n   174→    if (!equipmentId) {\n   175→      return new Response(JSON.stringify({ error: \"Equipment ID required\" }), {\n   176→        status: 400,\n   177→        headers: { \"Content-Type\": \"application/json\" },\n   178→      });\n   179→    }\n   180→\n   181→    try {\n   182→      await sql`DELETE FROM equipment WHERE id = ${equipmentId}::uuid`;\n   183→      return new Response(JSON.stringify({ success: true }), {\n   184→        status: 200,\n   185→        headers: { \"Content-Type\": \"application/json\" },\n   186→      });\n   187→    } catch (err) {\n   188→      console.error(\"Equipment DELETE error:\", err);\n   189→      return new Response(JSON.stringify({ error: \"Internal server error\" }), {\n   190→        status: 500,\n   191→        headers: { \"Content-Type\": \"application/json\" },\n   192→      });\n   193→    }\n   194→  }\n   195→\n   196→  return new Response(JSON.string