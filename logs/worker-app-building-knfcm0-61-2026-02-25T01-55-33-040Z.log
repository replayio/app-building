 try {\n    47→      const body = await req.json();\n    48→      const { equipment_id, author_name, author_role, text } = body;\n    49→\n    50→      if (!equipment_id || !text) {\n    51→        return new Response(\n    52→          JSON.stringify({ error: \"equipment_id and text are required\" }),\n    53→          { status: 400, headers: { \"Content-Type\": \"application/json\" } }\n    54→        );\n    55→      }\n    56→\n    57→      const rows = await sql`\n    58→        INSERT INTO equipment_notes (equipment_id, author_name, author_role, text)\n    59→        VALUES (\n    60→          ${equipment_id}::uuid,\n    61→          ${author_name || \"Admin\"},\n    62→          ${author_role || \"Operator\"},\n    63→          ${text}\n    64→        )\n    65→        RETURNING *\n    66→      `;\n    67→      return new Response(JSON.stringify(rows[0]), {\n    68→        status: 201,\n    69→        headers: { \"Content-Type\": \"application/json\" },\n    70→      });\n    71→    } catch (err) {\n    72→      console.error(\"Equipment notes POST error:\", err);\n    73→      return new Response(JSON.stringify({ error: \"Internal server error\" }), {\n    74→        status: 500,\n    75→        headers: { \"Content-Type\": \"application/json\" },\n    76→      });\n    77→    }\n    78→  }\n    79→\n    80→  if (req.method === \"DELETE\") {\n    81→    try {\n    82→      const segments = url.pathname.split(\"/\").filter(Boolean);\n    83→      const noteId = segments[3] || null;\n    84→\n    85→      if (!noteId) {\n    86→        return new Response(JSON.stringify({ error: \"Note ID required\" }), {\n    87→          status: 400,\n    88→          headers: { \"Content-Type\": \"application/json\" },\n    89→        });\n    90→      }\n    91→\n    92→      await sql`DELETE FROM equipment_notes WHERE id = ${noteId}::uuid`;\n    93→      return new Response(JSON.stringify({ success: true }), {\n    94→        status: 200,\n    95→        headers: { \"Content-Type\": \"application/json\" },\n    96→      });\n    97→    } catch (err) {\n    98→      console.error(\"Equipment notes DELETE error:\", err);\n    99→      return new Response(JSON.stringify({ error: \"Internal server error\" }), {\n   100→        status: 500,\n   101→        headers: { \"Content-Type\": \"application/json\" },\n   102→      });\n   103→    }\n   104→  }\n   105→\n   106→  return new Response(JSON.stringify({ error: \"Method not allowed\" }), {\n   107→    status: 405,\n   108→    headers: { \"Content-Type\": \"application/json\" },\n   109→  });\n   110→}\n   111→"}]},"parent_tool_use_id":"toolu_01XB9sCS1T8NL8XoNpLWjBFt","session_id":"99e80a81-e9e3-4c93-8a46-f50d02fdab7b","uuid":"e7390ff0-e2c5-4eaf-bbab-cc13d3a54568"}
