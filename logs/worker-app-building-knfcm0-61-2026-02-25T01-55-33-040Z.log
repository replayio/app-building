[2026-02-25T01:47:04.341Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_014kendQUJVG9t2AoRtcT4JB","type":"tool_result","content":"     1→import { createSlice, createAsyncThunk } from \"@reduxjs/toolkit\";\n     2→import type { Equipment, EquipmentNote } from \"../types\";\n     3→\n     4→interface EquipmentState {\n     5→  items: Equipment[];\n     6→  currentEquipment: Equipment | null;\n     7→  notes: EquipmentNote[];\n     8→  loading: boolean;\n     9→  error: string | null;\n    10→}\n    11→\n    12→const initialState: EquipmentState = {\n    13→  items: [],\n    14→  currentEquipment: null,\n    15→  notes: [],\n    16→  loading: false,\n    17→  error: null,\n    18→};\n    19→\n    20→export const fetchEquipment = createAsyncThunk(\n    21→  \"equipment/fetchEquipment\",\n    22→  async () => {\n    23→    const res = await fetch(\"/.netlify/functions/equipment\");\n    24→    if (!res.ok) throw new Error(\"Failed to fetch equipment\");\n    25→    return (await res.json()) as Equipment[];\n    26→  }\n    27→);\n    28→\n    29→export const fetchEquipmentById = createAsyncThunk(\n    30→  \"equipment/fetchEquipmentById\",\n    31→  async (id: string) => {\n    32→    const res = await fetch(`/.netlify/functions/equipment/${id}`);\n    33→    if (!res.ok) throw new Error(\"Failed to fetch equipment\");\n    34→    return (await res.json()) as Equipment;\n    35→  }\n    36→);\n    37→\n    38→export const createEquipment = createAsyncThunk(\n    39→  \"equipment/createEquipment\",\n    40→  async (data: Partial<Equipment>) => {\n    41→    const res = await fetch(\"/.netlify/functions/equipment\", {\n    42→      method: \"POST\",\n    43→      headers: { \"Content-Type\": \"application/json\" },\n    44→      body: JSON.stringify(data),\n    45→    });\n    46→    if (!res.ok) throw new Error(\"Failed to create equipment\");\n    47→    return (await res.json()) as Equipment;\n    48→  }\n    49→);\n    50→\n    51→export const updateEquipment = createAsyncThunk(\n    52→  \"equipment/updateEquipment\",\n    53→  async (data: Partial<Equipment> & { id: string }) => {\n    54→    const res = await fetch(`/.netlify/functions/equipment/${data.id}`, {\n    55→      method: \"PUT\",\n    56→      headers: { \"Content-Type\": \"application/json\" },\n    57→      body: JSON.stringify(data),\n    58→    });\n    59→    if (!res.ok) throw new Error(\"Failed to update equipment\");\n    60→    return (await res.json()) as Equipment;\n    61→  }\n    62→);\n    63→\n    64→export const deleteEquipment = createAsyncThunk(\n    65→  \"equipment/deleteEquipment\",\n    66→  async (id: string) => {\n    67→    const res = await fetch(`/.netlify/functions/equipment/${id}`, {\n    68→      method: \"DELETE\",\n    69→    });\n    70→    if (!res.ok) throw new Error(\"Failed to delete equipment\");\n    71→    return id;\n    72→  }\n    73→);\n    74→\n    75→export const fetchEquipmentNotes = createAsyncThunk(\n    76→  \"equipment/fetchEquipmentNotes\",\n    77→  async (equipmentId: string) => {\n    78→    const res = await fetch(\n    79→      `/.netlify/functions/equipment-notes?equipment_id=${equipmentId}`\n    80→    );\n    81→    if (!res.ok) throw new Error(\"Failed to fetch equipment notes\");\n    82→    return (await res.json()) as EquipmentNote[];\n    83→  }\n    84→);\n    85→\n    86→export const createEquipmentNote = createAsyncThunk(\n    87→  \"equipment/createEquipmentNote\",\n    88→  async (data: {\n    89→    equipment_id: string;\n    90→    author_name: string;\n    91→    author_role: string;\n    92→    text: string;\n    93→  }) => {\n    94→    const res = await fetch(\"/.netlify/functions/equipment-notes\", {\n    95→      method: \"POST\",\n    96→      headers: { \"Content-Type\": \"application/json\" },\n    97→      body: JSON.stringify(data),\n    98→    });\n    99→    if (!res.ok) throw new Error(\"Failed to create equipment note\");\n   100→    return (await res.json()) as EquipmentNote;\n   101→  }\n   102→);\n   103→\n   104→const equipmentSlice = createSlice({\n   105→  name: \"equipment\",\n   106→  initialState,\n   107→  reducers: {\n   108→    clearCurrentEquipment(state) {\n   109→      state.currentEquipment = null;\n   110→      state.notes = [];\n   111→    },\n   112→  },\n   113→  extraReducers: (builder) => {\n   114→    builder\n   115→      .addCase(fetchEquipment.pending, (state) => {\n   116→        if (state.items.length === 0) state.loading = true;\n   117→        state.error = null;\n   118→      })\n   119→      .addCase(fetchEquipment.fulfilled, (state, action) => {\n   120→        state.items = action.payload;\n   121→        state.loading = false;\n   122→      })\n   123→      .addCase(fetchEquipment.rejected, (state, action) => {\n   124→        state.loading = false;\n   125→        state.error = action.error.message ?? \"Failed to fetch equipment\";\n   126→      })\n   127→      .addCase(fetchEquipmentById.pending, (state) => {\n   128→        if (!state.currentEquipment) state.loading = true;\n   129→        state.error = null;\n   130→      })\n   131→      .addCase(fetchEquipmentById.fulfilled, (state, action) => {\n   132→        state.currentEquipment = action.payload;\n   133→        state.loading = false;\n   134→      })\n   135→      .addCase(fetchEquipmentById.rejected, (state, action) => {\n   136→        state.loading = false;\n   137→        state.error = action.error.message ?? \"Failed to fetch equipment\";\n   138→      })\n   139→      .addCase(createEquipment.fulfilled, (state, action) => {\n   140→        state.items.push(action.payload);\n   141→      })\n   142→      .addCase(updateEquipment.fulfilled, (state, action) => {\n   143→        const idx = state.items.findIndex((e) => e.id === action.payload.id);\n   144→        if (idx !== -1) state.items[idx] = action.payload;\n   145→        if (state.currentEquipment?.id === action.payload.id) {\n   146→          state.currentEquipment = action.payload;\n   147→        }\n   148→      })\n   149→      .addCase(deleteEquipment.fulfilled, (state, action) => {\n   150→        state.items = state.items.filter((e) => e.id !== action.payload);\n   151→      })\n   152→      .addCase(fetchEquipmentNotes.fulfilled, (state, action) => {\n   153→        state.notes = action.payload;\n   154→      })\n   155→      .addCase(createEquipmentNote.fulfilled, (state, action) => {\n   156→        state.notes.unshift(action.payload);\n   157→      });\n   158→  },\n   159→});\n   160→\n   161→export const { clearCurrentEquipment } = equipmentSlice.actions;\n   162→export const equipmentReducer = equipmentSlice.reducer;\n   163→"}]},"parent_tool_use_id":"toolu_01XB9sCS1T8NL8XoNpLWjBFt","session_id":"99e80a81-e9e3-4c93-8a46-f50d02fdab7b","uuid":"71937283-fbdc-4c70-8688-9891290f4996"}
