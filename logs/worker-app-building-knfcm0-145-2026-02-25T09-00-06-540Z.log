adius: 4px;\n  outline: none;\n  transition: border-color 0.15s ease;\n}\n\n.form-input:focus {\n  border-color: var(--accent-primary);\n  box-shadow: 0 0 0 2px rgba(77, 124, 94, 0.15);\n}\n\n.form-input::placeholder {\n  color: var(--text-disabled);\n}\n\n.form-select {\n  height: 36px;\n  padding: 0 10px;\n  font-size: 13px;\n  font-family: var(--font-family);\n  color: var(--text-primary);\n  background: var(--bg-base-color);\n  border: 1px solid var(--normal-border);\n  border-radius: 4px;\n  outline: none;\n  cursor: pointer;\n  appearance: none;\n  transition: border-color 0.15s ease;\n}\n\n/* Buttons */\n.btn {\n  display: inline-flex;\n  align-items: center;\n  justify-content: center;\n  gap: 6px;\n  height: 32px;\n  padding: 0 14px;\n  font-size: 13px;\n  font-weight: 400;\n  font-family: var(--font-family);\n  border-radius: 5px;\n  cursor: pointer;\n  transition: background-color 0.15s ease, color 0.15s ease, border-color 0.15s ease;\n  white-space: nowrap;\n  border: 1px solid transparent;\n}\n\n.btn--primary {\n  background: var(--accent-primary);\n  color: var(--text-on-dark);\n  border-color: transparent;\n}\n\n.btn--primary:hover {\n  background: var(--accent-dark);\n}\n\n.btn--secondary {\n  background: transparent;\n  color: var(--text-secondary);\n  border-color: var(--bg-border-color);\n}\n\n.btn--secondary:hover {\n  background: rgba(50, 45, 35, 0.06);\n  border-color: var(--normal-border-hover);\n}\n\n.btn--ghost {\n  background: transparent;\n  color: var(--text-secondary);\n  border-color: transparent;\n}\n\n.btn--ghost:hover {\n  background: rgba(50, 45, 35, 0.06);\n}\n\n.btn--danger {\n  background: transparent;\n  color: var(--status-error);\n  border-color: var(--status-error);\n}\n\n.btn--danger:hover {\n  background: rgba(176, 80, 80, 0.08);\n}\n\n.btn:disabled {\n  opacity: 0.5;\n  cursor: default;\n}\n\n/* Data table */\n.data-table {\n  width: 100%;\n  border-collapse: collapse;\n}\n\n.data-table th {\n  text-align: left;\n  font-size: 12px;\n  font-weight: 500;\n  color: var(--text-muted);\n  padding: 8px 12px;\n  border-bottom: 1px solid var(--bg-border-color);\n}\n\n.data-table td {\n  font-size: 13px;\n  color: var(--text-secondary);\n  padding: 10px 12px;\n  border-bottom: 1px solid var(--bg-border-color-light);\n  font-variant-numeric: tabular-nums;\n}\n\n.data-table tr:hover td {\n  background: rgba(50, 45, 35, 0.03);\n}\n\n/* Status badges */\n.badge {\n  display: inline-flex;\n  align-items: center;\n  height: 22px;\n  padding: 0 8px;\n  font-size: 11px;\n  font-weight: 500;\n  border-radius: 11px;\n}\n\n.badge--success {\n  background: #e6ede8;\n  color: var(--status-success);\n}\n\n.badge--warning {\n  background: #f5f0e3;\n  color: var(--status-warning);\n}\n\n.badge--error {\n  background: #f5e3e3;\n  color: var(--status-error);\n}\n\n.badge--info {\n  background: #e3ecf0;\n  color: var(--status-info);\n}\n\n/* Page header */\n.page-header {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  padding: 20px 24px 16px;\n}\n\n.page-title {\n  font-size: 22px;\n  font-weight: 600;\n  color: var(--text-primary);\n}\n\n/* Searchable select */\n.searchable-select {\n  position: relative;\n}\n\n.searchable-select-input {\n  width: 100%;\n  height: 32px;\n  padding: 0 8px;\n  font-size: 13px;\n  font-family: var(--font-family);\n  color: var(--text-primary);\n  background: var(--bg-base-color);\n  border: 1px solid var(--normal-border);\n  border-radius: 4px;\n  outline: none;\n}\n\n.searchable-select-input:focus {\n  border-color: var(--accent-primary);\n}\n\n.searchable-select-dropdown {\n  position: absolute;\n  top: 100%;\n  left: 0;\n  right: 0;\n  max-height: 180px;\n  overflow-y: auto;\n  background: #ffffff;\n  border: 1px solid var(--bg-border-color);\n  border-radius: 4px;\n  box-shadow: var(--shadow-elevation-2);\n  z-index: 200;\n}\n\n.searchable-select-option {\n  display: flex;\n  align-items: center;\n  width: 100%;\n  height: 32px;\n  padding: 0 10px;\n  font-size: 13px;\n  color: var(--text-secondary);\n  background: transparent;\n  border: none;\n  cursor: pointer;\n  text-align: left;\n}\n\n.searchable-select-option:hover {\n  background: rgba(50, 45, 35, 0.06);\n}\n\n/* Action menu dropdown */\n.action-menu {\n  position: absolute;\n  top: 100%;\n  right: 0;\n  min-width: 160px;\n  background: #ffffff;\n  border: 1px solid var(--bg-border-color);\n  border-radius: 6px;\n  box-shadow: var(--shadow-elevation-2);\n  z-index: 100;\n  padding: 4px;\n}\n\n.action-menu-item {\n  display: flex;\n  align-items: center;\n  width: 100%;\n  height: 32px;\n  padding: 0 12px;\n  font-size: 13px;\n  font-weight: 400;\n  color: var(--text-secondary);\n  background: transparent;\n  border: none;\n  border-radius: 4px;\n  cursor: pointer;\n  text-align: left;\n  transition: background-color 0.1s ease;\n}\n\n.action-menu-item:hover {\n  background: rgba(50, 45, 35, 0.06);\n}\n\n.action-menu-item--danger {\n  color: var(--status-error);\n}\n\n.action-menu-item--danger:hover {\n  background: rgba(176, 80, 80, 0.08);\n}\n\n/* Balance indicator (double-entry balance display) */\n.balance-indicator {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  padding: 12px 16px;\n  background: var(--bg-base-color);\n  border-radius: 4px;\n  margin-top: 12px;\n}\n\n.balance-totals {\n  display: flex;\n  gap: 24px;\n}\n\n.balance-total-item {\n  display: flex;\n  flex-direction: column;\n  gap: 2px;\n}\n\n.balance-total-label {\n  font-size: 12px;\n  color: var(--text-muted);\n}\n\n.balance-total-value {\n  font-size: 15px;\n  font-weight: 600;\n  color: var(--text-primary);\n  font-variant-numeric: tabular-nums;\n}\n\n.balance-status {\n  display: flex;\n  align-items: center;\n  gap: 6px;\n  font-size: 14px;\n  font-weight: 500;\n}\n\n.balance-status--balanced {\n  color: var(--status-success);\n}\n\n.balance-status--unbalanced {\n  color: var(--status-error);\n}\n\n/* Line items table */\n.line-items-table