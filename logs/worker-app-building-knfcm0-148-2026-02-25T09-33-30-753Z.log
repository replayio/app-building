    return [\n    36→      { value: \"\", label: \"Tags: All\" },\n    37→      ...sorted.map((t) => ({ value: t, label: t })),\n    38→    ];\n    39→  }, [clients]);\n    40→\n    41→  // Derive unique source options from data\n    42→  const sourceOptions = useMemo(() => {\n    43→    const sourceSet = new Set<string>();\n    44→    clients.forEach((c) => {\n    45→      if (c.sourceType) sourceSet.add(c.sourceType);\n    46→    });\n    47→    const sorted = Array.from(sourceSet).sort();\n    48→    return [\n    49→      { value: \"\", label: \"Source: All\" },\n    50→      ...sorted.map((s) => ({ value: s, label: s })),\n    51→    ];\n    52→  }, [clients]);\n    53→\n    54→  // Filter clients\n    55→  const filtered = useMemo(() => {\n    56→    let result = clients;\n    57→\n    58→    // Search filter\n    59→    if (search.trim()) {\n    60→      const q = search.toLowerCase();\n    61→      result = result.filter(\n    62→        (c) =>\n    63→          c.name.toLowerCase().includes(q) ||\n    64→          c.tags.some((t) => t.toLowerCase().includes(q)) ||\n    65→          (c.primaryContact && c.primaryContact.name.toLowerCase().includes(q))\n    66→      );\n    67→    }\n    68→\n    69→    // Status filter\n    70→    if (statusFilter) {\n    71→      result = result.filter((c) => c.status === statusFilter);\n    72→    }\n    73→\n    74→    // Tag filter\n    75→    if (tagFilter) {\n    76→      result = result.filter((c) => c.tags.includes(tagFilter));\n    77→    }\n    78→\n    79→    // Source filter\n    80→    if (sourceFilter) {\n    81→      result = result.filter((c) => c.sourceType === sourceFilter);\n    82→    }\n    83→\n    84→    // Sort\n    85→    result = [...result].sort((a, b) => {\n    86→      switch (sortBy) {\n    87→        case \"name_asc\":\n    88→          return a.name.localeCompare(b.name);\n    89→        case \"name_desc\":\n    90→          return b.name.localeCompare(a.name);\n    91→        case \"created_desc\":\n    92→          return new Date(b.createdAt).getTime() - new Date(a.createdAt).getTime();\n    93→        case \"created_asc\":\n    94→          return new Date(a.createdAt).getTime() - new Date(b.createdAt).getTime();\n    95→        case \"updated_desc\":\n    96→        default:\n    97→          return new Date(b.updatedAt).getTime() - new Date(a.updatedAt).getTime();\n    98→      }\n    99→    });\n   100→"}]},"parent_tool_use_id":"toolu_0162Yc72wAEupU4zi53cEyPR","session_id":"6ce9fdbd-ab27-4433-8cf3-fdd70c0c27a8","uuid":"e45968e9-3d01-4ba5-991e-0652327b8533"}
