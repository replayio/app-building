ed_at: string;\n   107→    }>(\n   108→      sql,\n   109→      `INSERT INTO deals (name, client_id, value, stage, owner_id, probability, expected_close_date)\n   110→       VALUES ($1, $2, $3, $4, $5, $6, $7)\n   111→       RETURNING *`,\n   112→      [\n   113→        body.name.trim(),\n   114→        body.clientId,\n   115→        body.value ?? null,\n   116→        body.stage || \"Lead\",\n   117→        body.ownerId || null,\n   118→        body.probability ?? null,\n   119→        body.expectedCloseDate || null,\n   120→      ]\n   121→    );\n   122→\n   123→    const d = created[0];\n   124→    const actor = user ? user.name : \"System\";\n   125→\n   126→    // Create timeline event\n   127→    await query(\n   128→      sql,\n   129→      `INSERT INTO timeline_events (client_id, event_type, description, related_entity_type, related_entity_id, created_by)\n   130→       VALUES ($1, $2, $3, $4, $5, $6)`,\n   131→      [body.clientId, \"Deal Created\", `Deal Created: '${body.name.trim()}'`, \"deal\", d.id, actor]\n   132→    );\n   133→\n   134→    // Create deal history entry\n   135→    await query(\n   136→      sql,\n   137→      `INSERT INTO deal_history (deal_id, new_stage, changed_by)\n   138→       VALUES ($1, $2, $3)`,\n   139→      [d.id, d.stage, actor]\n   140→    );\n   141→\n   142→    let ownerName: string | null = null;\n   143→    if (d.owner_id) {\n   144→      const ownerRow = await query<{ name: string }>(sql, \"SELECT name FROM users WHERE id = $1\", [d.owner_id]);\n   145→      ownerName = ownerRow[0]?.name || null;\n   146→    }\n   147→\n   148→    return jsonResponse(\n   149→      {\n   150→        id: d.id,\n   151→        name: d.name,\n   152→        clientId: d.client_id,\n   153→        value: d.value ? parseFloat(d.value) : null,\n   154→        stage: d.stage,\n   155→        ownerId: d.owner_id,\n   156→        ownerName,\n   157→        probability: d.probability,\n   158→        expectedCloseDate: d.expected_close_date,\n  