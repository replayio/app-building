tom: none;\n}\n\n.person-avatar {\n  width: 36px;\n  height: 36px;\n  border-radius: 50%;\n  background: var(--accent-subtle-bg);\n  color: var(--accent-primary);\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  font-size: 13px;\n  font-weight: 600;\n  flex-shrink: 0;\n}\n\n.person-info {\n  flex: 1;\n  min-width: 0;\n}\n\n.person-name {\n  font-size: 13px;\n  font-weight: 500;\n  color: var(--text-primary);\n  transition: color 0.1s ease;\n}\n\n.person-item:hover .person-name {\n  color: var(--accent-primary);\n}\n\n.person-role {\n  font-size: 12px;\n  color: var(--text-muted);\n  margin-top: 2px;\n}\n\n/* Client Detail Timeline */\n.client-timeline-event-links a {\n  color: var(--accent-primary);\n  text-decoration: none;\n  font-weight: 500;\n}\n\n.client-timeline-event-links a:hover {\n  text-decoration: underline;\n}\n\n/* Follow Button */\n.follow-btn {\n  display: inline-flex;\n  align-items: center;\n  gap: 6px;\n  height: 32px;\n  padding: 0 12px;\n  font-size: 13px;\n  font-weight: 400;\n  font-family: var(--font-family);\n  border-radius: 5px;\n  cursor: pointer;\n  transition: background-color 0.15s ease, color 0.15s ease, border-color 0.15s ease;\n  white-space: nowrap;\n  border: 1px solid var(--bg-border-color);\n  background: transparent;\n  color: var(--text-secondary);\n}\n\n.follow-btn:hover {\n  background: rgba(50, 45, 35, 0.06);\n  border-color: var(--normal-border-hover);\n}\n\n.follow-btn--following {\n  background: var(--accent-subtle-bg);\n  border-color: var(--accent-primary);\n  color: var(--accent-primary);\n}\n\n.follow-btn--following:hover {\n  background: var(--accent-subtle-bg);\n}\n\n.follow-btn:disabled {\n  opacity: 0.5;\n  cursor: default;\n}\n\n/* Edit modal for client header */\n.edit-form-tags {\n  display: flex;\n  flex-wrap: wrap;\n  gap: 6px;\n  margin-top: 4px;\n}\n\n.edit-form-tag {\n  display: inline-flex;\n  align-items: center;\n  gap: 4px;\n  height: 24px;\n  padding: 0 8px;\n  font-size: 12px;\n  background: var(--bg-base-color);\n  border: 1px solid var(--bg-border-color);\n  border-radius: 12px;\n  color: var(--text-secondary);\n}\n\n.edit-form-tag-remove {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 14px;\n  height: 14px;\n  background: transparent;\n  border: none;\n  border-radius: 50%;\n  cursor: pointer;\n  color: var(--text-muted);\n  font-size: 12px;\n  padding: 0;\n  line-height: 1;\n}\n\n.edit-form-tag-remove:hover {\n  color: var(--status-error);\n}\n\n.edit-form-tag-input {\n  height: 24px;\n  padding: 0 6px;\n  font-size: 12px;\n  border: 1px solid var(--normal-border);\n  border-radius: 12px;\n  background: transparent;\n  color: var(--text-primary);\n  outline: none;\n  width: 100px;\n}\n\n.edit-form-tag-input:focus {\n  border-color: var(--accent-primary);\n}\n\n/* Attachment modal toggle */\n.attachment-mode-toggle {\n  display: flex;\n  border: 1px solid var(--bg-border-color);\n  border-radius: 4px;\n  overflow: hidden;\n  margin-bottom: 16px;\n}\n\n.attachment-mode-btn {\n  flex: 1;\n  height: 32px;\n  font-size: 13px;\n  font-family: var(--font-family);\n  background: transparent;\n  border: none;\n  cursor: pointer;\n  color: var(--text-muted);\n  transition: background-color 0.15s ease, color 0.15s ease;\n}\n\n.attachment-mode-btn--active {\n  background: var(--accent-subtle-bg);\n  color: var(--accent-primary);\n  font-weight: 500;\n}\n\n.attachment-mode-btn:hover:not(.attachment-mode-btn--active) {\n  background: rgba(50, 45, 35, 0.04);\n}\n\n/* File upload area */\n.file-upload-area {\n  border: 2px dashed var(--bg-border-color);\n  border-radius: 6px;\n  padding: 24px;\n  text-align: center;\n  cursor: pointer;\n  transition: border-color 0.15s ease, background-color 0.15s ease;\n  margin-bottom: 16px;\n}\n\n.file-upload-area:hover {\n  border-color: var(--accent-muted);\n  background: var(--accent-subtle-bg);\n}\n\n.file-upload-label {\n  font-size: 13px;\n  color: var(--text-muted);\n}\n\n.file-upload-label strong {\n  color: var(--accent-primary);\n  cursor: pointer;\n}\n\n.file-upload-selected {\n  font-size: 13px;\n  color: var(--text-primary);\n  margin-top: 8px;\n}\n\n/* Two-column layout for sections */\n.client-detail-sections {\n  display: grid;\n  grid-template-columns: 1fr 1fr;\n  gap: 24px;\n}\n\n@media (max-width: 768px) {\n  .client-detail-sections {\n    grid-template-columns: 1fr;\n  }\n\n  .source-info-grid {\n    grid-template-columns: 1fr;\n  }\n}\n\n/* ========= Person Detail Page ========= */\n\n.person-detail {\n  display: flex;\n  flex-direction: column;\n  gap: 24px;\n}\n\n/* Person Header */\n.person-header {\n  display: flex;\n  flex-direction: column;\n  gap: 8px;\n}\n\n.person-header-name {\n  font-size: 22px;\n  font-weight: 600;\n  color: var(--text-primary);\n}\n\n.person-header-title {\n  font-size: 14px;\n  color: var(--text-muted);\n}\n\n.person-header-contact {\n  display: flex;\n  flex-direction: column;\n  gap: 6px;\n  margin-top: 4px;\n}\n\n.person-header-contact-row {\n  display: flex;\n  align-items: center;\n  gap: 8px;\n  font-size: 13px;\n  color: var(--text-secondary);\n}\n\n.person-header-contact-row svg {\n  flex-shrink: 0;\n  color: var(--text-muted);\n}\n\n.person-header-clients {\n  display: flex;\n  align-items: center;\n  gap: 6px;\n  font-size: 13px;\n  color: var(--text-secondary);\n  margin-top: 4px;\n  flex-wrap: wrap;\n}\n\n.person-header-clients-label {\n  font-weight: 500;\n  color: var(--text-muted);\n}\n\n.person-header-client-link {\n  background: none;\n  border: none;\n  color: var(--accent-primary);\n  cursor: pointer;\n  font-size: 13px;\n  font-family: var(--font-family);\n  padding: 0;\n  text-decoration: none;\n  transition: color 0.1s ease;\n}\n\n.person-header-client-link:hover {\n  color: var(--accent-dark);\n  text-decoration: underline;\n}\n\n.person-header-client-separator {\n  color: var(--text-muted);\n}\n\n/* Detail section layout helpers */\n.detail-section-header-left {\n  display: flex;\n  align-items: center;\n  gap: 8px;\n}\n\n.detail-section-header-actions {\n  display: flex;\n  align-items: center;\n  gap: 8px;\n}\n\n.section-icon {\n  flex-shrink: 0;\n  color: var(--text-muted);\n}\n\n.detail-section-filter {\n  padding: 8px 16px;\n  border-bottom: 1px solid var(--bg-border-color-light);\n}\n\n/* Button size variant */\n.btn--sm {\n  height: 28px;\n  padding: 0 10px;\n  font-size: 12px;\n}\n\n/* View tabs */\n.view-tabs {\n  display: flex;\n  border: 1px solid var(--bg-border-color);\n  border-radius: 4px;\n  overflow: hidden;\n}\n\n.view-tab {\n  height: 28px;\n  padding: 0 10px;\n  font-size: 12px;\n  font-family: var(--font-family);\n  background: transparent;\n  border: none;\n  cursor: pointer;\n  color: var(--text-muted);\n  transition: background-color 0.15s ease, color 0.15s ease;\n}\n\n.view-tab--active {\n  background: var(--accent-subtle-bg);\n  color: var(--accent-primary);\n  font-weight: 500;\n}\n\n.view-tab:hover:not(.view-tab--active) {\n  background: rgba(50, 45, 35, 0.04);\n}\n\n/* Relationships List */\n.relationships-list {\n  display: flex;\n  flex-direction: column;\n}\n\n.relationship-item {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  padding: 10px 0;\n  border-bottom: 1px solid var(--bg-border-color-light);\n}\n\n.relationship-item:last-child {\n  border-bottom: none;\n}\n\n.relationship-info {\n  display: flex;\n  align-items: baseline;\n  gap: 6px;\n  flex-wrap: wrap;\n}\n\n.relationship-name {\n  font-size: 13px;\n  font-weight: 600;\n  color: var(--text-primary);\n}\n\n.relationship-type {\n  font-size: 13px;\n  color: var(--text-muted);\n}\n\n.relationship-detail {\n  font-size: 12px;\n  color: var(--text-muted);\n}\n\n.relationship-actions {\n  display: flex;\n  align-items: center;\n  gap: 6px;\n  flex-shrink: 0;\n}\n\n.relationship-link {\n  font-size: 12px;\n  color: var(--accent-primary);\n  background: none;\n  border: none;\n  cursor: pointer;\n  font-family: var(--font-family);\n  padding: 2px 4px;\n  transition: color 0.1s ease;\n}\n\n.relationship-link:hover {\n  color: var(--accent-dark);\n  text-decoration: underline;\n}\n\n.relationship-delete-btn {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 24px;\n  height: 24px;\n  background: transparent;\n  border: none;\n  border-radius: 4px;\n  cursor: pointer;\n  color: var(--text-muted);\n  transition: background-color 0.1s ease, color 0.1s ease;\n}\n\n.relationship-delete-btn:hover {\n  background: rgba(176, 80, 80, 0.08);\n  color: var(--status-error);\n}\n\n/* Relationships Graph */\n.relationships-graph {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n}\n\n.relationships-graph-container {\n  width: 100%;\n  max-width: 400px;\n}\n\n.relationships-graph-svg {\n  width: 100%;\n  height: auto;\n}\n\n/* Contact History */\n.contact-history-list {\n  display: flex;\n  flex-direction: column;\n}\n\n.contact-history-item {\n  display: flex;\n  align-items: flex-start;\n  justify-content: space-between;\n  padding: 12px 0;\n  border-bottom: 1px solid var(--bg-border-color-light);\n  gap: 12px;\n}\n\n.contact-history-item:last-child {\n  border-bottom: none;\n}\n\n.contact-history-item-content {\n  flex: 1;\n  min-width: 0;\n  display: flex;\n  flex-direction: column;\n  gap: 4px;\n}\n\n.contact-history-item-header {\n  display: flex;\n  align-items: center;\n  gap: 10px;\n}\n\n.contact-history-date {\n  font-size: 13px;\n  font-weight: 500;\n  color: var(--text-primary);\n}\n\n.contact-history-type-badge {\n  display: inline-flex;\n  align-items: center;\n  height: 20px;\n  padding: 0 8px;\n  font-size: 11px;\n  font-weight: 500;\n  border-radius: 10px;\n  background: #e3ecf0;\n  color: var(--status-info);\n}\n\n.contact-history-summary {\n  font-size: 13px;\n  color: var(--text-secondary);\n  line-height: 1.5;\n}\n\n.contact-history-performer {\n  font-size: 12px;\n  color: var(--text-muted);\n}\n\n.contact-history-edit-btn {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 28px;\n  height: 28px;\n  background: transparent;\n  border: none;\n  border-radius: 4px;\n  cursor: pointer;\n  color: var(--text-muted);\n  flex-shrink: 0;\n  transition: background-color 0.1s ease, color 0.1s ease;\n}\n\n.contact-history-edit-btn:hover {\n  background: rgba(50, 45, 35, 0.06);\n  color: var(--text-primary);\n}\n\n/* Contact History Edit Inline */\n.contact-history-edit {\n  padding: 12px 0;\n  border-bottom: 1px solid var(--bg-border-color-light);\n}\n\n.contact-history-edit:last-child {\n  border-bottom: none;\n}\n\n.contact-history-edit-actions {\n  display: flex;\n  gap: 8px;\n  justify-content: flex-end;\n  margin-top: 8px;\n}\n\n/* Textarea */\n.form-textarea {\n  height: auto;\n  padding: 8px 10px;\n  resize: vertical;\n  font-family: var(--font-family);\n}\n\n/* Associated Clients */\n.associated-clients-grid {\n  display: grid;\n  grid-template-columns: repeat(auto-fill, minmax(280px, 1fr));\n  gap: 16px;\n}\n\n.associated-client-card {\n  border: 1px solid var(--bg-border-color-light);\n  border-radius: 6px;\n  padding: 16px;\n  display: flex;\n  flex-direction: column;\n  gap: 12px;\n  background: var(--bg-base-color);\n  transition: border-color 0.1s ease;\n}\n\n.associated-client-card:hover {\n  border-color: var(--normal-border-hover);\n}\n\n.associated-client-header {\n  display: flex;\n  align-items: center;\n  gap: 8px;\n}\n\n.associated-client-icon {\n  flex-shrink: 0;\n  color: var(--text-muted);\n}\n\n.associated-client-name {\n  font-size: 14px;\n  font-weight: 600;\n  color: var(--text-primary);\n}\n\n.associated-client-details {\n  display: flex;\n  flex-direction: column;\n  gap: 4px;\n}\n\n.associated-client-field {\n  font-size: 12px;\n  color: var(--text-muted);\n}\n\n.associated-client-field-label {\n  font-weight: 500;\n}\n\n.associated-client-field-value {\n  color: var(--text-secondary);\n}\n\n.associated-client-view-btn {\n  d