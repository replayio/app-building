t: 400;\n    67→  font-family: var(--font-family);\n    68→  border-radius: 5px;\n    69→  cursor: pointer;\n    70→  transition: background-color 0.15s ease, color 0.15s ease, border-color 0.15s ease;\n    71→  white-space: nowrap;\n    72→  border: 1px solid transparent;\n    73→}\n    74→\n    75→.btn--primary {\n    76→  background: var(--accent-primary);\n    77→  color: var(--text-on-dark);\n    78→  border-color: transparent;\n    79→}\n    80→\n    81→.btn--primary:hover {\n    82→  background: var(--accent-dark);\n    83→}\n    84→\n    85→.btn--secondary {\n    86→  background: transparent;\n    87→  color: var(--text-secondary);\n    88→  border-color: var(--bg-border-color);\n    89→}\n    90→\n    91→.btn--secondary:hover {\n    92→  background: rgba(50, 45, 35, 0.06);\n    93→  border-color: var(--normal-border-hover);\n    94→}\n    95→\n    96→.btn--ghost {\n    97→  background: transparent;\n    98→  color: var(--text-secondary);\n    99→  border-color: transparent;\n   100→}\n   101→\n   102→.btn--ghost:hover {\n   103→  background: rgba(50, 45, 35, 0.06);\n   104→}\n   105→\n   106→.btn--danger {\n   107→  background: transparent;\n   108→  color: var(--status-error);\n   109→  border-color: var(--status-error);\n   110→}\n   111→\n   112→.btn--danger:hover {\n   113→  background: rgba(176, 80, 80, 0.08);\n   114→}\n   115→\n   116→.btn:disabled {\n   117→  opacity: 0.5;\n   118→  cursor: default;\n   119→}\n   120→\n   121→/* Data table */\n   122→.data-table {\n   123→  width: 100%;\n   124→  border-collapse: collapse;\n   125→}\n   126→\n   127→.data-table th {\n   128→  text-align: left;\n   129→  font-size: 12px;\n   130→  font-weight: 500;\n   131→  color: var(--text-muted);\n   132→  padding: 8px 12px;\n   133→  border-bottom: 1px solid var(--bg-border-color);\n   134→}\n   135→\n   136→.data-table td {\n   137→  font-size: 13px;\n   138→  color: var(--text-secondary);\n   139→  padding: 10px 12px;\n   140→  border-bottom: 1px solid var(--b