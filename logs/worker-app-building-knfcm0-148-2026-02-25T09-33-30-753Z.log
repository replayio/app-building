utside]);\n    57→\n    58→  useEffect(() => {\n    59→    if (open && searchable && searchInputRef.current) {\n    60→      searchInputRef.current.focus();\n    61→    }\n    62→  }, [open, searchable]);\n    63→\n    64→  return (\n    65→    <div className=\"filter-select\" ref={containerRef} data-testid={testId}>\n    66→      <button\n    67→        className=\"filter-select-trigger\"\n    68→        data-testid={testId ? `${testId}-trigger` : undefined}\n    69→        onClick={() => {\n    70→          setOpen(!open);\n    71→          if (open) setSearch(\"\");\n    72→        }}\n    73→        type=\"button\"\n    74→      >\n    75→        <span className={selectedOption ? \"filter-select-value\" : \"filter-select-placeholder\"}>\n    76→          {selectedOption ? selectedOption.label : placeholder}\n    77→        </span>\n    78→        <svg\n    79→          className={`filter-select-chevron ${open ? \"filter-select-chevron--open\" : \"\"}`}\n    80→          width=\"12\"\n    81→          height=\"12\"\n    82→          viewBox=\"0 0 12 12\"\n    83→          fill=\"none\"\n    84→        >\n    85→          <path\n    86→            d=\"M3 4.5L6 7.5L9 4.5\"\n    87→            stroke=\"currentColor\"\n    88→            strokeWidth=\"1.5\"\n    89→            strokeLinecap=\"round\"\n    90→            strokeLinejoin=\"round\"\n    91→          />\n    92→        </svg>\n    93→      </button>\n    94→\n    95→      {open && (\n    96→        <div\n    97→          className=\"filter-select-dropdown\"\n    98→          data-testid={testId ? `${testId}-dropdown` : undefined}\n    99→        >\n   100→          {searchable && (\n   101→            <div className=\"filter-select-search\">\n   102→              <input\n   103→                ref={searchInputRef}\n   104→                className=\"filter-select-search-input\"\n   105→                data-testid={testId ? `${testId}-search` : undefined}\n   106→                type=\"text\"\n   107→                placeholder=\"Search...\"\n   108→                value={search}\n   109→                onChange={(e) => setSearch(e.target.value)}\n   110→              />\n   111→            </div>\n   112→          )}\n   113→          <div className=\"filter-select-options\">\n   114→            {filtered.length === 0 ? (\n   115→              <div className=\"filter-select-empty\">No matches</div>\n   116→            ) : (\n   117→              filtered.map((option) => (\n   118→                <button\n   119→                  key={option.value}\n   120→                  className={`filter-select-option ${option.value === value ? \"filter-select-option--selected\" : \"\"}`}\n   121→                  data-testid={testId ? `${testId}-option-${option.value}` : undefined}\n   122→                  type=\"button\"\n   123→                  onClick={() => {\n   124→                    onChange(option.value);\n   125→                    setOpen(false);\n   126→                    setSearch(\"\");\n   127→                  }}\n   128→                >\n   129→                  {option.label}\n   130→                </button>\n   131→              ))\n   132→            )}\n   133→          </div>\n   134→        </div>\n   135→      )}\n   136→    </div>\n   137→  );\n   138→}\n   139→"}]},"parent_tool_use_id":"toolu_0162Yc72wAEupU4zi53cEyPR","session_id":"6ce9fdbd-ab27-4433-8cf3-fdd70c0c27a8","uuid":"354068cf-3029-4b19-adb5-d14335c34a1a"}
