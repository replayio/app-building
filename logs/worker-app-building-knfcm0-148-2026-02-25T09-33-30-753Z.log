: string;\n    52→    }>(\n    53→      sql,\n    54→      \"SELECT id, name, email, avatar_url, created_at FROM users WHERE id = $1\",\n    55→      [subPath]\n    56→    );\n    57→\n    58→    if (userRows.length === 0) {\n    59→      return errorResponse(404, \"User not found\");\n    60→    }\n    61→\n    62→    const u = userRows[0];\n    63→\n    64→    const dealStats = await query<{ active_deals: string; total_deals: string }>(\n    65→      sql,\n    66→      `SELECT\n    67→        COUNT(*) FILTER (WHERE status = 'open')::text AS active_deals,\n    68→        COUNT(*)::text AS total_deals\n    69→       FROM deals WHERE owner_id = $1`,\n    70→      [subPath]\n    71→    );\n    72→\n    73→    const taskStats = await query<{ open_tasks: string }>(\n    74→      sql,\n    75→      `SELECT COUNT(*)::text AS open_tasks\n    76→       FROM tasks WHERE assignee_id = $1 AND status IN ('open', 'in_progress')`,\n    77→      [subPath]\n    78→    );\n    79→\n    80→    return jsonResponse({\n    81→      id: u.id,\n    82→      name: u.name,\n    83→      email: u.email,\n    84→      avatarUrl: u.avatar_url,\n    85→      createdAt: u.created_at,\n    86→      activeDeals: parseInt(dealStats[0]?.active_deals || \"0\", 10),\n    87→      totalDeals: parseInt(dealStats[0]?.total_deals || \"0\", 10),\n    88→      openTasks: parseInt(taskStats[0]?.open_tasks || \"0\", 10),\n    89→    });\n    90→  }\n    91→\n    92→  return errorResponse(405, \"Method not allowed\");\n    93→}\n    94→"}]},"parent_tool_use_id":"toolu_0162Yc72wAEupU4zi53cEyPR","session_id":"6ce9fdbd-ab27-4433-8cf3-fdd70c0c27a8","uuid":"6d2dfe02-06bb-4096-bc65-636b06833150"}
