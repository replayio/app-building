  await page.getByTestId('deals-view-pipeline').click();\n   280→\n   281→    // Pipeline view should be visible\n   282→    const pipelineView = page.getByTestId('deals-pipeline-view');\n   283→    await expect(pipelineView).toBeVisible();\n   284→\n   285→    // Should have stage columns\n   286→    await expect(page.getByTestId('pipeline-column-lead')).toBeVisible();\n   287→    await expect(page.getByTestId('pipeline-column-qualification')).toBeVisible();\n   288→    await expect(page.getByTestId('pipeline-column-discovery')).toBeVisible();\n   289→    await expect(page.getByTestId('pipeline-column-proposal')).toBeVisible();\n   290→    await expect(page.getByTestId('pipeline-column-negotiation')).toBeVisible();\n   291→    await expect(page.getByTestId('pipeline-column-closed_won')).toBeVisible();\n   292→\n   293→    // Table should not be visible\n   294→    await expect(page.getByTestId('deals-table')).not.toBeVisible();\n   295→  });\n   296→\n   297→  test('DLP-VW-03: Switching between views preserves data', async ({ page }) => {\n   298→    await page.goto('/deals');\n   299→    await page.waitForLoadState('networkidle');\n   300→\n   301→    // Get deal names from table view\n   302→    const rows = page.locator('[data-testid^=\"deal-row-\"]');\n   303→    const tableRowCount = await rows.count();\n   304→\n   305→    // Switch to Pipeline View\n   306→    await page.getByTestId('deals-view-pipeline').click();\n   307→    await expect(page.getByTestId('deals-pipeline-view')).toBeVisible();\n   308→\n   309→    // Switch back to Table View\n   310→    await page.getByTestId('deals-view-table').click();\n   311→    await expect(page.getByTestId('deals-table')).toBeVisible();\n   312→\n   313→    // Same number of deal rows\n   314→    const rowsAfter = page.locator('[data-testid^=\"deal-row-\"]');\n   315→    const tableRowCountAfter = await rowsAfter.count();\n   316→    expect(tableRowCountAfter).toBe(tableRowCount);\n   317→  });\n   318→\n   319→  test('DLP-VW-04: Pipeline View supports drag-and-drop to change deal stage', async ({ page }) => {\n   320→    await page.goto('/deals');\n   321→    await page.waitForLoadState('networkidle');\n   322→\n   323→    // Switch to Pipeline View\n   324→    await page.getByTestId('deals-view-pipeline').click();\n   325→    await expect(page.getByTestId('deals-pipeline-view')).toBeVisible();\n   326→\n   327→    // Find a column that has deals\n   328→    const stages = ['lead', 'qualification', 'discovery', 'proposal', 'negotiation', 'closed_won'];\n   329→    let sourceStage = '';\n   330→    let dealId = '';\n   331→\n   332→    for (const stage of stages) {\n   333→      const column = page.getByTestId(`pipeline-column-${stage}`);\n   334→      const cards = column.locator('[data-testid^=\"pipeline-deal-card-\"]');\n   335→      const count = await cards.count();\n   336→      if (count > 0) {\n   337→        sourceStage = stage;\n   338→        const testId = await cards.first().getAttribute('data-testid');\n   339→        dealId = testId?.replace('pipeline-deal-card-', '') ?? '';\n   340→        break;\n   341→      }\n   342→    }\n   343→\n   344→    if (!sourceStage || !dealId) return;\n   345→\n   346→    // Pick a target column that's different from source\n   347→    const targetStage = stages.find((s) => s !== sourceStage) ?? 'proposal';\n   348→    const targetColumn = page.getByTestId(`pipeline-column-${targetStage}`);\n   349→\n   350→    // Perform drag and drop with retry — dragTo can be unreliable\n   351→    await expect(async () => {\n   352→      // Re-locate the source card (it may have moved after a reload)\n   353→      const card = page.getByTestId(`pipeline-deal-card-${dealId}`);\n   354→      const cardInTarget = page.getByTestId(`pipeline-column-${targetStage}`).getByTestId(`pipeline-deal-card-${dealId}`);\n   355→\n   356→      if (await cardInTarget.count() === 0) {\n   357→        await card.dragTo(targetColumn);\n   358→        await page.waitForLoadState('networkidle');\n   359→      }\n   360→\n   361→      await expect(cardInTarget).toBeVisible();\n   362→    }).toPass({ timeout: 15000 });\n   363→\n   364→    // Navigate to the deal detail page and verify a history entry was created\n   365→    const stageDisplayNames: Record<string, string> = {\n   366→      'lead': 'Lead', 'qualification': 'Qualification', 'discovery': 'Discovery',\n   367→      'proposal': 'Proposal Sent', 'negotiation': 'Negotiation', 'closed_won': 'Closed Won',\n   368→    };\n   369→    const expectedFrom = stageDisplayNames[sourceStage] || sourceStage;\n   370→    const expectedTo = stageDisplayNames[targetStage] || targetStage;\n   371→\n   372→    await page.goto(`/deals/${dealId}`);\n   373→    await page.waitForLoadState('networkidle');\n   374→\n   375→    const expectedHistoryText = `Changed Stage from ${expectedFrom} to ${expectedTo}`;\n   376→    const historyEntry = page.locator('[data-testid^=\"deal-history-entry-\"]', { hasText: expectedHistoryText });\n   377→    await expect(historyEntry).toHaveCount(1, { timeout: 10000 });\n   378→  });\n   379→});\n   380→\n   381→test.describe('DealsListPage - FilterControls (DLP-FLT)', () => {\n   382→  test('DLP-FLT-01: Stage filter shows all stage options', async ({ page }) => {\n   383→    await page.goto('/deals');\n   384→    await page.waitForLoadState('networkidle');\n   385→\n   386→    const stageFilter = page.getByTestId('deals-filter-stage');\n   387→    await expect(stageFilter).toBeVisible();\n   388→\n   389→    // Open the dropdown menu\n   390→    await page.getByTestId('deals-filter-stage-trigger').click();\n   391→    const menu = page.getByTestId('deals-filter-stage-menu');\n   392→    await expect(menu).toBeVisible();\n   393→\n   394→    // Check that all stage options are present\n   395→    const optionTexts = await menu.locator('button').allTextContents();\n   396→\n   397→    expect(optionTexts.join(',')).toContain('All Stages');\n   398→    expect(op