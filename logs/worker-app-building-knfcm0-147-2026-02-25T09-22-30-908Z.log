solid var(--bg-border-color);\n}\n\n.sidebar-signin-btn {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 100%;\n  height: 32px;\n  padding: 0 12px;\n  font-size: 13px;\n  font-weight: 500;\n  color: var(--accent-primary);\n  background: transparent;\n  border: 1px solid var(--accent-primary);\n  border-radius: var(--control-border-radius);\n  cursor: pointer;\n  transition: background-color 0.15s ease;\n}\n\n.sidebar-signin-btn:hover {\n  background: var(--accent-subtle-bg);\n}\n\n.sidebar-auth-form {\n  display: flex;\n  flex-direction: column;\n  gap: 8px;\n  padding-top: 4px;\n}\n\n.sidebar-auth-form input {\n  width: 100%;\n  height: 32px;\n  padding: 0 8px;\n  font-size: 13px;\n  border: 1px solid var(--normal-border);\n  border-radius: var(--control-border-radius);\n  background: white;\n  color: var(--text-primary);\n}\n\n.sidebar-auth-form input:focus {\n  outline: none;\n  border-color: var(--accent-primary);\n}\n\n.sidebar-auth-submit-btn {\n  width: 100%;\n  height: 32px;\n  font-size: 13px;\n  font-weight: 500;\n  color: white;\n  background: var(--accent-primary);\n  border: none;\n  border-radius: var(--control-border-radius);\n  cursor: pointer;\n  transition: background-color 0.15s ease;\n}\n\n.sidebar-auth-submit-btn:hover {\n  background: var(--accent-dark);\n}\n\n.sidebar-auth-submit-btn:disabled {\n  opacity: 0.6;\n  cursor: not-allowed;\n}\n\n.sidebar-auth-error {\n  font-size: 12px;\n  color: var(--status-error);\n  padding: 2px 0;\n}\n\n.sidebar-auth-links {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  font-size: 12px;\n}\n\n.sidebar-auth-link {\n  color: var(--accent-primary);\n  background: none;\n  border: none;\n  cursor: pointer;\n  font-size: 12px;\n  padding: 0;\n  text-decoration: underline;\n}\n\n.sidebar-auth-link:hover {\n  color: var(--accent-dark);\n}\n\n.sidebar-auth-toggle {\n  color: var(--accent-primary);\n  background: none;\n  border: none;\n  cursor: pointer;\n  font-size: 12px;\n  padding: 0;\n  text-decoration: underline;\n}\n\n.sidebar-auth-toggle:hover {\n  color: var(--accent-dark);\n}\n\n.sidebar-auth-message {\n  font-size: 12px;\n  color: var(--status-success);\n  padding: 2px 0;\n}\n\n.sidebar-signout-btn {\n  display: inline-flex;\n  align-items: center;\n  height: 24px;\n  padding: 0 8px;\n  font-size: 12px;\n  font-weight: 400;\n  color: var(--text-muted);\n  background: transparent;\n  border: none;\n  border-radius: var(--control-border-radius);\n  cursor: pointer;\n  transition: background-color 0.1s ease, color 0.15s ease;\n}\n\n.sidebar-signout-btn:hover {\n  background: rgba(50, 45, 35, 0.06);\n  color: var(--text-primary);\n}\n\n/* Auth page styles */\n.auth-page {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  min-height: 100vh;\n  padding: 24px;\n}\n\n@media (max-width: 640px) {\n  .auth-page {\n    padding: 12px;\n  }\n}\n\n.auth-form-card {\n  width: 100%;\n  max-width: 400px;\n  background: white;\n  border: 1px solid var(--bg-border-color);\n  border-radius: 8px;\n  padding: 32px;\n  box-shadow: var(--shadow-elevation-1);\n}\n\n.auth-form-title {\n  font-size: 22px;\n  font-weight: 600;\n  color: var(--text-primary);\n  margin-bottom: 8px;\n}\n\n.auth-form-description {\n  font-size: 13px;\n  color: var(--text-muted);\n  margin-bottom: 20px;\n  line-height: 1.5;\n}\n\n.auth-form-card .form-group {\n  margin-bottom: 16px;\n}\n\n.auth-form-card .form-input {\n  width: 100%;\n}\n\n.auth-form-error {\n  font-size: 13px;\n  color: var(--status-error);\n  margin-bottom: 12px;\n  line-height: 1.4;\n}\n\n.auth-form-success {\n  font-size: 13px;\n  color: var(--status-success);\n  margin-bottom: 12px;\n  line-height: 1.4;\n}\n\n.auth-form-loading {\n  font-size: 13px;\n  color: var(--text-muted);\n  margin-bottom: 12px;\n}\n\n.auth-form-submit {\n  width: 100%;\n  margin-bottom: 12px;\n}\n\n.auth-form-link {\n  display: inline;\n  color: var(--accent-primary);\n  background: none;\n  border: none;\n  cursor: pointer;\n  font-size: 13px;\n  padding: 0;\n  text-decoration: underline;\n}\n\n.auth-form-link:hover {\n  color: var(--accent-dark);\n}\n\n.auth-form-link--block {\n  display: block;\n  margin-bottom: 12px;\n}\n\n/* Page placeholder styles */\n.page-placeholder {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  min-height: 200px;\n  font-size: 18px;\n  color: var(--text-muted);\n}\n\n/* Page padding */\n.p-6 {\n  padding: 24px;\n}\n\n@media (max-width: 640px) {\n  .max-sm\\:p-3 {\n    padding: 12px;\n  }\n}\n\n/* Clients page styles */\n.clients-page {\n  display: flex;\n  flex-direction: column;\n  min-height: 0;\n}\n\n.clients-header-actions {\n  display: flex;\n  align-items: center;\n  gap: 8px;\n}\n\n/* Clients search and filters */\n.clients-filters {\n  display: flex;\n  flex-direction: column;\n  gap: 12px;\n  padding: 0 24px 16px;\n}\n\n.clients-search-wrapper {\n  position: relative;\n  display: flex;\n  align-items: center;\n}\n\n.clients-search-icon {\n  position: absolute;\n  left: 10px;\n  color: var(--text-muted);\n  pointer-events: none;\n}\n\n.clients-search-input {\n  width: 100%;\n  height: 36px;\n  padding: 0 10px 0 32px;\n  font-size: 13px;\n  font-family: var(--font-family);\n  color: var(--text-primary);\n  background: var(--bg-base-color);\n  border: 1px solid var(--normal-border);\n  border-radius: 4px;\n  outline: none;\n  transition: border-color 0.15s ease;\n}\n\n.clients-search-input:focus {\n  border-color: var(--accent-primary);\n  box-shadow: 0 0 0 2px rgba(77, 124, 94, 0.15);\n}\n\n.clients-search-input::placeholder {\n  color: var(--text-disabled);\n}\n\n.clients-filter-row {\n  display: flex;\n  gap: 8px;\n  flex-wrap: wrap;\n}\n\n/* Clients table styles */\n.clients-name-cell {\n  font-weight: 500;\n  color: var(--text-primary);\n}\n\n.clients-tags {\n  display: flex;\n  gap: 4px;\n  flex-wrap: wrap;\n}\n\n.clients-tag-badge {\n  font-size: 10px;\n  height: 20px;\n  padding: 0 6px;\n}\n\n.clients-actions-btn {\n  width: 28px;\n  height: 28px;\n  padding: 0;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n.clients-empty {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  min-height: 200px;\n  font-size: 14px;\n  color: var(--text-muted);\n  padding: 0 24px;\n}\n\n.clients-loading {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  min-height: 200px;\n  font-size: 14px;\n  color: var(--text-muted);\n}\n\n/* Clients pagination */\n.clients-pagination {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  padding: 16px 24px;\n}\n\n.clients-pagination-info {\n  font-size: 13px;\n  color: var(--text-muted);\n}\n\n/* Inactive badge (not already in common.css) */\n.badge--neutral {\n  background: #eae8e4;\n  color: var(--status-neutral);\n}\n\n/* Modal styles */\n.modal-overlay {\n  position: fixed;\n  inset: 0;\n  background: rgba(0, 0, 0, 0.4);\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  z-index: 1000;\n}\n\n.modal {\n  width: 100%;\n  max-width: 520px;\n  background: #ffffff;\n  border-radius: 8px;\n  box-shadow: var(--shadow-elevation-2);\n  overflow: visible;\n}\n\n.modal-header {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  padding: 16px 20px;\n  border-bottom: 1px solid var(--bg-border-color-light);\n}\n\n.modal-title {\n  font-size: 16px;\n  font-weight: 600;\n  color: var(--text-primary);\n}\n\n.modal-close {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 28px;\n  height: 28px;\n  background: transparent;\n  border: none;\n  border-radius: 4px;\n  cursor: pointer;\n  color: var(--text-muted);\n  transition: background-color 0.1s ease;\n}\n\n.modal-close:hover {\n  background: rgba(50, 45, 35, 0.06);\n}\n\n.modal-body {\n  padding: 16px 20px;\n  max-height: 60vh;\n  overflow-y: auto;\n}\n\n.modal-error {\n  font-size: 13px;\n  color: var(-