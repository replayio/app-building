   key={task.id}\n   177→              task={task}\n   178→              onClick={handleTaskClick}\n   179→              onEdit={setEditTask}\n   180→              onMarkComplete={handleMarkComplete}\n   181→              onDelete={setDeleteConfirm}\n   182→            />\n   183→          ))}\n   184→        </div>\n   185→      )}\n   186→\n   187→      <CreateTaskModal\n   188→        open={createModalOpen}\n   189→        onClose={() => setCreateModalOpen(false)}\n   190→        onSave={handleCreateTask}\n   191→        availableClients={allClients}\n   192→        availableDeals={allDeals}\n   193→        availableUsers={availableUsers}\n   194→      />\n   195→\n   196→      <EditTaskModal\n   197→        open={editTask !== null}\n   198→        task={editTask}\n   199→        availableUsers={availableUsers}\n   200→        onClose={() => setEditTask(null)}\n   201→        onSave={handleEditTask}\n   202→      />\n   203→\n   204→      <ConfirmDialog\n   205→        open={deleteConfirm !== null}\n   206→        title=\"Delete Task\"\n   207→        message=\"Are you sure you want to delete this task? This action cannot be undone.\"\n   208→        confirmLabel=\"Delete\"\n   209→        onConfirm={handleDeleteTask}\n   210→        onCancel={() => setDeleteConfirm(null)}\n   211→      />\n   212→\n   213→      {importDialogOpen && (\n   214→        <ImportDialog\n   215→          entityName=\"Task\"\n   216→          entityNamePlural=\"Tasks\"\n   217→          columns={TASK_CSV_COLUMNS}\n   218→          headerMap={TASK_HEADER_MAP}\n   219→          templateFilename=\"tasks-import-template.csv\"\n   220→          templateExample=\"Follow up with client,Review Q1 proposal details,2024-03-15,high,Acme Corp,Jane Smith\"\n   221→          apiEndpoint=\"/.netlify/functions/tasks?action=import\"\n   222→          apiBodyKey=\"tasks\"\n   223→          onClose={() => setImportDialogOpen(false)}\n   224→          onImported={loadTasks}\n   225→        />\n   226→      )}\n   227→    </div>\n   228→  )\n   229→}\n   230→\n   231→const TASK_CSV_COLUMNS = [\n   232→  { name: 'Title', required: true, description: 'Task title' },\n   233→  { name: 'Description', required: false, description: 'Task description' },\n   234→  { name: 'Due Date', required: false, description: 'Date in YYYY-MM-DD format' },\n   235→  { name: 'Priority', required: false, description: '\"high\", \"medium\", \"low\", or \"normal\" (default: normal)' },\n   236→  { name: 'Client Name', required: false, description: 'Name of an existing client to associate' },\n   237→  { name: 'Assignee', required: false, description: 'Name of the person assigned' },\n   238→]\n   239→\n   240→const TASK_HEADER_MAP: Record<string, string> = {\n   241→  'title': 'title',\n   242→  'description': 'description',\n   243→  'due date': 'due_date',\n   244→  'due_date': 'due_date',\n   245→  'priority': 'priority',\n   246→  'client name': 'client_name',\n   247→  'client_name': 'client_name',\n   248→  'client': 'client_name',\n   249→  'assignee': 'assignee_name',\n   250→  'assignee name': 'assignee_name',\n   251→  'assignee_name': 'assignee_name',\n   252→}\n   253→"}]},"parent_tool_use_id":"toolu_01PqowybAnmZFrS6V9sJazgV","session_id":"8d83431d-ad7d-4d80-8e0d-64b63dd768fe","uuid":"6699b9f9-b2d5-4f2b-b43f-0381da88454a"}
