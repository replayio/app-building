tion loadUser() {\n    61→      try {\n    62→        const res = await fetch(`/.netlify/functions/users/${userId}`)\n    63→        if (!res.ok) {\n    64→          navigate('/users')\n    65→          return\n    66→        }\n    67→        const data = await res.json() as { user: UserDetail; deals: UserDeal[]; tasks: UserTask[]; activity: UserActivity[] }\n    68→        setUser(data.user)\n    69→        setDeals(data.deals)\n    70→        setTasks(data.tasks)\n    71→        setActivity(data.activity)\n    72→      } catch {\n    73→        navigate('/users')\n    74→      }\n    75→    }\n    76→    if (userId) loadUser()\n    77→  }, [userId, navigate])\n    78→\n    79→  if (!user) return null\n    80→\n    81→  const activeDeals = deals.filter(d => d.stage !== 'closed_won' && d.stage !== 'closed_lost')\n    82→  const openTasks = tasks.filter(t => !t.completed)\n    83→\n    84→  function formatDate(dateStr: string) {\n    85→    return new Date(dateStr).toLocaleDateString('en-US', { month: 'short', day: 'numeric', year: 'numeric' })\n    86→  }\n    87→\n    88→  function formatRelativeDate(dateStr: string) {\n    89→    const now = new Date()\n    90→    const date = new Date(dateStr)\n    91→    const diffMs = now.getTime() - date.getTime()\n    92→    const diffDays = Math.floor(diffMs / (1000 * 60 * 60 * 24))\n    93→    if (diffDays === 0) return 'Today'\n    94→    if (diffDays === 1) return 'Yesterday'\n    95→    if (diffDays < 7) return `${diffDays} days ago`\n    96→    if (diffDays < 30) return `${Math.floor(diffDays / 7)} week${Math.floor(diffDays / 7) > 1 ? 's' : ''} ago`\n    97→    return formatDate(dateStr)\n    98→  }\n    99→\n   100→  return (\n   101→    <div className=\"p-6 max-w-[900px]\">\n   102→      {/* Header */}\n   103→      <div data-testid=\"user-detail-header\" className=\"mb-6\">\n   104→        <button\n   105→          data-testid=\"user-detail-back\"\n   106→          onClick={() => navigate('/users')}\n   107→          className=\"inline-flex items-center gap-1.5 text-[13px] text-text-muted hover:text-text-primary transition-colors duration-100 mb-4\"\n   108→        >\n   109→          <ArrowLeft size={14} strokeWidth={1.75} />\n   110→          Back to Team Members\n   111→        </button>\n   112→\n   113→        <div className=\"flex items-center gap-4\">\n   114→          {user.avatar_url ? (\n   115→            <img src={user.avatar_url} alt={user.name} className=\"w-14 h-14 rounded-full\" />\n   116→          ) : (\n   117→            <div className=\"w-14 h-14 rounded-full bg-accent/20 flex items-center justify-center text-[20px] font-medium text-accent\">\n   118→              {user.name.charAt(0).toUpperCase()}\n   119→            </div>\n   120→          )}\n   121→          <div>\n   122→            <h1 data-testid=\"user-detail-name\" className=\"text-[22px] font-semibold text-text-primary\">\n   123→              {user.name}\n   124→            </h1>\n   125→            <div className=\"flex items-center gap-4 text-[13px] text-text-muted mt-1\">\n   126→              <span className=\"flex items-center gap-1.5\">\n   127→                <Mail size={13} strokeWidth={1.75} />\n   128→                {user.email}\n   129→              </span>\n   130→              <span className=\"flex items-center gap-1.5\">\n   131→                <Calendar size={13} strokeWidth={1.75} />\n   132→                Joined {formatDate(user.created_at)}\n   133→              </span>\n   134→            </div>\n   135→          </div>\n   136→        </div>\n   137→\n   138→        {/* Summary stats */}\n   139→        <div className=\"grid grid-cols-3 gap-4 mt-5\">\n   140→          <div className=\"border border-border rounded-[6px] bg-surface p-3\">\n   141→            <div className=\"text-[12px] text-text-muted mb-1\">Active Deals</div>\n   142→            <div className=\"text-[18px] font-semibold text-text-primary\">{activeDeals.length}</div>\n   143→          </div>\n   144→    