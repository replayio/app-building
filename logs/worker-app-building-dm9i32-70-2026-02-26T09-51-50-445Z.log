useEffect(() => {\n    46→    setSelectedOwner(deal.ownerId || \"\");\n    47→  }, [deal.ownerId]);\n    48→\n    49→  const formattedValue = formatCurrency(deal.value);\n    50→  const displayValue = deal.value != null ? `$${Math.round(deal.value / 1000)}k` : \"\";\n    51→  const heading = `DEAL DETAILS: ${deal.clientName || \"Unknown\"} - ${displayValue} ${deal.name}`;\n    52→\n    53→  const ownerOptions = [\n    54→    { value: \"\", label: \"Unassigned\" },\n    55→    ...users.map((u) => ({ value: u.id, label: u.name })),\n    56→  ];\n    57→\n    58→  const handleClientClick = () => {\n    59→    if (deal.clientId) {\n    60→      navigate(`/clients/${deal.clientId}`);\n    61→    }\n    62→  };\n    63→\n    64→  const handleValueSave = async () => {\n    65→    const parsed = parseFloat(valueInput.replace(/[^0-9.]/g, \"\"));\n    66→    const newValue = isNaN(parsed) ? 0 : parsed;\n    67→    await dispatch(updateDealDetail({ dealId: deal.id, data: { value: newValue } })).unwrap();\n    68→    setEditingValue(false);\n    69→  };\n    70→\n    71→  const handleValueKeyDown = (e: React.KeyboardEvent) => {\n    72→    if (e.key === \"Enter\") {\n    73→      handleValueSave();\n    74→    } else if (e.key === \"Escape\") {\n    75→      setEditingValue(false);\n    76→    }\n    77→  };\n    78→\n    79→  const handleOwnerChange = async (newOwnerId: string) => {\n    80→    setSelectedOwner(newOwnerId);\n    81→    await dispatch(updateDealDetail({ dealId: deal.id, data: { ownerId: newOwnerId || undefined } }));\n    82→  };\n    83→\n    84→  const handleChangeStage = async () => {\n    85→    if (selectedStage !== deal.stage) {\n    86→      await dispatch(updateDealDetail({ dealId: deal.id, data: { stage: selectedStage } }));\n    87→      await dispatch(fetchDealHistory(deal.id));\n    88→    }\n    89→  };\n    90→\n    91→  return (\n    92→    <div className=\"deal-header\" data-testid=\"deal-header\">\n    93→      <h1 className=\"deal-header-title