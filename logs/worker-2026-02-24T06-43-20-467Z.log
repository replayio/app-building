e, file_url, file_size, deal_id } = data\n   243→  if (!filename) return json(400, { error: 'Filename is required' })\n   244→\n   245→  const result = await sql`\n   246→    INSERT INTO attachments (client_id, deal_id, filename, file_type, file_url, file_size)\n   247→    VALUES (\n   248→      ${clientId},\n   249→      ${deal_id || null},\n   250→      ${filename},\n   251→      ${file_type || 'application/octet-stream'},\n   252→      ${file_url || null},\n   253→      ${file_size || null}\n   254→    )\n   255→    RETURNING *\n   256→  `\n   257→\n   258→  await sql`\n   259→    INSERT INTO timeline_events (client_id, event_type, title, description, actor, reference_id, reference_type)\n   260→    VALUES (\n   261→      ${clientId},\n   262→      'attachment_uploaded',\n   263→      ${'Attachment Uploaded: ' + filename},\n   264→      ${'File \"' + filename + '\" was uploaded'},\n   265→      ${null},\n   266→      ${result[0].id},\n   267→      'attachment'\n   268→    )\n   269→  `\n   270→\n   271→  return json(201, result[0])\n   272→}\n   273→\n   274→async function handleGetTimeline(sql: SqlFn, clientId: string) {\n   275→  const rows = await sql`\n   276→    SELECT * FROM timeline_events\n   277→    WHERE client_id = ${clientId}\n   278→    ORDER BY event_date DESC\n   279→    LIMIT 100\n   280→  `\n   281→  return json(200, { events: rows })\n   282→}\n   283→\n   284→async function handleCreateTimelineEvent(sql: SqlFn, clientId: string, body: string | null) {\n   285→  const data = JSON.parse(body || '{}')\n   286→  const { event_type, title, description, actor, reference_id, reference_type } = data\n   287→  if (!event_type || !title) return json(400, { error: 'Event type and title are required' })\n   288→\n   289→  const result = await sql`\n   290→    INSERT INTO timeline_events (client_id, event_type, title, description, actor, reference_id, reference_type)\n   291→    VALUES (\n   292→      ${clientId},\n   293→      ${event_type},\n   294→      