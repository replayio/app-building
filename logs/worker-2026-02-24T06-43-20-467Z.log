  cursor: pointer;\n  padding: 0;\n  text-align: left;\n  transition: color 0.1s ease;\n}\n\n.client-person-name-btn:hover {\n  color: var(--color-accent);\n}\n\n/* Timeline Section */\n.timeline-list {\n  display: flex;\n  flex-direction: column;\n  gap: 0;\n}\n\n.timeline-entry {\n  display: flex;\n  gap: 12px;\n  padding: 12px 0;\n  border-bottom: 1px solid var(--color-border-light);\n}\n\n.timeline-entry:last-child {\n  border-bottom: none;\n}\n\n.timeline-entry-icon {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 28px;\n  height: 28px;\n  border-radius: 50%;\n  background: var(--color-bg-sidebar);\n  color: var(--color-text-muted);\n  flex-shrink: 0;\n  margin-top: 2px;\n}\n\n.timeline-entry-content {\n  flex: 1;\n  min-width: 0;\n}\n\n.timeline-entry-header {\n  display: flex;\n  align-items: center;\n  gap: 8px;\n  margin-bottom: 2px;\n}\n\n.timeline-entry-date {\n  font-size: 12px;\n  font-weight: 500;\n  color: var(--color-text-muted);\n}\n\n.timeline-entry-type {\n  font-size: 12px;\n  font-weight: 500;\n  color: var(--color-accent-muted);\n}\n\n.timeline-entry-body {\n  font-size: 13px;\n  color: var(--color-text-secondary);\n  display: flex;\n  flex-direction: column;\n  gap: 2px;\n}\n\n.timeline-entry-description {\n  font-size: 12px;\n  color: var(--color-text-muted);\n}\n\n.timeline-entry-actor {\n  font-size: 12px;\n  color: var(--color-text-muted);\n  margin-top: 2px;\n}\n\n.timeline-subject-link {\n  border: none;\n  background: none;\n  color: var(--color-text-primary);\n  font-size: 13px;\n  font-weight: 500;\n  cursor: pointer;\n  padding: 0;\n  text-align: left;\n  transition: color 0.1s ease;\n}\n\n.timeline-subject-link:hover {\n  color: var(--color-accent);\n}\n\n.timeline-actor-link {\n  border: none;\n  background: none;\n  color: var(--color-accent);\n  font-size: 12px;\n  cursor: pointer;\n  padding: 0;\n  text-decoration: none;\n  transition: opacity 0.1s ease;\n}\n\n.timeline-actor-link:hover {\n  opac