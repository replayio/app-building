\n    49→          return await handleGetTimeline(sql, resourceId)\n    50→        }\n    51→        if (!subResource) {\n    52→          const rows = await sql`\n    53→            SELECT c.*,\n    54→              (SELECT COALESCE(json_agg(t.name), '[]'::json) FROM client_tags ct JOIN tags t ON ct.tag_id = t.id WHERE ct.client_id = c.id) as tags\n    55→            FROM clients c WHERE c.id = ${resourceId}\n    56→          `\n    57→          if (!rows.length) return json(404, { error: 'Not found' })\n    58→          return json(200, rows[0])\n    59→        }\n    60→      }\n    61→\n    62→      if (params.format === 'csv') {\n    63→        return await handleExport(sql, params)\n    64→      }\n    65→\n    66→      return await handleList(sql, params)\n    67→    }\n    68→\n    69→    if (event.httpMethod === 'POST') {\n    70→      if (resourceId && subResource === 'people') {\n    71→        return await handleAddPerson(sql, resourceId, event.body)\n    72→      }\n    73→      if (resourceId && subResource === 'attachments') {\n    74→        return await handleCreateClientAttachment(sql, resourceId, event.body)\n    75→      }\n    76→      if (resourceId && subResource === 'timeline') {\n    77→        return await handleCreateTimelineEvent(sql, resourceId, event.body)\n    78→      }\n    79→      if (params.action === 'import') {\n    80→        return await handleImport(sql, event.body)\n    81→      }\n    82→      return await handleCreate(sql, event.body)\n    83→    }\n    84→\n    85→    if (event.httpMethod === 'PUT') {\n    86→      if (!resourceId) return json(400, { error: 'Client ID required' })\n    87→      return await handleUpdate(sql, resourceId, event.body)\n    88→    }\n    89→\n    90→    if (event.httpMethod === 'DELETE') {\n    91→      if (resourceId && subResource === 'people' && subResourceId) {\n    92→        return await handleRemovePerson(sql, resourceId, subResourceId)\n    93→      }\n    94→      if (resourceId && subResource === 'attachments' && subResourceId) {\n    95→        await sql`DELETE FROM attachments WHERE id = ${subResourceId} AND client_id = ${resourceId}`\n    96→        return json(200, { deleted: true })\n    97→      }\n    98→      if (resourceId && !subResource) {\n    99→        await sql`DELETE FROM clients WHERE id = ${resourceId}`\n   100→        return json(200, { deleted: true })\n   101→      }\n   102→      return json(400, { error: 'Invalid endpoint' })\n   103→    }\n   104→\n   105→    return json(405, { error: 'Method not allowed' })\n   106→  } catch (err: unknown) {\n   107→    console.error('Error:', err)\n   108→    const message = err instanceof Error ? err.message : 'Internal server error'\n   109→    return json(500, { error: message })\n   110→  }\n   111→}\n   112→\n   113→type SqlFn = ReturnType<typeof neon>\n   114→\n   115→async function handleGetClientDeals(sql: SqlFn, clientId: string) {\n   116→  const rows = await sql`\n   117→    SELECT d.*, c.name as client_name\n   118→    FROM deals d\n   119→    LEFT JOIN clients c ON d.client_id = c.id\n   120→    WHERE d.client_id = ${clientId}\n   121→    ORDER BY d.created_at DESC\n   122→  `\n   123→  return json(200, { deals: rows })\n   124→}\n   125→\n   126→async function handleGetClientPeople(sql: SqlFn, clientId: string) {\n   127→  const rows = await sql`\n   128→    SELECT i.id, i.name, i.title, i.email, i.phone, ci.role, ci.is_primary\n   129→    FROM client_individuals ci\n   130→    JOIN individuals i ON ci.individual_id = i.id\n   131→    WHERE ci.client_id = ${clientId}\n   132→    ORDER BY ci.is_primary DESC, i.name\n   133→  `\n   134→  return json(200, { people: rows })\n   135→}\n   136→\n   137→async function handleAddPerson(sql: SqlFn, clientId: string, body: string | null) {\n   138→  const data = JSON.parse(body || '{}')\n   139→\n   140→  if (data.create_new) {\n   141→    const { name, title, email, phone, role } = data\n   142→    if (!name) return json(400, { error: 'Name is required' })\n   143→\n   144→    const result = await sql`\n   145→      INSERT INTO individuals (name, title, email, phone)\n   146→      VALUES (${name}, ${title || null}, ${email || null}, ${phone || null})\n   147→      RETURNING *\n   148→    `\n   149→    const newPerson = result[0]\n   150→\n   151→    await sql`\n   152→      INSERT INTO client_individuals (client_id, individual_id, role)\n   153→      VALUES (${clientId}, ${newPerson.id}, ${role || null})\n   154→      ON CONFLICT (client_id, individual_id) DO UPDATE SET role = ${role || null}\n   155→    `\n   156→\n   157→    await sql`\n   158→      INSERT INTO timeline_events (client_id, event_type, title, description, actor, reference_id, reference_type)\n   159→      VALUES (\n   160→        ${clientId},\n   161→        'contact_added',\n   162→        ${'Contact Added: ' + name},\n   163→        ${'New contact \"' + name + '\" was added' + (role ? ' as ' + role : '')},\n   164→        ${null},\n   165→        ${newPerson.id},\n   166→        'individual'\n   167→      )\n   168→    `\n   169→\n   170→    return json(201, { ...newPerson, role })\n   171→  }\n   172→\n   173→  const { individual_id, role } = data\n   174→  if (!individual_id) return json(400, { error: 'Person is required' })\n   175→\n   176→  await sql`\n   177→    INSERT INTO client_individuals (client_id, individual_id, role)\n   178→    VALUES (${clientId}, ${individual_id}, ${role || null})\n   179→    ON CONFLICT (client_id, individual_id) DO UPDATE SET role = ${role || null}\n   180→  `\n   181→\n   182→  const person = await sql`\n   183→    SELECT i.id, i.name, i.title, i.email, i.phone, ci.role, ci.is_primary\n   184→    FROM client_individuals ci\n   185→    JOIN individuals i ON ci.individual_id = i.id\n   186→    WHERE ci.client_id = ${clientId} AND ci.individual_id = ${individual_id}\n   187→  `\n   188→\n   189→  await sql`\n   190→    INSERT INTO timeline_events (client_id, event_type, title, description,