[2026-02-20T06:28:17.222Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01UwsLBiTp3qGYkbGaU23HjP","type":"tool_result","content":"     1→import { useState, useEffect, useCallback } from 'react'\n     2→import { useParams, useNavigate } from 'react-router-dom'\n     3→import { useAppDispatch, useAppSelector } from '../store/hooks'\n     4→import { fetchIndividual } from '../store/individualsSlice'\n     5→import { PersonHeader } from '../components/person-detail/PersonHeader'\n     6→import { RelationshipsSection } from '../components/person-detail/RelationshipsSection'\n     7→import { ContactHistorySection } from '../components/person-detail/ContactHistorySection'\n     8→import { AssociatedClientsSection } from '../components/person-detail/AssociatedClientsSection'\n     9→import { AddRelationshipModal } from '../components/person-detail/AddRelationshipModal'\n    10→import { AddContactHistoryModal } from '../components/person-detail/AddContactHistoryModal'\n    11→import { EditContactHistoryModal } from '../components/person-detail/EditContactHistoryModal'\n    12→import type { ContactHistoryEntry, ContactHistoryType, RelationshipType } from '../types'\n    13→\n    14→export function PersonDetailPage() {\n    15→  const { individualId } = useParams()\n    16→  const navigate = useNavigate()\n    17→  const dispatch = useAppDispatch()\n    18→  const { currentIndividual, loading } = useAppSelector((state) => state.individuals)\n    19→\n    20→  const [addRelationshipOpen, setAddRelationshipOpen] = useState(false)\n    21→  const [addContactHistoryOpen, setAddContactHistoryOpen] = useState(false)\n    22→  const [editingEntry, setEditingEntry] = useState<ContactHistoryEntry | null>(null)\n    23→\n    24→  const loadData = useCallback(() => {\n    25→    if (individualId) {\n    26→      dispatch(fetchIndividual(individualId))\n    27→    }\n    28→  }, [individualId, dispatch])\n    29→\n    30→  useEffect(() => {\n    31→    loadData()\n    32→  }, [loadData])\n    33→\n    34→  async function handleUpdatePerson(data: Partial<{ name: string; title: string; email: string; phone: string; location: string }>) {\n    35→    if (!individualId) return\n    36→    const res = await fetch(`/.netlify/functions/individuals/${individualId}`, {\n    37→      method: 'PUT',\n    38→      headers: { 'Content-Type': 'application/json' },\n    39→      body: JSON.stringify(data),\n    40→    })\n    41→    if (res.ok) {\n    42→      loadData()\n    43→    }\n    44→  }\n    45→\n    46→  async function handleAddRelationship(data: { related_individual_id: string; relationship_type: RelationshipType }) {\n    47→    if (!individualId) return\n    48→    const res = await fetch(`/.netlify/functions/individuals/${individualId}/relationships`, {\n    49→      method: 'POST',\n    50→      headers: { 'Content-Type': 'application/json' },\n    51→      body: JSON.stringify(data),\n    52→    })\n    53→    if (res.ok) {\n    54→      setAddRelationshipOpen(false)\n    55→      loadData()\n    56→    }\n    57→  }\n    58→\n    59→  async function handleAddContactHistory(data: { date: string; type: ContactHistoryType; summary: string; team_member: string }) {\n    60→    if (!individualId) return\n    61→    const res = await fetch(`/.netlify/functions/individuals/${individualId}/contact-history`, {\n    62→      method: 'POST',\n    63→      headers: { 'Content-Type': 'application/json' },\n    64→      body: JSON.stringify(data),\n    65→    })\n    66→    if (res.ok) {\n    67→      setAddContactHistoryOpen(false)\n    68→      loadData()\n    69→    }\n    70→  }\n    71→\n    72→  async function handleEditContactHistory(entryId: string, data: { date: string; type: ContactHistoryType; summary: string; team_member: string }) {\n    73→    if (!individualId) return\n    74→    const res = await fetch(`/.netlify/functions/individuals/${individualId}/contact-history/${entryId}`, {\n    75→      method: 'PUT',\n    76→      headers: { 'Content-Type': 'application/json' },\n    77→      body: JSON.stringify(data),\n    78→    })\n    79→    if (res.ok) {\n    80→      setEditingEntry(null)\n    81→      loadData()\n    82→    }\n    83→  }\n    84→\n    85→  if (loading || !currentIndividual) {\n    86→    return (\n    87→      <div className=\"p-6\" data-testid=\"person-detail-loading\">\n    88→        <div className=\"text-[13px] text-text-muted\">Loading individual...</div>\n    89→      </div>\n    90→    )\n    91→  }\n    92→\n    93→  return (\n    94→    <div className=\"p-6\" data-testid=\"person-detail-page\">\n    95→      {/* Breadcrumb */}\n    96→      <div className=\"flex items-center gap-1 text-[12px] text-text-muted mb-4\" data-testid=\"person-detail-breadcrumb\">\n    97→        <span\n    98→          className=\"hover:text-accent cursor-pointer\"\n    99→          onClick={() => navigate('/clients')}\n   100→        >\n   101→          Clients\n   102→        </span>\n   103→        <span>/</span>\n   104→        <span>Individuals</span>\n   105→        <span>/</span>\n   106→        <span>{currentIndividual.name}</span>\n   107→      </div>\n   108→\n   109→      {/* Header */}\n   110→      <PersonHeader individual={currentIndividual} onUpdate={handleUpdatePerson} />\n   111→\n   112→      {/* Two-column layout */}\n   113→      <div className=\"grid grid-cols-2 gap-4\">\n   114→        <div>\n   115→          {/* Relationships */}\n   116→          <RelationshipsSection\n   117→            relationships={currentIndividual.relationships ?? []}\n   118→            onAddEntry={() => setAddRelationshipOpen(true)}\n   119→          />\n   120→\n   121→          {/* Contact History */}\n   122→          <ContactHistorySection\n   123→            entries={currentIndividual.contact_history ?? []}\n   124→            onAddEntry={() => setAddContactHistoryOpen(true)}\n   125→            onEditEntry={(entry) => setEditingEntry(entry)}\n   126→          />\n   127→        </div>\n   128→        <div>\n   129→          {/* Associated Clients */}\n   130→          <AssociatedClientsSection associations={currentIndividual.client_associations ?? []} />\n   131→        </div>\n   132→      </div>\n   133→\n   134→      {/* Modals */}\n   135→      <AddRelationshipModal\n   136→        open={addRelationshipOpen}\n   137→        onClose={() => setAddRelationshipOpen(false)}\n   138→        onSave={handleAddRelationship}\n   139→      />\n   140→      <AddContactHistoryModal\n   141→        open={addContactHistoryOpen}\n   142→        onClose={() => setAddContactHistoryOpen(false)}\n   143→        onSave={handleAddContactHistory}\n   144→      />\n   145→      <EditContactHistoryModal\n   146→        entry={editingEntry}\n   147→        onClose={() => setEditingEntry(null)}\n   148→        onSave={handleEditContactHistory}\n   149→      />\n   150→    </div>\n   151→  )\n   152→}\n   153→"}]},"parent_tool_use_id":"toolu_01KeDQ29kut64tb2ahtz1p6b","session_id":"1b1d24c4-d91a-44c6-9a76-4966eca71683","uuid":"66dc4230-413f-4476-8b38-b59e09ffb6b8"}
