n   586→    page,\n   587→  }) => {\n   588→    await goToRun(page, scheduledRunId);\n   589→\n   590→    await expect(page.getByTestId(\"raw-materials-heading\")).toBeVisible();\n   591→    await expect(page.getByTestId(\"raw-materials-heading\")).toHaveText(\n   592→      \"Recipe and Raw Materials\"\n   593→    );\n   594→  });\n   595→\n   596→  test(\"RUN-MAT-2: Table displays Material, Amount Needed, and Units column headers\", async ({\n   597→    page,\n   598→  }) => {\n   599→    await goToRun(page, scheduledRunId);\n   600→\n   601→    const table = page.getByTestId(\"raw-materials-table\");\n   602→    await expect(table).toBeVisible();\n   603→\n   604→    const headers = table.locator(\"thead th\");\n   605→    await expect(headers.nth(0)).toHaveText(\"Material\");\n   606→    await expect(headers.nth(1)).toHaveText(\"Amount Needed\");\n   607→    await expect(headers.nth(2)).toHaveText(\"Units\");\n   608→  });\n   609→\n   610→  test(\"RUN-MAT-3: Table rows display raw material data for the run\", async ({\n   611→    page,\n   612→  }) => {\n   613→    await goToRun(page, scheduledRunId);\n   614→\n   615→    const materials = scheduledRunDetail.materials;\n   616→    if (materials.length === 0) {\n   617→      test.skip();\n   618→      return;\n   619→    }\n   620→\n   621→    // Verify each material row displays correct data\n   622→    for (const mat of materials) {\n   623→      const row = page\n   624→        .getByTestId(\"raw-material-row\")\n   625→        .filter({ hasText: mat.material_name });\n   626→      await expect(row).toBeVisible();\n   627→      await expect(row.getByTestId(\"material-name\")).toHaveText(\n   628→        mat.material_name\n   629→      );\n   630→      await expect(row.getByTestId(\"material-unit\")).toHaveText(mat.unit);\n   631→    }\n   632→  });\n   633→\n   634→  test(\"RUN-MAT-4: Material amounts reflect the planned quantity\", async ({\n   635→    page,\n   636→  }) => {\n   637→    await goToRun(page, s