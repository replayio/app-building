ring(initialAmount) : initialAmount.toFixed(2);\n   693→\n   694→    const materialRow = page\n   695→      .getByTestId(\"raw-material-row\")\n   696→      .filter({ hasText: mat.material_name });\n   697→    await expect(materialRow.getByTestId(\"material-amount\")).toHaveText(\n   698→      initialStr\n   699→    );\n   700→\n   701→    // Adjust quantity to 1000\n   702→    await page.getByTestId(\"adjust-quantities-btn\").click();\n   703→    await expect(page.getByTestId(\"adjust-quantity-modal\")).toBeVisible();\n   704→    await page.getByTestId(\"adjust-quantity-input\").clear();\n   705→    await page.getByTestId(\"adjust-quantity-input\").fill(\"1000\");\n   706→    await page.getByTestId(\"adjust-quantity-save\").click();\n   707→    await expect(page.getByTestId(\"adjust-quantity-modal\")).toBeHidden({\n   708→      timeout: 30000,\n   709→    });\n   710→\n   711→    // Verify updated amounts\n   712→    const newAmount = Number(mat.quantity) * 1000;\n   713→    const newStr =\n   714→      newAmount % 1 === 0 ? String(newAmount) : newAmount.toFixed(2);\n   715→\n   716→    await expect(materialRow.getByTestId(\"material-amount\")).toHaveText(\n   717→      newStr,\n   718→      { timeout: 15000 }\n   719→    );\n   720→  });\n   721→\n   722→  test(\"RUN-MAT-6: Empty materials table shows placeholder when run has no recipe materials\", async ({\n   723→    page,\n   724→  }) => {\n   725→    await goToRun(page, emptyRunId);\n   726→\n   727→    await expect(page.getByTestId(\"raw-materials-empty\")).toBeVisible();\n   728→  });\n   729→});\n   730→\n   731→// ─── ForecastTable ───────────────────────────────────────────────────────────\n   732→\n   733→test.describe(\"ForecastTable\", () => {\n   734→  test(\"RUN-FCST-1: Forecast/Availability section heading is displayed\", async ({\n   735→    page,\n   736→  }) => {\n   737→    await goToRun(page, scheduledRunId);\n   738→\n   739→    await expect(page.getByTestId(\"forecast-heading\")).toBeVisible(