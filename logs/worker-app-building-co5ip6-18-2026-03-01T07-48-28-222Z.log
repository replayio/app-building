() => {\n    46→    if (id) {\n    47→      dispatch(fetchAppById(id))\n    48→      dispatch(fetchActivity(id))\n    49→    }\n    50→  }, [dispatch, id])\n    51→\n    52→  if (!app) {\n    53→    return (\n    54→      <div className=\"app-page\" data-testid=\"app-page\">\n    55→        <div className=\"app-page__loading\" data-testid=\"app-page-loading\">\n    56→          Loading app...\n    57→        </div>\n    58→      </div>\n    59→    )\n    60→  }\n    61→\n    62→  return (\n    63→    <div className=\"app-page\" data-testid=\"app-page\">\n    64→      <button\n    65→        className=\"app-page__back\"\n    66→        onClick={() => navigate('/')}\n    67→        data-testid=\"app-page-back\"\n    68→      >\n    69→        <ArrowLeft size={18} />\n    70→        Back to Dashboard\n    71→      </button>\n    72→      <AppHeader app={app} />\n    73→      <AppActions app={app} />\n    74→      <ActivityLog\n    75→        entries={activityEntries}\n    76→        loading={activityLoading}\n    77→        error={activityError}\n    78→        appStatus={app.status}\n    79→        onRefresh={handleRefresh}\n    80→      />\n    81→    </div>\n    82→  )\n    83→}\n    84→\n    85→export default AppPage\n    86→"}]},"parent_tool_use_id":"toolu_01HrdDigJ1i7kFjEJ8Q1vP2J","session_id":"d3c1e583-b408-4725-beb9-5647bd07a4a4","uuid":"16a4d63e-d716-40ee-ac1b-ea2ad1ef5ff7"}
