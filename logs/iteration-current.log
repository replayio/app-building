ULL REFERENCES individuals(id) ON DELETE CASCADE,\n   101→      relationship_type TEXT NOT NULL CHECK (relationship_type IN ('colleague', 'decision_maker', 'influencer', 'manager', 'report', 'other')),\n   102→      created_at TIMESTAMPTZ DEFAULT NOW(),\n   103→      UNIQUE(individual_id, related_individual_id)\n   104→    )\n   105→  `\n   106→\n   107→  await sql`\n   108→    CREATE TABLE IF NOT EXISTS contact_history (\n   109→      id UUID PRIMARY KEY DEFAULT gen_random_uuid(),\n   110→      individual_id UUID NOT NULL REFERENCES individuals(id) ON DELETE CASCADE,\n   111→      date TIMESTAMPTZ NOT NULL,\n   112→      type TEXT NOT NULL CHECK (type IN ('email', 'phone_call', 'video_call', 'meeting', 'note')),\n   113→      summary TEXT NOT NULL,\n   114→      team_member TEXT,\n   115→      created_at TIMESTAMPTZ DEFAULT NOW(),\n   116→      updated_at TIMESTAMPTZ DEFAULT NOW()\n   117→    )\n   118→  `\n   119→\n   120→  await sql`\n   121→    CREATE TABLE IF NOT EXISTS deals (\n   122→      id UUID PRIMARY KEY DEFAULT gen_random_uuid(),\n   123→      name TEXT NOT NULL,\n   124→      client_id UUID NOT NULL REFERENCES clients(id) ON DELETE CASCADE,\n   125→      value NUMERIC(12, 2) DEFAULT 0,\n   126→      stage TEXT NOT NULL DEFAULT 'lead' CHECK (stage IN ('lead', 'qualification', 'discovery', 'proposal', 'negotiation', 'closed_won', 'closed_lost')),\n   127→      status TEXT NOT NULL DEFAULT 'active',\n   128→      owner TEXT,\n   129→      probability INTEGER DEFAULT 0,\n   130→      expected_close_date DATE,\n   131→      created_at TIMESTAMPTZ DEFAULT NOW(),\n   132→      updated_at TIMESTAMPTZ DEFAULT NOW()\n   133→    )\n   134→  `\n   135→\n   136→  await sql`\n   137→    CREATE TABLE IF NOT EXISTS deal_history (\n   138→      id UUID PRIMARY KEY DEFAULT gen_random_uuid(),\n   139→      deal_id UUID NOT NULL REFERENCES deals(id) ON DELETE CASCADE,\n   140→      old_stage TEXT NOT NULL,\n   141→      new_stage TEXT NOT NULL,\n   142→      changed_by TE