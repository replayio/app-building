  <div className=\"flex items-center justify-between\">\n    40→        <span className=\"text-[13px] font-medium text-text-primary\">{formatCurrency(deal.value)}</span>\n    41→        <DealStatusBadge status={deal.status} />\n    42→      </div>\n    43→    </div>\n    44→  )\n    45→}\n    46→\n    47→export function DealsPipelineView({ deals }: DealsPipelineViewProps) {\n    48→  const dealsByStage = PIPELINE_STAGES.map((stage) => ({\n    49→    ...stage,\n    50→    deals: deals.filter((d) => d.stage === stage.key),\n    51→    totalValue: deals\n    52→      .filter((d) => d.stage === stage.key)\n    53→      .reduce((sum, d) => sum + Number(d.value), 0),\n    54→  }))\n    55→\n    56→  return (\n    57→    <div className=\"grid grid-cols-6 gap-3 min-h-[400px]\" data-testid=\"deals-pipeline-view\">\n    58→      {dealsByStage.map((column) => (\n    59→        <div key={column.key} data-testid={`pipeline-column-${column.key}`} className={`flex flex-col border-t-2 ${column.color} rounded-t-none`}>\n    60→          <div className=\"px-2 py-2 mb-2\">\n    61→            <div className=\"flex items-center justify-between mb-0.5\">\n    62→              <span className=\"text-[13px] font-medium text-text-primary\">{column.label}</span>\n    63→              <span className=\"text-[11px] text-text-muted bg-hover rounded-full px-1.5 py-0.5\">\n    64→                {column.deals.length}\n    65→              </span>\n    66→            </div>\n    67→            <p className=\"text-[12px] text-text-muted\">{formatCurrency(column.totalValue)}</p>\n    68→          </div>\n    69→          <div className=\"flex flex-col gap-2 px-1 pb-2 flex-1 overflow-y-auto\">\n    70→            {column.deals.map((deal) => (\n    71→              <DealCard key={deal.id} deal={deal} />\n    72→            ))}\n    73→          </div>\n    74→        </div>\n    75→      ))}\n    76→    </div>\n    77→  )\n    78→}\n    79→"}]},"parent_tool_use_id":"toolu_01Fc41edjEf5rYyW8fLNSRxo","s