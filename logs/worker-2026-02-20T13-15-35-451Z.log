→        await dealOption.click();\n    70→      }\n    71→    }\n    72→\n    73→    await page.getByTestId('create-task-save').click();\n    74→    await expect(modal).not.toBeVisible();\n    75→    await page.waitForLoadState('networkidle');\n    76→\n    77→    // Navigate to the created task's detail page\n    78→    const createdCard = page.locator(`[data-testid^=\"task-card-\"]`, { hasText: taskTitle });\n    79→    await expect(createdCard).toBeVisible({ timeout: 10000 });\n    80→    await createdCard.click();\n    81→    await page.waitForLoadState('networkidle');\n    82→    await expect(page.getByTestId('task-detail-page')).toBeVisible();\n    83→\n    84→    // Title visible\n    85→    await expect(page.getByTestId('task-detail-title')).toBeVisible();\n    86→    await expect(page.getByTestId('task-detail-title')).toContainText(taskTitle);\n    87→\n    88→    // Priority badge visible\n    89→    await expect(page.locator('[data-testid^=\"task-priority-badge-\"]')).toBeVisible();\n    90→\n    91→    // Description visible\n    92→    await expect(page.getByTestId('task-detail-description')).toBeVisible();\n    93→    await expect(page.getByTestId('task-detail-description')).toContainText('This is a test task description');\n    94→\n    95→    // Status visible\n    96→    await expect(page.getByTestId('task-detail-status')).toBeVisible();\n    97→    const status = await page.getByTestId('task-detail-status').textContent();\n    98→    expect(['Open', 'Completed']).toContain(status!.trim());\n    99→\n   100→    // Due date visible\n   101→    await expect(page.getByTestId('task-detail-due-date')).toBeVisible();\n   102→\n   103→    // Assignee visible\n   104→    await expect(page.getByTestId('task-detail-assignee')).toBeVisible();\n   105→\n   106→    // Client name visible\n   107→    await expect(page.getByTestId('task-detail-client')).toBeVisible();\n   108→\n   109→    // Deal name visible (if a deal was selected during creation)\n   110→    c