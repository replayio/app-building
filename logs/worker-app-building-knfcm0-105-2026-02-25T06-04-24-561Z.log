{\n    43→        return new Response(JSON.stringify({ error: \"Not found\" }), { status: 404 });\n    44→      }\n    45→      return new Response(JSON.stringify(parseNumericFields(rows[0])), {\n    46→        headers: { \"Content-Type\": \"application/json\" },\n    47→      });\n    48→    }\n    49→\n    50→    if (req.method === \"DELETE\" && id) {\n    51→      await sql`DELETE FROM accounts WHERE id = ${id}`;\n    52→      return new Response(JSON.stringify({ success: true }), {\n    53→        headers: { \"Content-Type\": \"application/json\" },\n    54→      });\n    55→    }\n    56→\n    57→    return new Response(JSON.stringify({ error: \"Method not allowed\" }), { status: 405 });\n    58→  } catch (err) {\n    59→    console.error(\"accounts function error:\", err);\n    60→    return new Response(JSON.stringify({ error: \"Internal server error\" }), { status: 500 });\n    61→  }\n    62→}\n    63→"}]},"parent_tool_use_id":"toolu_01TU2wZRTPp1LDGEeXsxP8rM","session_id":"4783eb68-04ed-4dbc-b5a6-99a5bde1b6b3","uuid":"eb3b275d-61dd-494c-92fa-13d28a111f83"}
