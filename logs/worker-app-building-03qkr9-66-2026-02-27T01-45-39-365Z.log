ipmentModal\n    49→          onClose={() => setShowModal(false)}\n    50→          onSubmit={async (data) => {\n    51→            await dispatch(createEquipment(data));\n    52→            setShowModal(false);\n    53→          }}\n    54→        />\n    55→      )}\n    56→    </div>\n    57→  );\n    58→}\n    59→\n    60→interface AddEquipmentModalProps {\n    61→  onClose: () => void;\n    62→  onSubmit: (data: Partial<Equipment>) => Promise<void>;\n    63→}\n    64→\n    65→function AddEquipmentModal({ onClose, onSubmit }: AddEquipmentModalProps) {\n    66→  const [title, setTitle] = useState(\"\");\n    67→  const [description, setDescription] = useState(\"\");\n    68→  const [availableUnits, setAvailableUnits] = useState(\"\");\n    69→  const [submitting, setSubmitting] = useState(false);\n    70→\n    71→  const handleSubmit = async () => {\n    72→    if (!title.trim()) return;\n    73→    setSubmitting(true);\n    74→    await onSubmit({\n    75→      name: title.trim(),\n    76→      description: description.trim(),\n    77→      available_units: parseInt(availableUnits, 10) || 0,\n    78→    });\n    79→    setSubmitting(false);\n    80→  };\n    81→\n    82→  return (\n    83→    <div className=\"modal-overlay\" data-testid=\"add-equipment-modal\" onClick={onClose}>\n    84→      <div className=\"modal-content\" onClick={(e) => e.stopPropagation()}>\n    85→        <div className=\"modal-header\">\n    86→          <h2 className=\"modal-title\">Add Equipment</h2>\n    87→          <button\n    88→            className=\"modal-close-btn\"\n    89→            data-testid=\"add-equipment-modal-close\"\n    90→            onClick={onClose}\n    91→          >\n    92→            <svg\n    93→              width=\"16\"\n    94→              height=\"16\"\n    95→              viewBox=\"0 0 24 24\"\n    96→              fill=\"none\"\n    97→              stroke=\"currentColor\"\n    98→              strokeWidth=\"2\"\n    99→              strokeLinecap=\"round\"\n   100→              strokeLinejoin=\"round\"\n   101→            >\n   102→              <line x1=\"18\" y1=\"6\" x2=\"6\" y2=\"18\" />\n   103→              <line x1=\"6\" y1=\"6\" x2=\"18\" y2=\"18\" />\n   104→            </svg>\n   105→          </button>\n   106→        </div>\n   107→        <div className=\"form-group\">\n   108→          <label className=\"form-label\">Title</label>\n   109→          <input\n   110→            className=\"form-input\"\n   111→            data-testid=\"add-equipment-title-input\"\n   112→            type=\"text\"\n   113→            value={title}\n   114→            onChange={(e) => setTitle(e.target.value)}\n   115→            placeholder=\"Equipment name\"\n   116→          />\n   117→        </div>\n   118→        <div className=\"form-group\">\n   119→          <label className=\"form-label\">Description</label>\n   120→          <textarea\n   121→            className=\"form-textarea\"\n   122→            data-testid=\"add-equipment-description-input\"\n   123→            value={description}\n   124→            onChange={(e) => setDescription(e.target.value)}\n   125→            placeholder=\"Equipment description\"\n   126→          />\n   127→        </div>\n   128→        <div className=\"form-group\">\n   129→          <label className=\"form-label\">Available Units</label>\n   130→          <input\n   131→            className=\"form-input\"\n   132→            data-testid=\"add-equipment-units-input\"\n   133→            type=\"number\"\n   134→            min=\"0\"\n   135→            value={availableUnits}\n   136→            onChange={(e) => setAvailableUnits(e.target.value)}\n   137→            placeholder=\"0\"\n   138→          />\n   139→        </div>\n   140→        <div className=\"modal-footer\">\n   141→          <button\n   142→            className=\"btn-secondary\"\n   143→            data-testid=\"add-equipment-cancel-btn\"\n   144→            onClick={onClose}\n   145→          >\n   146→            Cancel\n   147→          </button>\n   148→          <button\n   149→            className=\"btn-primary\"\n   150→            data-testid=\"add-equipment-submit-btn\"\n   151→            disabled={!title.trim() || submitting}\n   152→            onClick={handleSubmit}\n   153→          >\n   154→            {submitting ? \"Adding...\" : \"Add Equipment\"}\n   155→          </button>\n   156→        </div>\n   157→      </div>\n   158→    </div>\n   159→  );\n   160→}\n   161→"}]},"parent_tool_use_id":"toolu_01QRRC36CaLjEpZUxRhHjqe9","session_id":"9d6d98ee-182e-4820-8eff-19beb6d76f81","uuid":"b22049c2-30a4-4237-9b40-6b153f9960f6"}
