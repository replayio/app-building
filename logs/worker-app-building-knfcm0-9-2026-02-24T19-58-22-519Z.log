rumb\">\n      <a\n        href=\"#\"\n        onClick={(e) => {\n          e.preventDefault();\n          navigate(\"/\");\n        }}\n      >\n        Home\n      </a>\n      <span className=\"breadcrumb-separator\">&gt;</span>\n      <span className=\"breadcrumb-current\">{currentLabel}</span>\n    </div>\n  );\n}\n","structuredPatch":[{"oldStart":56,"oldLines":7,"newStart":56,"newLines":7,"lines":[" ","           <div className=\"navbar-user\" data-testid=\"navbar-user\">","             <div className=\"navbar-avatar\" data-testid=\"navbar-avatar\">","-              {user?.name ? user.name.split(\" \").map((n) => n[0]).join(\"\") : \"U\"}","+              {user?.name ? user.name.split(\" \").map((n: string) => n[0]).join(\"\") : \"U\"}","             </div>","             <span className=\"navbar-user-name\" data-testid=\"navbar-user-name\">","               {user?.name ?? \"John Doe\"}"]}],"userModified":false,"replaceAll":false}}
