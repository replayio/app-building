50→\n    51→  /* Priority colors */\n    52→  --color-priority-high: #ef4444;\n    53→  --color-priority-high-bg: rgba(239, 68, 68, 0.1);\n    54→  --color-priority-medium: #f59e0b;\n    55→  --color-priority-medium-bg: rgba(245, 158, 11, 0.1);\n    56→  --color-priority-low: #22c55e;\n    57→  --color-priority-low-bg: rgba(34, 197, 94, 0.1);\n    58→  --color-priority-normal: #14b8a6;\n    59→  --color-priority-normal-bg: rgba(20, 184, 166, 0.1);\n    60→\n    61→  /* Deal stage colors */\n    62→  --color-stage-lead: #94a3b8;\n    63→  --color-stage-lead-bg: rgba(148, 163, 184, 0.1);\n    64→  --color-stage-qualified: #7180ff;\n    65→  --color-stage-qualified-bg: rgba(113, 128, 255, 0.1);\n    66→  --color-stage-proposal: #f59e0b;\n    67→  --color-stage-proposal-bg: rgba(245, 158, 11, 0.1);\n    68→  --color-stage-negotiation: #9577ff;\n    69→  --color-stage-negotiation-bg: rgba(149, 119, 255, 0.1);\n    70→  --color-stage-closed-won: #22c55e;\n    71→  --color-stage-closed-won-bg: rgba(34, 197, 94, 0.1);\n    72→  --color-stage-closed-lost: #ef4444;\n    73→  --color-stage-closed-lost-bg: rgba(239, 68, 68, 0.1);\n    74→}\n    75→\n    76→* {\n    77→  margin: 0;\n    78→  padding: 0;\n    79→  box-sizing: border-box;\n    80→}\n    81→\n    82→body {\n    83→  font-family: var(--font-sans);\n    84→  font-size: 16px;\n    85→  font-weight: 400;\n    86→  color: var(--color-text-primary);\n    87→  background-color: var(--color-bg-base);\n    88→  -webkit-font-smoothing: antialiased;\n    89→  -moz-osx-font-smoothing: grayscale;\n    90→}\n    91→\n    92→#root {\n    93→  display: flex;\n    94→  min-height: 100vh;\n    95→}\n    96→"}]},"parent_tool_use_id":"toolu_01XPF4ktYcAktkczr51HtCbq","session_id":"bea94831-e89c-4643-a613-7ea0a569818f","uuid":"78e9422b-c170-433a-b4f1-c7b449a0bbdd"}
