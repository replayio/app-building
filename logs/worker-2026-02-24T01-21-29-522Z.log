[2026-02-24T01:08:52.501Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01HjmcvcUXFEAPvnvq9qV8eE","type":"tool_result","content":"     1→import { clsx, type ClassValue } from 'clsx'\n     2→import { twMerge } from 'tailwind-merge'\n     3→\n     4→export function cn(...inputs: ClassValue[]) {\n     5→  return twMerge(clsx(inputs))\n     6→}\n     7→\n     8→export function formatCurrency(value: number | null | undefined): string {\n     9→  if (value == null) return '$0'\n    10→  return new Intl.NumberFormat('en-US', {\n    11→    style: 'currency',\n    12→    currency: 'USD',\n    13→    minimumFractionDigits: 0,\n    14→    maximumFractionDigits: 0,\n    15→  }).format(value)\n    16→}\n    17→\n    18→export function formatDate(date: string | null | undefined): string {\n    19→  if (!date) return ''\n    20→  return new Intl.DateTimeFormat('en-US', {\n    21→    month: 'short',\n    22→    day: 'numeric',\n    23→    year: 'numeric',\n    24→  }).format(new Date(date))\n    25→}\n    26→\n    27→export function formatDateTime(date: string | null | undefined): string {\n    28→  if (!date) return ''\n    29→  return new Intl.DateTimeFormat('en-US', {\n    30→    month: 'short',\n    31→    day: 'numeric',\n    32→    year: 'numeric',\n    33→    hour: 'numeric',\n    34→    minute: '2-digit',\n    35→  }).format(new Date(date))\n    36→}\n    37→\n    38→export function getInitials(name: string): string {\n    39→  return name\n    40→    .split(' ')\n    41→    .map((n) => n[0])\n    42→    .join('')\n    43→    .toUpperCase()\n    44→    .slice(0, 2)\n    45→}\n    46→\n    47→export function formatDueDate(date: string | null | undefined): string {\n    48→  if (!date) return ''\n    49→  const d = new Date(date)\n    50→  const now = new Date()\n    51→  const today = new Date(now.getFullYear(), now.getMonth(), now.getDate())\n    52→  const tomorrow = new Date(today)\n    53→  tomorrow.setDate(tomorrow.getDate() + 1)\n    54→  const dateOnly = new Date(d.getFullYear(), d.getMonth(), d.getDate())\n    55→  const timeStr = d.toLocaleTimeString('en-US', { hour: 'numeric', minute: '2-digit' })\n    56→\n    57→  if (dateOnly.getTime() === today.getTime()) {\n    58→    return `Today, ${timeStr}`\n    59→  }\n    60→  if (dateOnly.getTime() === tomorrow.getTime()) {\n    61→    return `Tomorrow, ${timeStr}`\n    62→  }\n    63→  return new Intl.DateTimeFormat('en-US', {\n    64→    month: 'short',\n    65→    day: 'numeric',\n    66→    year: 'numeric',\n    67→  }).format(d)\n    68→}\n    69→\n    70→export function getAuthHeader(): Record<string, string> {\n    71→  const token = localStorage.getItem('token')\n    72→  return token ? { Authorization: `Bearer ${token}` } : {}\n    73→}\n    74→"}]},"parent_tool_use_id":"toolu_01XPF4ktYcAktkczr51HtCbq","session_id":"bea94831-e89c-4643-a613-7ea0a569818f","uuid":"1061aed9-b71f-4f7f-ab0e-4d5e8b9c24b9"}
