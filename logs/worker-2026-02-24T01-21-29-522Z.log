6→        <div\n    47→          data-testid=\"user-avatar\"\n    48→          className=\"w-6 h-6 rounded-full bg-[var(--color-accent)] text-[var(--color-text-inverse)] flex items-center justify-center text-[11px] font-medium flex-shrink-0\"\n    49→        >\n    50→          {user.name.charAt(0).toUpperCase()}\n    51→        </div>\n    52→        <span data-testid=\"user-name\" className=\"text-[13px] text-[var(--color-text-secondary)] truncate flex-1\">\n    53→          {user.name}\n    54→        </span>\n    55→        <button\n    56→          data-testid=\"sign-out-button\"\n    57→          onClick={handleSignOut}\n    58→          className=\"p-1 rounded hover:bg-[var(--color-hover)] text-[var(--color-text-muted)]\"\n    59→        >\n    60→          <LogOut size={14} />\n    61→        </button>\n    62→      </div>\n    63→    )\n    64→  }\n    65→\n    66→  if (!showForm) {\n    67→    return (\n    68→      <div className=\"px-3 py-2\">\n    69→        <button\n    70→          data-testid=\"sign-in-button\"\n    71→          onClick={() => {\n    72→            setShowForm(true)\n    73→            setIsSignUp(false)\n    74→            setEmail('')\n    75→            setPassword('')\n    76→            setName('')\n    77→            setMessage('')\n    78→            dispatch(clearError())\n    79→          }}\n    80→          className=\"text-[13px] text-[var(--color-text-secondary)] hover:text-[var(--color-text-primary)] transition-colors\"\n    81→        >\n    82→          Sign In\n    83→        </button>\n    84→      </div>\n    85→    )\n    86→  }\n    87→\n    88→  return (\n    89→    <div data-testid=\"auth-form\" className=\"px-3 py-2\">\n    90→      <form onSubmit={handleSubmit} className=\"flex flex-col gap-2\">\n    91→        {isSignUp && (\n    92→          <input\n    93→            data-testid=\"auth-name-input\"\n    94→            type=\"text\"\n    95→            placeholder=\"Name\"\n    96→            value={name}\n    97→            onChange={(e) => setName(e.target.value)}\n    98→            className=\"h-7 px-2 text-[13px] border border-[var(--color-bg-border)] rounded bg-white focus:outline-none focus:border-[var(--color-accent)]\"\n    99→          />\n   100→        )}\n   101→        <input\n   102→          data-testid=\"auth-email-input\"\n   103→          type=\"email\"\n   104→          placeholder=\"Email\"\n   105→          value={email}\n   106→          onChange={(e) => setEmail(e.target.value)}\n   107→          className=\"h-7 px-2 text-[13px] border border-[var(--color-bg-border)] rounded bg-white focus:outline-none focus:border-[var(--color-accent)]\"\n   108→          required\n   109→        />\n   110→        <input\n   111→          data-testid=\"auth-password-input\"\n   112→          type=\"password\"\n   113→          placeholder=\"Password\"\n   114→          value={password}\n   115→          onChange={(e) => setPassword(e.target.value)}\n   116→          className=\"h-7 px-2 text-[13px] border border-[var(--color-bg-border)] rounded bg-white focus:outline-none focus:border-[var(--color-accent)]\"\n   117→          required\n   118→        />\n   119→        <button\n   120→          data-testid=\"auth-submit-button\"\n   121→          type=\"submit\"\n   122→          disabled={loading}\n   123→          className=\"h-7 px-3 text-[13px] font-medium bg-[var(--color-accent)] text-white rounded hover:opacity-90 disabled:opacity-60\"\n   124→        >\n   125→          {loading ? '...' : isSignUp ? 'Sign Up' : 'Sign In'}\n   126→        </button>\n   127→        {error && (\n   128→          <p data-testid=\"auth-error\" className=\"text-[11px] text-red-500\">{error}</p>\n   129→        )}\n   130→        {message && (\n   131→          <p data-testid=\"auth-message\" className=\"text-[11px] text-[var(--color-text-muted)]\">{message}</p>\n   132→        )}\n   133→        <div className=\"flex items-center justify-between\">\n   134→          <button\n   135→            data-testid=\"forgot-password-link\"\n   136→            type=\"button\"\n   137→            onClick={() => navigate('/auth/forgot-password')}\n   138→            className=\"text-[11px] text-[var(--color-text-muted)] hover:text-[var(--color-accent)]\"\n   139→          >\n   140→            Forgot password?\n   141→          </button>\n   142→          <button\n   143→            data-testid=\"auth-toggle\"\n   144→            type=\"button\"\n   145→            onClick={() => {\n   146→              setIsSignUp(!isSignUp)\n   147→              dispatch(clearError())\n   148→              setMessage('')\n   149→            }}\n   150→            className=\"text-[11px] text-[var(--color-accent)]\"\n   151→          >\n   152→            {isSignUp ? 'Sign In' : 'Sign Up'}\n   153→          </button>\n   154→        </div>\n   155→      </form>\n   156→    </div>\n   157→  )\n   158→}\n   159→"}]},"parent_tool_use_id":"toolu_01XPF4ktYcAktkczr51HtCbq","session_id":"bea94831-e89c-4643-a613-7ea0a569818f","uuid":"2f6bb7bd-f598-4c33-855e-6556944cf68f"}
