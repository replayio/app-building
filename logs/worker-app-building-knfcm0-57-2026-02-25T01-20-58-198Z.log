[2026-02-25T01:08:06.341Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01XJSNtdh5BAQJvW5DtGWk9G","type":"tool_result","content":"     1→import { test, expect } from \"@playwright/test\";\n     2→\n     3→test.describe(\"RecipesHeader\", () => {\n     4→  test('REC-HDR-1: Page title displays \"Recipes\"', async ({ page }) => {\n     5→    await page.goto(\"/recipes\");\n     6→    await expect(page.getByTestId(\"recipes-page-title\")).toBeVisible({ timeout: 30000 });\n     7→    await expect(page.getByTestId(\"recipes-page-title\")).toHaveText(\"Recipes\");\n     8→    await expect(page.getByTestId(\"recipes-page-route\")).toHaveText(\"/recipes\");\n     9→  });\n    10→\n    11→  test(\"REC-HDR-2: Search bar is displayed with placeholder text\", async ({ page }) => {\n    12→    await page.goto(\"/recipes\");\n    13→    await expect(page.getByTestId(\"recipes-page-title\")).toBeVisible({ timeout: 30000 });\n    14→\n    15→    const searchInput = page.getByTestId(\"recipes-search-input\");\n    16→    await expect(searchInput).toBeVisible();\n    17→    await expect(searchInput).toHaveAttribute(\"placeholder\", \"Search by name or product...\");\n    18→  });\n    19→\n    20→  test(\"REC-HDR-3: Search bar filters recipes by name\", async ({ page }) => {\n    21→    await page.goto(\"/recipes\");\n    22→    await expect(page.getByTestId(\"recipes-table\")).toBeVisible({ timeout: 30000 });\n    23→\n    24→    await page.getByTestId(\"recipes-search-input\").fill(\"Granola\");\n    25→\n    26→    // Only \"Organic Granola Mix\" matches (name contains \"Granola\")\n    27→    await expect(page.locator('[data-testid^=\"recipe-row-\"]')).toHaveCount(1);\n    28→    await expect(\n    29→      page.locator('[data-testid^=\"recipe-row-\"]').filter({ hasText: \"Organic Granola Mix\" })\n    30→    ).toBeVisible();\n    31→  });\n    32→\n    33→  test(\"REC-HDR-4: Search bar filters recipes by product\", async ({ page }) => {\n    34→    await page.goto(\"/recipes\");\n    35→    await expect(page.getByTestId(\"recipes-table\")).toBeVisible({ timeout: 30000 });\n    36→\n    37→    // \"Jam\" matches product \"Strawberry Jam\" but no recipe name contains \"Jam\"\n    38→    await page.getByTestId(\"recipes-search-input\").fill(\"Jam\");\n    39→\n    40→    await expect(page.locator('[data-testid^=\"recipe-row-\"]')).toHaveCount(1);\n    41→    await expect(\n    42→      page.locator('[data-testid^=\"recipe-row-\"]').filter({ hasText: \"Fruit Preserve\" })\n    43→    ).toBeVisible();\n    44→  });\n    45→\n    46→  test(\"REC-HDR-5: Clearing the search bar restores all recipes\", async ({ page }) => {\n    47→    await page.goto(\"/recipes\");\n    48→    await expect(page.getByTestId(\"recipes-table\")).toBeVisible({ timeout: 30000 });\n    49→\n    50→    // Filter to one result\n    51→    await page.getByTestId(\"recipes-search-input\").fill(\"Granola\");\n    52→    await expect(page.locator('[data-testid^=\"recipe-row-\"]')).toHaveCount(1);\n    53→\n    54→    // Clear search\n    55→    await page.getByTestId(\"recipes-search-input\").fill(\"\");\n    56→\n    57→    // Page 1 shows PAGE_SIZE (5) recipes\n    58→    await expect(page.locator('[data-testid^=\"recipe-row-\"]')).toHaveCount(5, { timeout: 15000 });\n    59→  });\n    60→\n    61→  test('REC-HDR-6: Filter Status dropdown is displayed with default value \"(All)\"', async ({ page }) => {\n    62→    await page.goto(\"/recipes\");\n    63→    await expect(page.getByTestId(\"recipes-header\")).toBeVisible({ timeout: 30000 });\n    64→\n    65→    const filterBtn = page.getByTestId(\"recipes-status-filter-btn\");\n    66→    await expect(filterBtn).toBeVisible();\n    67→    await expect(filterBtn).toContainText(\"(All)\");\n    68→  });\n    69→\n    70→  test(\"REC-HDR-7: Filter Status dropdown shows status options\", async ({ page }) => {\n    71→    await page.goto(\"/recipes\");\n    72→    await expect(page.getByTestId(\"recipes-header\")).toBeVisible({ timeout: 30000 });\n    73→\n    74→    await page.getByTestId(\"recipes-status-filter-btn\").click();\n    75→\n    76→    await expect(page.getByTestId(\"recipes-status-dropdown\")).toBeVisible();\n    77→    await expect(page.getByTestId(\"recipes-status-option-all\")).toBeVisible();\n    78→    await expect(page.getByTestId(\"recipes-status-option-active\")).toBeVisible();\n    79→    await expect(page.getByTestId(\"recipes-status-option-draft\")).toBeVisible();\n    80→    await expect(page.getByTestId(\"recipes-status-option-archived\")).toBeVisible();\n    81→  });\n    82→\n    83→  test('REC-HDR-8: Selecting \"Active\" filter shows only active recipes', async ({ page }) => {\n    84→    await page.goto(\"/recipes\");\n    85→    await expect(page.getByTestId(\"recipes-table\")).toBeVisible({ timeout: 30000 });\n    86→\n    87→    await page.getByTestId(\"recipes-status-filter-btn\").click();\n    88→    await page.getByTestId(\"recipes-status-option-active\").click();\n    89→\n    90→    // Wait for filter to apply - active recipes should be visible\n    91→    await expect(page.locator('[data-testid^=\"recipe-row-\"]').first()).toBeVisible();\n    92→\n    93→    // Draft recipes should not appear\n    94→    await expect(\n    95→      page.locator('[data-testid^=\"recipe-row-\"]').filter({ hasText: \"Trail Mix Classic\" })\n    96→    ).toHaveCount(0);\n    97→    await expect(\n    98→      page.locator('[data-testid^=\"recipe-row-\"]').filter({ hasText: \"Yogurt Bark Bites\" })\n    99→    ).toHaveCount(0);\n   100→    // Archived recipes should not appear"}]},"parent_tool_use_id":"toolu_01CEqxub9ufAmBRDT4PDEPTC","session_id":"df852bbf-dea6-4a14-9d4a-64c1c633d656","uuid":"abeb0b81-5fbc-448c-aa89-b8d9a18f3751"}
