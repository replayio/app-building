[2026-02-24T02:46:13.785Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01Gw3RHuZp8PKLmcZbywh7WM","type":"tool_result","content":"     1→import { useState, useRef, useEffect } from 'react'\n     2→import { useNavigate } from 'react-router-dom'\n     3→import { MoreHorizontal, Eye, Pencil, CheckCircle, Trash2 } from 'lucide-react'\n     4→import { useAppDispatch, useAppSelector } from '@/store/hooks'\n     5→import { deleteTask, updateTask, fetchTasks, type TaskRow } from '@/store/tasksSlice'\n     6→import TaskFormModal from './TaskFormModal'\n     7→\n     8→const priorityClasses: Record<string, string> = {\n     9→  High: 'priority-high',\n    10→  Medium: 'priority-medium',\n    11→  Low: 'priority-low',\n    12→  Normal: 'priority-normal',\n    13→}\n    14→\n    15→function formatDueDate(dateStr: string | null): string {\n    16→  if (!dateStr) return ''\n    17→  const date = new Date(dateStr)\n    18→  if (isNaN(date.getTime())) return dateStr\n    19→  const today = new Date()\n    20→  const tomorrow = new Date(today)\n    21→  tomorrow.setDate(tomorrow.getDate() + 1)\n    22→\n    23→  const time = date.toLocaleTimeString('en-US', { hour: 'numeric', minute: '2-digit' })\n    24→\n    25→  if (date.toDateString() === today.toDateString()) return `Due: Today, ${time}`\n    26→  if (date.toDateString() === tomorrow.toDateString()) return `Due: Tomorrow, ${time}`\n    27→  return `Due: ${date.toLocaleDateString('en-US', { month: 'short', day: 'numeric', year: 'numeric' })}`\n    28→}\n    29→\n    30→function abbreviateName(fullName: string): string {\n    31→  const parts = fullName.trim().split(/\\s+/)\n    32→  if (parts.length <= 1) return fullName\n    33→  return `${parts[0]} ${parts[parts.length - 1][0]}.`\n    34→}\n    35→\n    36→function getInitials(name: string): string {\n    37→  const parts = name.trim().split(/\\s+/)\n    38→  if (parts.length === 1) return parts[0][0]?.toUpperCase() || '?'\n    39→  return (parts[0][0] + parts[parts.length - 1][0]).toUpperCase()\n    40→}\n    41→\n    42→function TaskActionMenu({ task, onEdit, onComplete, onDelete }: {\n    43→  task: TaskRow\n    44→  onEdit: () => void\n    45→  onComplete: () => void\n    46→  onDelete: () => void\n    47→}) {\n    48→  const navigate = useNavigate()\n    49→  const [open, setOpen] = useState(false)\n    50→  const ref = useRef<HTMLDivElement>(null)\n    51→\n    52→  useEffect(() => {\n    53→    const handler = (e: MouseEvent) => {\n    54→      if (ref.current && !ref.current.contains(e.target as Node)) setOpen(false)\n    55→    }\n    56→    document.addEventListener('mousedown', handler)\n    57→    return () => document.removeEventListener('mousedown', handler)\n    58→  }, [])\n    59→\n    60→  return (\n    61→    <div className=\"row-action-wrapper\" ref={ref}>\n    62→      <button\n    63→        className=\"row-action-trigger\"\n    64→        onClick={(e) => { e.stopPropagation(); setOpen(!open) }}\n    65→        data-testid={`task-actions-${task.id}`}\n    66→      >\n    67→        <MoreHorizontal size={16} />\n    68→      </button>\n    69→      {open && (\n    70→        <div className=\"row-action-menu\" data-testid={`task-action-menu-${task.id}`}>\n    71→          {task.client_id && (\n    72→            <button\n    73→              className=\"row-action-item\"\n    74→              onClick={() => { setOpen(false); navigate(`/clients/${task.client_id}`) }}\n    75→              data-testid={`task-action-view-${task.id}`}\n    76→            >\n    77→              <Eye size={14} />\n    78→              <span>View Details</span>\n    79→            </button>\n    80→          )}\n    81→          <button\n    82→            className=\"row-action-item\"\n    83→            onClick={() => { setOpen(false); onEdit() }}\n    84→            data-testid={`task-action-edit-${task.id}`}\n    85→          >\n    86→            <Pencil size={14} />\n    87→            <span>Edit</span>\n    88→          </button>\n    89→          <button\n    90→            className=\"row-action-item\"\n    91→            onClick={() => { setOpen(false); onComplete() }}\n    92→            data-testid={`task-action-complete-${task.id}`}\n    93→          >\n    94→            <CheckCircle size={14} />\n    95→            <span>Mark as Complete</span>\n    96→          </button>\n    97→          <button\n    98→            className=\"row-action-item danger\"\n    99→            onClick={() => { setOpen(false); onDelete() }}\n   100→            data-testid={`task-action-delete-${task.id}`}\n   101→          >\n   102→            <Trash2 size={14} />\n   103→            <span>Delete</span>\n   104→          </button>\n   105→        </div>\n   106→      )}\n   107→    </div>\n   108→  )\n   109→}\n   110→\n   111→function DeleteConfirmModal({ open, taskTitle, onConfirm, onCancel }: {\n   112→  open: boolean\n   113→  taskTitle: string\n   114→  onConfirm: () => void\n   115→  onCancel: () => void\n   116→}) {\n   117→  if (!open) return null\n   118→  return (\n   119→    <div className=\"modal-overlay\" onClick={onCancel} data-testid=\"task-delete-confirm-modal\">\n   120→      <div className=\"modal-content modal-small\" onClick={(e) => e.stopPropagation()}>\n   121→        <h2 className=\"modal-title\">Delete Task</h2>\n   122→        <p className=\"modal-text\" data-testid=\"task-delete-confirm-message\">\n   123→          Are you sure you want to delete this task?\n   124→        </p>\n   125→        <p className=\"modal-text\" style={{ fontWeight: 500 }}>{taskTitle}</p>\n   126→        <div className=\"modal-actions\">\n   127→          <button className=\"btn-secondary\" onClick={onCancel} data-testid=\"task-delete-cancel\">\n   128→            Cancel\n   129→          </button>\n   130→          <button className=\"btn-danger\" onClick={onConfirm} data-testid=\"task-delete-confirm\">\n   131→            Delete\n   132→          </button>\n   133→        </div>\n   134→      </div>\n   135→    </div>\n   136→  )\n   137→}\n   138→\n   139→function filterTasks(tasks: TaskRow[], filterType: string, filterText: string): TaskRow[] {\n   140→  if (!filterText.trim()) return tasks\n   141→  const q = filterText.toLowerCase()\n   142→\n   143→  return tasks.filter(task => {\n   144→    switch (filterType) {\n   145→      case 'priority':\n   146→        return task.priority.toLowerCase().includes(q)\n   147→      case 'assignee':\n   148→        return (task.assignee || '').toLowerCase().includes(q)\n   149→      case 'client':\n   150→        return (task.client_name || '').toLowerCase().includes(q)\n   151→      case 'due_date':\n   152→        return formatDueDate(task.due_date).toLowerCase().includes(q)\n   153→      case 'all':\n   154→      default:\n   155→        return (\n   156→          task.title.toLowerCase().includes(q) ||\n   157→          task.priority.toLowerCase().includes(q) ||\n   158→          (task.assignee || '').toLowerCase().includes(q) ||\n   159→          (task.client_name || '').toLowerCase().includes(q) ||\n   160→          formatDueDate(task.due_date).toLowerCase().includes(q)\n   161→        )\n   162→    }\n   163→  })\n   164→}\n   165→\n   166→export default function TasksList() {\n   167→  const navigate = useNavigate()\n   168→  const dispatch = useAppDispatch()\n   169→  const items = useAppSelector(s => s.tasks.items)\n   170→  const loading = useAppSelector(s => s.tasks.loading)\n   171→  const filterType = useAppSelector(s => s.tasks.filterType)\n   172→  const filterText = useAppSelector(s => s.tasks.filterText)\n   173→  const [editTask, setEditTask] = useState<TaskRow | null>(null)\n   174→  const [deleteTarget, setDeleteTarget] = useState<TaskRow | null>(null)\n   175→\n   176→  const filtered = filterTasks(items, filterType, filterText)\n   177→\n   178→  const handleUpdate = async (data: {\n   179→    title: string\n   180→    due_date: string\n   181→    priority: string\n   182→    assignee: string\n   183→    assignee_role: string\n   184→    client_id: string\n   185→    deal_id: string\n   186→  }) => {\n   187→    if (!editTask) return\n   188→    await dispatch(updateTask({\n   189→      id: editTask.id,\n   190→      title: data.title,\n   191→      due_date: data.due_date || undefined,\n   192→      priority: data.priority,\n   193→      assignee: data.assignee || undefined,\n   194→      assignee_role: data.assignee_role || undefined,\n   195→      client_id: data.client_id || undefined,\n   196→      deal_id: data.deal_id || undefined,\n   197→    })).unwrap()\n   198→    await dispatch(fetchTasks())\n   199→  }\n   200→\n   201→  const handleComplete = async (task: TaskRow) => {\n   202→    await dispatch(updateTask({ id: task.id, completed: true })).unwrap()\n   203→  }\n   204→\n   205→  const handleDelete = async () => {\n   206→    if (!deleteTarget) return\n   207→    await dispatch(deleteTask(deleteTarget.id)).unwrap()\n   208→    setDeleteTarget(null)\n   209→  }\n   210→\n   211→  if (loading) {\n   212→    return (\n   213→      <div className=\"tasks-empty\" data-testid=\"tasks-list-loading\">\n   214→        <p>Loading tasks...</p>\n   215→      </div>\n   216→    )\n   217→  }\n   218→\n   219→  if (items.length === 0) {\n   220→    return (\n   221→      <div className=\"tasks-empty\" data-testid=\"tasks-list-empty\">\n   222→        <p className=\"empty-title\">No upcoming tasks</p>\n   223→        <p className=\"empty-subtitle\">No upcoming tasks. Create a new task to get started.</p>\n   224→      </div>\n   225→    )\n   226→  }\n   227→\n   228→  if (filtered.length === 0) {\n   229→    return (\n   230→      <div className=\"tasks-empty\" data-testid=\"tasks-list-no-results\">\n   231→        <p className=\"empty-title\">No tasks found</p>\n   232→        <p className=\"empty-subtitle\">No tasks match your current filter.</p>\n   233→      </div>\n   234→    )\n   235→  }\n   236→\n   237→  return (\n   238→    <>\n   239→      <div className=\"tasks-list\" data-testid=\"tasks-list\">\n   240→        {filtered.map(task => (\n   241→          <div\n   242→            key={task.id}\n   243→            className=\"task-card\"\n   244→            data-testid={`task-card-${task.id}`}\n   245→          >\n   246→            <span\n   247→              className={`priority-badge ${priorityClasses[task.priority] || 'priority-normal'}`}\n   248→              data-testid={`task-priority-${task.id}`}\n   249→            >\n   250→              {task.priority}\n   251→            </span>\n   252→            <button\n   253→              className=\"task-name\"\n   254→              onClick={() => { if (task.client_id) navigate(`/clients/${task.client_id}`) }}\n   255→              data-testid={`task-name-${task.id}`}\n   256→            >\n   257→              {task.title}\n   258→            </button>\n   259→            <span className=\"task-due-date\" data-testid={`task-due-date-${task.id}`}>\n   260→              {task.due_date ? formatDueDate(task.due_date) : ''}\n   261→            </span>\n   262→            <div className=\"task-assignee\" data-testid={`task-assignee-${task.id}`}>\n   263→              {task.assignee && (\n   264→                <>\n   265→                  <span className=\"task-assignee-avatar\">{getInitials(task.assignee)}</span>\n   266→                  <span className=\"task-assignee-name\">\n   267→                    {abbreviateName(task.assignee)}\n   268→                    {task.assignee_role && ` (${task.assignee_role})`}\n   269→                  </span>\n   270→                </>\n   271→              )}\n   272→            </div>\n   273→            <div className=\"task-card-action\">\n   274→              <TaskActionMenu\n   275→                task={task}\n   276→                onEdit={() => setEditTask(task)}\n   277→                onComplete={() => handleComplete(task)}\n   278→                onDelete={() => setDeleteTarget(task)}\n   279→              />\n   280→            </div>\n   281→          </div>\n   282→        ))}\n   283→      </div>\n   284→\n   285→      <TaskFormModal\n   286→        open={!!editTask}\n   287→        onClose={() => setEditTask(null)}\n   288→        onSubmit={handleUpdate}\n   289→        title=\"Edit Task\"\n   290→        initial={editTask ? {\n   291→          title: editTask.title,\n   292→          due_date: editTask.due_date || '',\n   293→          priority: editTask.priority,\n   294→          assignee: editTask.assignee || '',\n   295→          assignee_role: editTask.assignee_role || '',\n   296→          client_id: editTask.client_id || '',\n   297→          deal_id: editTask.deal_id || '',\n   298→        } : undefined}\n   299→      />\n   300→\n   301→      <DeleteConfirmModal\n   302→        open={!!deleteTarget}\n   303→        taskTitle={deleteTarget?.title || ''}\n   304→        onConfirm={handleDelete}\n   305→        onCancel={() => setDeleteTarget(null)}\n   306→      />\n   307→    </>\n   308→  )\n   309→}\n   310→"}]},"parent_tool_use_id":"toolu_01MX4oQGThGzDEHTDkfrhWMB","session_id":"2f2d80b5-66a7-48bf-b8eb-37d709784302","uuid":"9e690202-c14f-449f-957b-8687a05f5f33"}
