sc\" | \"none\" {\n    51→  if (sort === \"close_desc\") return \"desc\";\n    52→  if (sort === \"close_asc\") return \"asc\";\n    53→  return \"none\";\n    54→}\n    55→\n    56→function getSortArrow(direction: \"desc\" | \"asc\" | \"none\") {\n    57→  if (direction === \"desc\") {\n    58→    return (\n    59→      <svg width=\"12\" height=\"12\" viewBox=\"0 0 12 12\" fill=\"none\">\n    60→        <path d=\"M6 2v8M3 7l3 3 3-3\" stroke=\"currentColor\" strokeWidth=\"1.3\" strokeLinecap=\"round\" strokeLinejoin=\"round\" />\n    61→      </svg>\n    62→    );\n    63→  }\n    64→  if (direction === \"asc\") {\n    65→    return (\n    66→      <svg width=\"12\" height=\"12\" viewBox=\"0 0 12 12\" fill=\"none\">\n    67→        <path d=\"M6 10V2M3 5l3-3 3 3\" stroke=\"currentColor\" strokeWidth=\"1.3\" strokeLinecap=\"round\" strokeLinejoin=\"round\" />\n    68→      </svg>\n    69→    );\n    70→  }\n    71→  return (\n    72→    <svg width=\"12\" height=\"12\" viewBox=\"0 0 12 12\" fill=\"none\">\n    73→      <path d=\"M6 2v8M3 5l3-3 3 3M3 7l3 3 3-3\" stroke=\"currentColor\" strokeWidth=\"1.3\" strokeLinecap=\"round\" strokeLinejoin=\"round\" />\n    74→    </svg>\n    75→  );\n    76→}\n    77→\n    78→function ActionsMenu({ deal, onEdit }: { deal: Deal; onEdit: () => void }) {\n    79→  const [open, setOpen] = useState(false);\n    80→  const [confirmDelete, setConfirmDelete] = useState(false);\n    81→  const dispatch = useDispatch<AppDispatch>();\n    82→  const navigate = useNavigate();\n    83→  const menuRef = useRef<HTMLDivElement>(null);\n    84→\n    85→  const handleClickOutside = useCallback((e: MouseEvent) => {\n    86→    if (menuRef.current && !menuRef.current.contains(e.target as Node)) {\n    87→      setOpen(false);\n    88→    }\n    89→  }, []);\n    90→\n    91→  useEffect(() => {\n    92→    if (open) {\n    93→      document.addEventListener(\"mousedown\", handleClickOutside);\n    94→    }\n    95→    return () => {\n    96→      document.removeEventListener(\"mousedown\", handleClickOutside);\n    97→    };\n    98→  }, [open, handleClickOutside]);\n    99→\n   100→  return (\n   101→    <div className=\"clients-actions-wrapper\" ref={menuRef} style={{ position: \"relative\", zIndex: open ? 50 : 1 }}>\n   102→      <button\n   103→        className=\"btn btn--ghost clients-actions-btn\"\n   104→        data-testid={`deal-actions-${deal.id}`}\n   105→        onClick={(e) => {\n   106→          e.stopPropagation();\n   107→          setOpen(!open);\n   108→        }}\n   109→        type=\"button\"\n   110→      >\n   111→        <svg width=\"14\" height=\"14\" viewBox=\"0 0 14 14\" fill=\"none\">\n   112→          <circle cx=\"7\" cy=\"3\" r=\"1\" fill=\"currentColor\" />\n   113→          <circle cx=\"7\" cy=\"7\" r=\"1\" fill=\"currentColor\" />\n   114→          <circle cx=\"7\" cy=\"11\" r=\"1\" fill=\"currentColor\" />\n   115→        </svg>\n   116→      </button>\n   117→      {open && (\n   118→        <div className=\"action-menu\" data-testid={`deal-menu-${deal.id}`}>\n   119→          <button\n   120→            className=\"action-menu-item\"\n   121→            data-testid={`deal-edit-${deal.id}`}\n   122→            onClick={(e) => {\n   123→              e.stopPropagation();\n   124→              setOpen(false);\n   125→              onEdit();\n   126→            }}\n   127→            type=\"button\"\n   128→          >\n   129→            Edit\n   130→          </button>\n   131→          <button\n   132→            className=\"action-menu-item\"\n   133→            data-testid={`deal-view-${deal.id}`}\n   134→            onClick={(e) => {\n   135→              e.stopPropagation();\n   136→              setOpen(false);\n   137→              navigate(`/deals/${deal.id}`);\n   138→            }}\n   139→            type=\"button\"\n   140→          >\n   141→            View Details\n   142→          </button>\n   143→          <button\n   144→            className=\"action-menu-item action-menu-item--danger\"\n   145→            data-testid={`deal-delete-${deal.id}`}\n   146→            onClick={(e) => {\n   147→              e.stopPropagation();\n   148→              setOpen(false);\n   149→              setConfirmDelete(true);\n   150→            }}\n   151→            type=\"button\"\n   152→          >\n   153→            Delete\n   154→          </button>\n   155→        </div>\n   156→      )}\n   157→      <ConfirmDialog\n   158→        open={confirmDelete}\n   159→        title=\"Delete Deal\"\n   160→        message={`Are you sure you want to delete \"${deal.name}\"? This action cannot be undone.`}\n   161→        confirmLabel=\"Delete\"\n   162→        cancelLabel=\"Cancel\"\n   163→        onConfirm={() => {\n   164→          dispatch(deleteDeal(deal.id));\n   165→          setConfirmDelete(false);\n   166→        }}\n   167→        onCancel={() => setConfirmDelete(false)}\n   168→      />\n   169→    </div>\n   170→  );\n   171→}\n   172→\n   173→export function DealsTable({ deals, sort, onSortChange, onEditDeal }: DealsTableProps) {\n   174→  const navigate = useNavigate();\n   175→  const sortDirection = getSortDirection(sort);\n   176→\n   177→  function handleCloseDateSort() {\n   178→    if (sort === \"close_desc\") {\n   179→      onSortChange(\"close_asc\");\n   180→    } else {\n   181→      onSortChange(\"close_desc\");\n   182→    }\n   183→  }\n   184→\n   185→  if (deals.length === 0) {\n   186→    return (\n   187→      <div className=\"deals-empty\" data-testid=\"deals-table-empty\">\n   188→        <p>No deals found matching your filters.</p>\n   189→      </div>\n   190→    );\n   191→  }\n   192→\n   193→  return (\n   194→    <div data-testid=\"deals-table\">\n   195→      <table className=\"data-table\">\n   196→        <thead>\n   197→          <tr>\n   198→            <th>Deal Name</th>\n   199→            <th className=\"max-md:hidden\">Client</th>\n   200→            <th className=\"max-md:hidden\">Stage</th>\n   201→            <th className=\"max-lg:hidden\">Owner</th>\n   202→            <th>Value</th>\n   203→            <th className=\"max-lg:hidden\">\n   204→              <button\n   205→                className=\"deals-sort-header-btn\"\n   206→                data-testid=\"close-date-sort\"\n   207→                data-sort-direction={sortDirection}\n   208→                onClick={handleCloseDateSort}\n   209→                type=\"button\"\n   210→              >\n   211→                Close Date\n   212→                {getSortArrow(sortDirection)}\n   213→              </button>\n   214→            </th>\n   215→            <th>Status</th>\n   216→            <th></th>\n   217→          </tr>\n   218→        </thead>\n   219→        <tbody>\n   220→          {deals.map((deal) => (\n   221→            <tr\n   222→              key={deal.id}\n   223→              data-testid={`deal-row-${deal.id}`}\n   224→              className=\"deals-table-row\"\n   225→              onClick={() => navigate(`/deals/${deal.id}`)}\n   226→              style={{ cursor: \"pointer\" }}\n   227→            >\n   228→              <td className=\"deals-name-cell\">{deal.name}</td>\n   229→              <td className=\"max-md:hidden\">{deal.clientName || \"—\"}</td>\n   230→              <td className=\"max-md:hidden\">{deal.stage}</td>\n   231→              <td className=\"max-lg:hidden\">{deal.ownerName || \"—\"}</td>\n   232→              <td className=\"deals-value-cell\">{deal.value != null ? formatCurrency(deal.value) : \"—\"}</td>\n   233→              <td className=\"max-lg:hidden\">{deal.expectedCloseDate || \"—\"}</td>\n   234→              <td>\n   235→                <span className={statusBadgeClass(deal.status)} data-testid={`deal-status-${deal.id}`}>\n   236→                  {formatStatus(deal.status)}\n   237→                </span>\n   238→              </td>\n   239→              <td onClick={(e) => e.stopPropagation()}>\n   240→                <ActionsMenu