99→    } else {\n   100→      await query(\n   101→        sql,\n   102→        `INSERT INTO notification_preferences (user_id, client_updated, deal_created, deal_stage_changed, task_created, task_completed, contact_added, note_added)\n   103→        VALUES ($1, $2, $3, $4, $5, $6, $7, $8)`,\n   104→        [user.id, clientUpdated, dealCreated, dealStageChanged, taskCreated, taskCompleted, contactAdded, noteAdded]\n   105→      );\n   106→    }\n   107→\n   108→    return jsonResponse({\n   109→      clientUpdated,\n   110→      dealCreated,\n   111→      dealStageChanged,\n   112→      taskCreated,\n   113→      taskCompleted,\n   114→      contactAdded,\n   115→      noteAdded,\n   116→    });\n   117→  }\n   118→\n   119→  return errorResponse(405, \"Method not allowed\");\n   120→}\n   121→\n   122→export default withAuth(handler, { required: true });\n   123→"}]},"parent_tool_use_id":"toolu_01ShFSJoisqsmEaXxXn8ccon","session_id":"7e10e343-7a33-45b0-b256-8133ddd63f4e","uuid":"2ffdc6fa-a754-4053-921b-a41f75a8446d"}
