[2026-02-24T08:01:00.216Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01TMFNyorc7dKBreBW2SzQNT","type":"tool_result","content":"     1→import { useState, useEffect, useCallback } from 'react'\n     2→import { useNavigate } from 'react-router-dom'\n     3→\n     4→interface TimelineEvent {\n     5→  id: string\n     6→  event_type: string\n     7→  title: string\n     8→  description: string | null\n     9→  actor: string | null\n    10→  reference_id: string | null\n    11→  reference_type: string | null\n    12→  event_date: string\n    13→}\n    14→\n    15→interface ClientTimelineSectionProps {\n    16→  clientId: string\n    17→  refreshKey?: number\n    18→}\n    19→\n    20→function formatRelativeTime(dateStr: string): string {\n    21→  const date = new Date(dateStr)\n    22→  const now = new Date()\n    23→  const diffMs = now.getTime() - date.getTime()\n    24→  const diffDays = Math.floor(diffMs / (1000 * 60 * 60 * 24))\n    25→\n    26→  if (diffDays === 0) return 'Today'\n    27→  if (diffDays === 1) return 'Yesterday'\n    28→  if (diffDays <= 7) return `${diffDays} days ago`\n    29→  if (diffDays <= 14) return 'Last Week'\n    30→  if (diffDays <= 30) return 'Last Month'\n    31→  return date.toLocaleDateString('en-US', { month: 'short', day: 'numeric', year: 'numeric' })\n    32→}\n    33→\n    34→function getEventIcon(eventType: string) {\n    35→  switch (eventType) {\n    36→    case 'task_created':\n    37→    case 'task_completed':\n    38→      return (\n    39→        <svg width=\"14\" height=\"14\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" strokeWidth=\"2\" strokeLinecap=\"round\" strokeLinejoin=\"round\">\n    40→          <path d=\"M12 20h9\"/><path d=\"m16.376 3.622a1 1 0 0 1 3.002 3.002L7.368 18.635a2 2 0 0 1-.855.506l-2.872.838.838-2.872a2 2 0 0 1 .506-.854z\"/>\n    41→        </svg>\n    42→      )\n    43→    case 'deal_created':\n    44→    case 'deal_stage_change':\n    45→    case 'deal_deleted':\n    46→      return (\n    47→        <svg width=\"14\" height=\"14\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" strokeWidth=\"2\" strokeLinecap=\"round\" strokeLinejoin=\"round\">\n    48→          <line x1=\"12\" y1=\"1\" x2=\"12\" y2=\"23\"/><path d=\"M17 5H9.5a3.5 3.5 0 0 0 0 7h5a3.5 3.5 0 0 1 0 7H6\"/>\n    49→        </svg>\n    50→      )\n    51→    case 'contact_added':\n    52→    case 'contact_removed':\n    53→      return (\n    54→        <svg width=\"14\" height=\"14\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" strokeWidth=\"2\" strokeLinecap=\"round\" strokeLinejoin=\"round\">\n    55→          <path d=\"M16 21v-2a4 4 0 0 0-4-4H6a4 4 0 0 0-4 4v2\"/><circle cx=\"9\" cy=\"7\" r=\"4\"/>\n    56→        </svg>\n    57→      )\n    58→    case 'attachment_uploaded':\n    59→    case 'attachment_deleted':\n    60→      return (\n    61→        <svg width=\"14\" height=\"14\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" strokeWidth=\"2\" strokeLinecap=\"round\" strokeLinejoin=\"round\">\n    62→          <path d=\"m21.44 11.05-9.19 9.19a6 6 0 0 1-8.49-8.49l8.57-8.57A4 4 0 1 1 18 8.84l-8.59 8.57a2 2 0 0 1-2.83-2.83l8.49-8.48\"/>\n    63→        </svg>\n    64→      )\n    65→    case 'status_changed':\n    66→    case 'name_changed':\n    67→    case 'source_info_updated':\n    68→      return (\n    69→        <svg width=\"14\" height=\"14\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" strokeWidth=\"2\" strokeLinecap=\"round\" strokeLinejoin=\"round\">\n    70→          <circle cx=\"12\" cy=\"12\" r=\"10\"/><line x1=\"12\" y1=\"16\" x2=\"12\" y2=\"12\"/><line x1=\"12\" y1=\"8\" x2=\"12.01\" y2=\"8\"/>\n    71→        </svg>\n    72→      )\n    73→    case 'note_added':\n    74→      return (\n    75→        <svg width=\"14\" height=\"14\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" strokeWidth=\"2\" strokeLinecap=\"round\" strokeLinejoin=\"round\">\n    76→          <path d=\"M14.5 2H6a2 2 0 0 0-2 2v16a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V7.5L14.5 2z\"/>\n    77→          <polyline points=\"14 2 14 8 20 8\"/>\n    78→        </svg>\n    79→      )\n    80→    case 'email_sent':\n    81→      return (\n    82→        <svg width=\"14\" height=\"14\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" strokeWidth=\"2\" strokeLinecap=\"round\" strokeLinejoin=\"round\">\n    83→          <rect width=\"20\" height=\"16\" x=\"2\" y=\"4\" rx=\"2\"/><path d=\"m22 7-8.97 5.7a1.94 1.94 0 0 1-2.06 0L2 7\"/>\n    84→        </svg>\n    85→      )\n    86→    default:\n    87→      return (\n    88→        <svg width=\"14\" height=\"14\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" strokeWidth=\"2\" strokeLinecap=\"round\" strokeLinejoin=\"round\">\n    89→          <circle cx=\"12\" cy=\"12\" r=\"10\"/>\n    90→        </svg>\n    91→      )\n    92→  }\n    93→}\n    94→\n    95→function getEventTypeLabel(eventType: string): string {\n    96→  const labels: Record<string, string> = {\n    97→    task_created: 'Task Created',\n    98→    task_completed: 'Task Completed',\n    99→    deal_created: 'Deal Created',\n   100→    deal_stage_change: 'Deal Stage Changed',\n   101→    deal_deleted: 'Deal Deleted',\n   102→    contact_added: 'Contact Added',\n   103→    contact_removed: 'Contact Removed',\n   104→    attachment_uploaded: 'Attachment Uploaded',\n   105→    attachment_deleted: 'Attachment Deleted',\n   106→    status_changed: 'Status Changed',\n   107→    name_changed: 'Name Changed',\n   108→    source_info_updated: 'Source Info Updated',\n   109→    note_added: 'Note Added',\n   110→    email_sent: 'Email Sent',\n   111→  }\n   112→  return labels[eventType] || eventType.replace(/_/g, ' ').replace(/\\b\\w/g, l => l.toUpperCase())\n   113→}\n   114→\n   115→export default function ClientTimelineSection({ clientId, refreshKey }: ClientTimelineSectionProps) {\n   116→  const navigate = useNavigate()\n   117→  const [events, setEvents] = useState<TimelineEvent[]>([])\n   118→  const [loading, setLoading] = useState(true)\n   119→\n   120→  const fetchTimeline = useCallback(async () => {\n   121→    try {\n   122→      const res = await fetch(`/.netlify/functions/clients/${clientId}/timeline`)\n   123→      const data = await res.json()\n   124→      setEvents(data.events || [])\n   125→    } catch {\n   126→      // silently fail\n   127→    } finally {\n   128→      setLoading(false)\n   129→    }\n   130→  }, [clientId])\n   131→\n   132→  useEffect(() => {\n   133→    fetchTimeline()\n   134→  }, [fetchTimeline, refreshKey])\n   135→\n   136→  const handleSubjectClick = (event: TimelineEvent) => {\n   137→    if (!event.reference_id || !event.reference_type) return\n   138→    switch (event.reference_type) {\n   139→      case 'deal':\n   140→        navigate(`/deals/${event.reference_id}`)\n   141→        break\n   142→      case 'individual':\n   143→        navigate(`/individuals/${event.reference_id}`)\n   144→        break\n   145→      case 'task':\n   146→        navigate('/tasks')\n   147→        break\n   148→    }\n   149→  }\n   150→\n   151→  return (\n   152→    <div className=\"person-section\" data-testid=\"timeline-section\">\n   153→      <div className=\"person-section-header\">\n   154→        <div className=\"person-section-heading\">\n   155→          <svg width=\"16\" height=\"16\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" strokeWidth=\"2\" strokeLinecap=\"round\" strokeLinejoin=\"round\">\n   156→            <circle cx=\"12\" cy=\"12\" r=\"10\"/><polyline points=\"12 6 12 12 16 14\"/>\n   157→          </svg>\n   158→          <h2 data-testid=\"timeline-heading\">Timeline</h2>\n   159→        </div>\n   160→      </div>\n   161→\n   162→      {loading ? (\n   163→        <p className=\"person-section-loading\">Loading...</p>\n   164→      ) : events.length > 0 ? (\n   165→        <div className=\"timeline-list\" data-testid=\"timeline-list\">\n   166→          {events.map(event => (\n   167→            <div key={event.id} className=\"timeline-entry\" data-testid=\"timeline-entry\">\n   168→              <div className=\"timeline-entry-icon\" data-testid=\"timeline-icon\">\n   169→                {getEventIcon(event.event_type)}\n   170→              </div>\n   171→              <div className=\"timeline-entry-content\">\n   172→                <div className=\"timeline-entry-header\">\n   173→                  <span className=\"timeline-entry-date\" data-testid=\"timeline-date\">\n   174→                    {formatRelativeTime(event.event_date)}\n   175→                  </span>\n   176→                  <span className=\"timeline-entry-type\" data-testid=\"timeline-event-type\">\n   177→                    {getEventTypeLabel(event.event_type)}\n   178→                  </span>\n   179→                </div>\n   180→                <div className=\"timeline-entry-body\">\n   181→                  {event.reference_id && event.reference_type ? (\n   182→                    <button\n   183→                      className=\"timeline-subject-link\"\n   184→                      onClick={() => handleSubjectClick(event)}\n   185→                      data-testid=\"timeline-subject-link\"\n   186→                    >\n   187→                      {event.title}\n   188→                    </button>\n   189→                  ) : (\n   190→                    <span data-testid=\"timeline-title\">{event.title}</span>\n   191→                  )}\n   192→                  {event.description && (\n   193→                    <span className=\"timeline-entry-description\" data-testid=\"timeline-description\">{event.description}</span>\n   194→                  )}\n   195→                </div>\n   196→                {event.actor && (\n   197→                  <span className=\"timeline-entry-actor\">\n   198→                    by{' '}\n   199→                    <button\n   200→                      className=\"timeline-actor-link\"\n   201→                      onClick={() => {\n   202→                        if (event.reference_type === 'individual' && event.reference_id) {\n   203→                          navigate(`/individuals/${event.reference_id}`)\n   204→                        }\n   205→                      }}\n   206→                      data-testid=\"timeline-actor-link\"\n   207→                    >\n   208→                      {event.actor}\n   209→                    </button>\n   210→                  </span>\n   211→                )}\n   212→              </div>\n   213→            </div>\n   214→          ))}\n   215→        </div>\n   216→      ) : (\n   217→        <div className=\"person-section-empty\" data-testid=\"timeline-empty\">\n   218→          No activity yet\n   219→        </div>\n   220→      )}\n   221→    </div>\n   222→  )\n   223→}\n   224→"}]},"parent_tool_use_id":"toolu_01RgeYcGd8iphHJeEVvisWfZ","session_id":"08ccbeb6-c99f-4e9b-a9e9-98260d7f7c42","uuid":"8d0b3e3e-d605-4298-aaf0-d9233684e9ba"}
