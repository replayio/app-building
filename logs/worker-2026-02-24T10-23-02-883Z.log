ingTags])\n    52→\n    53→  useEffect(() => {\n    54→    const handleClickOutside = (e: MouseEvent) => {\n    55→      if (statusRef.current && !statusRef.current.contains(e.target as Node)) {\n    56→        setStatusDropdownOpen(false)\n    57→      }\n    58→    }\n    59→    document.addEventListener('mousedown', handleClickOutside)\n    60→    return () => document.removeEventListener('mousedown', handleClickOutside)\n    61→  }, [])\n    62→\n    63→  const handleNameSave = async () => {\n    64→    if (nameValue.trim() && nameValue !== name) {\n    65→      await onUpdateName(nameValue.trim())\n    66→    } else {\n    67→      setNameValue(name)\n    68→    }\n    69→    setEditingName(false)\n    70→  }\n    71→\n    72→  const handleNameKeyDown = (e: React.KeyboardEvent) => {\n    73→    if (e.key === 'Enter') handleNameSave()\n    74→    if (e.key === 'Escape') { setNameValue(name); setEditingName(false) }\n    75→  }\n    76→\n    77→  const handleStatusSelect = async (newStatus: string) => {\n    78→    setStatusDropdownOpen(false)\n    79→    if (newStatus !== status) {\n    80→      await onUpdateStatus(newStatus)\n    81→    }\n    82→  }\n    83→\n    84→  const statusClass = `status-${status.toLowerCase()}`\n    85→\n    86→  const handleTagKeyDown = (e: React.KeyboardEvent) => {\n    87→    if (e.key === 'Enter' && tagInput.trim()) {\n    88→      e.preventDefault()\n    89→      const newTag = tagInput.trim()\n    90→      if (!localTags.includes(newTag)) {\n    91→        setLocalTags([...localTags, newTag])\n    92→      }\n    93→      setTagInput('')\n    94→    }\n    95→    if (e.key === 'Backspace' && !tagInput && localTags.length > 0) {\n    96→      setLocalTags(localTags.slice(0, -1))\n    97→    }\n    98→  }\n    99→\n   100→  const removeTag = (tag: string) => {\n   101→    setLocalTags(localTags.filter(t => t !== tag))\n   102→  }\n   103→\n   104→  const handleTagsSave = async () => {\n   105→    await onUpdateTags(localTags)\n   106→    setEditingTags(false)\n   107→  }\n   108→\n   109→  const handleTagsCancel = () => {\n   110→    setLocalTags(tags)\n   111→    setTagInput('')\n   112→    setEditingTags(false)\n   113→  }\n   114→\n   115→  return (\n   116→    <div className=\"client-header\" data-testid=\"client-detail-header\">\n   117→      <div className=\"client-header-top\">\n   118→        {editingName ? (\n   119→          <input\n   120→            ref={nameInputRef}\n   121→            className=\"client-header-name-input\"\n   122→            value={nameValue}\n   123→            onChange={e => setNameValue(e.target.value)}\n   124→            onBlur={handleNameSave}\n   125→            onKeyDown={handleNameKeyDown}\n   126→            data-testid=\"client-name-input\"\n   127→          />\n   128→        ) : (\n   129→          <h1\n   130→            className=\"client-header-name\"\n   131→            onClick={() => setEditingName(true)}\n   132→            data-testid=\"client-detail-name\"\n   133→          >\n   134→            {name}\n   135→          </h1>\n   136→        )}\n   137→\n   138→        <div className=\"client-header-badges\">\n   139→          <span className=\"client-type-badge\" data-testid=\"client-type-badge\">\n   140→            {type}\n   141→          </span>\n   142→\n   143→          <div className=\"client-status-wrapper\" ref={statusRef}>\n   144→            <button\n   145→              className={`status-badge ${statusClass}`}\n   146→              onClick={() => setStatusDropdownOpen(!statusDropdownOpen)}\n   147→              data-testid=\"client-status-badge\"\n   148→            >\n   149→              {status}\n   150→            </button>\n   151→            {statusDropdownOpen && (\n   152→              <div className=\"client-status-dropdown\" data-testid=\"client-status-dropdown\">\n   153→                {STATUSES.map(s => (\n   154→                  <button\n   155→                    key={s}\n   156→                    className