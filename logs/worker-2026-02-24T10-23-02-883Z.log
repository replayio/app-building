     })\n    98→      if (res.ok) {\n    99→        const updated = await res.json()\n   100→        setClient(updated)\n   101→      }\n   102→    } catch {\n   103→      // silently fail\n   104→    }\n   105→  }\n   106→\n   107→  const handleUpdateSourceInfo = async (fields: Record<string, string | undefined>) => {\n   108→    if (!clientId) return\n   109→    try {\n   110→      const res = await fetch(`/.netlify/functions/clients/${clientId}`, {\n   111→        method: 'PUT',\n   112→        headers: { 'Content-Type': 'application/json' },\n   113→        body: JSON.stringify(fields),\n   114→      })\n   115→      if (res.ok) {\n   116→        const updated = await res.json()\n   117→        setClient(updated)\n   118→        setTimelineRefreshKey(k => k + 1)\n   119→      }\n   120→    } catch {\n   121→      // silently fail\n   122→    }\n   123→  }\n   124→\n   125→  const refreshAll = () => {\n   126→    setTimelineRefreshKey(k => k + 1)\n   127→  }\n   128→\n   129→  if (loading) {\n   130→    return (\n   131→      <div className=\"page-content p-6 max-sm:p-3\" data-testid=\"client-detail-page\">\n   132→        <p>Loading...</p>\n   133→      </div>\n   134→    )\n   135→  }\n   136→\n   137→  if (!client) {\n   138→    return (\n   139→      <div className=\"page-content p-6 max-sm:p-3\" data-testid=\"client-detail-page\">\n   140→        <p>Client not found</p>\n   141→      </div>\n   142→    )\n   143→  }\n   144→\n   145→  return (\n   146→    <div className=\"page-content p-6 max-sm:p-3\" data-testid=\"client-detail-page\">\n   147→      <div className=\"client-detail-top\">\n   148→        <ClientHeader\n   149→          name={client.name}\n   150→          type={client.type}\n   151→          status={client.status}\n   152→          tags={client.tags || []}\n   153→          source={client.source}\n   154→          onUpdateName={handleUpdateName}\n   155→          onUpdateStatus={handleUpdateStatus}\n   156→          onUpdateTags={handleUpdate