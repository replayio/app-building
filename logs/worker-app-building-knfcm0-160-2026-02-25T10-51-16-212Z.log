sName=\"supplier-comments-list\" data-testid=\"comments-list\">\n    94→            {comments.map((comment) => (\n    95→              <div className=\"supplier-comment-item\" key={comment.id} data-testid=\"comment-item\">\n    96→                <span className=\"supplier-comment-text\">\n    97→                  {formatCommentDate(comment.created_at)} - {comment.text} - {comment.author_name}\n    98→                </span>\n    99→              </div>\n   100→            ))}\n   101→            {comments.length === 0 && (\n   102→              <div className=\"empty-state\">\n   103→                <p className=\"empty-state-message\">No comments yet.</p>\n   104→              </div>\n   105→            )}\n   106→          </div>\n   107→        </div>\n   108→      )}\n   109→    </div>\n   110→  );\n   111→}\n   112→"}]},"parent_tool_use_id":"toolu_01R6w7zeJ55JZuSCLLHJbQPK","session_id":"b91a9124-a800-4478-92dd-c9f56c5620c1","uuid":"abe4da30-2f88-4d9d-9609-abe527707cca"}
