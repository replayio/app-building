ge,\n   455→  }) => {\n   456→    test.slow();\n   457→    await page.goto(`/recipe/${fullRecipe.id}`);\n   458→    await expect(page.getByTestId(\"equipment-required-card\")).toBeVisible({\n   459→      timeout: 30000,\n   460→    });\n   461→\n   462→    // Click the first equipment link\n   463→    const firstEquip = fullRecipe.equipment[0];\n   464→    await page\n   465→      .getByTestId(\"equipment-required-list\")\n   466→      .getByTestId(\"equipment-required-item\")\n   467→      .filter({ hasText: firstEquip.equipment_name })\n   468→      .getByTestId(\"equipment-link\")\n   469→      .click();\n   470→    await expect(page).toHaveURL(\n   471→      new RegExp(`/equipment/${firstEquip.equipment_id}`),\n   472→      { timeout: 30000 }\n   473→    );\n   474→\n   475→    // Navigate back to recipe details page\n   476→    await page.goBack();\n   477→    await expect(page.getByTestId(\"equipment-required-card\")).toBeVisible({\n   478→      timeout: 30000,\n   479→    });\n   480→\n   481→    // Click a different equipment link\n   482→    if (fullRecipe.equipment.length > 1) {\n   483→      const secondEquip = fullRecipe.equipment[1];\n   484→      await page\n   485→        .getByTestId(\"equipment-required-list\")\n   486→        .getByTestId(\"equipment-required-item\")\n   487→        .filter({ hasText: secondEquip.equipment_name })\n   488→        .getByTestId(\"equipment-link\")\n   489→        .click();\n   490→      await expect(page).toHaveURL(\n   491→        new RegExp(`/equipment/${secondEquip.equipment_id}`),\n   492→        { timeout: 30000 }\n   493→      );\n   494→    }\n   495→  });\n   496→\n   497→  test(\"RD-EQUIP-5: Empty equipment list shows placeholder\", async ({\n   498→    page,\n   499→  }) => {\n   500→    await page.goto(`/recipe/${emptyRecipeId}`);\n   501→    await expect(page.getByTestId(\"equipment-required-card\")).toBeVisible({\n   502→      timeout: 30000,\n   503→    });\n   504→\n   505→    await expect(page.getByTe