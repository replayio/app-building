 43→        return;\n    44→      }\n    45→\n    46→      if (data.token) {\n    47→        localStorage.setItem(\"auth_token\", data.token);\n    48→      }\n    49→\n    50→      login({\n    51→        id: data.user.id,\n    52→        name: data.user.name,\n    53→        email: data.user.email,\n    54→        avatarUrl: data.user.avatarUrl,\n    55→      });\n    56→\n    57→      setShowAuthForm(false);\n    58→      setEmail(\"\");\n    59→      setPassword(\"\");\n    60→    } catch {\n    61→      setAuthError(\"Network error. Please try again.\");\n    62→    } finally {\n    63→      setIsSubmitting(false);\n    64→    }\n    65→  };\n    66→\n    67→  const handleSignOut = () => {\n    68→    localStorage.removeItem(\"auth_token\");\n    69→    logout();\n    70→  };\n    71→\n    72→  return (\n    73→    <div className=\"sidebar-user-area\" data-testid=\"sidebar-user-area\">\n    74→      {isAuthenticated && user ? (\n    75→        <div data-testid=\"sidebar-user-info\">\n    76→          <div className=\"sidebar-user\">\n    77→            <div className=\"sidebar-avatar\" data-testid=\"sidebar-avatar\">\n    78→              {user.avatarUrl ? (\n    79→                <img src={user.avatarUrl} alt={user.name} style={{ width: 24, height: 24, borderRadius: \"50%\" }} />\n    80→              ) : (\n    81→                user.name.charAt(0).toUpperCase()\n    82→              )}\n    83→            </div>\n    84→            <span className=\"sidebar-user-name\" data-testid=\"sidebar-user-name\">{user.name}</span>\n    85→          </div>\n    86→          <button\n    87→            className=\"sidebar-signout-btn\"\n    88→            data-testid=\"sidebar-signout-btn\"\n    89→            onClick={handleSignOut}\n    90→          >\n    91→            Sign Out\n    92→          </button>\n    93→        </div>\n    94→      ) : (\n    95→        <div data-testid=\"sidebar-auth-area\">\n    96→          {!showAuthForm ? (\n    97→            <button\n    98→              className=\"sidebar-signin-btn\"\n    99→              data-testid=\"sidebar-signin-btn\"\n   100→              onClick={() => {\n   101→                setShowAuthForm(true);\n   102→                setAuthMode(\"signin\");\n   103→                setEmail(\"\");\n   104→                setPassword(\"\");\n   105→                setAuthError(\"\");\n   106→                setAuthMessage(\"\");\n   107→              }}\n   108→            >\n   109→              Sign In\n   110→            </button>\n   111→          ) : (\n   112→            <form\n   113→              className=\"sidebar-auth-form\"\n   114→              data-testid=\"sidebar-auth-form\"\n   115→              onSubmit={handleAuthSubmit}\n   116→            >\n   117→              <input\n   118→                type=\"email\"\n   119→                placeholder=\"Email\"\n   120→                value={email}\n   121→                onChange={(e) => setEmail(e.target.value)}\n   122→                data-testid=\"sidebar-auth-email\"\n   123→                required\n   124→              />\n   125→              <input\n   126→                type=\"password\"\n   127→                placeholder=\"Password\"\n   128→                value={password}\n   129→                onChange={(e) => setPassword(e.target.value)}\n   130→                data-testid=\"sidebar-auth-password\"\n   131→                required\n   132→              />\n   133→              {authError && (\n   134→                <div className=\"sidebar-auth-error\" data-testid=\"sidebar-auth-error\">\n   135→                  {authError}\n   136→                </div>\n   137→              )}\n   138→              {authMessage && (\n   139→                <div className=\"sidebar-auth-message\" data-testid=\"sidebar-auth-message\">\n   140→                  {authMessage}\n   141→                </div>\n   142→              )}\n   143→              <button\n   144→                type=\"submit\"\n   145→                className=\"sidebar-auth-submit-btn\"\n   146→                data-testid=\"sidebar-auth-submit-btn\"\n   147→                disabled={isSubmitting}\n   148→              >\n   149→                {authMode === \"signin\" ? \"Sign In\" : \"Sign Up\"}\n   150→              </button>\n   151→              <div className=\"sidebar-auth-links\">\n   152→                <button\n   153→                  type=\"button\"\n   154→                  className=\"sidebar-auth-link\"\n   155→                  data-testid=\"sidebar-forgot-password-link\"\n   156→                  onClick={() => navigate(\"/auth/forgot-password\")}\n   157→                >\n   158→                  Forgot password?\n   159→                </button>\n   160→                <button\n   161→                  type=\"button\"\n   162→                  className=\"sidebar-auth-toggle\"\n   163→                  data-testid=\"sidebar-auth-toggle\"\n   164→                  onClick={() => {\n   165→                    setAuthMode(authMode === \"signin\" ? \"signup\" : \"signin\");\n   166→                    setAuthError(\"\");\n   167→                    setAuthMessage(\"\");\n   168→                  }}\n   169→                >\n   170→                  {authMode === \"signin\" ? \"Sign Up\" : \"Sign In\"}\n   171→                </button>\n   172→              </div>\n   173→            </form>\n   174→          )}\n   175→        </div>\n   176→      )}\n   177→    </div>\n   178→  );\n   179→}\n   180→"}]},"parent_tool_use_id":"toolu_01MgRgiYG4eYj3qshhy1yXM5","session_id":"15ac4053-5830-474d-afda-1f7ac4406993","uuid":"6f9bff6e-269b-48ee-87eb-638d9a3860c0"}
