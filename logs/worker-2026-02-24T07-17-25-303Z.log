    const timer = setTimeout(async () => {\n   179→      if (!search.trim()) {\n   180→        setResults([])\n   181→        return\n   182→      }\n   183→      setSearching(true)\n   184→      try {\n   185→        const res = await fetch(`/.netlify/functions/individuals?search=${encodeURIComponent(search)}`)\n   186→        const data = await res.json()\n   187→        const filtered = (data.individuals || []).filter(\n   188→          (i: SearchResult) => !existingContactIds.includes(i.id)\n   189→        )\n   190→        setResults(filtered)\n   191→      } catch {\n   192→        // silently fail\n   193→      } finally {\n   194→        setSearching(false)\n   195→      }\n   196→    }, 300)\n   197→    return () => clearTimeout(timer)\n   198→  }, [search, existingContactIds])\n   199→\n   200→  const addContact = async (individual: SearchResult) => {\n   201→    setSaving(true)\n   202→    try {\n   203→      const res = await fetch(`/.netlify/functions/deals/${dealId}/contacts`, {\n   204→        method: 'POST',\n   205→        headers: { 'Content-Type': 'application/json' },\n   206→        body: JSON.stringify({ individual_id: individual.id }),\n   207→      })\n   208→      if (res.ok) {\n   209→        onSaved()\n   210→      }\n   211→    } finally {\n   212→      setSaving(false)\n   213→    }\n   214→  }\n   215→\n   216→  return (\n   217→    <div className=\"modal-overlay\" data-testid=\"add-contact-dialog\">\n   218→      <div className=\"modal-content\" onClick={e => e.stopPropagation()}>\n   219→        <div className=\"modal-header\">\n   220→          <h3 className=\"modal-title\">Add Contact</h3>\n   221→          <button className=\"modal-close\" onClick={onClose} data-testid=\"add-contact-close\">\n   222→            <svg width=\"16\" height=\"16\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" strokeWidth=\"2\" strokeLinecap=\"round\" strokeLinejoin=\"round\">\n   223→              <line x1=\"18\" y1=\"6\" x2=\"6\" y2=\"18\"/><line x1=\"6\" y1=\"6\" x2=\"18\" y2=\"18\"/>\n   224→            </svg>\n   225→          </button>\n   226→        </div>\n   227→\n   228→        <div className=\"modal-form\">\n   229→          <div className=\"form-field\">\n   230→            <label className=\"form-label\">Search Individuals</label>\n   231→            <div className=\"searchable-select-search\">\n   232→              <svg width=\"14\" height=\"14\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" strokeWidth=\"2\" strokeLinecap=\"round\" strokeLinejoin=\"round\" className=\"searchable-select-icon\">\n   233→                <circle cx=\"11\" cy=\"11\" r=\"8\"/><line x1=\"21\" y1=\"21\" x2=\"16.65\" y2=\"16.65\"/>\n   234→              </svg>\n   235→              <input\n   236→                type=\"text\"\n   237→                className=\"searchable-select-input\"\n   238→                value={search}\n   239→                onChange={e => setSearch(e.target.value)}\n   240→                placeholder=\"Type a name to search...\"\n   241→                autoFocus\n   242→                data-testid=\"add-contact-search-input\"\n   243→              />\n   244→            </div>\n   245→          </div>\n   246→\n   247→          <div className=\"searchable-select-options\" data-testid=\"add-contact-results\">\n   248→            {searching ? (\n   249→              <div className=\"searchable-select-empty\">Searching...</div>\n   250→            ) : results.length > 0 ? (\n   251→              results.map(person => (\n   252→                <button\n   253→                  key={person.id}\n   254→                  className=\"deal-contact-search-result\"\n   255→                  onClick={() => addContact(person)}\n   256→                  disabled={saving}\n   257→                  data-testid=\"add-contact-result\"\n   258→                >\n   259→                  <span className=\"deal-contact-search-name\">{person.name}</span>\n   260→                  {person.title 