   50→\n    51→  const formatValue = (val: string | number | null) => {\n    52→    if (val === null || val === undefined || val === '') return ''\n    53→    const num = typeof val === 'string' ? parseFloat(val) : val\n    54→    if (isNaN(num)) return String(val)\n    55→    return `$${num.toLocaleString('en-US', { minimumFractionDigits: 0, maximumFractionDigits: 0 })}`\n    56→  }\n    57→\n    58→  const startEditing = (field: string, currentValue: string) => {\n    59→    setEditingField(field)\n    60→    setEditValue(currentValue)\n    61→  }\n    62→\n    63→  const handleSave = async () => {\n    64→    if (!editingField) return\n    65→    await onUpdateField(editingField, editValue)\n    66→    setEditingField(null)\n    67→  }\n    68→\n    69→  const handleKeyDown = (e: React.KeyboardEvent) => {\n    70→    if (e.key === 'Enter') {\n    71→      handleSave()\n    72→    } else if (e.key === 'Escape') {\n    73→      setEditingField(null)\n    74→    }\n    75→  }\n    76→\n    77→  const handleChangeStage = async () => {\n    78→    if (selectedStage === stage) return\n    79→    await onChangeStage(selectedStage)\n    80→  }\n    81→\n    82→  const displayTitle = `DEAL DETAILS: ${clientName} - ${dealName}`\n    83→\n    84→  return (\n    85→    <div className=\"deal-header\" data-testid=\"deal-header\">\n    86→      <h1 className=\"deal-header-title\" data-testid=\"deal-title\">{displayTitle}</h1>\n    87→\n    88→      <div className=\"deal-header-fields\">\n    89→        <div className=\"deal-header-field\" data-testid=\"deal-client-field\">\n    90→          <span className=\"deal-header-label\">Client:</span>\n    91→          {editingField === 'client_name' ? (\n    92→            <input\n    93→              ref={inputRef}\n    94→              className=\"deal-header-input\"\n    95→              value={editValue}\n    96→              onChange={e => setEditValue(e.target.value)}\n    97→              onBlur={handleSave}\n    98→            