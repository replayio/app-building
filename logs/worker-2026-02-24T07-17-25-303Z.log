  data-testid=\"deals-pagination-next\"\n    41→        >\n    42→          <span>Next</span>\n    43→          <ChevronRight size={14} />\n    44→        </button>\n    45→      </div>\n    46→    </div>\n    47→  )\n    48→}\n    49→\n    50→export default function DealsListPage() {\n    51→  const dispatch = useAppDispatch()\n    52→  const page = useAppSelector(s => s.deals.page)\n    53→  const filters = useAppSelector(s => s.deals.filters)\n    54→  const viewMode = useAppSelector(s => s.deals.viewMode)\n    55→\n    56→  useEffect(() => {\n    57→    dispatch(fetchDeals())\n    58→  }, [dispatch, page, filters.search, filters.stage, filters.client, filters.status, filters.sort, filters.dateFrom, filters.dateTo])\n    59→\n    60→  return (\n    61→    <div className=\"page-content p-6 max-sm:p-3\" data-testid=\"deals-list-page\">\n    62→      <div className=\"page-header\">\n    63→        <h1 className=\"page-title\">Deals</h1>\n    64→        <CreateDealButton />\n    65→      </div>\n    66→\n    67→      <div className=\"mb-4\">\n    68→        <DealsSummaryCards />\n    69→      </div>\n    70→\n    71→      <div className=\"deals-toolbar\">\n    72→        <DealsViewTabs />\n    73→      </div>\n    74→\n    75→      <div className=\"mb-4\">\n    76→        <DealsFilters />\n    77→      </div>\n    78→\n    79→      {viewMode === 'table' && <DealsTable />}\n    80→\n    81→      {viewMode === 'pipeline' && (\n    82→        <div className=\"deals-pipeline-placeholder\" data-testid=\"deals-pipeline-view\">\n    83→          <p className=\"empty-subtitle\">Pipeline view coming soon</p>\n    84→        </div>\n    85→      )}\n    86→\n    87→      <div className=\"mt-4\">\n    88→        <DealsPagination />\n    89→      </div>\n    90→    </div>\n    91→  )\n    92→}\n    93→"}]},"parent_tool_use_id":"toolu_01ASz3BXrNoQ5t6mHtf7Qb72","session_id":"bb718862-2399-4cb3-8f1e-e54caa1573e3","uuid":"0af7328c-2e3e-493c-8aa0-bd1fac5de182"}
