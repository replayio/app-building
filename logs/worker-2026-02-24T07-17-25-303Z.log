          {options.map(opt => (\n    47→            <button\n    48→              key={opt.value}\n    49→              className={`filter-dropdown-option${opt.value === value ? ' selected' : ''}`}\n    50→              onClick={() => { onChange(opt.value); setOpen(false) }}\n    51→              data-testid={`${testId}-option-${opt.value || 'all'}`}\n    52→            >\n    53→              {opt.label}\n    54→            </button>\n    55→          ))}\n    56→        </div>\n    57→      )}\n    58→    </div>\n    59→  )\n    60→}\n    61→\n    62→function DateRangeDropdown({ dateFrom, dateTo, onChange, testId }: {\n    63→  dateFrom: string\n    64→  dateTo: string\n    65→  onChange: (from: string, to: string) => void\n    66→  testId: string\n    67→}) {\n    68→  const [open, setOpen] = useState(false)\n    69→  const ref = useRef<HTMLDivElement>(null)\n    70→\n    71→  useEffect(() => {\n    72→    const handleClickOutside = (e: MouseEvent) => {\n    73→      if (ref.current && !ref.current.contains(e.target as Node)) setOpen(false)\n    74→    }\n    75→    document.addEventListener('mousedown', handleClickOutside)\n    76→    return () => document.removeEventListener('mousedown', handleClickOutside)\n    77→  }, [])\n    78→\n    79→  const hasRange = dateFrom || dateTo\n    80→  const displayText = hasRange\n    81→    ? `${dateFrom || '...'} – ${dateTo || '...'}`\n    82→    : 'Date - Range'\n    83→\n    84→  return (\n    85→    <div className=\"filter-dropdown-wrapper\" ref={ref} data-testid={testId}>\n    86→      <button\n    87→        className=\"filter-trigger\"\n    88→        onClick={() => setOpen(!open)}\n    89→        data-testid={`${testId}-trigger`}\n    90→      >\n    91→        <Calendar size={14} />\n    92→        <span>{displayText}</span>\n    93→        <ChevronDown size={14} />\n    94→      </button>\n    95→      {open && (\n    96→        <div className=\"filter-dropdown-menu\" data-testid={`${testId}-menu`}>\n    97→      