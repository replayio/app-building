alActive: 0,\n    69→    pipelineValue: 0,\n    70→    wonCount: 0,\n    71→    wonValue: 0,\n    72→    lostCount: 0,\n    73→    lostValue: 0,\n    74→    currentQuarter: 1,\n    75→  },\n    76→  availableClients: [],\n    77→  viewMode: 'table',\n    78→}\n    79→\n    80→export const fetchDeals = createAsyncThunk(\n    81→  'deals/fetchDeals',\n    82→  async (_, { getState }) => {\n    83→    const state = (getState() as RootState).deals\n    84→    const params = new URLSearchParams()\n    85→    params.set('page', state.page.toString())\n    86→    params.set('pageSize', state.pageSize.toString())\n    87→    if (state.filters.search) params.set('search', state.filters.search)\n    88→    if (state.filters.stage) params.set('stage', state.filters.stage)\n    89→    if (state.filters.client) params.set('client', state.filters.client)\n    90→    if (state.filters.status) params.set('status', state.filters.status)\n    91→    if (state.filters.sort) params.set('sort', state.filters.sort)\n    92→    if (state.filters.dateFrom) params.set('date_from', state.filters.dateFrom)\n    93→    if (state.filters.dateTo) params.set('date_to', state.filters.dateTo)\n    94→\n    95→    const res = await fetch(`/.netlify/functions/deals?${params}`)\n    96→    if (!res.ok) throw new Error('Failed to fetch deals')\n    97→    return res.json()\n    98→  }\n    99→)\n   100→\n   101→export const createDeal = createAsyncThunk(\n   102→  'deals/createDeal',\n   103→  async (data: {\n   104→    name: string\n   105→    client_id: string\n   106→    stage: string\n   107→    owner: string\n   108→    value: string\n   109→    close_date: string\n   110→    status: string\n   111→  }) => {\n   112→    const res = await fetch('/.netlify/functions/deals', {\n   113→      method: 'POST',\n   114→      headers: { 'Content-Type': 'application/json' },\n   115→      body: JSON.stringify({\n   116→        ...data,\n   117→        value: data.value ? parseFloat(data.value.replace(/[$,]/