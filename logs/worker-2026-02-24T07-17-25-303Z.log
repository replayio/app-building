el: 'All Statuses', value: '' },\n   148→  { label: 'On Track', value: 'On Track' },\n   149→  { label: 'Needs Attention', value: 'Needs Attention' },\n   150→  { label: 'At Risk', value: 'At Risk' },\n   151→  { label: 'Won', value: 'Won' },\n   152→  { label: 'Lost', value: 'Lost' },\n   153→]\n   154→\n   155→const sortOptions = [\n   156→  { label: 'Close Date (Newest)', value: 'close_date_newest' },\n   157→  { label: 'Close Date (Oldest)', value: 'close_date_oldest' },\n   158→  { label: 'Value (Highest)', value: 'value_highest' },\n   159→  { label: 'Value (Lowest)', value: 'value_lowest' },\n   160→]\n   161→\n   162→export default function DealsFilters() {\n   163→  const dispatch = useAppDispatch()\n   164→  const filters = useAppSelector(s => s.deals.filters)\n   165→  const availableClients = useAppSelector(s => s.deals.availableClients)\n   166→  const [localSearch, setLocalSearch] = useState(filters.search)\n   167→\n   168→  useEffect(() => {\n   169→    const timer = setTimeout(() => {\n   170→      dispatch(setDealsSearch(localSearch))\n   171→    }, 300)\n   172→    return () => clearTimeout(timer)\n   173→  }, [localSearch, dispatch])\n   174→\n   175→  const [prevReduxSearch, setPrevReduxSearch] = useState(filters.search)\n   176→  if (filters.search !== prevReduxSearch) {\n   177→    setPrevReduxSearch(filters.search)\n   178→    setLocalSearch(filters.search)\n   179→  }\n   180→\n   181→  const clientOptions = [\n   182→    { label: 'All Clients', value: '' },\n   183→    ...availableClients.map(c => ({ label: c, value: c })),\n   184→  ]\n   185→\n   186→  return (\n   187→    <div className=\"deals-filters\" data-testid=\"deals-filters\">\n   188→      <div className=\"deals-filters-row\">\n   189→        <FilterDropdown\n   190→          label=\"All Stages\"\n   191→          value={filters.stage}\n   192→          options={stageOptions}\n   193→          onChange={(v) => dispatch(setDealsStageFilter(v))}\n   194→          testId=\"deals-stage-filter\"\n   195→        />\n   196→        <FilterDropdown\n   197→          label=\"All Clients\"\n   198→          value={filters.client}\n   199→          options={clientOptions}\n   200→          onChange={(v) => dispatch(setDealsClientFilter(v))}\n   201→          testId=\"deals-client-filter\"\n   202→        />\n   203→        <FilterDropdown\n   204→          label=\"Active\"\n   205→          value={filters.status}\n   206→          options={statusOptions}\n   207→          onChange={(v) => dispatch(setDealsStatusFilter(v))}\n   208→          testId=\"deals-status-filter\"\n   209→        />\n   210→        <DateRangeDropdown\n   211→          dateFrom={filters.dateFrom}\n   212→          dateTo={filters.dateTo}\n   213→          onChange={(from, to) => dispatch(setDealsDateRange({ from, to }))}\n   214→          testId=\"deals-date-filter\"\n   215→        />\n   216→        <FilterDropdown\n   217→          label=\"Close Date (Newest)\"\n   218→          value={filters.sort}\n   219→          options={sortOptions}\n   220→          onChange={(v) => dispatch(setDealsSort(v))}\n   221→          testId=\"deals-sort-filter\"\n   222→        />\n   223→        <div className=\"deals-search-wrapper\" data-testid=\"deals-search-wrapper\">\n   224→          <Search size={14} className=\"deals-search-icon\" />\n   225→          <input\n   226→            type=\"text\"\n   227→            className=\"deals-search-input\"\n   228→            placeholder=\"Search deals...\"\n   229→            value={localSearch}\n   230→            onChange={(e) => setLocalSearch(e.target.value)}\n   231→            data-testid=\"deals-search-input\"\n   232→          />\n   233→          {localSearch && (\n   234→            <button\n   235→              className=\"deals-search-clear\"\n   236→              onClick={() => setLocalSearch('')}\n   237→              data-testid=\"deals-search-clear\"\n   238→            >\n   239→              <X size={12} />\n   240→           