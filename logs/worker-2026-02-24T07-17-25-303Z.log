   50→\n    51→  const formatValue = (val: string | number | null) => {\n    52→    if (val === null || val === undefined || val === '') return ''\n    53→    const num = typeof val === 'string' ? parseFloat(val) : val\n    54→    if (isNaN(num)) return String(val)\n    55→    return `$${num.toLocaleString('en-US', { minimumFractionDigits: 0, maximumFractionDigits: 0 })}`\n    56→  }\n    57→\n    58→  const startEditing = (field: string, currentValue: string) => {\n    59→    setEditingField(field)\n    60→    setEditValue(currentValue)\n    61→  }\n    62→\n    63→  const handleSave = async () => {\n    64→    if (!editingField) return\n    65→    await onUpdateField(editingField, editValue)\n    66→    setEditingField(null)\n    67→  }\n    68→\n    69→  const handleKeyDown = (e: React.KeyboardEvent) => {\n    70→    if (e.key === 'Enter') {\n    71→      handleSave()\n    72→    } else if (e.key === 'Escape') {\n    73→      setEditingField(null)\n    74→    }\n    75→  }\n    76→\n    77→  const handleChangeStage = async () => {\n    78→    if (selectedStage === stage) return\n    79→    await onChangeStage(selectedStage)\n    80→  }\n    81→\n    82→  const displayTitle = `DEAL DETAILS: ${clientName} - ${dealName}`\n    83→\n    84→  return (\n    85→    <div className=\"deal-header\" data-testid=\"deal-header\">\n    86→      <h1 className=\"deal-header-title\" data-testid=\"deal-title\">{displayTitle}</h1>\n    87→\n    88→      <div className=\"deal-header-fields\">\n    89→        <div className=\"deal-header-field\" data-testid=\"deal-client-field\">\n    90→          <span className=\"deal-header-label\">Client:</span>\n    91→          {editingField === 'client_name' ? (\n    92→            <input\n    93→              ref={inputRef}\n    94→              className=\"deal-header-input\"\n    95→              value={editValue}\n    96→              onChange={e => setEditValue(e.target.value)}\n    97→              onBlur={handleSave}\n    98→              onKeyDown={handleKeyDown}\n    99→              data-testid=\"deal-client-input\"\n   100→            />\n   101→          ) : (\n   102→            <button\n   103→              className=\"deal-header-value-btn\"\n   104→              onClick={() => startEditing('client_name', clientName)}\n   105→              data-testid=\"deal-client-value\"\n   106→            >\n   107→              {clientName}\n   108→            </button>\n   109→          )}\n   110→        </div>\n   111→\n   112→        <div className=\"deal-header-field\" data-testid=\"deal-value-field\">\n   113→          <span className=\"deal-header-label\">Value:</span>\n   114→          {editingField === 'value' ? (\n   115→            <input\n   116→              ref={inputRef}\n   117→              className=\"deal-header-input\"\n   118→              value={editValue}\n   119→              onChange={e => setEditValue(e.target.value)}\n   120→              onBlur={handleSave}\n   121→              onKeyDown={handleKeyDown}\n   122→              data-testid=\"deal-value-input\"\n   123→            />\n   124→          ) : (\n   125→            <button\n   126→              className=\"deal-header-value-btn\"\n   127→              onClick={() => startEditing('value', value !== null && value !== undefined ? String(value) : '')}\n   128→              data-testid=\"deal-value-value\"\n   129→            >\n   130→              {formatValue(value)}\n   131→            </button>\n   132→          )}\n   133→        </div>\n   134→\n   135→        <div className=\"deal-header-field\" data-testid=\"deal-owner-field\">\n   136→          <span className=\"deal-header-label\">Owner:</span>\n   137→          {editingField === 'owner' ? (\n   138→            <input\n   139→              ref={inputRef}\n   140→              className=\"deal-header-input\"\n   141→              value={editValue}\n   142→              onChange={e => setEditValue(e.target.value)}\n   143→              onBlur={handleSave}\n   14