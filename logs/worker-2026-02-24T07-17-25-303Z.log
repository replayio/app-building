ventListener('mousedown', handler)\n    43→    return () => document.removeEventListener('mousedown', handler)\n    44→  }, [])\n    45→\n    46→  return (\n    47→    <div className=\"row-action-wrapper\" ref={ref}>\n    48→      <button\n    49→        className=\"row-action-trigger\"\n    50→        onClick={(e) => { e.stopPropagation(); setOpen(!open) }}\n    51→        data-testid={`deal-actions-${deal.id}`}\n    52→      >\n    53→        <MoreHorizontal size={16} />\n    54→      </button>\n    55→      {open && (\n    56→        <div className=\"row-action-menu\" data-testid={`deal-action-menu-${deal.id}`}>\n    57→          <button\n    58→            className=\"row-action-item\"\n    59→            onClick={() => { setOpen(false); navigate(`/deals/${deal.id}`) }}\n    60→            data-testid={`deal-action-view-${deal.id}`}\n    61→          >\n    62→            <Eye size={14} />\n    63→            <span>View Details</span>\n    64→          </button>\n    65→          <button\n    66→            className=\"row-action-item\"\n    67→            onClick={() => { setOpen(false); onEdit() }}\n    68→            data-testid={`deal-action-edit-${deal.id}`}\n    69→          >\n    70→            <Pencil size={14} />\n    71→            <span>Edit</span>\n    72→          </button>\n    73→          <button\n    74→            className=\"row-action-item danger\"\n    75→            onClick={() => { setOpen(false); onDelete() }}\n    76→            data-testid={`deal-action-delete-${deal.id}`}\n    77→          >\n    78→            <Trash2 size={14} />\n    79→            <span>Delete</span>\n    80→          </button>\n    81→        </div>\n    82→      )}\n    83→    </div>\n    84→  )\n    85→}\n    86→\n    87→function DeleteConfirmModal({ open, dealName, onConfirm, onCancel }: {\n    88→  open: boolean\n    89→  dealName: string\n    90→  onConfirm: () => void\n    91→  onCancel: () => void\n    92→}) {\n    93→  if (!open) return null\n    94→  return (\n    95→    <div className=\"modal-overlay\" onClick={onCancel} data-testid=\"deal-delete-confirm-modal\">\n    96→      <div className=\"modal-content modal-small\" onClick={(e) => e.stopPropagation()}>\n    97→        <h2 className=\"modal-title\">Delete Deal</h2>\n    98→        <p className=\"modal-text\" data-testid=\"deal-delete-confirm-message\">\n    99→          Are you sure you want to delete {dealName}?\n   100→        </p>\n   101→        <div className=\"modal-actions\">\n   102→          <button className=\"btn-secondary\" onClick={onCancel} data-testid=\"deal-delete-cancel\">\n   103→            Cancel\n   104→          </button>\n   105→          <button className=\"btn-danger\" onClick={onConfirm} data-testid=\"deal-delete-confirm\">\n   106→            Delete\n   107→          </button>\n   108→        </div>\n   109→      </div>\n   110→    </div>\n   111→  )\n   112→}\n   113→\n   114→function EditDealModal({ open, deal, onSave, onCancel }: {\n   115→  open: boolean\n   116→  deal: DealRow | null\n   117→  onSave: (data: { id: string; name: string; client_id: string; stage: string; owner: string; value: string; close_date: string; status: string }) => Promise<void>\n   118→  onCancel: () => void\n   119→}) {\n   120→  const [name, setName] = useState('')\n   121→  const [clientId, setClientId] = useState('')\n   122→  const [stage, setStage] = useState('')\n   123→  const [owner, setOwner] = useState('')\n   124→  const [value, setValue] = useState('')\n   125→  const [closeDate, setCloseDate] = useState('')\n   126→  const [status, setStatus] = useState('')\n   127→  const [saving, setSaving] = useState(false)\n   128→  const [clientDropdownOpen, setClientDropdownOpen] = useState(false)\n   129→  const [stageDropdownOpen, setStageDropdownOpen] = useState(false)\n   130→  const [statusDropdownOpen, setStatusDropdownOpen] = useState(false)\n   131→  const [allClients, setAllClients] = useState<{ id: string; name: string }[]>([])\n   132→  const clientRef = useRef<HTMLDivElement>(null)\n   133→  const stageRef = useRef<HTMLDivElement>(null)\n   134→  const statusRef = useRef<HTMLDivElement>(null)\n   135→\n   136→  useEffect(() => {\n   137→    if (deal) {\n   138→      setName(deal.name)\n   139→      setClientId(deal.client_id)\n   140→      setStage(deal.stage)\n   141→      setOwner(deal.owner || '')\n   142→      const v = deal.value\n   143→      setValue(v !== null && v !== undefined ? String(v) : '')\n   144→      setCloseDate(deal.close_date ? deal.close_date.split('T')[0] : '')\n   145→      setStatus(deal.status)\n   146→    }\n   147→  }, [deal])\n   148→\n   149→  useEffect(() => {\n   150→    if (open) {\n   151→      fetch('/.netlify/functions/clients?page=1&pageSize=1000')\n   152→        .then(r => r.json())\n   153→        .then(data => setAllClients((data.clients || []).map((c: { id: string; name: string }) => ({ id: c.id, name: c.name }))))\n   154→        .catch(() => {})\n   155→    }\n   156→  }, [open])\n   157→\n   158→  useEffect(() => {\n   159→    const handler = (e: MouseEvent) => {\n   160→      if (clientRef.current && !clientRef.current.contains(e.target as Node)) setClientDropdownOpen(false)\n   161→      if (stageRef.current && !stageRef.current.contains(e.target as Node)) setStageDropdownOpen(false)\n   162→      if (statusRef.current && !statusRef.current.contains(e.target as Node)) setStatusDropdownOpen(false)\n   163→    }\n   164→    document.addEventListener('mousedown', handler)\n   165→    return () => document.removeEventListener('mousedown', handler)\n   166→  }, [])\n   167→\n   168→  if (!open || !deal) return null\n   169→\n   170→  const stages = ['Lead', 'Qualification', 'Discovery', 'Proposal', 'Negotiation', 'Closed Won']\n   171→  const statuses = ['On Track', 'Needs Attention', 'At Risk']\n   172→  const selectedClient = allClients.find(c => c.id === clientId)\n   173→\n   174→  const handleSubmit = async () => {\n   175→    if (!name.trim()) return\n