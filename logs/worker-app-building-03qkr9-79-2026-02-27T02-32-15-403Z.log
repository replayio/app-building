\n      if (!res.ok) {\n        setError(data.error || \"Something went wrong. Please try again.\");\n        setIsSubmitting(false);\n        return;\n      }\n\n      setSuccess(true);\n    } catch {\n      setError(\"Network error. Please try again.\");\n    } finally {\n      setIsSubmitting(false);\n    }\n  };\n\n  if (success) {\n    return (\n      <div className=\"auth-form-card\" data-testid=\"forgot-password-form\">\n        <h1 className=\"auth-form-title\">Forgot Password</h1>\n        <div className=\"auth-form-success\" data-testid=\"forgot-password-success\">\n          If an account exists with that email, a password reset link has been sent.\n        </div>\n        <button\n          type=\"button\"\n          className=\"auth-form-link\"\n          data-testid=\"forgot-password-back-link\"\n          onClick={() => navigate(\"/clients\")}\n        >\n          Back to Sign In\n        </button>\n      </div>\n    );\n  }\n\n  return (\n    <div className=\"auth-form-card\" data-testid=\"forgot-password-form\">\n      <h1 className=\"auth-form-title\">Forgot Password</h1>\n      <p className=\"auth-form-description\">\n        Enter your email address and we'll send you a link to reset your password.\n      </p>\n      <form onSubmit={handleSubmit}>\n        <div className=\"form-group\">\n          <label className=\"form-label\" htmlFor=\"forgot-email\">Email</label>\n          <input\n            id=\"forgot-email\"\n            type=\"email\"\n            className=\"form-input\"\n            placeholder=\"Enter your email address\"\n            value={email}\n            onChange={(e) => setEmail(e.target.value)}\n            data-testid=\"forgot-password-email\"\n          />\n        </div>\n        {error && (\n          <div className=\"auth-form-error\" data-testid=\"forgot-password-error\">\n            {error}\n          </div>\n        )}\n        <button\n          type=\"submit\"\n          className=\"btn btn--primary auth-form-submit\"\n          data-testid=\"forgot-password-submit\"\n          disabled={isSubmitting}\n        >\n          {isSubmitting ? \"Sending...\" : \"Send Reset Link\"}\n        </button>\n      </form>\n      <button\n        type=\"button\"\n        className=\"auth-form-link\"\n        data-testid=\"forgot-password-back-link\"\n        onClick={() => navigate(\"/clients\")}\n      >\n        Back to Sign In\n      </button>\n    </div>\n  );\n}\n","numLines":115,"startLine":1,"totalLines":115}}}
