: \"inherit\" });\n   139→\n   140→  console.log(\"\\nDeploying to Netlify...\");\n   141→  const output = execSync(\n   142→    `npx netlify deploy --prod --dir dist --functions ./netlify/functions --site ${siteId} --json`,\n   143→    { cwd: appDir, encoding: \"utf-8\" }\n   144→  );\n   145→  const result = JSON.parse(output);\n   146→  const deployUrl: string = result.deploy_ssl_url || result.ssl_url || result.url;\n   147→  console.log(`Deployed to: ${deployUrl}`);\n   148→  return deployUrl;\n   149→}\n   150→\n   151→// ---------------------------------------------------------------------------\n   152→// Main\n   153→// ---------------------------------------------------------------------------\n   154→\n   155→async function main(): Promise<void> {\n   156→  // Populate .env from deployment.txt if .env is missing values\n   157→  if (fs.existsSync(deploymentPath)) {\n   158→    const env = readEnv();\n   159→    if (!env.NETLIFY_SITE_ID || !env.NEON_PROJECT_ID || !env.DATABASE_URL) {\n   160→      console.log(\"Populating .env from deployment.txt...\");\n   161→      const deploymentLines = fs.readFileSync(deploymentPath, \"utf-8\").split(\"\\n\");\n   162→      for (const line of deploymentLines) {\n   163→        const trimmed = line.trim();\n   164→        if (!trimmed || trimmed.startsWith(\"#\")) continue;\n   165→        const idx = trimmed.indexOf(\"=\");\n   166→        if (idx === -1) continue;\n   167→        const key = trimmed.slice(0, idx);\n   168→        const value = trimmed.slice(idx + 1);\n   169→        if (\n   170→          [\"NETLIFY_SITE_ID\", \"NEON_PROJECT_ID\", \"DATABASE_URL\"].includes(key) &&\n   171→          !env[key]\n   172→        ) {\n   173→          writeEnvValue(key, value);\n   174→        }\n   175→      }\n   176→    }\n   177→  }\n   178→\n   179→  const neonApiKey = requireEnv(\"NEON_API_KEY\");\n   180→  const netlifyAccountSlug = requireEnv(\"NETLIFY_ACCOUNT_SLUG\");\n   181→  requireEnv(\"NETLIFY_AUTH_TOKEN\");\