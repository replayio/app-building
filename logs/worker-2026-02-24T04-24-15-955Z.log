ledRecordings.length === 0) {\n   259→    console.log(\"No failed recordings found to upload.\");\n   260→    return;\n   261→  }\n   262→\n   263→  failedRecordings.sort((a, b) => (b.duration || 0) - (a.duration || 0));\n   264→  const toUpload = failedRecordings[0];\n   265→\n   266→  console.log(`Uploading recording: ${toUpload.id}...`);\n   267→  try {\n   268→    execSync(`npx ${replayCli} upload ${toUpload.id}`, {\n   269→      cwd: appDir,\n   270→      stdio: \"inherit\",\n   271→    });\n   272→    console.log(`REPLAY UPLOADED: ${toUpload.id}`);\n   273→  } catch (err) {\n   274→    const exitCode = (err as { status?: number }).status ?? 1;\n   275→    console.error(`Upload failed with exit code: ${exitCode}`);\n   276→  }\n   277→}\n   278→\n   279→// ---------------------------------------------------------------------------\n   280→// Main\n   281→// ---------------------------------------------------------------------------\n   282→\n   283→async function main(): Promise<void> {\n   284→  const testFile = process.argv[2];\n   285→  if (!testFile) {\n   286→    console.error(\"Usage: tsx scripts/test.ts <test-file>\");\n   287→    console.error(\"Example: tsx scripts/test.ts tests/auth.spec.ts\");\n   288→    process.exit(1);\n   289→  }\n   290→\n   291→  const neonApiKey = requireEnvOrFile(\"NEON_API_KEY\");\n   292→  const projectId = requireEnvOrFile(\"NEON_PROJECT_ID\");\n   293→  const replayCli = process.env.REPLAY_CLI || \"replayio\";\n   294→  const runId = Date.now().toString(36);\n   295→\n   296→  let branchId: string | undefined;\n   297→  let playwrightExitCode = 1;\n   298→\n   299→  try {\n   300→    // 1. Kill stale processes\n   301→    killStaleProcesses();\n   302→\n   303→    // 2. Clean up stale branches\n   304→    await cleanupStaleBranches(neonApiKey, projectId);\n   305→\n   306→    // 3. Create ephemeral branch\n   307→    const branch = await createTestBranch(neonApiKey, projectId, runId);\n   308→    branchId = branch.branchI