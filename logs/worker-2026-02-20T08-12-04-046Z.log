39→        >\n    40→          <Plus size={14} strokeWidth={1.75} />\n    41→          Add Task\n    42→        </button>\n    43→      </div>\n    44→\n    45→      {tasks.length === 0 ? (\n    46→        <div data-testid=\"deal-linked-tasks-empty\" className=\"text-[13px] text-text-muted py-2\">No linked tasks</div>\n    47→      ) : (\n    48→        <div className=\"flex flex-col gap-1\">\n    49→          {tasks.map((task) => (\n    50→            <div\n    51→              key={task.id}\n    52→              data-testid={`deal-linked-task-${task.id}`}\n    53→              className=\"flex items-center gap-3 px-3 py-2.5 rounded-[4px] hover:bg-hover transition-colors duration-100 cursor-pointer\"\n    54→            >\n    55→              <button\n    56→                data-testid={`deal-linked-task-toggle-${task.id}`}\n    57→                onClick={() => onToggleTask(task.id, !task.completed)}\n    58→                className=\"flex-shrink-0 text-text-muted hover:text-status-active transition-colors duration-100\"\n    59→              >\n    60→                {task.completed ? (\n    61→                  <CheckSquare size={16} strokeWidth={1.75} />\n    62→                ) : (\n    63→                  <Square size={16} strokeWidth={1.75} />\n    64→                )}\n    65→              </button>\n    66→              <div className=\"flex-1 min-w-0\" onClick={() => navigate(`/tasks/${task.id}`)}>\n    67→                <span\n    68→                  className={`text-[13px] ${\n    69→                    task.completed ? 'text-text-muted line-through' : 'text-text-primary'\n    70→                  }`}\n    71→                >\n    72→                  {task.title}\n    73→                </span>\n    74→                {task.due_date && (\n    75→                  <span className=\"text-[12px] text-text-muted ml-2\">\n    76→                    · {task.completed ? 'Completed' : `Due: ${formatDueDate(task.due_date)}`}\n    77→                  </span>\n    78→                )}\n    79→                {task.completed && task.completed_at && (\n    80→                  <span className=\"text-[12px] text-text-muted ml-1\">\n    81→                    {new Date(task.completed_at).toLocaleDateString()}\n    82→                  </span>\n    83→                )}\n    84→              </div>\n    85→            </div>\n    86→          ))}\n    87→        </div>\n    88→      )}\n    89→    </div>\n    90→  )\n    91→}\n    92→"}]},"parent_tool_use_id":"toolu_01NmE36AncFuu9sTqDVKVAqU","session_id":"537fe758-8935-4056-919c-581fbb48ecdd","uuid":"51e62b09-6335-443b-9e62-b512c2ae98e0"}
