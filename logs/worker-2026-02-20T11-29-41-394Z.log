T') {\n    30→    const page = parseInt(url.searchParams.get('page') ?? '1')\n    31→    const pageSizeParam = url.searchParams.get('pageSize')\n    32→    const pageSize = pageSizeParam ? parseInt(pageSizeParam) : 50\n    33→    const offset = (page - 1) * pageSize\n    34→    const search = url.searchParams.get('search') ?? ''\n    35→    const status = url.searchParams.get('status') ?? ''\n    36→    const tag = url.searchParams.get('tag') ?? ''\n    37→    const source = url.searchParams.get('source') ?? ''\n    38→    const sort = url.searchParams.get('sort') ?? 'updated_at'\n    39→\n    40→    const searchPattern = search ? '%' + search + '%' : null\n    41→\n    42→    // Count query using conditional WHERE with tagged template\n    43→    const countResult = await sql`\n    44→      SELECT COUNT(*) as count FROM clients c\n    45→      WHERE (${searchPattern}::text IS NULL OR (\n    46→        c.name ILIKE ${searchPattern}\n    47→        OR EXISTS (SELECT 1 FROM unnest(c.tags) AS tg WHERE tg ILIKE ${searchPattern})\n    48→        OR EXISTS (SELECT 1 FROM client_individuals ci JOIN individuals i ON ci.individual_id = i.id WHERE ci.client_id = c.id AND i.name ILIKE ${searchPattern})\n    49→      ))\n    50→      AND (${status || null}::text IS NULL OR c.status = ${status || null})\n    51→      AND (${tag || null}::text IS NULL OR ${tag || null} = ANY(c.tags))\n    52→      AND (${source || null}::text IS NULL OR c.source_type = ${source || null})\n    53→    `\n    54→\n    55→    // Map sort parameter to safe sort key\n    56→    const sortMap: Record<string, string> = {\n    57→      'name_asc': 'name_asc',\n    58→      'name_desc': 'name_desc',\n    59→      'status': 'status',\n    60→      'updated_at': 'updated_at',\n    61→      'most_deals': 'most_deals',\n    62→    }\n    63→    const sortKey = sortMap[sort] ?? 'updated_at'\n    64→\n    65→    // Data query — use CASE-based ORDER BY to avoid dynamic SQL\n    66→    const dataResult = await sql`\n    67→      SELECT c.*,\n    68→        (SELECT COUNT(*) FROM deals d WHERE d.client_id = c.id AND d.stage NOT IN ('closed_won', 'closed_lost')) as open_deals_count,\n    69→        COALESCE((SELECT SUM(d.value) FROM deals d WHERE d.client_id = c.id AND d.stage NOT IN ('closed_won', 'closed_lost')), 0) as open_deals_value,\n    70→        (SELECT i.name FROM client_individuals ci JOIN individuals i ON ci.individual_id = i.id WHERE ci.client_id = c.id AND ci.is_primary = true LIMIT 1) as primary_contact_name,\n    71→        (SELECT ci.role FROM client_individuals ci WHERE ci.client_id = c.id AND ci.is_primary = true LIMIT 1) as primary_contact_role,\n    72→        (SELECT t.title FROM tasks t WHERE t.client_id = c.id AND t.completed = false ORDER BY t.due_date ASC NULLS LAST LIMIT 1) as next_task_title,\n    73→        (SELECT t.due_date FROM tasks t WHERE t.client_id = c.id AND t.completed = false ORDER BY t.due_date ASC NULLS LAST LIMIT 1) as next_task_due\n    74→      FROM clients c\n    75→      WHERE (${searchPattern}::text IS NULL OR (\n    76→        c.name ILIKE ${searchPattern}\n    77→        OR EXISTS (SELECT 1 FROM unnest(c.tags) AS tg WHERE tg ILIKE ${searchPattern})\n    78→        OR EXISTS (SELECT 1 FROM client_individuals ci JOIN individuals i ON ci.individual_id = i.id WHERE ci.client_id = c.id AND i.name ILIKE ${searchPattern})\n    79→      ))\n    80→      AND (${status || null}::text IS NULL OR c.status = ${status || null})\n    81→      AND (${tag || null}::text IS NULL OR ${tag || null} = ANY(c.tags))\n    82→      AND (${source || null}::text IS NULL OR c.source_type = ${source || null})\n    83→      ORDER BY\n    84→        CASE WHEN ${sortKey} = 'name_asc' THEN c.name END ASC,\n    85→        CASE WHEN ${sortKey} = 'name_desc' THEN c.name END DESC,\n    86→        CASE WHEN ${sortKey} = 'status' THEN c.status END ASC,\n    87→        CASE WHEN ${sortKey} = 'updated_at' THEN c.updated_at END DESC,\n    88→        CASE WHEN ${sortKey}