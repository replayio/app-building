 const typeText = await typeBadge.textContent();\n    42→    expect(['Organization', 'Individual']).toContain(typeText!.trim());\n    43→\n    44→    // Status dropdown visible\n    45→    const statusDropdown = page.getByTestId('client-header-status-dropdown');\n    46→    await expect(statusDropdown).toBeVisible();\n    47→  });\n    48→\n    49→  test('CDP-HDR-02: Edit button opens edit mode and saving name persists the change', async ({ page }) => {\n    50→    await navigateToFirstClientDetail(page);\n    51→\n    52→    // Get original name\n    53→    const originalName = await page.getByTestId('client-header-title').textContent();\n    54→\n    55→    // Click edit button\n    56→    await page.getByTestId('client-header-edit-button').click();\n    57→\n    58→    // Edit form fields should be visible\n    59→    await expect(page.getByTestId('client-header-name-input')).toBeVisible();\n    60→    await expect(page.getByTestId('client-header-type-select')).toBeVisible();\n    61→    await expect(page.getByTestId('client-header-status-select')).toBeVisible();\n    62→    await expect(page.getByTestId('client-header-tags-input')).toBeVisible();\n    63→\n    64→    // Save and Cancel buttons visible\n    65→    await expect(page.getByTestId('client-header-save-button')).toBeVisible();\n    66→    await expect(page.getByTestId('client-header-cancel-button')).toBeVisible();\n    67→\n    68→    // Change name and save\n    69→    const nameInput = page.getByTestId('client-header-name-input');\n    70→    await nameInput.clear();\n    71→    await nameInput.fill('Acme Corporation');\n    72→    await page.getByTestId('client-header-save-button').click();\n    73→\n    74→    // Verify name updated in header\n    75→    await expect(page.getByTestId('client-header-title')).toHaveText('Acme Corporation');\n    76→\n    77→    // Verify persistence by reloading\n    78→    await page.reload();\n    79→    await expect(page.getByTestId('client-header-title')).toHaveT