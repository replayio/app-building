[2026-02-18T01:21:18.632Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01XXwX4PTBkKvRW3woeTHFFe","type":"tool_result","content":"     1→import { neon } from '@neondatabase/serverless'\n     2→import { optionalAuth, type OptionalAuthRequest } from '../utils/auth'\n     3→\n     4→function getDb() {\n     5→  const url = process.env.DATABASE_URL\n     6→  if (!url) throw new Error('DATABASE_URL not set')\n     7→  return neon(url)\n     8→}\n     9→\n    10→async function handler(req: OptionalAuthRequest) {\n    11→  const sql = getDb()\n    12→  const url = new URL(req.url)\n    13→  const pathParts = url.pathname.split('/').filter(Boolean)\n    14→  // Path: /deals or /deals/:dealId\n    15→  const dealId = pathParts.length >= 4 ? pathParts[3] : null\n    16→\n    17→  // GET /deals — list all deals with filters and pagination\n    18→  if (req.method === 'GET' && !dealId) {\n    19→    const page = parseInt(url.searchParams.get('page') ?? '1', 10)\n    20→    const pageSize = parseInt(url.searchParams.get('pageSize') ?? '20', 10)\n    21→    const search = url.searchParams.get('search') ?? ''\n    22→    const stage = url.searchParams.get('stage') ?? ''\n    23→    const client = url.searchParams.get('client') ?? ''\n    24→    const status = url.searchParams.get('status') ?? ''\n    25→    const sort = url.searchParams.get('sort') ?? 'close_date_desc'\n    26→    const dateFrom = url.searchParams.get('dateFrom') ?? ''\n    27→    const dateTo = url.searchParams.get('dateTo') ?? ''\n    28→\n    29→    const offset = (page - 1) * pageSize\n    30→\n    31→    const searchPattern = search ? '%' + search + '%' : null\n    32→    const stageFilter = stage || null\n    33→    const clientFilter = client || null\n    34→    const dateFromFilter = dateFrom || null\n    35→    const dateToFilter = dateTo || null\n    36→\n    37→    // Normalize status into stage-based or status-based filter\n    38→    const statusIsActive = status === 'active'\n    39→    const statusIsWon = status === 'won'\n    40→    const statusIsLost = status === 'lost'\n    41→    const statusOther = (status && !statusIsActive && !statusIsWon && !statusIsLost) ? status : null\n    42→\n    43→    // Count total matching deals\n    44→    const countResult = await sql`\n    45→      SELECT COUNT(*) as count FROM deals d\n    46→      JOIN clients c ON d.client_id = c.id\n    47→      WHERE (${searchPattern}::text IS NULL OR (d.name ILIKE ${searchPattern} OR c.name ILIKE ${searchPattern}))\n    48→      AND (${stageFilter}::text IS NULL OR d.stage = ${stageFilter})\n    49→      AND (${clientFilter}::text IS NULL OR d.client_id = ${clientFilter}::uuid)\n    50→      AND (NOT ${statusIsActive}::boolean OR d.stage NOT IN ('closed_won', 'closed_lost'))\n    51→      AND (NOT ${statusIsWon}::boolean OR d.stage = 'closed_won')\n    52→      AND (NOT ${statusIsLost}::boolean OR d.stage = 'closed_lost')\n    53→      AND (${statusOther}::text IS NULL OR d.status = ${statusOther})\n    54→      AND (${dateFromFilter}::date IS NULL OR d.expected_close_date >= ${dateFromFilter}::date)\n    55→      AND (${dateToFilter}::date IS NULL OR d.expected_close_date <= ${dateToFilter}::date)\n    56→    `\n    57→    const total = parseInt(String(countResult[0].count), 10)\n    58→\n    59→    // Map sort parameter to safe sort key\n    60→    const sortMap: Record<string, string> = {\n    61→      'close_date_asc': 'close_date_asc',\n    62→      'close_date_desc': 'close_date_desc',\n    63→      'name_asc': 'name_asc',\n    64→      'name_desc': 'name_desc',\n    65→      'value_desc': 'value_desc',\n    66→      'value_asc': 'value_asc',\n    67→      'updated_at': 'updated_at',\n    68→    }\n    69→    const sortKey = sortMap[sort] ?? 'close_date_desc'\n    70→\n    71→    // Fetch deals with CASE-based ORDER BY to avoid dynamic SQL\n    72→    const deals = await sql`\n    73→      SELECT d.*, c.name as client_name FROM deals d\n    74→      JOIN clients c ON d.client_id = c.id\n    75→      WHERE (${searchPattern}::text IS NULL OR (d.name ILIKE ${searchPattern} OR c.name ILIKE ${searchPattern}))\n    76→      AND (${stageFilter}::text IS NULL OR d.stage = ${stageFilter})\n    77→      AND (${clientFilter}::text IS NULL OR d.client_id = ${clientFilter}::uuid)\n    78→      AND (NOT ${statusIsActive}::boolean OR d.stage NOT IN ('closed_won', 'closed_lost'))\n    79→      AND (NOT ${statusIsWon}::boolean OR d.stage = 'closed_won')\n    80→      AND (NOT ${statusIsLost}::boolean OR d.stage = 'closed_lost')\n    81→      AND (${statusOther}::text IS NULL OR d.status = ${statusOther})\n    82→      AND (${dateFromFilter}::date IS NULL OR d.expected_close_date >= ${dateFromFilter}::date)\n    83→      AND (${dateToFilter}::date IS NULL OR d.expected_close_date <= ${dateToFilter}::date)\n    84→      ORDER BY\n    85→        CASE WHEN ${sortKey} = 'close_date_asc' THEN d.expected_close_date END ASC NULLS LAST,\n    86→        CASE WHEN ${sortKey} = 'close_date_desc' THEN d.expected_close_date END DESC NULLS LAST,\n    87→        CASE WHEN ${sortKey} = 'name_asc' THEN d.name END ASC,\n    88→        CASE WHEN ${sortKey} = 'name_desc' THEN d.name END DESC,\n    89→        CASE WHEN ${sortKey} = 'value_desc' THEN d.value END DESC,\n    90→        CASE WHEN ${sortKey} = 'value_asc' THEN d.value END ASC,\n    91→        CASE WHEN ${sortKey} = 'updated_at' THEN d.updated_at END DESC,\n    92→        d.expected_close_date DESC NULLS LAST\n    93→      LIMIT ${pageSize} OFFSET ${offset}\n    94→    `\n    95→\n    96→    // Get summary metrics\n    97→    const metrics = await sql`\n    98→      SELECT\n    99→        COUNT(*) FILTER (WHERE status = 'active') as total_active,\n   100→        COALESCE(SUM(value) FILTER (WHERE status = 'active'), 0) as pipeline_value,\n   101→        COUNT(*) FILTER (WHERE stage = 'closed_won' AND updated_at >= date_trunc('quarter', CURRENT_DATE)) as won_count,\n   102→        COALESCE(SUM(value) FILTER (WHERE stage = 'closed_won' AND updated_at >= date_trunc('quarter', CURRENT_DATE)), 0) as won_value,\n   103→        COUNT(*) FILTER (WHERE stage = 'closed_lost' AND updated_at >= date_trunc('quarter', CURRENT_DATE)) as lost_count,\n   104→        COALESCE(SUM(value) FILTER (WHERE stage = 'closed_lost' AND updated_at >= date_trunc('quarter', CURRENT_DATE)), 0) as lost_value\n   105→      FROM deals\n   106→    `\n   107→\n   108→    // Get unique clients for filter dropdown\n   109→    const clients = await sql`\n   110→      SELECT DISTINCT c.id, c.name FROM clients c\n   111→      JOIN deals d ON d.client_id = c.id\n   112→      ORDER BY c.name ASC\n   113→    `\n   114→\n   115→    return Response.json({\n   116→      deals,\n   117→      total,\n   118→      metrics: metrics[0],\n   119→      clients,\n   120→    })\n   121→  }\n   122→\n   123→  // GET /deals/:dealId — get single deal with full details\n   124→  if (req.method === 'GET' && dealId) {\n   125→    const rows = await sql`\n   126→      SELECT d.*, c.name as client_name FROM deals d\n   127→      JOIN clients c ON d.client_id = c.id\n   128→      WHERE d.id = ${dealId}::uuid\n   129→    `\n   130→    if (rows.length === 0) {\n   131→      return new Response(JSON.stringify({ error: 'Deal not found' }), { status: 404 })\n   132→    }\n   133→    return Response.json(rows[0])\n   134→  }\n   135→\n   136→  // POST /deals — create a new deal\n   137→  if (req.method === 'POST' && !dealId) {\n   138→    const body = await req.json() as {\n   139→      name: string\n   140→      client_id: string\n   141→      value?: number\n   142→      stage?: string\n   143→      owner?: string\n   144→      probability?: number\n   145→      expected_close_date?: string\n   146→      status?: string\n   147→    }\n   148→\n   149→    const rows = await sql`\n   150→      INSERT INTO deals (name, client_id, value, stage, owner, probability, expected_close_date, status)\n   151→      VALUES (\n   152→        ${body.name},\n   153→        ${body.client_id}::uuid,\n   154→        ${body.value ?? 0},\n   155→        ${body.stage ?? 'lead'},\n   156→        ${body.owner ?? null},\n   157→        ${body.probability ?? 0},\n   158→        ${body.expected_close_date || null},\n   159→        ${body.status ?? 'active'}\n   160→      )\n   161→      RETURNING *\n   162→    `\n   163→\n   164→    const deal = rows[0]\n   165→\n   166→    // Create deal history entry for initial stage\n   167→    await sql`\n   168→      INSERT INTO deal_history (deal_id, old_stage, new_stage, changed_by)\n   169→      VALUES (${deal.id}::uuid, 'none', ${deal.stage}, ${req.user?.name ?? 'System'})\n   170→    `\n   171→\n   172→    // Create timeline event\n   173→    await sql`\n   174→      INSERT INTO timeline_events (client_id, event_type, description, user_name, related_entity_id, related_entity_type)\n   175→      VALUES (${body.client_id}::uuid, 'deal_created', ${'Deal Created: \\'' + body.name + '\\''}, ${req.user?.name ?? 'System'}, ${deal.id}::uuid, 'deal')\n   176→    `\n   177→\n   178→    // Add client_name\n   179→    const clientRows = await sql`SELECT name FROM clients WHERE id = ${body.client_id}::uuid`\n   180→    deal.client_name = clientRows.length > 0 ? clientRows[0].name : null\n   181→\n   182→    return Response.json(deal, { status: 201 })\n   183→  }\n   184→\n   185→  // PUT /deals/:dealId — update deal\n   186→  if (req.method === 'PUT' && dealId) {\n   187→    const body = await req.json() as {\n   188→      name?: string\n   189→      client_id?: string\n   190→      value?: number\n   191→      stage?: string\n   192→      owner?: string\n   193→      probability?: number\n   194→      expected_close_date?: string\n   195→      status?: string\n   196→    }\n   197→\n   198→    // If stage is changing, record history\n   199→    if (body.stage) {\n   200→      const existing = await sql`SELECT stage, client_id FROM deals WHERE id = ${dealId}::uuid`\n   201→      if (existing.length > 0 && existing[0].stage !== body.stage) {\n   202→        await sql`\n   203→          INSERT INTO deal_history (deal_id, old_stage, new_stage, changed_by)\n   204→          VALUES (${dealId}::uuid, ${existing[0].stage}, ${body.stage}, ${req.user?.name ?? 'System'})\n   205→        `\n   206→        // Create timeline event for stage change\n   207→        await sql`\n   208→          INSERT INTO timeline_events (client_id, event_type, description, user_name, related_entity_id, related_entity_type)\n   209→          VALUES (\n   210→            ${existing[0].client_id}::uuid,\n   211→            'deal_stage_changed',\n   212→            ${'Deal Stage Changed: from \\'' + existing[0].stage + '\\' to \\'' + body.stage + '\\''},\n   213→            ${req.user?.name ?? 'System'},\n   214→            ${dealId}::uuid,\n   215→            'deal'\n   216→          )\n   217→        `\n   218→      }\n   219→    }\n   220→\n   221→    const rows = await sql`\n   222→      UPDATE deals SET\n   223→        name = COALESCE(${body.name ?? null}, name),\n   224→        value = COALESCE(${body.value ?? null}, value),\n   225→        stage = COALESCE(${body.stage ?? null}, stage),\n   226→        owner = COALESCE(${body.owner ?? null}, owner),\n   227→        probability = COALESCE(${body.probability ?? null}, probability),\n   228→        expected_close_date = COALESCE(${body.expected_close_date || null}, expected_close_date),\n   229→        status = COALESCE(${body.status ?? null}, status),\n   230→        updated_at = NOW()\n   231→      WHERE id = ${dealId}::uuid\n   232→      RETURNING *\n   233→    `\n   234→\n   235→    if (rows.length === 0) {\n   236→      return new Response(JSON.stringify({ error: 'Deal not found' }), { status: 404 })\n   237→    }\n   238→\n   239→    const deal = rows[0]\n   240→    const clientRows = await sql`SELECT name FROM clients WHERE id = ${deal.client_id}::uuid`\n   241→    deal.client_name = clientRows.length > 0 ? clientRows[0].name : null\n   242→\n   243→    return Response.json(deal)\n   244→  }\n   245→\n   246→  // DELETE /deals/:dealId — delete deal\n   247→  if (req.method === 'DELETE' && dealId) {\n   248→    await sql`DELETE FROM deals WHERE id = ${dealId}::uuid`\n   249→    return new Response(null, { status: 204 })\n   250→  }\n   251→\n   252→  return new Response(JSON.stringify({ error: 'Method not allowed' }), { status: 405 })\n   253→}\n   254→\n   255→export default optionalAuth(handler)\n   256→"}]},"parent_tool_use_id":"toolu_01NbwY21ix5LRp8EYH41W2Xj","session_id":"8ddab264-c05c-4638-949d-8f41caef49d3","uuid":"c31673dd-97cb-4aeb-9c7c-ec1b6bcf5f3c"}
