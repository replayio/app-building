als ci ON ci.individual_id = ir.related_individual_id\n    99→        AND ci.client_id = (\n   100→          SELECT ci2.client_id FROM client_individuals ci2\n   101→          WHERE ci2.individual_id = ${individualId} LIMIT 1\n   102→        )\n   103→      WHERE ir.individual_id = ${individualId}\n   104→      ORDER BY i.name ASC\n   105→    ` as RelationshipRow[]\n   106→\n   107→    // Fetch contact history\n   108→    const contactHistory = await sql`\n   109→      SELECT * FROM contact_history\n   110→      WHERE individual_id = ${individualId}\n   111→      ORDER BY date DESC\n   112→    ` as ContactHistoryRow[]\n   113→\n   114→    return Response.json({\n   115→      ...individual,\n   116→      client_associations: clientAssociations,\n   117→      relationships,\n   118→      contact_history: contactHistory,\n   119→    })\n   120→  }\n   121→\n   122→  // PUT /individuals/:id — update individual info\n   123→  if (req.method === 'PUT' && individualId && !subResource) {\n   124→    const body = await req.json() as Partial<{\n   125→      name: string\n   126→      title: string\n   127→      email: string\n   128→      phone: string\n   129→      location: string\n   130→    }>\n   131→\n   132→    const rows = await sql`\n   133→      UPDATE individuals SET\n   134→        name = COALESCE(${body.name ?? null}, name),\n   135→        title = COALESCE(${body.title ?? null}, title),\n   136→        email = COALESCE(${body.email ?? null}, email),\n   137→        phone = COALESCE(${body.phone ?? null}, phone),\n   138→        location = COALESCE(${body.location ?? null}, location),\n   139→        updated_at = NOW()\n   140→      WHERE id = ${individualId}\n   141→      RETURNING *\n   142→    ` as IndividualRow[]\n   143→\n   144→    if (rows.length === 0) {\n   145→      return new Response(JSON.stringify({ error: 'Individual not found' }), { status: 404 })\n   146→    }\n   147→\n   148→    return Response.json(rows[0])\n   149→  }\n   150→\n   151→  // POS