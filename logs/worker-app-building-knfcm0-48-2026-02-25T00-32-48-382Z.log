","+  background: rgba(50, 45, 35, 0.015);","+}","+","+/* Daily view */","+.calendar-grid-daily {","+  border: 1px solid var(--bg-border-color);","+  border-radius: 6px;","+  overflow: hidden;","+  background: var(--card-bg);","+}","+","+.calendar-daily-header {","+  display: grid;","+  grid-template-columns: 60px 1fr;","+  border-bottom: 1px solid var(--bg-border-color);","+}","+","+.calendar-daily-day-header {","+  padding: 8px;","+  text-align: center;","+  font-size: 14px;","+  font-weight: 500;","+  color: var(--text-primary);","+  background: var(--bg-base-color);","+  border-left: 1px solid var(--bg-border-color-light);","+}","+","+.calendar-daily-body {","+  max-height: 600px;","+  overflow-y: auto;","+}","+","+.calendar-daily-time-row {","+  display: grid;","+  grid-template-columns: 60px 1fr;","+  min-height: 48px;","+  border-bottom: 1px solid var(--bg-border-color-light);","+}","+","+.calendar-daily-time-cell {","+  border-left: 1px solid var(--bg-border-color-light);","+  padding: 2px;","+  position: relative;","+}","+","+.calendar-daily-time-cell:hover {","+  background: rgba(50, 45, 35, 0.015);","+}","+","+/* Run details popup */","+.run-popup-overlay {","+  position: fixed;","+  inset: 0;","+  background: rgba(0, 0, 0, 0.4);","+  display: flex;","+  align-items: center;","+  justify-content: center;","+  z-index: 1000;","+}","+","+.run-popup-content {","+  background: var(--card-bg);","+  border-radius: 8px;","+  box-shadow: var(--shadow-elevation-2);","+  width: 90%;","+  max-width: 500px;","+  max-height: 90vh;","+  overflow-y: auto;","+  padding: 24px;","+}","+","+.run-popup-title {","+  font-size: 18px;","+  font-weight: 600;","+  color: var(--text-primary);","+  margin-bottom: 16px;","+}","+","+.run-popup-field {","+  display: flex;","+  justify-content: space-between;","+  align-items: baseline;","+  padding: 6px 0;","+  border-bottom: 1px solid var(--bg-border-color-light);","+}","+","+.run-popup-label {","+  font-size: 13px;","+  font-weight: