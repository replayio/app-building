51→          data-testid=\"edit-recipe-btn\"\n    52→          className=\"btn-primary\"\n    53→          onClick={() => setShowEditModal(true)}\n    54→        >\n    55→          Edit Recipe\n    56→        </button>\n    57→      </div>\n    58→\n    59→      {showEditModal && (\n    60→        <EditRecipeModal\n    61→          recipe={recipe}\n    62→          onClose={() => setShowEditModal(false)}\n    63→          onSave={async (data) => {\n    64→            await dispatch(updateRecipe({ id: recipe.id, ...data }));\n    65→            setShowEditModal(false);\n    66→          }}\n    67→        />\n    68→      )}\n    69→    </div>\n    70→  );\n    71→}\n    72→\n    73→interface EditRecipeModalProps {\n    74→  recipe: RecipeWithRelations;\n    75→  onClose: () => void;\n    76→  onSave: (data: Partial<Recipe>) => Promise<void>;\n    77→}\n    78→\n    79→function EditRecipeModal({ recipe, onClose, onSave }: EditRecipeModalProps) {\n    80→  const [name, setName] = useState(recipe.name);\n    81→  const [description, setDescription] = useState(recipe.description);\n    82→  const [product, setProduct] = useState(recipe.product);\n    83→  const [version, setVersion] = useState(recipe.version);\n    84→  const [status, setStatus] = useState(recipe.status);\n    85→  const [saving, setSaving] = useState(false);\n    86→\n    87→  const handleSubmit = async (e: React.FormEvent) => {\n    88→    e.preventDefault();\n    89→    setSaving(true);\n    90→    await onSave({ name, description, product, version, status });\n    91→    setSaving(false);\n    92→  };\n    93→\n    94→  return (\n    95→    <div className=\"modal-overlay\" onClick={onClose} data-testid=\"edit-recipe-modal\">\n    96→      <div className=\"modal-content\" onClick={(e) => e.stopPropagation()}>\n    97→        <div className=\"modal-header\">\n    98→          <h2 className=\"modal-title\">Edit Recipe</h2>\n    99→          <button className=\"modal-close-btn\" onClick={onClose}>\n   100→            <svg width=\"16\" height=\"16\" viewBox=\"0 0 16 16\" fill=\"none\">\n   101→              <path d=\"M4 4L12 12M12 4L4 12\" stroke=\"currentColor\" strokeWidth=\"1.5\" strokeLinecap=\"round\" />\n   102→            </svg>\n   103→          </button>\n   104→        </div>\n   105→        <form onSubmit={handleSubmit}>\n   106→          <div className=\"form-group\">\n   107→            <label className=\"form-label\">Recipe Name</label>\n   108→            <input\n   109→              data-testid=\"edit-recipe-name\"\n   110→              className=\"form-input\"\n   111→              value={name}\n   112→              onChange={(e) => setName(e.target.value)}\n   113→              required\n   114→            />\n   115→          </div>\n   116→          <div className=\"form-group\">\n   117→            <label className=\"form-label\">Description</label>\n   118→            <textarea\n   119→              data-testid=\"edit-recipe-description\"\n   120→              className=\"form-textarea\"\n   121→              value={description}\n   122→              onChange={(e) => setDescription(e.target.value)}\n   123→            />\n   124→          </div>\n   125→          <div className=\"form-row\">\n   126→            <div className=\"form-group\">\n   127→              <label className=\"form-label\">Product</label>\n   128→              <input\n   129→                data-testid=\"edit-recipe-product\"\n   130→                className=\"form-input\"\n   131→                value={product}\n   132→                onChange={(e) => setProduct(e.target.value)}\n   133→              />\n   134→            </div>\n   135→            <div className=\"form-group\">\n   136→              <label className=\"form-label\">Version</label>\n   137→              <input\n   138→                data-testid=\"edit-recipe-version\"\n   139→                className=\"form-input\"\n   140→                value={version}\n   141→                onChange={(e) => setVersion(e.target.value)}\n   142→              />\n   143→            </div>\n   144→          </div>\n   145→          <div className=\"form-group\">\n   146→            <label className=\"form-label\">Status</label>\n   147→            <div className=\"custom-select-container\" style={{ position: \"relative\" }}>\n   148→              <StatusSelect value={status} onChange={setStatus} />\n   149→            </div>\n   150→          </div>\n   151→          <div className=\"modal-footer\">\n   152→            <button type=\"button\" className=\"btn-secondary\" onClick={onClose}>\n   153→              Cancel\n   154→            </button>\n   155→            <button\n   156→              type=\"submit\"\n   157→              className=\"btn-primary\"\n   158→              disabled={saving || !name.trim()}\n   159→              data-testid=\"edit-recipe-save\"\n   160→            >\n   161→              {saving ? \"Saving...\" : \"Save Changes\"}\n   162→            </button>\n   163→          </div>\n   164→        </form>\n   165→      </div>\n   166→    </div>\n   167→  );\n   168→}\n   169→\n   170→function StatusSelect({\n   171→  value,\n   172→  onChange,\n   173→}: {\n   174→  value: string;\n   175→  onChange: (v: \"Active\" | \"Draft\" | \"Archived\") => void;\n   176→}) {\n   177→  const [open, setOpen] = useState(false);\n   178→  const options: (\"Active\" | \"Draft\" | \"Archived\")[] = [\"Active\", \"Draft\", \"Archived\"];\n   179→\n   180→  return (\n   181→    <div style={{ position: \"relative\" }}>\n   182→      <button\n   183→        type=\"button\"\n   184→        className=\"form-input\"\n   185→        data-testid=\"edit-recipe-status\"\n   186→        onClick={() => setOpen(!open)}\n   187→        style={{\n   188→          textAlign: \"left\",\n   189→          cursor: \"pointer\",\n   190→          display: \"flex\",\n   191→          alignItems: \"center\",\n   192→          justifyContent: \"space-between\",\n   193→        }}\n   194→      >\n   195→        <span>{value}</span>\n   196→        <svg width=\"12\" height=\"12\" viewBox=\"0 0 12 12\" fill=\"none\">\n   197→          <path d=\"M3 5L6 8L9 5\" stroke=\"currentColor\" strokeWidth=\"1.5\" strokeLinecap=\"round\" strokeLinejoin=\"round\" />\n   198→        </svg>\n   199→      </button>\n   200→      {open && (\n   201→        <div\n   202→          style={{\n   203→            position: \"absolute\",\n   204→            top: \"100%\",\n   205→            left: 0,\n   206→            right: 0,\n   207→            background: \"var(--card-bg)\",\n   208→            border: \"1px solid var(--bg-border-color)\",\n   209→            borderRadius: 4,\n   210→            boxShadow: \"var(--shadow-elevation-1)\",\n   211→            zIndex: 10,\n   212→            marginTop: 4,\n   213→          }}\n   214→        >\n   215→          {options.map((opt) => (\n   216→            <div\n   217→              key={opt}\n   218→              onClick={() => {\n   219→                onChange(opt);\n   220→                setOpen(false);\n   221→              }}\n   222→              style={{\n   223→                padding: \"8px 12px\",\n   224→                fontSize: 13,\n   225→                cursor: \"pointer\",\n   226→                background: opt === value ? \"var(--accent-subtle-bg)\" : \"transparent\",\n   227→                color: \"var(--text-primary)\",\n   228→                transition: \"background-color 0.1s ease\",\n   229→              }}\n   230→              onMouseEnter={(e) => {\n   231→                if (opt !== value) (e.target as HTMLElement).style.background = \"rgba(50, 45, 35, 0.05)\";\n   232→              }}\n   233→              onMouseLeave={(e) => {\n   234→                (e.target as HTMLElement).style.background = opt === value ? \"var(--accent-subtle-bg)\" : \"transparent\";\n   235→              }}\n   236→            >\n   237→              {opt}\n   238→            </div>\n   239→          ))}\n   240→        </div>\n   241→      )}\n   242→    </div>\n   243→  );\n   244→}\n   245→"}]},"parent_tool_use_id":"toolu_014dzqjrFYGVNa7eAUVGRPLn","session_id":"e31e799d-63d1-4d44-9a10-85942a0da33a","uuid":"28050783-e14f-4337-92f8-0d73c0c457b8"}
