   39→      dispatch(fetchSupplier(supplierId));\n    40→      dispatch(fetchSupplierComments(supplierId));\n    41→      dispatch(fetchSupplierDocuments(supplierId));\n    42→      dispatch(fetchOrdersBySupplier(supplierId));\n    43→    }\n    44→  }, [dispatch, supplierId]);\n    45→\n    46→  const scrollToSection = (ref: React.RefObject<HTMLDivElement | null>) => {\n    47→    ref.current?.scrollIntoView({ behavior: \"smooth\", block: \"start\" });\n    48→  };\n    49→\n    50→  if (loading && !supplier) {\n    51→    return (\n    52→      <div className=\"page-content p-6 max-sm:p-3\" data-testid=\"supplier-details-page\">\n    53→        <div className=\"loading-state\">Loading supplier details...</div>\n    54→      </div>\n    55→    );\n    56→  }\n    57→\n    58→  if (error) {\n    59→    return (\n    60→      <div className=\"page-content p-6 max-sm:p-3\" data-testid=\"supplier-details-page\">\n    61→        <div className=\"error-state\">Error loading supplier: {error}</div>\n    62→      </div>\n    63→    );\n    64→  }\n    65→\n    66→  if (!supplier) {\n    67→    return (\n    68→      <div className=\"page-content p-6 max-sm:p-3\" data-testid=\"supplier-details-page\">\n    69→        <div className=\"error-state\">Supplier not found.</div>\n    70→      </div>\n    71→    );\n    72→  }\n    73→\n    74→  const breadcrumbItems = [\n    75→    { label: \"Home\", onClick: () => navigate(\"/\") },\n    76→    { label: \"Suppliers\", onClick: () => navigate(\"/\") },\n    77→    { label: supplier.name },\n    78→    { label: \"Supplier Details\" },\n    79→  ];\n    80→\n    81→  return (\n    82→    <div className=\"page-content p-6 max-sm:p-3\" data-testid=\"supplier-details-page\">\n    83→      <Breadcrumb items={breadcrumbItems} />\n    84→\n    85→      <SupplierOverview\n    86→        supplier={supplier}\n    87→        onUpdate={async (data) => {\n    88→          await dispatch(updateSupplier({ id: supplier.id, ...data })).unwrap();\n    89→          await dispatch(fetchSupplier(supplier.id));\n    90→        }}\n    91→        onDelete={async () => {\n    92→          await dispatch(deleteSupplier(supplier.id)).unwrap();\n    93→        }}\n    94→      />\n    95→\n    96→      {/* Tests: Display Section Tab Links */}\n    97→      <div className=\"supplier-detail-tabs\" data-testid=\"supplier-detail-tabs\">\n    98→        {/* Tests: Click Documents Tab Scrolls to Documents Section */}\n    99→        <button\n   100→          className=\"tab-nav-item tab-nav-item--active-link\"\n   101→          data-testid=\"tab-documents\"\n   102→          onClick={() => scrollToSection(documentsRef)}\n   103→        >\n   104→          Documents\n   105→        </button>\n   106→        {/* Tests: Click Comments Tab Scrolls to Comments Section */}\n   107→        <button\n   108→          className=\"tab-nav-item tab-nav-item--active-link\"\n   109→          data-testid=\"tab-comments\"\n   110→          onClick={() => scrollToSection(commentsRef)}\n   111→        >\n   112→          Comments\n   113→        </button>\n   114→        {/* Tests: Click Orders Tab Scrolls to Orders Section */}\n   115→        <button\n   116→          className=\"tab-nav-item tab-nav-item--active-link\"\n   117→          data-testid=\"tab-orders\"\n   118→          onClick={() => scrollToSection(ordersRef)}\n   119→        >\n   120→          Orders\n   121→        </button>\n   122→      </div>\n   123→\n   124→      <div className=\"section-card\" ref={documentsRef} data-testid=\"documents-section\">\n   125→        <div className=\"section-card-body\">\n   126→          <DocumentsTab\n   127→            documents={documents}\n   128→            onUpload={async (file, documentType) => {\n   129→              await dispatch(uploadSupplierDocument({ supplierId: supplier.id, file, documentType })).unwrap();\n   130→              await dispatch(fetchSupplierDocuments(supplier.id));\n   131→            }}\n   132→            o