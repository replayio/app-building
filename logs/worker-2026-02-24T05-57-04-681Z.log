[2026-02-24T05:47:09.889Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01SwJHaL4N71NF9G5Z4Ef7gQ","type":"tool_result","content":"     1→import { useState, useCallback } from 'react'\n     2→import { Download, Upload, Plus, X, Upload as UploadIcon } from 'lucide-react'\n     3→import { useAppDispatch, useAppSelector } from '@/store/hooks'\n     4→import { createClient, importClients, fetchClients } from '@/store/clientsSlice'\n     5→import ClientFormModal from './ClientFormModal'\n     6→\n     7→function parseCSV(text: string): Record<string, string>[] {\n     8→  const lines = text.split('\\n').filter(l => l.trim())\n     9→  if (lines.length < 2) return []\n    10→\n    11→  const headers = parseCSVLine(lines[0])\n    12→  return lines.slice(1).map(line => {\n    13→    const values = parseCSVLine(line)\n    14→    const obj: Record<string, string> = {}\n    15→    headers.forEach((h, i) => { obj[h.trim()] = (values[i] || '').trim() })\n    16→    return obj\n    17→  })\n    18→}\n    19→\n    20→function parseCSVLine(line: string): string[] {\n    21→  const result: string[] = []\n    22→  let current = ''\n    23→  let inQuotes = false\n    24→\n    25→  for (let i = 0; i < line.length; i++) {\n    26→    if (line[i] === '\"') {\n    27→      if (inQuotes && line[i + 1] === '\"') {\n    28→        current += '\"'\n    29→        i++\n    30→      } else {\n    31→        inQuotes = !inQuotes\n    32→      }\n    33→    } else if (line[i] === ',' && !inQuotes) {\n    34→      result.push(current)\n    35→      current = ''\n    36→    } else {\n    37→      current += line[i]\n    38→    }\n    39→  }\n    40→  result.push(current)\n    41→  return result\n    42→}\n    43→\n    44→function ImportModal({ open, onClose }: { open: boolean; onClose: () => void }) {\n    45→  const dispatch = useAppDispatch()\n    46→  const [file, setFile] = useState<File | null>(null)\n    47→  const [preview, setPreview] = useState<Record<string, string>[]>([])\n    48→  const [error, setError] = useState('')\n    49→  const [importing, setImporting] = useState(false)\n    50→  const [success, setSuccess] = useState('')\n    51→\n    52→  const handleFile = useCallback((f: File) => {\n    53→    setError('')\n    54→    setSuccess('')\n    55→    if (!f.name.endsWith('.csv')) {\n    56→      setError('Please upload a CSV file')\n    57→      return\n    58→    }\n    59→    setFile(f)\n    60→    const reader = new FileReader()\n    61→    reader.onload = (e) => {\n    62→      const text = e.target?.result as string\n    63→      const rows = parseCSV(text)\n    64→      if (rows.length === 0) {\n    65→        setError('CSV file is empty or has no data rows')\n    66→        return\n    67→      }\n    68→      const headers = Object.keys(rows[0])\n    69→      const requiredCols = ['Client Name']\n    70→      const missing = requiredCols.filter(c => !headers.some(h => h.toLowerCase() === c.toLowerCase()))\n    71→      if (missing.length) {\n    72→        setError(`Missing required column: ${missing.join(', ')}`)\n    73→        return\n    74→      }\n    75→      setPreview(rows)\n    76→    }\n    77→    reader.readAsText(f)\n    78→  }, [])\n    79→\n    80→  const handleImport = async () => {\n    81→    if (!preview.length) return\n    82→    setImporting(true)\n    83→    setError('')\n    84→    try {\n    85→      const clients = preview.map(row => ({\n    86→        name: row['Client Name'] || row['client name'] || row['Name'] || row['name'] || '',\n    87→        type: row['Type'] || row['type'] || 'Organization',\n    88→        status: row['Status'] || row['status'] || 'Prospect',\n    89→        tags: row['Tags'] || row['tags'] || '',\n    90→        source: row['Source'] || row['source'] || '',\n    91→      }))\n    92→      const result = await dispatch(importClients(clients)).unwrap()\n    93→      setSuccess(`${result.imported} clients imported successfully`)\n    94→      await dispatch(fetchClients())\n    95→      setTimeout(() => {\n    96→        onClose()\n    97→        setFile(null)\n    98→        setPreview([])\n    99→        setSuccess('')\n   100→      }, 1500)\n   101→    } catch {\n   102→      setError('Failed to import clients')\n   103→    } finally {\n   104→      setImporting(false)\n   105→    }\n   106→  }\n   107→\n   108→  const reset = () => {\n   109→    setFile(null)\n   110→    setPreview([])\n   111→    setError('')\n   112→    setSuccess('')\n   113→    onClose()\n   114→  }\n   115→\n   116→  if (!open) return null\n   117→\n   118→  return (\n   119→    <div className=\"modal-overlay\" onClick={reset} data-testid=\"import-modal\">\n   120→      <div className=\"modal-content modal-wide\" onClick={(e) => e.stopPropagation()} data-testid=\"import-modal-content\">\n   121→        <div className=\"modal-header\">\n   122→          <h2 className=\"modal-title\">Import Clients</h2>\n   123→          <button className=\"modal-close\" onClick={reset} data-testid=\"import-modal-close\">\n   124→            <X size={18} />\n   125→          </button>\n   126→        </div>\n   127→        <div className=\"modal-body\">\n   128→          <div className=\"import-instructions\">\n   129→            <p className=\"import-instructions-title\">Expected CSV Format</p>\n   130→            <p className=\"import-instructions-text\">\n   131→              Required columns: <strong>Client Name</strong>, <strong>Type</strong>, <strong>Status</strong>\n   132→            </p>\n   133→            <p className=\"import-instructions-text\">\n   134→              Optional columns: Tags, Source, Primary Contact\n   135→            </p>\n   136→          </div>\n   137→\n   138→          {!preview.length && (\n   139→            <div\n   140→              className=\"import-dropzone\"\n   141→              onDragOver={(e) => { e.preventDefault(); e.stopPropagation() }}\n   142→              onDrop={(e) => { e.preventDefault(); e.stopPropagation(); if (e.dataTransfer.files[0]) handleFile(e.dataTransfer.files[0]) }}\n   143→              data-testid=\"import-dropzone\"\n   144→            >\n   145→              <UploadIcon size={32} className=\"import-dropzone-icon\" />\n   146→              <p className=\"import-dropzone-text\">Drag and drop a CSV file here, or click to browse</p>\n   147→              <label className=\"btn-secondary import-browse-btn\">\n   148→                Browse Files\n   149→                <input\n   150→                  type=\"file\"\n   151→                  accept=\".csv\"\n   152→                  className=\"import-file-input\"\n   153→                  onChange={(e) => { if (e.target.files?.[0]) handleFile(e.target.files[0]) }}\n   154→                  data-testid=\"import-file-input\"\n   155→                />\n   156→              </label>\n   157→            </div>\n   158→          )}\n   159→\n   160→          {preview.length > 0 && (\n   161→            <div className=\"import-preview\" data-testid=\"import-preview\">\n   162→              <p className=\"import-preview-info\">\n   163→                {file?.name} — {preview.length} client{preview.length !== 1 ? 's' : ''} found\n   164→              </p>\n   165→              <div className=\"import-preview-table\">\n   166→                <div className=\"import-preview-header\">\n   167→                  {Object.keys(preview[0]).slice(0, 5).map(key => (\n   168→                    <div key={key} className=\"import-preview-cell header\">{key}</div>\n   169→                  ))}\n   170→                </div>\n   171→                {preview.slice(0, 5).map((row, i) => (\n   172→                  <div key={i} className=\"import-preview-row\">\n   173→                    {Object.keys(preview[0]).slice(0, 5).map(key => (\n   174→                      <div key={key} className=\"import-preview-cell\">{row[key] || '—'}</div>\n   175→                    ))}\n   176→                  </div>\n   177→                ))}\n   178→                {preview.length > 5 && (\n   179→                  <p className=\"import-preview-more\">...and {preview.length - 5} more</p>\n   180→                )}\n   181→              </div>\n   182→            </div>\n   183→          )}\n   184→\n   185→          {error && <p className=\"import-error\" data-testid=\"import-error\">{error}</p>}\n   186→          {success && <p className=\"import-success\" data-testid=\"import-success\">{success}</p>}\n   187→        </div>\n   188→        <div className=\"modal-actions\">\n   189→          <button type=\"button\" className=\"btn-secondary\" onClick={reset} data-testid=\"import-cancel\">\n   190→            Cancel\n   191→          </button>\n   192→          {preview.length > 0 && (\n   193→            <button\n   194→              type=\"button\"\n   195→              className=\"btn-primary\"\n   196→              onClick={handleImport}\n   197→              disabled={importing}\n   198→              data-testid=\"import-confirm\"\n   199→            >\n   200→              {importing ? 'Importing...' : `Import ${preview.length} Client${preview.length !== 1 ? 's' : ''}`}\n   201→            </button>\n   202→          )}\n   203→        </div>\n   204→      </div>\n   205→    </div>\n   206→  )\n   207→}\n   208→\n   209→export default function ClientsActions() {\n   210→  const dispatch = useAppDispatch()\n   211→  const filters = useAppSelector(s => s.clients.filters)\n   212→  const [importOpen, setImportOpen] = useState(false)\n   213→  const [addOpen, setAddOpen] = useState(false)\n   214→  const [successMsg, setSuccessMsg] = useState('')\n   215→\n   216→  const handleExport = async () => {\n   217→    const params = new URLSearchParams()\n   218→    params.set('format', 'csv')\n   219→    params.set('pageSize', '10000')\n   220→    if (filters.search) params.set('search', filters.search)\n   221→    if (filters.status) params.set('status', filters.status)\n   222→    if (filters.tag) params.set('tag', filters.tag)\n   223→    if (filters.source) params.set('source', filters.source)\n   224→\n   225→    const res = await fetch(`/.netlify/functions/clients?${params}`)\n   226→    const blob = await res.blob()\n   227→    const url = URL.createObjectURL(blob)\n   228→    const a = document.createElement('a')\n   229→    a.href = url\n   230→    a.download = 'clients.csv'\n   231→    document.body.appendChild(a)\n   232→    a.click()\n   233→    document.body.removeChild(a)\n   234→    URL.revokeObjectURL(url)\n   235→  }\n   236→\n   237→  const handleCreate = async (data: { name: string; type: string; status: string; tags: string[]; source: string }) => {\n   238→    await dispatch(createClient(data)).unwrap()\n   239→    await dispatch(fetchClients())\n   240→    setSuccessMsg('Client created successfully')\n   241→    setTimeout(() => setSuccessMsg(''), 3000)\n   242→  }\n   243→\n   244→  return (\n   245→    <div className=\"clients-actions\" data-testid=\"clients-actions\">\n   246→      <button className=\"btn-ghost\" onClick={() => setImportOpen(true)} data-testid=\"import-button\">\n   247→        <Download size={14} />\n   248→        <span>Import</span>\n   249→      </button>\n   250→      <button className=\"btn-ghost\" onClick={handleExport} data-testid=\"export-button\">\n   251→        <Upload size={14} />\n   252→        <span>Export</span>\n   253→      </button>\n   254→      <button className=\"btn-primary\" onClick={() => setAddOpen(true)} data-testid=\"add-client-button\">\n   255→        <Plus size={14} />\n   256→        <span>Add New Client</span>\n   257→      </button>\n   258→\n   259→      {successMsg && (\n   260→        <div className=\"success-toast\" data-testid=\"client-success-message\">{successMsg}</div>\n   261→      )}\n   262→\n   263→      <ImportModal open={importOpen} onClose={() => setImportOpen(false)} />\n   264→      <ClientFormModal\n   265→        open={addOpen}\n   266→        onClose={() => setAddOpen(false)}\n   267→        onSubmit={handleCreate}\n   268→        title=\"Add New Client\"\n   269→      />\n   270→    </div>\n   271→  )\n   272→}\n   273→"}]},"parent_tool_use_id":"toolu_01Y7jWjPXi18N15SYj1NTCvp","session_id":"8ad7049c-1643-463a-b992-f57b445688cd","uuid":"549b26f6-0099-4593-8ce0-e6a77ebade83"}
