,\n   132→  ]\n   133→\n   134→  for (const d of deals) {\n   135→    await sql`\n   136→      INSERT INTO deals (name, client_id, stage, value)\n   137→      VALUES (${d.name}, ${clientIds[d.clientIndex]}, ${d.stage}, ${d.value})\n   138→    `\n   139→  }\n   140→\n   141→  // Insert tasks for some clients\n   142→  const now = new Date()\n   143→  const tomorrow = new Date(now)\n   144→  tomorrow.setDate(tomorrow.getDate() + 1)\n   145→  const nextWeek = new Date(now)\n   146→  nextWeek.setDate(nextWeek.getDate() + 7)\n   147→\n   148→  const tasks = [\n   149→    { title: 'Follow-up call', clientIndex: 0, dueDate: now.toISOString(), completed: false },\n   150→    { title: 'Send proposal', clientIndex: 1, dueDate: tomorrow.toISOString(), completed: false },\n   151→    { title: 'Review contract', clientIndex: 2, dueDate: nextWeek.toISOString(), completed: false },\n   152→  ]\n   153→\n   154→  for (const t of tasks) {\n   155→    await sql`\n   156→      INSERT INTO tasks (title, client_id, due_date, completed)\n   157→      VALUES (${t.title}, ${clientIds[t.clientIndex]}, ${t.dueDate}, ${t.completed})\n   158→    `\n   159→  }\n   160→\n   161→  console.log(`Seeded ${clients.length} clients, ${contacts.length} contacts, ${deals.length} deals, ${tasks.length} tasks, 2 relationships, 2 contact history entries`)\n   162→}\n   163→\n   164→seed().catch((err) => {\n   165→  console.error('Seed failed:', err)\n   166→  process.exit(1)\n   167→})\n   168→"}]},"parent_tool_use_id":"toolu_01Y7jWjPXi18N15SYj1NTCvp","session_id":"8ad7049c-1643-463a-b992-f57b445688cd","uuid":"4abee227-c28e-4a45-b97f-769962bcb0c5"}
