   const entryDate = new Date(e.contact_date)\n    48→      const startDate = new Date(filterDateStart)\n    49→      if (entryDate < startDate) return false\n    50→    }\n    51→    if (filterDateEnd) {\n    52→      const entryDate = new Date(e.contact_date)\n    53→      const endDate = new Date(filterDateEnd)\n    54→      endDate.setHours(23, 59, 59, 999)\n    55→      if (entryDate > endDate) return false\n    56→    }\n    57→    return true\n    58→  })\n    59→\n    60→  const formatDate = (dateStr: string) => {\n    61→    const date = new Date(dateStr)\n    62→    return date.toLocaleDateString('en-US', {\n    63→      month: 'short', day: 'numeric', year: 'numeric', hour: 'numeric', minute: '2-digit'\n    64→    })\n    65→  }\n    66→\n    67→  return (\n    68→    <div className=\"person-section\" data-testid=\"contact-history-section\">\n    69→      <div className=\"person-section-header\">\n    70→        <div className=\"person-section-heading\">\n    71→          {/* Test: Contact History section displays heading with icon */}\n    72→          <svg width=\"16\" height=\"16\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" strokeWidth=\"2\" strokeLinecap=\"round\" strokeLinejoin=\"round\">\n    73→            <circle cx=\"12\" cy=\"12\" r=\"10\"/>\n    74→            <polyline points=\"12 6 12 12 16 14\"/>\n    75→          </svg>\n    76→          <h2 data-testid=\"contact-history-heading\">History of Contact</h2>\n    77→        </div>\n    78→        <div className=\"person-section-actions\">\n    79→          {/* Test: Filter button is displayed in Contact History section */}\n    80→          {/* Test: Filter button opens filter options for contact history */}\n    81→          {/* Test: Filtering contact history by interaction type shows only matching entries */}\n    82→          {/* Test: Resetting contact history type filter to All Types shows all entries */}\n    83→          {/* Test: Filtering contact history by date range