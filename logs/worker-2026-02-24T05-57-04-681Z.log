[2026-02-24T05:47:09.480Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01VbbLiHQKQ8XkuKfkgTPgut","type":"tool_result","content":"     1→import { useState, useRef, useEffect } from 'react'\n     2→import { useNavigate } from 'react-router-dom'\n     3→import { MoreHorizontal, Eye, Pencil, Trash2 } from 'lucide-react'\n     4→import { useAppDispatch, useAppSelector } from '@/store/hooks'\n     5→import { deleteClient, updateClient, fetchClients, type ClientRow } from '@/store/clientsSlice'\n     6→import ClientFormModal from './ClientFormModal'\n     7→\n     8→function formatValue(value: number): string {\n     9→  if (value >= 1000000) return `$${(value / 1000000).toFixed(1)}M`\n    10→  if (value >= 1000) return `$${Math.round(value / 1000)}k`\n    11→  return `$${value.toFixed(0)}`\n    12→}\n    13→\n    14→function formatTaskDate(dateStr: string | null): string {\n    15→  if (!dateStr) return 'No date'\n    16→  const date = new Date(dateStr)\n    17→  if (isNaN(date.getTime())) return dateStr\n    18→  const today = new Date()\n    19→  const tomorrow = new Date(today)\n    20→  tomorrow.setDate(tomorrow.getDate() + 1)\n    21→\n    22→  const time = date.toLocaleTimeString('en-US', { hour: 'numeric', minute: '2-digit' })\n    23→\n    24→  if (date.toDateString() === today.toDateString()) return `Today, ${time}`\n    25→  if (date.toDateString() === tomorrow.toDateString()) return `Tomorrow, ${time}`\n    26→  return date.toLocaleDateString('en-US', { month: 'short', day: 'numeric' }) + `, ${time}`\n    27→}\n    28→\n    29→const statusColors: Record<string, string> = {\n    30→  Active: 'status-active',\n    31→  Inactive: 'status-inactive',\n    32→  Prospect: 'status-prospect',\n    33→  Churned: 'status-churned',\n    34→}\n    35→\n    36→function RowActionMenu({ client, onEdit, onDelete }: {\n    37→  client: ClientRow\n    38→  onEdit: () => void\n    39→  onDelete: () => void\n    40→}) {\n    41→  const navigate = useNavigate()\n    42→  const [open, setOpen] = useState(false)\n    43→  const ref = useRef<HTMLDivElement>(null)\n    44→\n    45→  useEffect(() => {\n    46→    const handler = (e: MouseEvent) => {\n    47→      if (ref.current && !ref.current.contains(e.target as Node)) setOpen(false)\n    48→    }\n    49→    document.addEventListener('mousedown', handler)\n    50→    return () => document.removeEventListener('mousedown', handler)\n    51→  }, [])\n    52→\n    53→  return (\n    54→    <div className=\"row-action-wrapper\" ref={ref}>\n    55→      <button\n    56→        className=\"row-action-trigger\"\n    57→        onClick={(e) => { e.stopPropagation(); setOpen(!open) }}\n    58→        data-testid={`client-actions-${client.id}`}\n    59→      >\n    60→        <MoreHorizontal size={16} />\n    61→      </button>\n    62→      {open && (\n    63→        <div className=\"row-action-menu\" data-testid={`action-menu-${client.id}`}>\n    64→          <button\n    65→            className=\"row-action-item\"\n    66→            onClick={() => { setOpen(false); navigate(`/clients/${client.id}`) }}\n    67→            data-testid={`action-view-${client.id}`}\n    68→          >\n    69→            <Eye size={14} />\n    70→            <span>View Details</span>\n    71→          </button>\n    72→          <button\n    73→            className=\"row-action-item\"\n    74→            onClick={() => { setOpen(false); onEdit() }}\n    75→            data-testid={`action-edit-${client.id}`}\n    76→          >\n    77→            <Pencil size={14} />\n    78→            <span>Edit</span>\n    79→          </button>\n    80→          <button\n    81→            className=\"row-action-item danger\"\n    82→            onClick={() => { setOpen(false); onDelete() }}\n    83→            data-testid={`action-delete-${client.id}`}\n    84→          >\n    85→            <Trash2 size={14} />\n    86→            <span>Delete</span>\n    87→          </button>\n    88→        </div>\n    89→      )}\n    90→    </div>\n    91→  )\n    92→}\n    93→\n    94→function DeleteConfirmModal({ open, clientName, onConfirm, onCancel }: {\n    95→  open: boolean\n    96→  clientName: string\n    97→  onConfirm: () => void\n    98→  onCancel: () => void\n    99→}) {\n   100→  if (!open) return null\n   101→  return (\n   102→    <div className=\"modal-overlay\" onClick={onCancel} data-testid=\"delete-confirm-modal\">\n   103→      <div className=\"modal-content modal-small\" onClick={(e) => e.stopPropagation()}>\n   104→        <h2 className=\"modal-title\">Delete Client</h2>\n   105→        <p className=\"modal-text\" data-testid=\"delete-confirm-message\">\n   106→          Are you sure you want to delete {clientName}?\n   107→        </p>\n   108→        <div className=\"modal-actions\">\n   109→          <button className=\"btn-secondary\" onClick={onCancel} data-testid=\"delete-cancel\">\n   110→            Cancel\n   111→          </button>\n   112→          <button className=\"btn-danger\" onClick={onConfirm} data-testid=\"delete-confirm\">\n   113→            Delete\n   114→          </button>\n   115→        </div>\n   116→      </div>\n   117→    </div>\n   118→  )\n   119→}\n   120→\n   121→export default function ClientsTable() {\n   122→  const navigate = useNavigate()\n   123→  const dispatch = useAppDispatch()\n   124→  const items = useAppSelector(s => s.clients.items)\n   125→  const loading = useAppSelector(s => s.clients.loading)\n   126→  const [editClient, setEditClient] = useState<ClientRow | null>(null)\n   127→  const [deleteTarget, setDeleteTarget] = useState<ClientRow | null>(null)\n   128→\n   129→  const handleUpdate = async (data: { name: string; type: string; status: string; tags: string[]; source: string }) => {\n   130→    if (!editClient) return\n   131→    await dispatch(updateClient({ id: editClient.id, ...data })).unwrap()\n   132→    await dispatch(fetchClients())\n   133→  }\n   134→\n   135→  const handleDelete = async () => {\n   136→    if (!deleteTarget) return\n   137→    await dispatch(deleteClient(deleteTarget.id)).unwrap()\n   138→    setDeleteTarget(null)\n   139→    await dispatch(fetchClients())\n   140→  }\n   141→\n   142→  if (loading) {\n   143→    return (\n   144→      <div className=\"clients-table-empty\" data-testid=\"clients-table-loading\">\n   145→        <p>Loading clients...</p>\n   146→      </div>\n   147→    )\n   148→  }\n   149→\n   150→  if (items.length === 0) {\n   151→    return (\n   152→      <div className=\"clients-table-empty\" data-testid=\"clients-table-empty\">\n   153→        <p className=\"empty-title\">No clients found</p>\n   154→        <p className=\"empty-subtitle\">No clients yet. Add your first client to get started.</p>\n   155→      </div>\n   156→    )\n   157→  }\n   158→\n   159→  return (\n   160→    <>\n   161→      <div className=\"clients-table\" data-testid=\"clients-table\">\n   162→        <div className=\"clients-table-header\">\n   163→          <div className=\"table-cell\">Client Name</div>\n   164→          <div className=\"table-cell\">Type</div>\n   165→          <div className=\"table-cell\">Status</div>\n   166→          <div className=\"table-cell\">Tags</div>\n   167→          <div className=\"table-cell\">Primary Contact</div>\n   168→          <div className=\"table-cell\">Open Deals</div>\n   169→          <div className=\"table-cell\">Next Task</div>\n   170→          <div className=\"table-cell table-cell-action\"></div>\n   171→        </div>\n   172→        {items.map((client) => {\n   173→          const pc = client.primary_contact\n   174→          const od = client.open_deals\n   175→          const nt = client.next_task\n   176→\n   177→          const contactDisplay = pc\n   178→            ? client.type === 'Individual' && pc.name === client.name\n   179→              ? `${client.name} (Self)`\n   180→              : `${pc.name}${pc.role ? ` (${pc.role})` : ''}`\n   181→            : '—'\n   182→\n   183→          const dealsDisplay = od && od.count > 0\n   184→            ? `${od.count} (Value: ${formatValue(Number(od.totalValue))})`\n   185→            : '0'\n   186→\n   187→          const taskDisplay = nt\n   188→            ? `${nt.title} - ${formatTaskDate(nt.dueDate)}`\n   189→            : 'No task scheduled'\n   190→\n   191→          return (\n   192→            <div\n   193→              key={client.id}\n   194→              className=\"clients-table-row\"\n   195→              data-testid={`client-row-${client.id}`}\n   196→            >\n   197→              <div className=\"table-cell\">\n   198→                <button\n   199→                  className=\"client-name-link\"\n   200→                  onClick={() => navigate(`/clients/${client.id}`)}\n   201→                  data-testid={`client-name-${client.id}`}\n   202→                >\n   203→                  {client.name}\n   204→                </button>\n   205→              </div>\n   206→              <div className=\"table-cell table-cell-type\" data-testid={`client-type-${client.id}`}>\n   207→                {client.type}\n   208→              </div>\n   209→              <div className=\"table-cell\">\n   210→                <span\n   211→                  className={`status-badge ${statusColors[client.status] || ''}`}\n   212→                  data-testid={`client-status-${client.id}`}\n   213→                >\n   214→                  {client.status}\n   215→                </span>\n   216→              </div>\n   217→              <div className=\"table-cell table-cell-tags\" data-testid={`client-tags-${client.id}`}>\n   218→                {(client.tags || []).map(tag => (\n   219→                  <span key={tag} className=\"tag-chip\">{tag}</span>\n   220→                ))}\n   221→              </div>\n   222→              <div className=\"table-cell table-cell-contact\" data-testid={`client-contact-${client.id}`}>\n   223→                {contactDisplay}\n   224→              </div>\n   225→              <div className=\"table-cell\" data-testid={`client-deals-${client.id}`}>\n   226→                {dealsDisplay}\n   227→              </div>\n   228→              <div className=\"table-cell table-cell-task\" data-testid={`client-task-${client.id}`}>\n   229→                {taskDisplay}\n   230→              </div>\n   231→              <div className=\"table-cell table-cell-action\">\n   232→                <RowActionMenu\n   233→                  client={client}\n   234→                  onEdit={() => setEditClient(client)}\n   235→                  onDelete={() => setDeleteTarget(client)}\n   236→                />\n   237→              </div>\n   238→            </div>\n   239→          )\n   240→        })}\n   241→      </div>\n   242→\n   243→      <ClientFormModal\n   244→        open={!!editClient}\n   245→        onClose={() => setEditClient(null)}\n   246→        onSubmit={handleUpdate}\n   247→        title=\"Edit Client\"\n   248→        initial={editClient ? {\n   249→          name: editClient.name,\n   250→          type: editClient.type,\n   251→          status: editClient.status,\n   252→          tags: editClient.tags || [],\n   253→          source: editClient.source || '',\n   254→        } : undefined}\n   255→      />\n   256→\n   257→      <DeleteConfirmModal\n   258→        open={!!deleteTarget}\n   259→        clientName={deleteTarget?.name || ''}\n   260→        onConfirm={handleDelete}\n   261→        onCancel={() => setDeleteTarget(null)}\n   262→      />\n   263→    </>\n   264→  )\n   265→}\n   266→"}]},"parent_tool_use_id":"toolu_01Y7jWjPXi18N15SYj1NTCvp","session_id":"8ad7049c-1643-463a-b992-f57b445688cd","uuid":"6d0d114f-58e1-4905-8821-a3a328170adf"}
