alue}\n    42→              className={`filter-dropdown-option${opt.value === value ? ' selected' : ''}`}\n    43→              onClick={() => { onChange(opt.value); setOpen(false) }}\n    44→              data-testid={`${testId}-option-${opt.value || 'all'}`}\n    45→            >\n    46→              {opt.label}\n    47→            </button>\n    48→          ))}\n    49→        </div>\n    50→      )}\n    51→    </div>\n    52→  )\n    53→}\n    54→\n    55→const statusOptions = [\n    56→  { label: 'All', value: '' },\n    57→  { label: 'Active', value: 'Active' },\n    58→  { label: 'Inactive', value: 'Inactive' },\n    59→  { label: 'Prospect', value: 'Prospect' },\n    60→  { label: 'Churned', value: 'Churned' },\n    61→]\n    62→\n    63→const sortOptions = [\n    64→  { label: 'Recently Updated', value: 'recently_updated' },\n    65→  { label: 'Name A-Z', value: 'name_asc' },\n    66→  { label: 'Name Z-A', value: 'name_desc' },\n    67→]\n    68→\n    69→export default function ClientsSearchAndFilters() {\n    70→  const dispatch = useAppDispatch()\n    71→  const filters = useAppSelector(s => s.clients.filters)\n    72→  const availableTags = useAppSelector(s => s.clients.availableTags)\n    73→  const availableSources = useAppSelector(s => s.clients.availableSources)\n    74→  const [localSearch, setLocalSearch] = useState(filters.search)\n    75→\n    76→  useEffect(() => {\n    77→    const timer = setTimeout(() => {\n    78→      dispatch(setSearch(localSearch))\n    79→    }, 300)\n    80→    return () => clearTimeout(timer)\n    81→  }, [localSearch, dispatch])\n    82→\n    83→  const [prevReduxSearch, setPrevReduxSearch] = useState(filters.search)\n    84→  if (filters.search !== prevReduxSearch) {\n    85→    setPrevReduxSearch(filters.search)\n    86→    setLocalSearch(filters.search)\n    87→  }\n    88→\n    89→  const tagOptions = [\n    90→    { label: 'All', value: '' },\n    91→    ...availableTags.map(t => ({ label: t, value: t })),\n    92→  ]\n    93→\n    94→  const sourceOptions = [\n    95→    { label: 'All', value: '' },\n    96→    ...availableSources.map(s => ({ label: s, value: s })),\n    97→  ]\n    98→\n    99→  return (\n   100→    <div className=\"clients-filters\" data-testid=\"clients-search-and-filters\">\n   101→      <div className=\"clients-search-wrapper\" data-testid=\"search-input-wrapper\">\n   102→        <Search size={16} className=\"clients-search-icon\" />\n   103→        <input\n   104→          type=\"text\"\n   105→          className=\"clients-search-input\"\n   106→          placeholder=\"Search clients by name, tag, or contact...\"\n   107→          value={localSearch}\n   108→          onChange={(e) => setLocalSearch(e.target.value)}\n   109→          data-testid=\"search-input\"\n   110→        />\n   111→        {localSearch && (\n   112→          <button\n   113→            className=\"clients-search-clear\"\n   114→            onClick={() => setLocalSearch('')}\n   115→            data-testid=\"search-clear\"\n   116→          >\n   117→            <X size={14} />\n   118→          </button>\n   119→        )}\n   120→      </div>\n   121→      <div className=\"clients-filter-row\">\n   122→        <FilterDropdown\n   123→          label=\"Status\"\n   124→          value={filters.status}\n   125→          options={statusOptions}\n   126→          onChange={(v) => dispatch(setStatusFilter(v))}\n   127→          testId=\"status-filter\"\n   128→        />\n   129→        <FilterDropdown\n   130→          label=\"Tags\"\n   131→          value={filters.tag}\n   132→          options={tagOptions}\n   133→          onChange={(v) => dispatch(setTagFilter(v))}\n   134→          testId=\"tags-filter\"\n   135→        />\n   136→        <FilterDropdown\n   137→          label=\"Source\"\n   138→          value={filters.source}\n   139→          options={sourceOptions}\n   140→          onChange={(v) => dispatch(setSourceFilter(v))}\n   141→          testId=\"source-filter\"\n   142→        />\n   143→        <FilterDropdown\n   144→          label=\"Sort\"\n   145→          value={filters.sort}\n   146→          options={sortOptions}\n   147→          onChange={(v) => dispatch(setSort(v))}\n   148→          testId=\"sort-filter\"\n   149→        />\n   150→      </div>\n   151→    </div>\n   152→  )\n   153→}\n   154→"}]},"parent_tool_use_id":"toolu_01Y7jWjPXi18N15SYj1NTCvp","session_id":"8ad7049c-1643-463a-b992-f57b445688cd","uuid":"3a9b2250-6ddc-4bf1-a91c-aedb9b5a3d10"}
