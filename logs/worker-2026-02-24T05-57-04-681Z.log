/div>\n   229→  )\n   230→}\n   231→\n   232→interface ContactHistoryFormModalProps {\n   233→  individualId: string\n   234→  entry?: ContactEntry\n   235→  onClose: () => void\n   236→  onSaved: () => void\n   237→}\n   238→\n   239→function ContactHistoryFormModal({ individualId, entry, onClose, onSaved }: ContactHistoryFormModalProps) {\n   240→  const isEditing = !!entry\n   241→  const [interactionType, setInteractionType] = useState(entry?.interaction_type || '')\n   242→  const [summary, setSummary] = useState(entry?.summary || '')\n   243→  const [teamMember, setTeamMember] = useState(entry?.team_member || '')\n   244→  const [contactDate, setContactDate] = useState(\n   245→    entry?.contact_date ? new Date(entry.contact_date).toISOString().slice(0, 16) : ''\n   246→  )\n   247→  const [showTypeDropdown, setShowTypeDropdown] = useState(false)\n   248→  const [errors, setErrors] = useState<Record<string, string>>({})\n   249→  const [saving, setSaving] = useState(false)\n   250→\n   251→  const handleSubmit = async () => {\n   252→    const newErrors: Record<string, string> = {}\n   253→    if (!interactionType) newErrors.type = 'Interaction type is required'\n   254→    if (!summary) newErrors.summary = 'Summary is required'\n   255→    if (!contactDate) newErrors.date = 'Date/time is required'\n   256→    if (!teamMember) newErrors.teamMember = 'Team member is required'\n   257→    if (Object.keys(newErrors).length > 0) {\n   258→      setErrors(newErrors)\n   259→      return\n   260→    }\n   261→\n   262→    setSaving(true)\n   263→    try {\n   264→      const url = isEditing\n   265→        ? `/.netlify/functions/individuals/${individualId}/contact-history/${entry!.id}`\n   266→        : `/.netlify/functions/individuals/${individualId}/contact-history`\n   267→\n   268→      const res = await fetch(url, {\n   269→        method: isEditing ? 'PUT' : 'POST',\n   270→        headers: { 'Content-Type': 'application/json' },\n   271→        body: JSON.stringify({\n   272→          interaction_type: interactionType,\n   273→          summary,\n   274→          team_member: teamMember || null,\n   275→          contact_date: contactDate || null,\n   276→        }),\n   277→      })\n   278→      if (res.ok) {\n   279→        onSaved()\n   280→      }\n   281→    } finally {\n   282→      setSaving(false)\n   283→    }\n   284→  }\n   285→\n   286→  return (\n   287→    <div className=\"modal-overlay\" data-testid=\"contact-history-form-modal\">\n   288→      <div className=\"modal-content\">\n   289→        <div className=\"modal-header\">\n   290→          <h3 className=\"modal-title\">{isEditing ? 'Edit Contact History' : 'Add Contact History Entry'}</h3>\n   291→          <button className=\"modal-close\" onClick={onClose} data-testid=\"contact-history-form-close\">\n   292→            <svg width=\"16\" height=\"16\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" strokeWidth=\"2\" strokeLinecap=\"round\" strokeLinejoin=\"round\">\n   293→              <line x1=\"18\" y1=\"6\" x2=\"6\" y2=\"18\"/><line x1=\"6\" y1=\"6\" x2=\"18\" y2=\"18\"/>\n   294→            </svg>\n   295→          </button>\n   296→        </div>\n   297→\n   298→        <div className=\"modal-form\">\n   299→          <div className=\"form-field\">\n   300→            <label className=\"form-label\">Date/Time *</label>\n   301→            <input\n   302→              type=\"datetime-local\"\n   303→              className={`form-input ${errors.date ? 'error' : ''}`}\n   304→              value={contactDate}\n   305→              onChange={e => setContactDate(e.target.value)}\n   306→              data-testid=\"contact-history-date-input\"\n   307→            />\n   308→            {errors.date && <span className=\"form-error\">{errors.date}</span>}\n   309→          </div>\n   310→\n   311→          <div className=\"form-field\">\n   312→            <label className=\"form-label\">Interaction Type *</label>\n   313→            <div className=\"form-dropdown-wrapper\">\n   314→              <button\n   315→                className=\"form-dropdown-trigger\"\n   316→                onClick={() => setShowTypeDropdown(!showTypeDropdown)}\n   317→                data-testid=\"contact-history-type-dropdown\"\n   318→              >\n   319→                <span>{interactionType || 'Select type...'}</span>\n   320→                <svg width=\"12\" height=\"12\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" strokeWidth=\"2\">\n   321→                  <polyline points=\"6 9 12 15 18 9\"/>\n   322→                </svg>\n   323→              </button>\n   324→              {showTypeDropdown && (\n   325→                <div className=\"form-dropdown-menu\">\n   326→                  {INTERACTION_TYPES.map(type => (\n   327→                    <button\n   328→                      key={type}\n   329→                      className={`form-dropdown-option ${interactionType === type ? 'selected' : ''}`}\n   330→                      onClick={() => { setInteractionType(type); setShowTypeDropdown(false) }}\n   331→                      data-testid=\"contact-history-type-option\"\n   332→                    >\n   333→                      {type}\n   334→                    </button>\n   335→                  ))}\n   336→                </div>\n   337→              )}\n   338→            </div>\n   339→            {errors.type && <span className=\"form-error\">{errors.type}</span>}\n   340→          </div>\n   341→\n   342→          <div className=\"form-field\">\n   343→            <label className=\"form-label\">Summary *</label>\n   344→            <textarea\n   345→              className={`form-input form-textarea ${errors.summary ? 'error' : ''}`}\n   346→              value={summary}\n   347→              onChange={e => setSummary(e.target.value)}\n   348→              placeholder=\"Enter summary...\"\n   349→              rows={3}\n   350→              data-testid=\"contact-history-summary-input\"\n   351→            />\n   352→            {errors.summary && <span className=\"form-error\">{errors.summary}</span>}\n   353→          </div>\n   354→\n   355→          <div className=\"form-field\">\n   356→            <label className=\"form-label\">Team Member *</label>\n   357→            <input\n   358→              type=\"text\"\n   359→              className={`form-input ${errors.teamMember ? 'error' : ''}`}\n   360→              value={teamMember}\n   361→              onChange={e => setTeamMember(e.target.value)}\n   362→              placeholder=\"e.g. Michael B. (Sales Lead)\"\n   363→              data-testid=\"contact-history-team-input\"\n   364→            />\n   365→            {errors.teamMember && <span className=\"form-error\">{errors.teamMember}</span>}\n   366→          </div>\n   367→        </div>\n   368→\n   369→        <div className=\"modal-actions\">\n   370→          <button className=\"btn-secondary\" onClick={onClose} data-testid=\"contact-history-form-cancel\">Cancel</button>\n   371→          <button className=\"btn-primary\" onClick={handleSubmit} disabled={saving} data-testid=\"contact-history-form-save\">\n   372→            {saving ? 'Saving...' : 'Save'}\n   373→          </button>\n   374→        </div>\n   375→      </div>\n   376→    </div>\n   377→  )\n   378→}\n   379→"}]},"parent_tool_use_id":"toolu_01Y7jWjPXi18N15SYj1NTCvp","session_id":"8ad7049c-1643-463a-b992-f57b445688cd","uuid":"3f1d5a48-b783-4c15-bc26-c85f3d4e895a"}
