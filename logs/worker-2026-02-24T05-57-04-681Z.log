                <button\n   193→                  className=\"relationship-link\"\n   194→                  data-testid=\"relationship-link\"\n   195→                  onClick={() => navigate(`/individuals/${rel.related_id}`)}\n   196→                >\n   197→                  [Link]\n   198→                </button>\n   199→                {/* Test: Relationship entry can be deleted */}\n   200→                <button\n   201→                  className=\"relationship-delete\"\n   202→                  data-testid=\"relationship-delete-button\"\n   203→                  onClick={() => setDeleteConfirmId(rel.id)}\n   204→                  title=\"Delete relationship\"\n   205→                >\n   206→                  <svg width=\"14\" height=\"14\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" strokeWidth=\"2\" strokeLinecap=\"round\" strokeLinejoin=\"round\">\n   207→                    <polyline points=\"3 6 5 6 21 6\"/><path d=\"M19 6v14a2 2 0 0 1-2 2H7a2 2 0 0 1-2-2V6m3 0V4a2 2 0 0 1 2-2h4a2 2 0 0 1 2 2v2\"/>\n   208→                  </svg>\n   209→                </button>\n   210→              </div>\n   211→            ))}\n   212→          </div>\n   213→        ) : (\n   214→          /* Test: Relationships section shows empty state when no relationships exist */\n   215→          <div className=\"person-section-empty\" data-testid=\"relationships-empty\">\n   216→            No relationships yet\n   217→          </div>\n   218→        )\n   219→      ) : (\n   220→        <div className=\"relationships-graph\" data-testid=\"relationships-graph\">\n   221→          {filteredRelationships.length > 0 ? (\n   222→            <RelationshipGraph relationships={filteredRelationships} />\n   223→          ) : (\n   224→            <div className=\"person-section-empty\" data-testid=\"relationships-empty\">\n   225→              No relationships yet\n   226→            </div>\n   227→          )}\n   228→        </div>\n   229→      )}\n   230→\n