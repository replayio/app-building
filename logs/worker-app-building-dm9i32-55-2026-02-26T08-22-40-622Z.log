  });\n   111→  emptyRunId = (await res.json()).id;\n   112→});\n   113→\n   114→async function goToRun(page: Page, runId: string) {\n   115→  await page.goto(`/runs/${runId}`);\n   116→  await expect(page.getByTestId(\"run-header\")).toBeVisible({ timeout: 30000 });\n   117→}\n   118→\n   119→// ─── RunHeader ───────────────────────────────────────────────────────────────\n   120→\n   121→test.describe(\"RunHeader\", () => {\n   122→  test(\"RUN-HDR-1: Breadcrumb displays Home > Runs > Run ID\", async ({\n   123→    page,\n   124→  }) => {\n   125→    await goToRun(page, scheduledRunId);\n   126→\n   127→    const breadcrumb = page.getByTestId(\"breadcrumb\");\n   128→    await expect(breadcrumb).toBeVisible();\n   129→\n   130→    // Home link\n   131→    await expect(breadcrumb.locator(\"a\").first()).toHaveText(\"Home\");\n   132→    // Separator\n   133→    await expect(breadcrumb.locator(\".breadcrumb-separator\")).toHaveCount(2);\n   134→    // Runs link\n   135→    await expect(breadcrumb.locator(\"a\").nth(1)).toHaveText(\"Runs\");\n   136→    // Current item (run ID)\n   137→    await expect(breadcrumb.locator(\".breadcrumb-current\")).toHaveText(\n   138→      scheduledRunId\n   139→    );\n   140→  });\n   141→\n   142→  test(\"RUN-HDR-2: Breadcrumb Home link navigates to home/dashboard\", async ({\n   143→    page,\n   144→  }) => {\n   145→    await goToRun(page, scheduledRunId);\n   146→\n   147→    await page\n   148→      .getByTestId(\"breadcrumb\")\n   149→      .locator(\"a\", { hasText: \"Home\" })\n   150→      .click();\n   151→    // \"/\" redirects to \"/recipes\" in this app\n   152→    await expect(page).toHaveURL(/\\/recipes/, { timeout: 30000 });\n   153→  });\n   154→\n   155→  test(\"RUN-HDR-3: Breadcrumb Runs link navigates to CalendarPage\", async ({\n   156→    page,\n   157→  }) => {\n   158→    await goToRun(page, scheduledRunId);\n   159→\n   160→    await page\n   161→      .getByTestId(\"breadcrumb\")\n   162→      .locator(\"a