px;\n  height: 14px;\n  background: transparent;\n  border: none;\n  border-radius: 50%;\n  cursor: pointer;\n  color: var(--text-muted);\n  font-size: 12px;\n  padding: 0;\n  line-height: 1;\n}\n\n.edit-form-tag-remove:hover {\n  color: var(--status-error);\n}\n\n.edit-form-tag-input {\n  height: 24px;\n  padding: 0 6px;\n  font-size: 12px;\n  border: 1px solid var(--normal-border);\n  border-radius: 12px;\n  background: transparent;\n  color: var(--text-primary);\n  outline: none;\n  width: 100px;\n}\n\n.edit-form-tag-input:focus {\n  border-color: var(--accent-primary);\n}\n\n/* Attachment modal toggle */\n.attachment-mode-toggle {\n  display: flex;\n  border: 1px solid var(--bg-border-color);\n  border-radius: 4px;\n  overflow: hidden;\n  margin-bottom: 16px;\n}\n\n.attachment-mode-btn {\n  flex: 1;\n  height: 32px;\n  font-size: 13px;\n  font-family: var(--font-family);\n  background: transparent;\n  border: none;\n  cursor: pointer;\n  color: var(--text-muted);\n  transition: background-color 0.15s ease, color 0.15s ease;\n}\n\n.attachment-mode-btn--active {\n  background: var(--accent-subtle-bg);\n  color: var(--accent-primary);\n  font-weight: 500;\n}\n\n.attachment-mode-btn:hover:not(.attachment-mode-btn--active) {\n  background: rgba(50, 45, 35, 0.04);\n}\n\n/* File upload area */\n.file-upload-area {\n  border: 2px dashed var(--bg-border-color);\n  border-radius: 6px;\n  padding: 24px;\n  text-align: center;\n  cursor: pointer;\n  transition: border-color 0.15s ease, background-color 0.15s ease;\n  margin-bottom: 16px;\n}\n\n.file-upload-area:hover {\n  border-color: var(--accent-muted);\n  background: var(--accent-subtle-bg);\n}\n\n.file-upload-label {\n  font-size: 13px;\n  color: var(--text-muted);\n}\n\n.file-upload-label strong {\n  color: var(--accent-primary);\n  cursor: pointer;\n}\n\n.file-upload-selected {\n  font-size: 13px;\n  color: var(--text-primary);\n  margin-top: 8px;\n}\n\n/* Two-column layout for sections */\n.client-detail-sections {\n  display: grid;\n  grid-template-columns: 1fr 1fr;\n  gap: 24px;\n}\n\n@media (max-width: 768px) {\n  .client-detail-sections {\n    grid-template-columns: 1fr;\n  }\n\n  .source-info-grid {\n    grid-template-columns: 1fr;\n  }\n}\n\n/* ========= Person Detail Page ========= */\n\n.person-detail {\n  display: flex;\n  flex-direction: column;\n  gap: 24px;\n}\n\n/* Person Header */\n.person-header {\n  display: flex;\n  flex-direction: column;\n  gap: 8px;\n}\n\n.person-header-name {\n  font-size: 22px;\n  font-weight: 600;\n  color: var(--text-primary);\n}\n\n.person-header-title {\n  font-size: 14px;\n  color: var(--text-muted);\n}\n\n.person-header-contact {\n  display: flex;\n  flex-direction: column;\n  gap: 6px;\n  margin-top: 4px;\n}\n\n.person-header-contact-row {\n  display: flex;\n  align-items: center;\n  gap: 8px;\n  font-size: 13px;\n  color: var(--text-secondary);\n}\n\n.person-header-contact-row svg {\n  flex-shrink: 0;\n  color: var(--text-muted);\n}\n\n.person-header-clients {\n  display: flex;\n  align-items: center;\n  gap: 6px;\n  font-size: 13px;\n  color: var(--text-secondary);\n  margin-top: 4px;\n  flex-wrap: wrap;\n}\n\n.person-header-clients-label {\n  font-weight: 500;\n  color: var(--text-muted);\n}\n\n.person-header-client-link {\n  background: none;\n  border: none;\n  color: var(--accent-primary);\n  cursor: pointer;\n  font-size: 13px;\n  font-family: var(--font-family);\n  padding: 0;\n  text-decoration: none;\n  transition: color 0.1s ease;\n}\n\n.person-header-client-link:hover {\n  color: var(--accent-dark);\n  text-decoration: underline;\n}\n\n.person-header-client-separator {\n  color: var(--text-muted);\n}\n\n/* Detail section layout helpers */\n.detail-section-header-left {\n  display: flex;\n  align-items: center;\n  gap: 8px;\n}\n\n.detail-section-header-actions {\n  display: flex;\n  align-items: center;\n  gap: 8px;\n}\n\n.section-icon {\n  flex-shrink: 0;\n  color: var(--text-muted);\n}\n\n.detail-section-filter {\n  padding: 8px 16px;\n  border-bottom: 1px solid var(--bg-border-color-light);\n}\n\n/* Button size variant */\n.btn--sm {\n  height: 28px;\n  padding: 0 10px;\n  font-size: 12px;\n}\n\n/* View tabs */\n.view-tabs {\n  display: flex;\n  border: 1px solid var(--bg-border-color);\n  border-radius: 4px;\n  overflow: hidden;\n}\n\n.view-tab {\n  height: 28px;\n  padding: 0 10px;\n  font-size: 12px;\n  font-family: var(--font-family);\n  background: transparent;\n  border: none;\n  cursor: pointer;\n  color: var(--text-muted);\n  transition: background-color 0.15s ease, color 0.15s ease;\n}\n\n.view-tab--active {\n  background: var(--accent-subtle-bg);\n  color: var(--accent-primary);\n  font-weight: 500;\n}\n\n.view-tab:hover:not(.view-tab--active) {\n  background: rgba(50, 45, 35, 0.04);\n}\n\n/* Relationships List */\n.relationships-list {\n  display: flex;\n  flex-direction: column;\n}\n\n.relationship-item {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  padding: 10px 0;\n  border-bottom: 1px solid var(--bg-border-color-light);\n}\n\n.relationship-item:last-child {\n  border-bottom: none;\n}\n\n.relationship-info {\n  display: flex;\n  align-items: baseline;\n  gap: 6px;\n  flex-wrap: wrap;\n}\n\n.relationship-name {\n  font-size: 13px;\n  font-weight: 600;\n  color: var(--text-primary);\n}\n\n.relationship-type {\n  font-size: 13px;\n  color: var(--text-muted);\n}\n\n.relationship-detail {\n  font-size: 12px;\n  color: var(--text-muted);\n}\n\n.relationship-actions {\n  display: flex;\n  align-items: center;\n  gap: 6px;\n  flex-shrink: 0;\n}\n\n.relationship-link {\n  font-size: 12px;\n  color: var(--accent-primary);\n  background: none;\n  border: none;\n  cursor: pointer;\n  font-family: var(--font-family);\n  padding: 2px 4px;\n  transition: color 0.1s ease;\n}\n\n.relationship-link:hover {\n  color: var(--accent-dark);\n  text-decoration: underline;\n}\n\n.relationship-delete-btn {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 24px;\n  height: 24px;\n  background: transparent;\n  border: none;\n  border-radius: 4px;\n  cursor: pointer;\n  color: var(--text-muted);\n  transition: background-color 0.1s ease, color 0.1s ease;\n}\n\n.relationship-delete-btn:hover {\n  background: rgba(176, 80, 80, 0.08);\n  color: var(--status-error);\n}\n\n/* Relationships Graph */\n.relationships-graph {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n}\n\n.relationships-graph-container {\n  width: 100%;\n  max-width: 400px;\n}\n\n.relationships-graph-svg {\n  width: 100%;\n  height: auto;\n}\n\n/* Contact History */\n.contact-history-list {\n  display: flex;\n  flex-direction: column;\n}\n\n.contact-history-item {\n  display: flex;\n  align-items: flex-start;\n  justify-content: space-between;\n  padding: 12px 0;\n  border-bottom: 1px solid var(--bg-border-color-light);\n  gap: 12px;\n}\n\n.contact-history-item:last-child {\n  border-bottom: none;\n}\n\n.contact-history-item-content {\n  flex: 1;\n  min-width: 0;\n  display: flex;\n  flex-direction: column;\n  gap: 4px;\n}\n\n.contact-history-item-header {\n  display: flex;\n  align-items: center;\n  gap: 10px;\n}\n\n.contact-history-date {\n  font-size: 13px;\n  font-weight: 500;\n  color: var(--text-primary);\n}\n\n.contact-history-type-badge {\n  display: inline-flex;\n  align-items: center;\n  height: 20px;\n  padding: 0 8px;\n  font-size: 11px;\n  font-weight: 500;\n  border-radius: 10px;\n  background: #e3ecf0;\n  color: var(--status-info);\n}\n\n.contact-history-summary {\n  font-size: 13px;\n  color: var(--text-secondary);\n  line-height: 1.5;\n}\n\n.contact-history-performer {\n  font-size: 12px;\n  color: var(--text-muted);\n}\n\n.contact-history-edit-btn {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 28px;\n  height: 28px;\n  background: transparent;\n  border: none;\n  border-radius: 4px;\n  cursor: pointer;\n  color: var(--text-muted);\n  flex-shrink: 0;\n  transition: background-color 0.1s ease, color 0.1s ease;\n}\n\n.contact-history-edit-btn:hover {\n  background: rgba(50, 45, 35, 0.06);\n  color: var(--text-primary);\n}\n\n/* Contact History Edit Inline */\n.contact-history-edit {\n  padding: 12px 0;\n  border-bottom: 1px solid var(--bg-border-color-light);\n}\n\n.contact-history-edit:last-child {\n  border-bottom: none;\n}\n\n.contact-history-edit-actions {\n  display: flex;\n  gap: 8px;\n  justify-content: flex-end;\n  margin-top: 8px;\n}\n\n/* Textarea */\n.form-textarea {\n  height: auto;\n  padding: 8px 10px;\n  resize: vertical;\n  font-family: var(--font-family);\n}\n\n/* Associated Clients */\n.associated-clients-grid {\n  display: grid;\n  grid-template-columns: repeat(auto-fill, minmax(280px, 1fr));\n  gap: 16px;\n}\n\n.associated-client-card {\n  border: 1px solid var(--bg-border-color-light);\n  border-radius: 6px;\n  padding: 16px;\n  display: flex;\n  flex-direction: column;\n  gap: 12px;\n  background: var(--bg-base-color);\n  transition: border-color 0.1s ease;\n}\n\n.associated-client-card:hover {\n  border-color: var(--normal-border-hover);\n}\n\n.associated-client-header {\n  display: flex;\n  align-items: center;\n  gap: 8px;\n}\n\n.associated-client-icon {\n  flex-shrink: 0;\n  color: var(--text-muted);\n}\n\n.associated-client-name {\n  font-size: 14px;\n  font-weight: 600;\n  color: var(--text-primary);\n}\n\n.associated-client-details {\n  display: flex;\n  flex-direction: column;\n  gap: 4px;\n}\n\n.associated-client-field {\n  font-size: 12px;\n  color: var(--text-muted);\n}\n\n.associated-client-field-label {\n  font-weight: 500;\n}\n\n.associated-client-field-value {\n  color: var(--text-secondary);\n}\n\n.associated-client-view-btn {\n  display: inline-flex;\n  align-items: center;\n  gap: 6px;\n  align-self: flex-start;\n  font-size: 12px;\n}\n\n/* ========= Deals List Page ========= */\n\n.deals-page {\n  display: flex;\n  flex-direction: column;\n  min-height: 0;\n}\n\n.deals-header-actions {\n  display: flex;\n  align-items: center;\n  gap: 8px;\n}\n\n.deals-breadcrumb {\n  font-size: 12px;\n  color: var(--text-muted);\n  margin-bottom: 4px;\n}\n\n/* Deals Summary Cards */\n.deals-summary-cards {\n  display: grid;\n  grid-template-columns: repeat(4, 1fr);\n  gap: 16px;\n  margin-bottom: 20px;\n}\n\n@media (max-width: 768px) {\n  .deals-summary-cards {\n    grid-template-columns: repeat(2, 1fr);\n  }\n}\n\n.deals-summary-card {\n  display: flex;\n  align-items: center;\n  gap: 12px;\n  padding: 16px;\n  background: #ffffff;\n  border: 1px solid var(--bg-border-color-light);\n  border-radius: 6px;\n}\n\n.deals-summary-card-icon {\n  flex-shrink: 0;\n  width: 36px;\n  height: 36px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  border-radius: 8px;\n}\n\n.deals-summary-card-icon--active {\n  background: var(--accent-subtle-bg);\n  color: var(--accent-primary);\n}\n\n.deals-summary-card-icon--pipeline {\n  background: #e3ecf0;\n  color: var(--status-info);\n}\n\n.deals-summary-card-icon--won {\n  background: #e6ede8;\n  color: var(--status-success);\n}\n\n.deals-summary-card-icon--lost {\n  background: #f5e3e3;\n  color: var(--status-error);\n}\n\n.deals-summary-card-content {\n  display: flex;\n  flex-direction: column;\n  gap: 2px;\n}\n\n.deals-summary-card-label {\n  font-size: 12px;\n  font-weight: 500;\n  color: var(--text-muted);\n}\n\n.deals-summary-card-value {\n  font-size: 18px;\n  font-weight: 600;\n  color: var(--text-primary);\n  font-variant-numeric: tabular-nums;\n}\n\n/* Deals Controls (View Toggle row) */\n.deals-controls {\n  display: flex;\n  align-items: center;\n  justify-content: flex-end;\n  margin-bottom: 16px;\n}\n\n/* Deals Filters */\n.deals-filters {\n  display: flex;\n  flex-direction: column;\n  gap: 12px;\n  margin-bottom: 16px;\n}\n\n.deals-filters-row {\n  display: flex;\n  gap: 8px;\n  flex-wrap: wrap;\n  align-items: center;\n}\n\n.deals-sort-group {\n  display: flex;\n  align-items: center;\n  gap: 6px;\n}\n\n.deals-sort-label {\n  font-size: 13px;\n  color: var(--text-muted);\n  white-space: nowrap;\n}\n\n/* Deals Date Range */\n.deals-date-range {\n  display: flex;\n  align-items: center;\n  gap: 6px;\n  height: 32px;\n  padding: 0 8px;\n  background: var(--bg-base-color);\n  border: 1px solid var(--normal-border);\n  border-radius: 4px;\n  font-size: 13px;\n}\n\n.deals-date-range-icon {\n  color: var(--text-muted);\n  flex-shrink: 0;\n}\n\n.deals-date-input {\n  height: 28px;\n  padding: 0 4px;\n  font-size: 12px;\n  font-family: var(--font-family);\n  color: var(--text-primary);\n  background: transparent;\n  border: none;\n  outline: none;\n  min-width: 0;\n  width: 110px;\n}\n\n.deals-date-separator {\n  color: var(--text-muted);\n  font-size: 12px;\n}\n\n.deals-date-clear {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 18px;\n  height: 18px;\n  background: transparent;\n  border: none;\n  border-radius: 50%;\n  cursor: pointer;\n  color: var(--text-muted);\n  padding: 0;\n  transition: background-color 0.1s ease, color 0.1s ease;\n}\n\n.deals-date-clear:hover {\n  background: rgba(50, 45, 35, 0.08);\n  color: var(--text-primary);\n}\n\n/* Deals Search */\n.deals-search-wrapper {\n  position: relative;\n  display: flex;\n  align-items: center;\n  flex: 1;\n  min-width: 180px;\n}\n\n.deals-search-icon {\n  position: absolute;\n  left: 10px;\n  color: var(--text-muted);\n  pointer-events: none;\n}\n\n.deals-search-input {\n  width: 100%;\n  height: 32px;\n  padding: 0 10px 0 32px;\n  font-size: 13px;\n  font-family: var(--font-family);\n  color: var(--text-primary);\n  background: var(--bg-base-color);\n  border: 1px solid var(--normal-border);\n  border-radius: 4px;\n  outline: none;\n  transition: border-color 0.15s ease;\n}\n\n.deals-search-input:focus {\n  border-color: var(--accent-primary);\n  box-shadow: 0 0 0 2px rgba(77, 124, 94, 0.15);\n}\n\n.deals-search-input::placeholder {\n  color: var(--text-disabled);\n}\n\n/* Deals Table */\n.deals-name-cell {\n  font-weight: 500;\n  color: var(--text-primary);\n}\n\n.deals-value-cell {\n  font-variant-numeric: tabular-nums;\n  font-weight: 500;\n}\n\n.deals-table-row {\n  transition: background-color 0.1s ease;\n}\n\n.deals-sort-header-btn {\n  display: inline-flex;\n  align-items: center;\n  gap: 4px;\n  background: transparent;\n  border: none;\n  cursor: pointer;\n  font-size: inherit;\n  font-weight: inherit;\n  font-family: var(--font-family);\n  color: inherit;\n  padding: 0;\n}\n\n.deals-sort-header-btn:hover {\n  color: var(--accent-primary);\n}\n\n.deals-empty {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  min-height: 200px;\n  font-size: 14px;\n  color: var(--text-muted);\n  padding: 0 24px;\n}\n\n.deals-loading {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  min-height: 200px;\n  font-size: 14px;\n  color: var(--text-muted);\n}\n\n/* Deals Pagination */\n.deals-pagination {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  padding: 16px 0;\n}\n\n/* ========= Pipeline View ========= */\n\n.pipeline-view {\n  display: grid;\n  grid-template-columns: repeat(6, 1fr);\n  gap: 12px;\n  min-height: 400px;\n  overflow-x: auto;\n}\n\n@media (max-width: 1024px) {\n  .pipeline-view {\n    grid-template-columns: repeat(3, minmax(200px, 1fr));\n  }\n}\n\n.pipeline-column {\n  display: flex;\n  flex-direction: column;\n  background: var(--bg-base-color);\n  border: 1px solid var(--bg-border-color-light);\n  border-radius: 6px;\n  min-height: 300px;\n  transition: border-color 0.15s ease, background-color 0.15s ease;\n}\n\n.pipeline-column--drag-over {\n  border-color: var(--accent-primary);\n  background: var(--accent-subtle-bg);\n}\n\n.pipeline-column-header {\n  display: flex;\n  align-items: center;\n  gap: 6px;\n  padding: 12px 12px 8px;\n  border-bottom: 1px solid var(--bg-border-color-light);\n}\n\n.pipeline-column-title {\n  font-size: 13px;\n  font-weight: 600;\n  color: var(--text-primary);\n}\n\n.pipeline-column-count {\n  font-size: 12px;\n  color: var(--text-muted);\n}\n\n.pipeline-column-body {\n  flex: 1;\n  padding: 8px;\n  display: flex;\n  flex-direction: column;\n  gap: 8px;\n}\n\n.pipeline-empty {\n  flex: 1;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  font-size: 12px;\n  color: var(--text-disabled);\n  min-height: 60px;\n}\n\n.pipeline-card {\n  padding: 10px 12px;\n  background: #ffffff;\n  border: 1px solid var(--bg-border-color-light);\n  border-radius: 5px;\n  cursor: grab;\n  transition: box-shadow 0.15s ease, border-color 0.15s ease;\n}\n\n.pipeline-card:hover {\n  border-color: var(--normal-border-hover);\n  box-shadow: var(--shadow-elevation-1);\n}\n\n.pipeline-card--dragging {\n  opacity: 0.5;\n}\n\n.pipeline-card-name {\n  font-size: 13px;\n  font-weight: 500;\n  color: var(--text-primary);\n  margin-bottom: 4px;\n}\n\n.pipeline-card-client {\n  font-size: 12px;\n  color: var(--text-muted);\n  margin-bottom: 8px;\n}\n\n.pipeline-card-footer {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n}\n\n.pipeline-card-value {\n  font-size: 13px;\n  font-weight: 600;\n  color: var(--text-primary);\n  font-variant-numeric: tabular-nums;\n}\n\n.pipeline-card-owner {\n  font-size: 11px;\n  color: var(--text-muted);\n}\n\n/* ===== Deal Detail Page ===== */\n\n.deal-detail-page {\n  max-width: 1200px;\n}\n\n.deal-detail-loading,\n.deal-detail-error {\n  padding: 24px;\n  text-align: center;\n  color: var(--text-muted);\n  font-size: 14px;\n}\n\n.deal-detail-error {\n  color: var(--status-error);\n}\n\n/* Deal Header */\n\n.deal-header {\n  margin-bottom: 20px;\n}\n\n.deal-header-title {\n  font-size: 20px;\n  font-weight: 700;\n  color: var(--text-primary);\n  margin-bottom: 16px;\n  letter-spacing: 0.01em;\n}\n\n.deal-header-fields {\n  display: flex;\n  flex-wrap: wrap;\n  gap: 16px;\n  align-items: center;\n}\n\n.deal-header-field {\n  display: flex;\n  align-items: center;\n  gap: 6px;\n}\n\n.deal-header-field-label {\n  font-size: 13px;\n  font-weight: 600;\n  color: var(--text-secondary);\n}\n\n.deal-header-client-link {\n  font-size: 13px;\n  color: var(--accent-primary);\n  background: none;\n  border: none;\n  padding: 0;\n  cursor: pointer;\n  text-decoration: underline;\n  font-family: inherit;\n}\n\n.deal-header-client-link:hover {\n  color: var(--accent-primary-hover);\n}\n\n.deal-header-field-value {\n  font-size: 13px;\n  color: var(--text-primary);\n  background: none;\n  border: none;\n  padding: 2px 4px;\n  font-family: inherit;\n  cursor: default;\n}\n\n.deal-header-field-value--editable {\n  cursor: pointer;\n  border-radius: 3px;\n}\n\n.deal-header-field-value--editable:hover {\n  background: var(--bg-secondary);\n}\n\n.deal-header-inline-input {\n  font-size: 13px;\n  font-family: inherit;\n  padding: 2px 6px;\n  border: 1px solid var(--bg-border-color);\n  border-radius: 4px;\n  background: var(--bg-base);\n  width: 120px;\n}\n\n.deal-header-inline-input:focus {\n  outline: none;\n  border-color: var(--accent-primary);\n  box-shadow: 0 0 0 2px rgba(77, 124, 94, 0.15);\n}\n\n.deal-header-field-select {\n  min-width: 140px;\n}\n\n.deal-header-stage-field {\n  gap: 8px;\n}\n\n/* Deal Stage Pipeline */\n\n.deal-stage-pipeline {\n  margin-bottom: 24px;\n  padding: 16px;\n  background: var(--bg-base);\n  border: 1px solid var(--bg-border-color-light);\n  border-radius: 6px;\n}\n\n.deal-stage-labels {\n  display: flex;\n  justify-content: space-between;\n  margin-bottom: 12px;\n}\n\n.deal-stage-label {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  gap: 4px;\n  flex: 1;\n}\n\n.deal-stage-label-text {\n  font-size: 11px;\n  font-weight: 500;\n  color: var(--text-muted);\n  text-align: center;\n  white-space: nowrap;\n}\n\n.deal-stage-label--current .deal-stage-label-text {\n  color: var(--accent-primary);\n  font-weight: 600;\n}\n\n.deal-stage-label--completed .deal-stage-label-text {\n  color: var(--text-secondary);\n}\n\n.deal-stage-progress-bar {\n  display: flex;\n  height: 6px;\n  border-radius: 3px;\n  overflow: hidden;\n  background: var(--bg-border-color-light);\n  gap: 2px;\n}\n\n.deal-stage-progress-segment {\n  flex: 1;\n  background: var(--bg-border-color-light);\n  border-radius: 3px;\n}\n\n.deal-stage-progress-segment--filled {\n  background: var(--accent-primary);\n}\n\n.pipeline-stage-icon--filled {\n  color: var(--accent-primary);\n}\n\n.pipeline-stage-icon--unfilled {\n  color: var(--text-disabled);\n}\n\n/* Deal Detail Grid */\n\n.deal-detail-grid {\n  display: grid;\n  grid-template-columns: 1fr 380px;\n  gap: 20px;\n}\n\n.deal-detail-main {\n  display: flex;\n  flex-direction: column;\n  gap: 16px;\n}\n\n.deal-detail-sidebar {\n  display: flex;\n  flex-direction: column;\n  gap: 16px;\n}\n\n@media (max-width: 900px) {\n  .deal-detail-grid {\n    grid-template-columns: 1fr;\n  }\n}\n\n/* Deal History Section */\n\n.deal-history-list {\n  display: flex;\n  flex-direction: column;\n  gap: 8px;\n}\n\n.deal-history-entry {\n  padding: 8px 0;\n  border-bottom: 1px solid var(--bg-border-color-light);\n}\n\n.deal-history-entry:last-child {\n  border-bottom: none;\n}\n\n.deal-history-entry-text {\n  font-size: 13px;\n  color: var(--text-secondary);\n}\n\n/* Deal Metrics Section */\n\n.deal-metrics-grid {\n  display: flex;\n  gap: 32px;\n}\n\n.deal-metric {\n  display: flex;\n  align-items: center;\n  gap: 6px;\n}\n\n.deal-metric-label {\n  font-size: 13px;\n  font-weight: 600;\n  color: var(--text-secondary);\n}\n\n.deal-metric-value {\n  font-size: 13px;\n  color: var(--text-primary);\n  background: none;\n  border: none;\n  padding: 2px 4px;\n  font-family: inherit;\n  cursor: default;\n}\n\n.deal-metric-value--editable {\n  cursor: pointer;\n  border-radius: 3px;\n}\n\n.deal-metric-value--editable:hover {\n  background: var(--bg-secondary);\n}\n\n.deal-metric-inline-input {\n  font-size: 13px;\n  font-family: inherit;\n  padding: 2px 6px;\n  border: 1px solid var(--bg-border-color);\n  border-radius: 4px;\n  background: var(--bg-base);\n  width: 100px;\n}\n\n.deal-metric-inline-input:focus {\n  outline: none;\n  border-color: var(--accent-primary);\n  box-shadow: 0 0 0 2px rgba(77, 124, 94, 0.15);\n}\n\n/* Writeups Section */\n\n.writeups-list {\n  display: flex;\n  flex-direction: column;\n  gap: 12px;\n}\n\n.writeup-card {\n  padding: 12px;\n  border: 1px solid var(--bg-border-color-light);\n  border-radius: 6px;\n  background: var(--bg-base);\n}\n\n.writeup-card-header {\n  margin-bottom: 8px;\n}\n\n.writeup-card-title {\n  font-size: 14px;\n  font-weight: 600;\n  color: var(--text-primary);\n}\n\n.writeup-card-meta {\n  font-size: 12px;\n  color: var(--text-muted);\n  margin-left: 4px;\n}\n\n.writeup-card-content {\n  font-size: 13px;\n  color: var(--text-secondary);\n  line-height: 1.5;\n  margin-bottom: 8px;\n}\n\n.writeup-card-actions {\n  display: flex;\n  justify-content: space-between;\n}\n\n.writeup-action-btn {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 28px;\n  height: 28px;\n  border: none;\n  border-radius: 4px;\n  background: none;\n  color: var(--text-muted);\n  cursor: pointer;\n  padding: 0;\n}\n\n.writeup-action-btn:hover {\n  background: var(--bg-secondary);\n  color: var(--text-primary);\n}\n\n.writeup-edit-form {\n  display: flex;\n  flex-direction: column;\n  gap: 8px;\n}\n\n.writeup-edit-actions {\n  display: flex;\n  justify-content: flex-end;\n  gap: 8px;\n}\n\n.writeup-versions-list {\n  display: flex;\n  flex-direction: column;\n  gap: 12px;\n}\n\n.writeup-version-entry {\n  padding: 12px;\n  border: 1px solid var(--bg-border-color-light);\n  border-radius: 6px;\n}\n\n.writeup-version-header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  margin-bottom: 6px;\n}\n\n.writeup-version-meta {\n  font-size: 12px;\n  color: var(--text-muted);\n}\n\n.writeup-version-content {\n  font-size: 13px;\n  color: var(--text-secondary);\n  line-height: 1.5;\n}\n\n/* Linked Tasks Section */\n\n.linked-tasks-list {\n  display: flex;\n  flex-direction: column;\n}\n\n.task-item {\n  display: flex;\n  align-items: center;\n  gap: 8px;\n  padding: 8px 0;\n  border-bottom: 1px solid var(--bg-border-color-light);\n}\n\n.task-item:last-child {\n  border-bottom: none;\n}\n\n.task-checkbox {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 18px;\n  height: 18px;\n  min-width: 18px;\n  border: 2px solid var(--bg-border-color);\n  border-radius: 3px;\n  background: none;\n  cursor: pointer;\n  padding: 0;\n}\n\n.task-checkbox:hover:not(:disabled) {\n  border-color: var(--accent-primary);\n}\n\n.task-checkbox--checked {\n  border-color: var(--accent-primary);\n  background: rgba(77, 124, 94, 0.1);\n  cursor: default;\n}\n\n.task-content {\n  display: flex;\n  align-items: center;\n  gap: 6px;\n  flex: 1;\n  cursor: pointer;\n}\n\n.task-content:hover .task-title:not(.task-title--completed) {\n  color: var(--accent-primary);\n}\n\n.task-title {\n  font-size: 13px;\n  color: var(--text-primary);\n}\n\n.task-title--completed {\n  text-decoration: line-through;\n  color: var(--text-muted);\n}\n\n.task-meta {\n  font-size: 12px;\n  color: var(--text-muted);\n}\n\n/* Deal Attachments Section */\n\n.deal-attachments-list {\n  display: flex;\n  flex-direction: column;\n}\n\n.deal-attachment-row {\n  display: flex;\n  align-items: center;\n  gap: 8px;\n  padding: 8px 0;\n  border-bottom: 1px solid var(--bg-border-color-light);\n}\n\n.deal-attachment-row:last-child {\n  border-bottom: none;\n}\n\n.attachment-icon {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 28px;\n  height: 28px;\n  min-width: 28px;\n  color: var(--text-muted);\n}\n\n.attachment-icon--document { color: var(--status-info); }\n.attachment-icon--image { color: var(--status-success); }\n.attachment-icon--spreadsheet { color: var(--status-warning); }\n.attachment-icon--code { color: var(--text-muted); }\n.attachment-icon--link { color: var(--accent-primary); }\n.attachment-icon--default { color: var(--text-muted); }\n\n.attachment-action-btn {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 28px;\n  height: 28px;\n  border: none;\n  border-radius: 4px;\n  background: none;\n  color: var(--text-muted);\n  cursor: pointer;\n  padding: 0;\n}\n\n.attachment-action-btn:hover {\n  background: var(--bg-secondary);\n  color: var(--accent-primary);\n}\n\n.deal-attachment-name {\n  font-size: 13px;\n  color: var(--text-primary);\n  flex: 1;\n  min-width: 0;\n  overflow: hidden;\n  text-overflow: ellipsis;\n  white-space: nowrap;\n}\n\n.deal-attachment-size {\n  font-size: 12px;\n  color: var(--text-muted);\n  white-space: nowrap;\n}\n\n.deal-attachment-actions {\n  display: flex;\n  align-items: center;\n  gap: 6px;\n  white-space: nowrap;\n}\n\n.deal-attachment-link {\n  font-size: 12px;\n  color: var(--accent-primary);\n  background: none;\n  border: none;\n  padding: 0;\n  cursor: pointer;\n  font-family: inherit;\n  text-decoration: none;\n}\n\n.deal-attachment-link:hover {\n  text-decoration: underline;\n}\n\n.deal-attachment-link--danger {\n  color: var(--status-error);\n}\n\n.deal-attachment-separator {\n  font-size: 12px;\n  color: var(--text-disabled);\n}\n\n.file-upload-area {\n  border: 2px dashed var(--bg-border-color);\n  border-radius: 6px;\n  padding: 32px;\n  text-align: center;\n  cursor: pointer;\n  transition: border-color 0.15s;\n}\n\n.file-upload-area:hover {\n  border-color: var(--accent-primary);\n}\n\n.file-upload-label {\n  font-size: 13px;\n  color: var(--text-muted);\n}\n\n.file-upload-selected {\n  margin-top: 8px;\n  font-size: 12px;\n  color: var(--text-primary);\n  font-weight: 500;\n}\n\n/* Contacts/Individuals Section */\n\n.contacts-individuals-list {\n  display: flex;\n  flex-direction: column;\n}\n\n.contacts-individual-row {\n  display: flex;