   41→            <span className=\"status-dot\" style={{ backgroundColor: statusDotColor }} />\n    42→            <span className=\"status-label\">Status: {supplier.status}</span>\n    43→          </div>\n    44→          <button\n    45→            className=\"btn-secondary\"\n    46→            data-testid=\"supplier-edit-btn\"\n    47→            onClick={() => setShowEditModal(true)}\n    48→          >\n    49→            <svg width=\"14\" height=\"14\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" strokeWidth=\"2\" strokeLinecap=\"round\" strokeLinejoin=\"round\">\n    50→              <path d=\"M11 4H4a2 2 0 0 0-2 2v14a2 2 0 0 0 2 2h14a2 2 0 0 0 2-2v-7\" />\n    51→              <path d=\"M18.5 2.5a2.121 2.121 0 0 1 3 3L12 15l-4 1 1-4 9.5-9.5z\" />\n    52→            </svg>\n    53→            Edit\n    54→          </button>\n    55→          <button\n    56→            className=\"btn-danger\"\n    57→            data-testid=\"supplier-delete-btn\"\n    58→            onClick={() => setShowDeleteConfirm(true)}\n    59→          >\n    60→            <svg width=\"14\" height=\"14\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" strokeWidth=\"2\" strokeLinecap=\"round\" strokeLinejoin=\"round\">\n    61→              <polyline points=\"3 6 5 6 21 6\" />\n    62→              <path d=\"M19 6v14a2 2 0 0 1-2 2H7a2 2 0 0 1-2-2V6m3 0V4a2 2 0 0 1 2-2h4a2 2 0 0 1 2 2v2\" />\n    63→            </svg>\n    64→            Delete\n    65→          </button>\n    66→          <button\n    67→            className=\"btn-secondary\"\n    68→            data-testid=\"back-to-dashboard-btn\"\n    69→            onClick={() => navigate(\"/\")}\n    70→          >\n    71→            <svg width=\"14\" height=\"14\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" strokeWidth=\"2\" strokeLinecap=\"round\" strokeLinejoin=\"round\">\n    72→              <line x1=\"19\" y1=\"12\" x2=\"5\" y2=\"12\" />\n    73→              <polyline poin