[2026-02-25T01:21:26.655Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01HDK3cUBPNyqQroSWYQqjSa","type":"tool_result","content":"     1→import { useState } from \"react\";\n     2→import { ConfirmDialog } from \"@shared/components/ConfirmDialog\";\n     3→import { useAppDispatch } from \"../hooks\";\n     4→import { updateRun } from \"../slices/runsSlice\";\n     5→import type { RunDetail, RunStatus } from \"../types\";\n     6→\n     7→interface RunActionsProps {\n     8→  run: RunDetail;\n     9→}\n    10→\n    11→export function RunActions({ run }: RunActionsProps) {\n    12→  const dispatch = useAppDispatch();\n    13→  const [showQuantityModal, setShowQuantityModal] = useState(false);\n    14→  const [showCancelConfirm, setShowCancelConfirm] = useState(false);\n    15→\n    16→  const isCancelled = run.status === \"Cancelled\";\n    17→  const isConfirmed = run.status === \"Confirmed\";\n    18→\n    19→  const showAdjust = !isCancelled && !isConfirmed;\n    20→  const showConfirm = !isCancelled && !isConfirmed;\n    21→  const showCancel = !isCancelled;\n    22→\n    23→  const handleConfirmRun = async () => {\n    24→    await dispatch(updateRun({ id: run.id, status: \"Confirmed\" as RunStatus }));\n    25→  };\n    26→\n    27→  const handleCancelRun = async () => {\n    28→    await dispatch(updateRun({ id: run.id, status: \"Cancelled\" as RunStatus }));\n    29→    setShowCancelConfirm(false);\n    30→  };\n    31→\n    32→  if (!showAdjust && !showConfirm && !showCancel) {\n    33→    return null;\n    34→  }\n    35→\n    36→  return (\n    37→    <div data-testid=\"run-actions\" className=\"run-actions-container\">\n    38→      {showAdjust && (\n    39→        <button\n    40→          data-testid=\"adjust-quantities-btn\"\n    41→          className=\"btn-primary\"\n    42→          onClick={() => setShowQuantityModal(true)}\n    43→        >\n    44→          Adjust Quantities\n    45→        </button>\n    46→      )}\n    47→      {showConfirm && (\n    48→        <button\n    49→          data-testid=\"confirm-run-btn\"\n    50→          className=\"btn-primary\"\n    51→          onClick={handleConfirmRun}\n    52→        >\n    53→          Confirm Run\n    54→        </button>\n    55→      )}\n    56→      {showCancel && (\n    57→        <button\n    58→          data-testid=\"cancel-run-btn\"\n    59→          className=\"btn-primary\"\n    60→          onClick={() => setShowCancelConfirm(true)}\n    61→        >\n    62→          Cancel Run\n    63→        </button>\n    64→      )}\n    65→\n    66→      {showQuantityModal && (\n    67→        <AdjustQuantityModal\n    68→          run={run}\n    69→          onClose={() => setShowQuantityModal(false)}\n    70→        />\n    71→      )}\n    72→\n    73→      <ConfirmDialog\n    74→        open={showCancelConfirm}\n    75→        title=\"Cancel Production Run\"\n    76→        message={`Are you sure you want to cancel run ${run.id}? This action cannot be undone.`}\n    77→        confirmLabel=\"Cancel Run\"\n    78→        cancelLabel=\"Keep Run\"\n    79→        onConfirm={handleCancelRun}\n    80→        onCancel={() => setShowCancelConfirm(false)}\n    81→      />\n    82→    </div>\n    83→  );\n    84→}\n    85→\n    86→interface AdjustQuantityModalProps {\n    87→  run: RunDetail;\n    88→  onClose: () => void;\n    89→}\n    90→\n    91→function AdjustQuantityModal({ run, onClose }: AdjustQuantityModalProps) {\n    92→  const dispatch = useAppDispatch();\n    93→  const [quantity, setQuantity] = useState(String(run.planned_quantity));\n    94→  const [saving, setSaving] = useState(false);\n    95→\n    96→  const handleSubmit = async (e: React.FormEvent) => {\n    97→    e.preventDefault();\n    98→    setSaving(true);\n    99→    await dispatch(\n   100→      updateRun({\n   101→        id: run.id,\n   102→        planned_quantity: Number(quantity),\n   103→      })\n   104→    );\n   105→    setSaving(false);\n   106→    onClose();\n   107→  };\n   108→\n   109→  return (\n   110→    <div\n   111→      className=\"modal-overlay\"\n   112→      onClick={onClose}\n   113→      data-testid=\"adjust-quantity-modal\"\n   114→    >\n   115→      <div className=\"modal-content\" onClick={(e) => e.stopPropagation()}>\n   116→        <div className=\"modal-header\">\n   117→          <h2 className=\"modal-title\">Adjust Planned Quantity</h2>\n   118→          <button className=\"modal-close-btn\" onClick={onClose}>\n   119→            <svg width=\"16\" height=\"16\" viewBox=\"0 0 16 16\" fill=\"none\">\n   120→              <path\n   121→                d=\"M4 4L12 12M12 4L4 12\"\n   122→                stroke=\"currentColor\"\n   123→                strokeWidth=\"1.5\"\n   124→                strokeLinecap=\"round\"\n   125→              />\n   126→            </svg>\n   127→          </button>\n   128→        </div>\n   129→        <form onSubmit={handleSubmit}>\n   130→          <div className=\"form-group\">\n   131→            <label className=\"form-label\">\n   132→              Planned Quantity ({run.unit})\n   133→            </label>\n   134→            <input\n   135→              data-testid=\"adjust-quantity-input\"\n   136→              className=\"form-input\"\n   137→              type=\"number\"\n   138→              min=\"0\"\n   139→              step=\"any\"\n   140→              value={quantity}\n   141→              onChange={(e) => setQuantity(e.target.value)}\n   142→              required\n   143→            />\n   144→          </div>\n   145→          <div className=\"modal-footer\">\n   146→            <button type=\"button\" className=\"btn-secondary\" onClick={onClose}>\n   147→              Cancel\n   148→            </button>\n   149→            <button\n   150→              type=\"submit\"\n   151→              className=\"btn-primary\"\n   152→              disabled={saving || !quantity}\n   153→              data-testid=\"adjust-quantity-save\"\n   154→            >\n   155→              {saving ? \"Saving...\" : \"Save Changes\"}\n   156→            </button>\n   157→          </div>\n   158→        </form>\n   159→      </div>\n   160→    </div>\n   161→  );\n   162→}\n   163→"}]},"parent_tool_use_id":"toolu_013nVVX2mA9frTcXqVqVqZPr","session_id":"2c47f5c6-af02-4a26-9fd4-86798a345fba","uuid":"6b4df562-2f3d-4229-b2a0-3bc0b8053c37"}
