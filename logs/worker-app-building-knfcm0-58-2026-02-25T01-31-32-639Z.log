it page.goto(\"/recipes\");\n    35→    await expect(page.getByTestId(\"recipes-table\")).toBeVisible({ timeout: 30000 });\n    36→\n    37→    // \"Jam\" matches product \"Strawberry Jam\" but no recipe name contains \"Jam\"\n    38→    await page.getByTestId(\"recipes-search-input\").fill(\"Jam\");\n    39→\n    40→    await expect(page.locator('[data-testid^=\"recipe-row-\"]')).toHaveCount(1);\n    41→    await expect(\n    42→      page.locator('[data-testid^=\"recipe-row-\"]').filter({ hasText: \"Fruit Preserve\" })\n    43→    ).toBeVisible();\n    44→  });\n    45→\n    46→  test(\"REC-HDR-5: Clearing the search bar restores all recipes\", async ({ page }) => {\n    47→    await page.goto(\"/recipes\");\n    48→    await expect(page.getByTestId(\"recipes-table\")).toBeVisible({ timeout: 30000 });\n    49→\n    50→    // Filter to one result\n    51→    await page.getByTestId(\"recipes-search-input\").fill(\"Granola\");\n    52→    await expect(page.locator('[data-testid^=\"recipe-row-\"]')).toHaveCount(1);\n    53→\n    54→    // Clear search\n    55→    await page.getByTestId(\"recipes-search-input\").fill(\"\");\n    56→\n    57→    // Page 1 shows PAGE_SIZE (5) recipes\n    58→    await expect(page.locator('[data-testid^=\"recipe-row-\"]')).toHaveCount(5, { timeout: 15000 });\n    59→  });\n    60→"}]},"parent_tool_use_id":"toolu_013nVVX2mA9frTcXqVqVqZPr","session_id":"2c47f5c6-af02-4a26-9fd4-86798a345fba","uuid":"ad77ba4d-57fc-45c3-879d-86c52ad7db3a"}
