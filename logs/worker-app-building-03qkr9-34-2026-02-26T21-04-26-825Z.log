on data (derived from batches)\n    34→  const [distribution, setDistribution] = useState<AccountDistribution[]>([]);\n    35→\n    36→  // Edit material modal\n    37→  const [editModalOpen, setEditModalOpen] = useState(false);\n    38→  const [editName, setEditName] = useState(\"\");\n    39→  const [editCategoryId, setEditCategoryId] = useState(\"\");\n    40→  const [editUnit, setEditUnit] = useState(\"\");\n    41→  const [editDescription, setEditDescription] = useState(\"\");\n    42→  const [editErrors, setEditErrors] = useState<Record<string, string>>({});\n    43→\n    44→  // Create batch modal\n    45→  const [batchModalOpen, setBatchModalOpen] = useState(false);\n    46→  const [batchAccountId, setBatchAccountId] = useState(\"\");\n    47→  const [batchQuantity, setBatchQuantity] = useState(\"\");\n    48→  const [batchLocation, setBatchLocation] = useState(\"\");\n    49→  const [batchLotNumber, setBatchLotNumber] = useState(\"\");\n    50→  const [batchExpiration, setBatchExpiration] = useState(\"\");\n    51→  const [batchErrors, setBatchErrors] = useState<Record<string, string>>({});\n    52→\n    53→  const loadData = useCallback(() => {\n    54→    if (materialId) {\n    55→      dispatch(fetchMaterialById(materialId));\n    56→      dispatch(fetchBatches({ material_id: materialId }));\n    57→      dispatch(fetchTransactions({ material_id: materialId }));\n    58→      dispatch(fetchAccounts());\n    59→      dispatch(fetchCategories());\n    60→\n    61→      // Fetch distribution data\n    62→      fetch(\n    63→        `/.netlify/functions/material-distribution?material_id=${materialId}`\n    64→      )\n    65→        .then((r) => r.json())\n    66→        .then((data) => setDistribution(data))\n    67→        .catch(() => setDistribution([]));\n    68→    }\n    69→  }, [dispatch, materialId]);\n    70→\n    71→  useEffect(() => {\n    72→    loadData();\n    73→    return () => {\n    74→      dispatch(clearCurrentMaterial());\n    75→      dispatch(clearTransactions());\n    76→    };\n    77→  }, [loadData, dispatch]);\n    78→\n    79→  // Edit material handlers\n    80→  const handleOpenEdit = () => {\n    81→    if (!material) return;\n    82→    setEditName(material.name);\n    83→    setEditCategoryId(material.category_id);\n    84→    setEditUnit(material.unit_of_measure);\n    85→    setEditDescription(material.description);\n    86→    setEditErrors({});\n    87→    setEditModalOpen(true);\n    88→  };\n    89→\n    90→  const handleSaveEdit = async () => {\n    91→    const errors: Record<string, string> = {};\n    92→    if (!editName.trim()) errors.name = \"Material Name is required\";\n    93→    if (!editCategoryId) errors.category = \"Category is required\";\n    94→    if (!editUnit.trim()) errors.unit = \"Unit of Measure is required\";\n    95→\n    96→    if (Object.keys(errors).length > 0) {\n    97→      setEditErrors(errors);\n    98→      return;\n    99→    }\n   100→\n   101→    if (!material) return;\n   102→    await dispatch(\n   103→      updateMaterial({\n   104→        id: material.id,\n   105→        name: editName.trim(),\n   106→        category_id: editCategoryId,\n   107→        unit_of_measure: editUnit.trim(),\n   108→        description: editDescription.trim(),\n   109→      })\n   110→    );\n   111→    setEditModalOpen(false);\n   112→  };\n   113→\n   114→  // Create batch handlers\n   115→  const handleOpenBatchModal = () => {\n   116→    setBatchAccountId(\"\");\n   117→    setBatchQuantity(\"\");\n   118→    setBatchLocation(\"\");\n   119→    setBatchLotNumber(\"\");\n   120→    setBatchExpiration(\"\");\n   121→    setBatchErrors({});\n   122→    setBatchModalOpen(true);\n   123→  };\n   124→\n   125→  const handleCreateBatch = async () => {\n   126→    const errors: Record<string, string> = {};\n   127→    if (!batchAccountId) errors.account = \"Account is required\";\n   128→    const qty = parseFloat(batchQuantity);\n   129→    if (!batchQuantity || is