    46→\n    47→  if (req.method === \"POST\") {\n    48→    try {\n    49→      const formData = await req.formData();\n    50→      const file = formData.get(\"file\") as File | null;\n    51→      const documentType = (formData.get(\"document_type\") as string) || \"\";\n    52→\n    53→      if (!file) {\n    54→        return new Response(JSON.stringify({ error: \"file is required\" }), {\n    55→          status: 400,\n    56→          headers: { \"Content-Type\": \"application/json\" },\n    57→        });\n    58→      }\n    59→\n    60→      const utapi = new UTApi();\n    61→      const uploadResult = await utapi.uploadFiles([file]);\n    62→      const uploaded = uploadResult[0];\n    63→\n    64→      if (!uploaded || uploaded.error) {\n    65→        console.error(\"UploadThing error:\", uploaded?.error);\n    66→        return new Response(JSON.stringify({ error: \"File upload failed\" }), {\n    67→          status: 500,\n    68→          headers: { \"Content-Type\": \"application/json\" },\n    69→        });\n    70→      }\n    71→\n    72→      const fileUrl = uploaded.data.ufsUrl;\n    73→      const fileName = file.name;\n    74→\n    75→      const rows = await sql`\n    76→        INSERT INTO order_documents (order_id, file_name, file_url, document_type)\n    77→        VALUES (\n    78→          ${orderId}::uuid,\n    79→          ${fileName},\n    80→          ${fileUrl},\n    81→          ${documentType}\n    82→        )\n    83→        RETURNING id, order_id, file_name, file_url, document_type, upload_date\n    84→      `;\n    85→\n    86→      // Add history entry\n    87→      await sql`\n    88→        INSERT INTO order_history (order_id, event_type, description, actor)\n    89→        VALUES (\n    90→          ${orderId}::uuid,\n    91→          'document',\n    92→          ${\"Document uploaded: \" + fileName},\n    93→          'System'\n    94→        )\n    95→      `;\n    96→\n    97→      return new Response(JSON.stringify(rows[0]), {\n    98→        status: 201,\n    99→        headers: { \"Content-Type\": \"application/json\" },\n   100→      });\n   101→    } catch (err) {\n   102→      console.error(\"Order documents POST error:\", err);\n   103→      return new Response(JSON.stringify({ error: \"Internal server error\" }), {\n   104→        status: 500,\n   105→        headers: { \"Content-Type\": \"application/json\" },\n   106→      });\n   107→    }\n   108→  }\n   109→\n   110→  return new Response(JSON.stringify({ error: \"Method not allowed\" }), {\n   111→    status: 405,\n   112→    headers: { \"Content-Type\": \"application/json\" },\n   113→  });\n   114→}\n   115→"}]},"parent_tool_use_id":"toolu_01D48bpp2TYGonFLbyBKrbKM","session_id":"06cc4bfe-b987-441b-99bc-d60e98adecca","uuid":"9bb6924a-e1e2-4228-a0bd-76248316c031"}
