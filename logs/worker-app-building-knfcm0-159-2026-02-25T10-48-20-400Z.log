    89→          await dispatch(fetchSupplier(supplier.id));\n    90→        }}\n    91→        onDelete={async () => {\n    92→          await dispatch(deleteSupplier(supplier.id)).unwrap();\n    93→        }}\n    94→      />\n    95→\n    96→      <div className=\"supplier-detail-tabs\" data-testid=\"supplier-detail-tabs\">\n    97→        <button\n    98→          className=\"tab-nav-item tab-nav-item--active-link\"\n    99→          data-testid=\"tab-documents\"\n   100→          onClick={() => scrollToSection(documentsRef)}\n   101→        >\n   102→          Documents\n   103→        </button>\n   104→        <button\n   105→          className=\"tab-nav-item tab-nav-item--active-link\"\n   106→          data-testid=\"tab-comments\"\n   107→          onClick={() => scrollToSection(commentsRef)}\n   108→        >\n   109→          Comments\n   110→        </button>\n   111→        <button\n   112→          className=\"tab-nav-item tab-nav-item--active-link\"\n   113→          data-testid=\"tab-orders\"\n   114→          onClick={() => scrollToSection(ordersRef)}\n   115→        >\n   116→          Orders\n   117→        </button>\n   118→      </div>\n   119→\n   120→      <div className=\"section-card\" ref={documentsRef} data-testid=\"documents-section\">\n   121→        <div className=\"section-card-body\">\n   122→          <DocumentsTab\n   123→            documents={documents}\n   124→            onUpload={async (file, documentType) => {\n   125→              await dispatch(uploadSupplierDocument({ supplierId: supplier.id, file, documentType })).unwrap();\n   126→              await dispatch(fetchSupplierDocuments(supplier.id));\n   127→            }}\n   128→            onDelete={(documentId) => {\n   129→              dispatch(deleteSupplierDocument({ supplierId: supplier.id, documentId }));\n   130→            }}\n   131→          />\n   132→        </div>\n   133→      </div>\n   134→\n   135→      <div className=\"section-card\" ref={commentsRef} da