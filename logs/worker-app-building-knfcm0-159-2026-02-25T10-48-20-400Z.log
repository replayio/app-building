rId = segments[3] || null;\n    44→\n    45→  if (!orderId) {\n    46→    return new Response(JSON.stringify({ error: \"Order ID required\" }), {\n    47→      status: 400,\n    48→      headers: { \"Content-Type\": \"application/json\" },\n    49→    });\n    50→  }\n    51→\n    52→  if (req.method === \"GET\") {\n    53→    try {\n    54→      const rows = await sql`\n    55→        SELECT id, order_id, sku, item_name, quantity, unit_price, line_total\n    56→        FROM order_line_items\n    57→        WHERE order_id = ${orderId}::uuid\n    58→        ORDER BY created_at\n    59→      `;\n    60→      return new Response(JSON.stringify(rows), {\n    61→        status: 200,\n    62→        headers: { \"Content-Type\": \"application/json\" },\n    63→      });\n    64→    } catch (err) {\n    65→      console.error(\"Order line items GET error:\", err);\n    66→      return new Response(JSON.stringify({ error: \"Internal server error\" }), {\n    67→        status: 500,\n    68→        headers: { \"Content-Type\": \"application/json\" },\n    69→      });\n    70→    }\n    71→  }\n    72→\n    73→  if (req.method === \"POST\") {\n    74→    try {\n    75→      const body = await req.json();\n    76→      const { sku, item_name, quantity, unit_price } = body;\n    77→\n    78→      if (!item_name) {\n    79→        return new Response(JSON.stringify({ error: \"item_name is required\" }), {\n    80→          status: 400,\n    81→          headers: { \"Content-Type\": \"application/json\" },\n    82→        });\n    83→      }\n    84→\n    85→      const lineTotal = (quantity || 0) * (unit_price || 0);\n    86→\n    87→      const rows = await sql`\n    88→        INSERT INTO order_line_items (order_id, sku, item_name, quantity, unit_price, line_total)\n    89→        VALUES (\n    90→          ${orderId}::uuid,\n    91→          ${sku || \"\"},\n    92→          ${item_name},\n    93→          ${quantity || 0},\n    94→          ${unit_price || 0},\n    95→      