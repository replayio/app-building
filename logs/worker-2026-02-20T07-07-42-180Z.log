tion) => {\n   107→        state._lastFetchRequestId = action.meta.requestId\n   108→        if (state.items.length === 0) {\n   109→          state.loading = true\n   110→        }\n   111→        state.error = null\n   112→      })\n   113→      .addCase(fetchTasks.fulfilled, (state, action) => {\n   114→        if (action.meta.requestId !== state._lastFetchRequestId) return\n   115→        state.loading = false\n   116→        state.items = action.payload.tasks\n   117→        state.total = action.payload.total\n   118→        state.availableAssignees = action.payload.assignees\n   119→        state.availableClients = action.payload.clients\n   120→      })\n   121→      .addCase(fetchTasks.rejected, (state, action) => {\n   122→        if (action.meta.requestId !== state._lastFetchRequestId) return\n   123→        state.loading = false\n   124→        state.error = action.error.message ?? 'Unknown error'\n   125→      })\n   126→      .addCase(createTask.fulfilled, (state, action) => {\n   127→        state.items.unshift(action.payload)\n   128→        state.total += 1\n   129→      })\n   130→      .addCase(updateTask.fulfilled, (state, action) => {\n   131→        const idx = state.items.findIndex((t) => t.id === action.payload.id)\n   132→        if (action.payload.completed) {\n   133→          state.items = state.items.filter((t) => t.id !== action.payload.id)\n   134→          state.total = Math.max(0, state.total - 1)\n   135→        } else if (idx !== -1) {\n   136→          state.items[idx] = action.payload\n   137→        }\n   138→      })\n   139→      .addCase(deleteTask.fulfilled, (state, action) => {\n   140→        state.items = state.items.filter((t) => t.id !== action.payload)\n   141→        state.total = Math.max(0, state.total - 1)\n   142→      })\n   143→  },\n   144→})\n   145→"}]},"parent_tool_use_id":"toolu_01VrW5MhdyBtnLrRoafDytSb","session_id":"91bb54e5-9f7f-4846-b922-5598398981df","uuid":"c9cd2934-83aa-411d-b12e-3919c290d446"}
