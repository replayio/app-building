\n    41→      sql`\n    42→        SELECT a.*, d.name as deal_name\n    43→        FROM attachments a\n    44→        LEFT JOIN deals d ON a.deal_id = d.id\n    45→        WHERE a.deal_id = ${dealId}::uuid\n    46→        ORDER BY a.created_at ASC\n    47→      `,\n    48→      sql`\n    49→        SELECT dc.id, dc.role, i.id as individual_id, i.name as individual_name, i.title,\n    50→          (SELECT ci.role FROM client_individuals ci WHERE ci.individual_id = i.id LIMIT 1) as company_role,\n    51→          (SELECT c.name FROM clients c JOIN client_individuals ci2 ON ci2.client_id = c.id WHERE ci2.individual_id = i.id LIMIT 1) as company\n    52→        FROM deal_contacts dc\n    53→        JOIN individuals i ON dc.individual_id = i.id\n    54→        WHERE dc.deal_id = ${dealId}::uuid\n    55→        ORDER BY i.name ASC\n    56→      `,\n    57→    ])\n    58→\n    59→    return Response.json({ history, writeups, tasks, attachments, contacts })\n    60→  }\n    61→\n    62→  // POST /deal-detail/writeups — create writeup\n    63→  if (req.method === 'POST' && resource === 'writeups') {\n    64→    const body = await req.json() as {\n    65→      deal_id: string\n    66→      title: string\n    67→      content: string\n    68→      author: string\n    69→    }\n    70→\n    71→    const rows = await sql`\n    72→      INSERT INTO writeups (deal_id, title, content, author, version)\n    73→      VALUES (${body.deal_id}::uuid, ${body.title}, ${body.content}, ${req.user?.name ?? 'System'}, 1)\n    74→      RETURNING *\n    75→    `\n    76→\n    77→    return Response.json(rows[0], { status: 201 })\n    78→  }\n    79→\n    80→  // PUT /deal-detail/writeups/:writeupId — edit writeup (creates version)\n    81→  if (req.method === 'PUT' && resource === 'writeups' && resourceId) {\n    82→    const body = await req.json() as {\n    83→      title?: string\n    84→      content?: string\n    85→      author?: string\n    86→    }\n    87→\n    88→    // Get current v