[2026-02-18T19:19:19.977Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_019oJRVt3yUsjV1WPn3DGGyb","type":"tool_result","content":"     1→import { neon } from '@neondatabase/serverless'\n     2→import { optionalAuth, type OptionalAuthRequest } from '../utils/auth'\n     3→\n     4→function getDb() {\n     5→  const url = process.env.DATABASE_URL\n     6→  if (!url) throw new Error('DATABASE_URL not set')\n     7→  return neon(url)\n     8→}\n     9→\n    10→async function handler(req: OptionalAuthRequest) {\n    11→  const sql = getDb()\n    12→  const url = new URL(req.url)\n    13→  const pathParts = url.pathname.split('/').filter(Boolean)\n    14→  // Path: /deal-detail?dealId=... or /deal-detail/<resource>/<resourceId>\n    15→  const resource = pathParts.length >= 4 ? pathParts[3] : null\n    16→  const resourceId = pathParts.length >= 5 ? pathParts[4] : null\n    17→\n    18→  const dealId = url.searchParams.get('dealId') ?? ''\n    19→\n    20→  // GET /deal-detail?dealId=... — get all deal detail data\n    21→  if (req.method === 'GET' && !resource) {\n    22→    if (!dealId) {\n    23→      return new Response(JSON.stringify({ error: 'dealId required' }), { status: 400 })\n    24→    }\n    25→\n    26→    const [history, writeups, tasks, attachments, contacts] = await Promise.all([\n    27→      sql`\n    28→        SELECT id, deal_id, old_stage, new_stage, changed_by, created_at\n    29→        FROM deal_history\n    30→        WHERE deal_id = ${dealId}::uuid\n    31→        ORDER BY created_at DESC\n    32→      `,\n    33→      sql`\n    34→        SELECT id, deal_id, title, content, author, version, created_at, updated_at\n    35→        FROM writeups\n    36→        WHERE deal_id = ${dealId}::uuid\n    37→        ORDER BY created_at DESC\n    38→      `,\n    39→      sql`\n    40→        SELECT t.*, c.name as client_name, d.name as deal_name\n    41→        FROM tasks t\n    42→        LEFT JOIN clients c ON t.client_id = c.id\n    43→        LEFT JOIN deals d ON t.deal_id = d.id\n    44→        WHERE t.deal_id = ${dealId}::uuid\n    45→        ORDER BY t.completed ASC, t.due_date ASC NULLS LAST\n    46→      `,\n    47→      sql`\n    48→        SELECT a.*, d.name as deal_name\n    49→        FROM attachments a\n    50→        LEFT JOIN deals d ON a.deal_id = d.id\n    51→        WHERE a.deal_id = ${dealId}::uuid\n    52→        ORDER BY a.created_at ASC\n    53→      `,\n    54→      sql`\n    55→        SELECT dc.id, dc.role, i.id as individual_id, i.name as individual_name, i.title,\n    56→          (SELECT ci.role FROM client_individuals ci WHERE ci.individual_id = i.id LIMIT 1) as company_role,\n    57→          (SELECT c.name FROM clients c JOIN client_individuals ci2 ON ci2.client_id = c.id WHERE ci2.individual_id = i.id LIMIT 1) as company\n    58→        FROM deal_contacts dc\n    59→        JOIN individuals i ON dc.individual_id = i.id\n    60→        WHERE dc.deal_id = ${dealId}::uuid\n    61→        ORDER BY i.name ASC\n    62→      `,\n    63→    ])\n    64→\n    65→    return Response.json({ history, writeups, tasks, attachments, contacts })\n    66→  }\n    67→\n    68→  // POST /deal-detail/writeups — create writeup\n    69→  if (req.method === 'POST' && resource === 'writeups') {\n    70→    const body = await req.json() as {\n    71→      deal_id: string\n    72→      title: string\n    73→      content: string\n    74→      author: string\n    75→    }\n    76→\n    77→    const rows = await sql`\n    78→      INSERT INTO writeups (deal_id, title, content, author, version)\n    79→      VALUES (${body.deal_id}::uuid, ${body.title}, ${body.content}, ${req.user?.name ?? 'System'}, 1)\n    80→      RETURNING *\n    81→    `\n    82→\n    83→    return Response.json(rows[0], { status: 201 })\n    84→  }\n    85→\n    86→  // PUT /deal-detail/writeups/:writeupId — edit writeup (creates version)\n    87→  if (req.method === 'PUT' && resource === 'writeups' && resourceId) {\n    88→    const body = await req.json() as {\n    89→      title?: string\n    90→      content?: string\n    91→      author?: string\n    92→    }\n    93→\n    94→    // Get current version to save as history\n    95→    const existing = await sql`SELECT * FROM writeups WHERE id = ${resourceId}::uuid`\n    96→    if (existing.length === 0) {\n    97→      return new Response(JSON.stringify({ error: 'Writeup not found' }), { status: 404 })\n    98→    }\n    99→\n   100→    const current = existing[0]\n   101→\n   102→    // Save current version to writeup_versions\n   103→    await sql`\n   104→      INSERT INTO writeup_versions (writeup_id, title, content, author, version, created_at)\n   105→      VALUES (${resourceId}::uuid, ${current.title}, ${current.content}, ${current.author}, ${current.version}, ${current.updated_at})\n   106→    `\n   107→\n   108→    // Update the writeup with new content\n   109→    const newVersion = Number(current.version) + 1\n   110→    const rows = await sql`\n   111→      UPDATE writeups SET\n   112→        title = COALESCE(${body.title ?? null}, title),\n   113→        content = COALESCE(${body.content ?? null}, content),\n   114→        author = ${req.user?.name ?? 'System'},\n   115→        version = ${newVersion},\n   116→        updated_at = NOW()\n   117→      WHERE id = ${resourceId}::uuid\n   118→      RETURNING *\n   119→    `\n   120→\n   121→    return Response.json(rows[0])\n   122→  }\n   123→\n   124→  // GET /deal-detail/writeup-versions/:writeupId — get version history\n   125→  if (req.method === 'GET' && resource === 'writeup-versions' && resourceId) {\n   126→    const versions = await sql`\n   127→      SELECT id, writeup_id, title, content, author, version, created_at\n   128→      FROM writeup_versions\n   129→      WHERE writeup_id = ${resourceId}::uuid\n   130→      ORDER BY version DESC\n   131→    `\n   132→\n   133→    // Also include the current version\n   134→    const current = await sql`\n   135→      SELECT id, id as writeup_id, title, content, author, version, updated_at as created_at\n   136→      FROM writeups\n   137→      WHERE id = ${resourceId}::uuid\n   138→    `\n   139→\n   140→    return Response.json({ versions: [...current, ...versions] })\n   141→  }\n   142→\n   143→  // POST /deal-detail/tasks — create task for deal\n   144→  if (req.method === 'POST' && resource === 'tasks') {\n   145→    const body = await req.json() as {\n   146→      deal_id: string\n   147→      client_id: string\n   148→      title: string\n   149→      description?: string\n   150→      due_date?: string\n   151→      priority?: string\n   152→      assignee_name?: string\n   153→      assignee_role?: string\n   154→    }\n   155→\n   156→    const rows = await sql`\n   157→      INSERT INTO tasks (title, description, due_date, priority, client_id, deal_id, assignee_name, assignee_role)\n   158→      VALUES (\n   159→        ${body.title},\n   160→        ${body.description ?? null},\n   161→        ${body.due_date || null},\n   162→        ${body.priority ?? 'normal'},\n   163→        ${body.client_id}::uuid,\n   164→        ${body.deal_id}::uuid,\n   165→        ${body.assignee_name ?? null},\n   166→        ${body.assignee_role ?? null}\n   167→      )\n   168→      RETURNING *\n   169→    `\n   170→\n   171→    // Create timeline event\n   172→    await sql`\n   173→      INSERT INTO timeline_events (client_id, event_type, description, user_name, related_entity_id, related_entity_type)\n   174→      VALUES (${body.client_id}::uuid, 'task_created', ${'Task Created: \\'' + body.title + '\\''}, ${req.user?.name ?? 'System'}, ${rows[0].id}::uuid, 'task')\n   175→    `\n   176→\n   177→    return Response.json(rows[0], { status: 201 })\n   178→  }\n   179→\n   180→  // PUT /deal-detail/tasks/:taskId — update task (toggle complete)\n   181→  if (req.method === 'PUT' && resource === 'tasks' && resourceId) {\n   182→    const body = await req.json() as {\n   183→      completed?: boolean\n   184→      title?: string\n   185→      due_date?: string\n   186→      priority?: string\n   187→    }\n   188→\n   189→    const updates: string[] = []\n   190→    if (body.completed !== undefined) {\n   191→      if (body.completed) {\n   192→        updates.push('completed_at = NOW()')\n   193→      } else {\n   194→        updates.push('completed_at = NULL')\n   195→      }\n   196→    }\n   197→\n   198→    const rows = await sql`\n   199→      UPDATE tasks SET\n   200→        completed = COALESCE(${body.completed ?? null}, completed),\n   201→        completed_at = CASE WHEN ${body.completed ?? null}::boolean = true THEN NOW() ELSE NULL END,\n   202→        title = COALESCE(${body.title ?? null}, title),\n   203→        due_date = COALESCE(${body.due_date || null}, due_date),\n   204→        priority = COALESCE(${body.priority ?? null}, priority),\n   205→        updated_at = NOW()\n   206→      WHERE id = ${resourceId}::uuid\n   207→      RETURNING *\n   208→    `\n   209→\n   210→    if (rows.length === 0) {\n   211→      return new Response(JSON.stringify({ error: 'Task not found' }), { status: 404 })\n   212→    }\n   213→\n   214→    // Create timeline event for completion\n   215→    if (body.completed && rows[0].client_id) {\n   216→      await sql`\n   217→        INSERT INTO timeline_events (client_id, event_type, description, user_name, related_entity_id, related_entity_type)\n   218→        VALUES (${rows[0].client_id}::uuid, 'task_completed', ${'Task Completed: \\'' + rows[0].title + '\\''}, ${req.user?.name ?? 'System'}, ${resourceId}::uuid, 'task')\n   219→      `\n   220→    }\n   221→\n   222→    return Response.json(rows[0])\n   223→  }\n   224→\n   225→  // POST /deal-detail/attachments — add attachment to deal\n   226→  if (req.method === 'POST' && resource === 'attachments') {\n   227→    const body = await req.json() as {\n   228→      deal_id: string\n   229→      client_id: string\n   230→      filename: string\n   231→      type: string\n   232→      url: string\n   233→      size?: number\n   234→    }\n   235→\n   236→    const rows = await sql`\n   237→      INSERT INTO attachments (filename, type, url, size, client_id, deal_id)\n   238→      VALUES (\n   239→        ${body.filename},\n   240→        ${body.type},\n   241→        ${body.url},\n   242→        ${body.size ?? null},\n   243→        ${body.client_id}::uuid,\n   244→        ${body.deal_id}::uuid\n   245→      )\n   246→      RETURNING *\n   247→    `\n   248→\n   249→    return Response.json(rows[0], { status: 201 })\n   250→  }\n   251→\n   252→  // DELETE /deal-detail/attachments/:attachmentId — delete attachment\n   253→  if (req.method === 'DELETE' && resource === 'attachments' && resourceId) {\n   254→    await sql`DELETE FROM attachments WHERE id = ${resourceId}::uuid`\n   255→    return new Response(null, { status: 204 })\n   256→  }\n   257→\n   258→  // POST /deal-detail/contacts — add contact to deal\n   259→  if (req.method === 'POST' && resource === 'contacts') {\n   260→    const body = await req.json() as {\n   261→      deal_id: string\n   262→      individual_id: string\n   263→      role: string\n   264→    }\n   265→\n   266→    const rows = await sql`\n   267→      INSERT INTO deal_contacts (deal_id, individual_id, role)\n   268→      VALUES (${body.deal_id}::uuid, ${body.individual_id}::uuid, ${body.role})\n   269→      ON CONFLICT (deal_id, individual_id) DO UPDATE SET role = ${body.role}\n   270→      RETURNING *\n   271→    `\n   272→\n   273→    return Response.json(rows[0], { status: 201 })\n   274→  }\n   275→\n   276→  // DELETE /deal-detail/contacts/:contactId — remove contact from deal\n   277→  if (req.method === 'DELETE' && resource === 'contacts' && resourceId) {\n   278→    await sql`DELETE FROM deal_contacts WHERE id = ${resourceId}::uuid`\n   279→    return new Response(null, { status: 204 })\n   280→  }\n   281→\n   282→  return new Response(JSON.stringify({ error: 'Method not allowed' }), { status: 405 })\n   283→}\n   284→\n   285→export default optionalAuth(handler)\n   286→"}]},"parent_tool_use_id":"toolu_01H4o7rQA4F3jXe6rJCEGT9w","session_id":"530d8c59-bfa9-4164-b537-f4814fcddf55","uuid":"0c615bdf-0d83-4d14-bc90-e3488ec6c1a6"}
