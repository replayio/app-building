41→\n   242→    // Create timeline event\n   243→    await sql`\n   244→      INSERT INTO timeline_events (client_id, event_type, description, user_name, related_entity_id, related_entity_type)\n   245→      VALUES (${body.client_id}::uuid, 'deal_created', ${'Deal Created: \\'' + body.name + '\\''}, ${req.user?.name ?? 'System'}, ${deal.id}::uuid, 'deal')\n   246→    `\n   247→\n   248→    // Add client_name\n   249→    const clientRows = await sql`SELECT name FROM clients WHERE id = ${body.client_id}::uuid`\n   250→    deal.client_name = clientRows.length > 0 ? clientRows[0].name : null\n   251→\n   252→    return Response.json(deal, { status: 201 })\n   253→  }\n   254→\n   255→  // PUT /deals/:dealId — update deal\n   256→  if (req.method === 'PUT' && dealId) {\n   257→    const body = await req.json() as {\n   258→      name?: string\n   259→      client_id?: string\n   260→      value?: number\n   261→      stage?: string\n   262→      owner?: string\n   263→      probability?: number\n   264→      expected_close_date?: string\n   265→      status?: string\n   266→    }\n   267→\n   268→    // If stage is changing, record history\n   269→    if (body.stage) {\n   270→      const existing = await sql`SELECT stage, client_id FROM deals WHERE id = ${dealId}::uuid`\n   271→      if (existing.length > 0 && existing[0].stage !== body.stage) {\n   272→        await sql`\n   273→          INSERT INTO deal_history (deal_id, old_stage, new_stage, changed_by)\n   274→          VALUES (${dealId}::uuid, ${existing[0].stage}, ${body.stage}, ${req.user?.name ?? 'System'})\n   275→        `\n   276→        // Create timeline event for stage change\n   277→        await sql`\n   278→          INSERT INTO timeline_events (client_id, event_type, description, user_name, related_entity_id, related_entity_type)\n   279→          VALUES (\n   280→            ${existing[0].client_id}::uuid,\n   281→            'deal_stage_changed',\n   282→            ${'Deal Stage Changed: from \\'' 