   setTasks(data.tasks || [])\n    44→    } catch {\n    45→      // silently fail\n    46→    } finally {\n    47→      setLoading(false)\n    48→    }\n    49→  }, [clientId])\n    50→\n    51→  useEffect(() => {\n    52→    fetchTasks()\n    53→  }, [fetchTasks, refreshKey])\n    54→\n    55→  const toggleTask = async (task: ClientTask) => {\n    56→    const newCompleted = !task.completed\n    57→    try {\n    58→      await fetch(`/.netlify/functions/tasks/${task.id}`, {\n    59→        method: 'PUT',\n    60→        headers: { 'Content-Type': 'application/json' },\n    61→        body: JSON.stringify({ completed: newCompleted }),\n    62→      })\n    63→      setTasks(prev =>\n    64→        prev.map(t => t.id === task.id\n    65→          ? { ...t, completed: newCompleted, updated_at: new Date().toISOString() }\n    66→          : t\n    67→        )\n    68→      )\n    69→    } catch {\n    70→      // silently fail\n    71→    }\n    72→  }\n    73→\n    74→  const unresolvedTasks = tasks.filter(t => !t.completed)\n    75→\n    76→  return (\n    77→    <div className=\"person-section\" data-testid=\"tasks-section\">\n    78→      <div className=\"person-section-header\">\n    79→        <div className=\"person-section-heading\">\n    80→          <svg width=\"16\" height=\"16\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" strokeWidth=\"2\" strokeLinecap=\"round\" strokeLinejoin=\"round\">\n    81→            <path d=\"M12 20h9\"/><path d=\"m16.376 3.622a1 1 0 0 1 3.002 3.002L7.368 18.635a2 2 0 0 1-.855.506l-2.872.838.838-2.872a2 2 0 0 1 .506-.854z\"/>\n    82→          </svg>\n    83→          <h2 data-testid=\"tasks-heading\">Tasks</h2>\n    84→        </div>\n    85→        <span className=\"source-info-label\" data-testid=\"tasks-unresolved-label\">Unresolved tasks</span>\n    86→      </div>\n    87→\n    88→      {loading ? (\n    89→        <p className=\"person-section-loading\">Loading...</p>\n    90→      ) : unresolvedTasks.length > 0 ? (\n    91→        <div className=\"linked-tasks-list\" data-testid=\"tasks-list\">\n    92→          {unresolvedTasks.map(task => (\n    93→            <div key={task.id} className=\"linked-task-item\" data-testid=\"task-item\">\n    94→              <label className=\"linked-task-checkbox-label\">\n    95→                <input\n    96→                  type=\"checkbox\"\n    97→                  checked={task.completed}\n    98→                  onChange={() => toggleTask(task)}\n    99→                  className=\"linked-task-checkbox\"\n   100→                  data-testid=\"task-checkbox\"\n   101→                />\n   102→              </label>\n   103→              <button\n   104→                className=\"linked-task-name\"\n   105→                onClick={() => navigate('/tasks')}\n   106→                data-testid=\"task-name\"\n   107→              >\n   108→                {task.title}\n   109→              </button>\n   110→              <span className=\"linked-task-date\" data-testid=\"task-due-date\">\n   111→                {formatRelativeDate(task.due_date)}\n   112→              </span>\n   113→              {task.deal_name && (\n   114→                <span className=\"client-task-deal\" data-testid=\"task-deal-association\">\n   115→                  Deal: {task.deal_name}\n   116→                </span>\n   117→              )}\n   118→            </div>\n   119→          ))}\n   120→        </div>\n   121→      ) : (\n   122→        <div className=\"person-section-empty\" data-testid=\"tasks-empty\">\n   123→          No tasks for this client\n   124→        </div>\n   125→      )}\n   126→    </div>\n   127→  )\n   128→}\n   129→"}]},"parent_tool_use_id":"toolu_01GMHUD9CzzAApH8PN2jNhCw","session_id":"7a5dc0eb-162d-4d08-b1b8-eb72ced2b342","uuid":"feee0f12-9aaf-4fb1-8c6c-8bc9043f5198"}
