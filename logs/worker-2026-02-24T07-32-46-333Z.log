e, file_url, file_size, deal_id } = data\n   243→  if (!filename) return json(400, { error: 'Filename is required' })\n   244→\n   245→  const result = await sql`\n   246→    INSERT INTO attachments (client_id, deal_id, filename, file_type, file_url, file_size)\n   247→    VALUES (\n   248→      ${clientId},\n   249→      ${deal_id || null},\n   250→      ${filename},\n   251→      ${file_type || 'application/octet-stream'},\n   252→      ${file_url || null},\n   253→      ${file_size || null}\n   254→    )\n   255→    RETURNING *\n   256→  `\n   257→\n   258→  await sql`\n   259→    INSERT INTO timeline_events (client_id, event_type, title, description, actor, reference_id, reference_type)\n   260→    VALUES (\n   261→      ${clientId},\n   262→      'attachment_uploaded',\n   263→      ${'Attachment Uploaded: ' + filename},\n   264→      ${'File \"' + filename + '\" was uploaded'},\n   265→      ${null},\n   266→      ${result[0].id},\n   267→      'attachment'\n   268→    )\n   269→  `\n   270→\n   271→  return json(201, result[0])\n   272→}\n   273→\n   274→async function handleGetTimeline(sql: SqlFn, clientId: string) {\n   275→  const rows = await sql`\n   276→    SELECT * FROM timeline_events\n   277→    WHERE client_id = ${clientId}\n   278→    ORDER BY event_date DESC\n   279→    LIMIT 100\n   280→  `\n   281→  return json(200, { events: rows })\n   282→}\n   283→\n   284→async function handleCreateTimelineEvent(sql: SqlFn, clientId: string, body: string | null) {\n   285→  const data = JSON.parse(body || '{}')\n   286→  const { event_type, title, description, actor, reference_id, reference_type } = data\n   287→  if (!event_type || !title) return json(400, { error: 'Event type and title are required' })\n   288→\n   289→  const result = await sql`\n   290→    INSERT INTO timeline_events (client_id, event_type, title, description, actor, reference_id, reference_type)\n   291→    VALUES (\n   292→      ${clientId},\n   293→      ${event_type},\n   294→      ${title},\n   295→      ${description || null},\n   296→      ${actor || null},\n   297→      ${reference_id || null},\n   298→      ${reference_type || null}\n   299→    )\n   300→    RETURNING *\n   301→  `\n   302→  return json(201, result[0])\n   303→}\n   304→\n   305→async function handleList(sql: SqlFn, params: Record<string, string>) {\n   306→  const page = parseInt(params.page || '1')\n   307→  const pageSize = parseInt(params.pageSize || '50')\n   308→  const offset = (page - 1) * pageSize\n   309→  const search = params.search || ''\n   310→  const statusFilter = params.status || ''\n   311→  const tagFilter = params.tag || ''\n   312→  const sourceFilter = params.source || ''\n   313→  const sort = params.sort || 'recently_updated'\n   314→  const searchPattern = search ? `%${search}%` : ''\n   315→\n   316→  const countResult = await sql`\n   317→    SELECT COUNT(*)::int as total FROM clients c\n   318→    WHERE\n   319→      (${statusFilter} = '' OR c.status = ${statusFilter})\n   320→      AND (${sourceFilter} = '' OR c.source = ${sourceFilter})\n   321→      AND (${tagFilter} = '' OR EXISTS (\n   322→        SELECT 1 FROM client_tags ct JOIN tags t ON ct.tag_id = t.id\n   323→        WHERE ct.client_id = c.id AND t.name = ${tagFilter}\n   324→      ))\n   325→      AND (${searchPattern} = '' OR c.name ILIKE ${searchPattern}\n   326→        OR EXISTS (SELECT 1 FROM client_tags ct2 JOIN tags t2 ON ct2.tag_id = t2.id\n   327→                   WHERE ct2.client_id = c.id AND t2.name ILIKE ${searchPattern})\n   328→        OR EXISTS (SELECT 1 FROM client_individuals ci JOIN individuals i ON ci.individual_id = i.id\n   329→                   WHERE ci.client_id = c.id AND i.name ILIKE ${searchPattern})\n   330→      )\n   331→  `\n   332→  const total = countResult[0].total\n   333→\n   334→  const rows = await sql`\n   335→    SELECT c.*,\n   336→      (SELECT COALESCE(json_agg(t.name), '[]'::json)\n   337→       FROM client_tags ct JOIN tags t ON ct.tag_