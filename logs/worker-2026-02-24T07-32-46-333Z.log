[2026-02-24T07:26:49.242Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01Qjt9qkXyhL1XSjnri6HfgD","type":"tool_result","content":"     1→import { neon } from '@neondatabase/serverless'\n     2→\n     3→interface HandlerEvent {\n     4→  httpMethod: string\n     5→  path: string\n     6→  queryStringParameters: Record<string, string> | null\n     7→  body: string | null\n     8→}\n     9→\n    10→const corsHeaders = {\n    11→  'Access-Control-Allow-Origin': '*',\n    12→  'Access-Control-Allow-Headers': 'Content-Type',\n    13→  'Access-Control-Allow-Methods': 'GET, POST, PUT, DELETE, OPTIONS',\n    14→}\n    15→\n    16→function json(statusCode: number, body: unknown) {\n    17→  return {\n    18→    statusCode,\n    19→    headers: { 'Content-Type': 'application/json', ...corsHeaders },\n    20→    body: JSON.stringify(body),\n    21→  }\n    22→}\n    23→\n    24→export const handler = async (event: HandlerEvent) => {\n    25→  if (event.httpMethod === 'OPTIONS') {\n    26→    return { statusCode: 204, headers: corsHeaders, body: '' }\n    27→  }\n    28→\n    29→  const sql = neon(process.env.DATABASE_URL!)\n    30→  const segments = event.path.split('/').filter(Boolean)\n    31→  const resourceId = segments[3] || null\n    32→  const subResource = segments[4] || null\n    33→  const subResourceId = segments[5] || null\n    34→  const params = event.queryStringParameters || {}\n    35→\n    36→  try {\n    37→    if (event.httpMethod === 'GET') {\n    38→      if (resourceId) {\n    39→        if (subResource === 'deals') {\n    40→          return await handleGetClientDeals(sql, resourceId)\n    41→        }\n    42→        if (subResource === 'people') {\n    43→          return await handleGetClientPeople(sql, resourceId)\n    44→        }\n    45→        if (subResource === 'attachments') {\n    46→          return await handleGetClientAttachments(sql, resourceId)\n    47→        }\n    48→        if (subResource === 'timeline') {\n    49→          return await handleGetTimeline(sql, resourceId)\n    50→        }\n    51→        if (!subResource) {\n    52→          const rows = await sql`\n    53→            SELECT c.*,\n    54→              (SELECT COALESCE(json_agg(t.name), '[]'::json) FROM client_tags ct JOIN tags t ON ct.tag_id = t.id WHERE ct.client_id = c.id) as tags\n    55→            FROM clients c WHERE c.id = ${resourceId}\n    56→          `\n    57→          if (!rows.length) return json(404, { error: 'Not found' })\n    58→          return json(200, rows[0])\n    59→        }\n    60→      }\n    61→\n    62→      if (params.format === 'csv') {\n    63→        return await handleExport(sql, params)\n    64→      }\n    65→\n    66→      return await handleList(sql, params)\n    67→    }\n    68→\n    69→    if (event.httpMethod === 'POST') {\n    70→      if (resourceId && subResource === 'people') {\n    71→        return await handleAddPerson(sql, resourceId, event.body)\n    72→      }\n    73→      if (resourceId && subResource === 'attachments') {\n    74→        return await handleCreateClientAttachment(sql, resourceId, event.body)\n    75→      }\n    76→      if (resourceId && subResource === 'timeline') {\n    77→        return await handleCreateTimelineEvent(sql, resourceId, event.body)\n    78→      }\n    79→      if (params.action === 'import') {\n    80→        return await handleImport(sql, event.body)\n    81→      }\n    82→      return await handleCreate(sql, event.body)\n    83→    }\n    84→\n    85→    if (event.httpMethod === 'PUT') {\n    86→      if (!resourceId) return json(400, { error: 'Client ID required' })\n    87→      return await handleUpdate(sql, resourceId, event.body)\n    88→    }\n    89→\n    90→    if (event.httpMethod === 'DELETE') {\n    91→      if (resourceId && subResource === 'people' && subResourceId) {\n    92→        return await handleRemovePerson(sql, resourceId, subResourceId)\n    93→      }\n    94→      if (resourceId && subResource === 'attachments' && subResourceId) {\n    95→        await sql`DELETE FROM attachments WHERE id = ${subResourceId} AND client_id = ${resourceId}`\n    96→        return json(200, { deleted: true })\n    97→      }\n    98→      if (resourceId && !subResource) {\n    99→        await sql`DELETE FROM clients WHERE id = ${resourceId}`\n   100→        return json(200, { deleted: true })\n   101→      }\n   102→      return json(400, { error: 'Invalid endpoint' })\n   103→    }\n   104→\n   105→    return json(405, { error: 'Method not allowed' })\n   106→  } catch (err: unknown) {\n   107→    console.error('Error:', err)\n   108→    const message = err instanceof Error ? err.message : 'Internal server error'\n   109→    return json(500, { error: message })\n   110→  }\n   111→}\n   112→\n   113→type SqlFn = ReturnType<typeof neon>\n   114→\n   115→async function handleGetClientDeals(sql: SqlFn, clientId: string) {\n   116→  const rows = await sql`\n   117→    SELECT d.*, c.name as client_name\n   118→    FROM deals d\n   119→    LEFT JOIN clients c ON d.client_id = c.id\n   120→    WHERE d.client_id = ${clientId}\n   121→    ORDER BY d.created_at DESC\n   122→  `\n   123→  return json(200, { deals: rows })\n   124→}\n   125→\n   126→async function handleGetClientPeople(sql: SqlFn, clientId: string) {\n   127→  const rows = await sql`\n   128→    SELECT i.id, i.name, i.title, i.email, i.phone, ci.role, ci.is_primary\n   129→    FROM client_individuals ci\n   130→    JOIN individuals i ON ci.individual_id = i.id\n   131→    WHERE ci.client_id = ${clientId}\n   132→    ORDER BY ci.is_primary DESC, i.name\n   133→  `\n   134→  return json(200, { people: rows })\n   135→}\n   136→\n   137→async function handleAddPerson(sql: SqlFn, clientId: string, body: string | null) {\n   138→  const data = JSON.parse(body || '{}')\n   139→\n   140→  if (data.create_new) {\n   141→    const { name, title, email, phone, role } = data\n   142→    if (!name) return json(400, { error: 'Name is required' })\n   143→\n   144→    const result = await sql`\n   145→      INSERT INTO individuals (name, title, email, phone)\n   146→      VALUES (${name}, ${title || null}, ${email || null}, ${phone || null})\n   147→      RETURNING *\n   148→    `\n   149→    const newPerson = result[0]\n   150→\n   151→    await sql`\n   152→      INSERT INTO client_individuals (client_id, individual_id, role)\n   153→      VALUES (${clientId}, ${newPerson.id}, ${role || null})\n   154→      ON CONFLICT (client_id, individual_id) DO UPDATE SET role = ${role || null}\n   155→    `\n   156→\n   157→    await sql`\n   158→      INSERT INTO timeline_events (client_id, event_type, title, description, actor, reference_id, reference_type)\n   159→      VALUES (\n   160→        ${clientId},\n   161→        'contact_added',\n   162→        ${'Contact Added: ' + name},\n   163→        ${'New contact \"' + name + '\" was added' + (role ? ' as ' + role : '')},\n   164→        ${null},\n   165→        ${newPerson.id},\n   166→        'individual'\n   167→      )\n   168→    `\n   169→\n   170→    return json(201, { ...newPerson, role })\n   171→  }\n   172→\n   173→  const { individual_id, role } = data\n   174→  if (!individual_id) return json(400, { error: 'Person is required' })\n   175→\n   176→  await sql`\n   177→    INSERT INTO client_individuals (client_id, individual_id, role)\n   178→    VALUES (${clientId}, ${individual_id}, ${role || null})\n   179→    ON CONFLICT (client_id, individual_id) DO UPDATE SET role = ${role || null}\n   180→  `\n   181→\n   182→  const person = await sql`\n   183→    SELECT i.id, i.name, i.title, i.email, i.phone, ci.role, ci.is_primary\n   184→    FROM client_individuals ci\n   185→    JOIN individuals i ON ci.individual_id = i.id\n   186→    WHERE ci.client_id = ${clientId} AND ci.individual_id = ${individual_id}\n   187→  `\n   188→\n   189→  await sql`\n   190→    INSERT INTO timeline_events (client_id, event_type, title, description, actor, reference_id, reference_type)\n   191→    VALUES (\n   192→      ${clientId},\n   193→      'contact_added',\n   194→      ${'Contact Added: ' + (person[0]?.name || 'Unknown')},\n   195→      ${'Contact \"' + (person[0]?.name || 'Unknown') + '\" was associated' + (role ? ' as ' + role : '')},\n   196→      ${null},\n   197→      ${individual_id},\n   198→      'individual'\n   199→    )\n   200→  `\n   201→\n   202→  return json(201, person[0])\n   203→}\n   204→\n   205→async function handleRemovePerson(sql: SqlFn, clientId: string, individualId: string) {\n   206→  const person = await sql`\n   207→    SELECT i.name FROM individuals i WHERE i.id = ${individualId}\n   208→  `\n   209→  const personName = person[0]?.name || 'Unknown'\n   210→\n   211→  await sql`DELETE FROM client_individuals WHERE client_id = ${clientId} AND individual_id = ${individualId}`\n   212→\n   213→  await sql`\n   214→    INSERT INTO timeline_events (client_id, event_type, title, description, actor, reference_id, reference_type)\n   215→    VALUES (\n   216→      ${clientId},\n   217→      'contact_removed',\n   218→      ${'Contact Removed: ' + personName},\n   219→      ${'Contact \"' + personName + '\" was removed'},\n   220→      ${null},\n   221→      ${individualId},\n   222→      'individual'\n   223→    )\n   224→  `\n   225→\n   226→  return json(200, { deleted: true })\n   227→}\n   228→\n   229→async function handleGetClientAttachments(sql: SqlFn, clientId: string) {\n   230→  const rows = await sql`\n   231→    SELECT a.*, d.name as deal_name\n   232→    FROM attachments a\n   233→    LEFT JOIN deals d ON a.deal_id = d.id\n   234→    WHERE a.client_id = ${clientId}\n   235→    ORDER BY a.created_at DESC\n   236→  `\n   237→  return json(200, { attachments: rows })\n   238→}\n   239→\n   240→async function handleCreateClientAttachment(sql: SqlFn, clientId: string, body: string | null) {\n   241→  const data = JSON.parse(body || '{}')\n   242→  const { filename, file_type, file_url, file_size, deal_id } = data\n   243→  if (!filename) return json(400, { error: 'Filename is required' })\n   244→\n   245→  const result = await sql`\n   246→    INSERT INTO attachments (client_id, deal_id, filename, file_type, file_url, file_size)\n   247→    VALUES (\n   248→      ${clientId},\n   249→      ${deal_id || null},\n   250→      ${filename},\n   251→      ${file_type || 'application/octet-stream'},\n   252→      ${file_url || null},\n   253→      ${file_size || null}\n   254→    )\n   255→    RETURNING *\n   256→  `\n   257→\n   258→  await sql`\n   259→    INSERT INTO timeline_events (client_id, event_type, title, description, actor, reference_id, reference_type)\n   260→    VALUES (\n   261→      ${clientId},\n   262→      'attachment_uploaded',\n   263→      ${'Attachment Uploaded: ' + filename},\n   264→      ${'File \"' + filename + '\" was uploaded'},\n   265→      ${null},\n   266→      ${result[0].id},\n   267→      'attachment'\n   268→    )\n   269→  `\n   270→\n   271→  return json(201, result[0])\n   272→}\n   273→\n   274→async function handleGetTimeline(sql: SqlFn, clientId: string) {\n   275→  const rows = await sql`\n   276→    SELECT * FROM timeline_events\n   277→    WHERE client_id = ${clientId}\n   278→    ORDER BY event_date DESC\n   279→    LIMIT 100\n   280→  `\n   281→  return json(200, { events: rows })\n   282→}\n   283→\n   284→async function handleCreateTimelineEvent(sql: SqlFn, clientId: string, body: string | null) {\n   285→  const data = JSON.parse(body || '{}')\n   286→  const { event_type, title, description, actor, reference_id, reference_type } = data\n   287→  if (!event_type || !title) return json(400, { error: 'Event type and title are required' })\n   288→\n   289→  const result = await sql`\n   290→    INSERT INTO timeline_events (client_id, event_type, title, description, actor, reference_id, reference_type)\n   291→    VALUES (\n   292→      ${clientId},\n   293→      ${event_type},\n   294→      ${title},\n   295→      ${description || null},\n   296→      ${actor || null},\n   297→      ${reference_id || null},\n   298→      ${reference_type || null}\n   299→    )\n   300→    RETURNING *\n   301→  `\n   302→  return json(201, result[0])\n   303→}\n   304→\n   305→async function handleList(sql: SqlFn, params: Record<string, string>) {\n   306→  const page = parseInt(params.page || '1')\n   307→  const pageSize = parseInt(params.pageSize || '50')\n   308→  const offset = (page - 1) * pageSize\n   309→  const search = params.search || ''\n   310→  const statusFilter = params.status || ''\n   311→  const tagFilter = params.tag || ''\n   312→  const sourceFilter = params.source || ''\n   313→  const sort = params.sort || 'recently_updated'\n   314→  const searchPattern = search ? `%${search}%` : ''\n   315→\n   316→  const countResult = await sql`\n   317→    SELECT COUNT(*)::int as total FROM clients c\n   318→    WHERE\n   319→      (${statusFilter} = '' OR c.status = ${statusFilter})\n   320→      AND (${sourceFilter} = '' OR c.source = ${sourceFilter})\n   321→      AND (${tagFilter} = '' OR EXISTS (\n   322→        SELECT 1 FROM client_tags ct JOIN tags t ON ct.tag_id = t.id\n   323→        WHERE ct.client_id = c.id AND t.name = ${tagFilter}\n   324→      ))\n   325→      AND (${searchPattern} = '' OR c.name ILIKE ${searchPattern}\n   326→        OR EXISTS (SELECT 1 FROM client_tags ct2 JOIN tags t2 ON ct2.tag_id = t2.id\n   327→                   WHERE ct2.client_id = c.id AND t2.name ILIKE ${searchPattern})\n   328→        OR EXISTS (SELECT 1 FROM client_individuals ci JOIN individuals i ON ci.individual_id = i.id\n   329→                   WHERE ci.client_id = c.id AND i.name ILIKE ${searchPattern})\n   330→      )\n   331→  `\n   332→  const total = countResult[0].total\n   333→\n   334→  const rows = await sql`\n   335→    SELECT c.*,\n   336→      (SELECT COALESCE(json_agg(t.name), '[]'::json)\n   337→       FROM client_tags ct JOIN tags t ON ct.tag_id = t.id WHERE ct.client_id = c.id) as tags,\n   338→      (SELECT json_build_object('name', i.name, 'role', COALESCE(ci.role, ''))\n   339→       FROM client_individuals ci JOIN individuals i ON ci.individual_id = i.id\n   340→       WHERE ci.client_id = c.id AND ci.is_primary = true LIMIT 1) as primary_contact,\n   341→      (SELECT json_build_object('count', COUNT(*)::int, 'totalValue', COALESCE(SUM(d.value), 0)::numeric)\n   342→       FROM deals d WHERE d.client_id = c.id AND d.stage NOT IN ('Closed Won', 'Closed Lost')) as open_deals,\n   343→      (SELECT json_build_object('title', tk.title, 'dueDate', tk.due_date)\n   344→       FROM tasks tk WHERE tk.client_id = c.id AND tk.completed = false\n   345→       ORDER BY tk.due_date ASC NULLS LAST LIMIT 1) as next_task\n   346→    FROM clients c\n   347→    WHERE\n   348→      (${statusFilter} = '' OR c.status = ${statusFilter})\n   349→      AND (${sourceFilter} = '' OR c.source = ${sourceFilter})\n   350→      AND (${tagFilter} = '' OR EXISTS (\n   351→        SELECT 1 FROM client_tags ct JOIN tags t ON ct.tag_id = t.id\n   352→        WHERE ct.client_id = c.id AND t.name = ${tagFilter}\n   353→      ))\n   354→      AND (${searchPattern} = '' OR c.name ILIKE ${searchPattern}\n   355→        OR EXISTS (SELECT 1 FROM client_tags ct2 JOIN tags t2 ON ct2.tag_id = t2.id\n   356→                   WHERE ct2.client_id = c.id AND t2.name ILIKE ${searchPattern})\n   357→        OR EXISTS (SELECT 1 FROM client_individuals ci JOIN individuals i ON ci.individual_id = i.id\n   358→                   WHERE ci.client_id = c.id AND i.name ILIKE ${searchPattern})\n   359→      )\n   360→    ORDER BY\n   361→      CASE WHEN ${sort} = 'name_asc' THEN c.name END ASC NULLS LAST,\n   362→      CASE WHEN ${sort} = 'name_desc' THEN c.name END DESC NULLS LAST,\n   363→      c.updated_at DESC NULLS LAST\n   364→    LIMIT ${pageSize} OFFSET ${offset}\n   365→  `\n   366→\n   367→  const allTags = await sql`SELECT DISTINCT name FROM tags ORDER BY name`\n   368→  const allSources = await sql`\n   369→    SELECT DISTINCT source FROM clients WHERE source IS NOT NULL AND source != '' ORDER BY source\n   370→  `\n   371→\n   372→  return json(200, {\n   373→    clients: rows,\n   374→    total,\n   375→    page,\n   376→    pageSize,\n   377→    availableTags: allTags.map((t: Record<string, string>) => t.name),\n   378→    availableSources: allSources.map((s: Record<string, string>) => s.source),\n   379→  })\n   380→}\n   381→\n   382→async function handleExport(sql: SqlFn, params: Record<string, string>) {\n   383→  const exportParams = { ...params, page: '1', pageSize: '10000' }\n   384→  delete exportParams.format\n   385→  const search = exportParams.search || ''\n   386→  const statusFilter = exportParams.status || ''\n   387→  const tagFilter = exportParams.tag || ''\n   388→  const sourceFilter = exportParams.source || ''\n   389→  const sort = exportParams.sort || 'recently_updated'\n   390→  const searchPattern = search ? `%${search}%` : ''\n   391→\n   392→  const rows = await sql`\n   393→    SELECT c.*,\n   394→      (SELECT COALESCE(json_agg(t.name), '[]'::json)\n   395→       FROM client_tags ct JOIN tags t ON ct.tag_id = t.id WHERE ct.client_id = c.id) as tags,\n   396→      (SELECT json_build_object('name', i.name, 'role', COALESCE(ci.role, ''))\n   397→       FROM client_individuals ci JOIN individuals i ON ci.individual_id = i.id\n   398→       WHERE ci.client_id = c.id AND ci.is_primary = true LIMIT 1) as primary_contact,\n   399→      (SELECT json_build_object('count', COUNT(*)::int, 'totalValue', COALESCE(SUM(d.value), 0)::numeric)\n   400→       FROM deals d WHERE d.client_id = c.id AND d.stage NOT IN ('Closed Won', 'Closed Lost')) as open_deals,\n   401→      (SELECT json_build_object('title', tk.title, 'dueDate', tk.due_date)\n   402→       FROM tasks tk WHERE tk.client_id = c.id AND tk.completed = false\n   403→       ORDER BY tk.due_date ASC NULLS LAST LIMIT 1) as next_task\n   404→    FROM clients c\n   405→    WHERE\n   406→      (${statusFilter} = '' OR c.status = ${statusFilter})\n   407→      AND (${sourceFilter} = '' OR c.source = ${sourceFilter})\n   408→      AND (${tagFilter} = '' OR EXISTS (\n   409→        SELECT 1 FROM client_tags ct JOIN tags t ON ct.tag_id = t.id\n   410→        WHERE ct.client_id = c.id AND t.name = ${tagFilter}\n   411→      ))\n   412→      AND (${searchPattern} = '' OR c.name ILIKE ${searchPattern}\n   413→        OR EXISTS (SELECT 1 FROM client_tags ct2 JOIN tags t2 ON ct2.tag_id = t2.id\n   414→                   WHERE ct2.client_id = c.id AND t2.name ILIKE ${searchPattern})\n   415→        OR EXISTS (SELECT 1 FROM client_individuals ci JOIN individuals i ON ci.individual_id = i.id\n   416→                   WHERE ci.client_id = c.id AND i.name ILIKE ${searchPattern})\n   417→      )\n   418→    ORDER BY\n   419→      CASE WHEN ${sort} = 'name_asc' THEN c.name END ASC NULLS LAST,\n   420→      CASE WHEN ${sort} = 'name_desc' THEN c.name END DESC NULLS LAST,\n   421→      c.updated_at DESC NULLS LAST\n   422→  `\n   423→\n   424→  const csvHeader = 'Client Name,Type,Status,Tags,Source,Primary Contact,Open Deals,Next Task'\n   425→  const csvRows = rows.map((r: Record<string, unknown>) => {\n   426→    const tags = (r.tags as string[]) || []\n   427→    const pc = r.primary_contact as { name: string; role: string } | null\n   428→    const od = r.open_deals as { count: number; totalValue: number } | null\n   429→    const nt = r.next_task as { title: string; dueDate: string } | null\n   430→    return [\n   431→      `\"${String(r.name || '').replace(/\"/g, '\"\"')}\"`,\n   432→      r.type,\n   433→      r.status,\n   434→      `\"${tags.join(', ')}\"`,\n   435→      r.source || '',\n   436→      pc ? `${pc.name} (${pc.role || 'N/A'})` : '',\n   437→      od ? `${od.count} ($${od.totalValue})` : '0',\n   438→      nt ? `${nt.title} - ${nt.dueDate || 'No date'}` : 'N/A',\n   439→    ].join(',')\n   440→  })\n   441→\n   442→  return {\n   443→    statusCode: 200,\n   444→    headers: {\n   445→      'Content-Type': 'text/csv',\n   446→      'Content-Disposition': 'attachment; filename=\"clients.csv\"',\n   447→      ...corsHeaders,\n   448→    },\n   449→    body: [csvHeader, ...csvRows].join('\\n'),\n   450→  }\n   451→}\n   452→\n   453→async function handleCreate(sql: SqlFn, body: string | null) {\n   454→  const data = JSON.parse(body || '{}')\n   455→  const { name, type, status, tags, source, source_detail, campaign, channel, date_acquired, industry } = data\n   456→  if (!name) return json(400, { error: 'Client name is required' })\n   457→\n   458→  const result = await sql`\n   459→    INSERT INTO clients (name, type, status, source, source_detail, campaign, channel, date_acquired, industry)\n   460→    VALUES (${name}, ${type || 'Organization'}, ${status || 'Prospect'}, ${source || null}, ${source_detail || null}, ${campaign || null}, ${channel || null}, ${date_acquired || null}, ${industry || null})\n   461→    RETURNING *\n   462→  `\n   463→  const newClient = result[0]\n   464→\n   465→  if (tags && tags.length) {\n   466→    for (const tagName of tags) {\n   467→      await sql`INSERT INTO tags (name) VALUES (${tagName}) ON CONFLICT (name) DO NOTHING`\n   468→      const tagRows = await sql`SELECT id FROM tags WHERE name = ${tagName}`\n   469→      if (tagRows.length) {\n   470→        await sql`INSERT INTO client_tags (client_id, tag_id) VALUES (${newClient.id}, ${tagRows[0].id}) ON CONFLICT DO NOTHING`\n   471→      }\n   472→    }\n   473→  }\n   474→\n   475→  return json(201, newClient)\n   476→}\n   477→\n   478→async function handleImport(sql: SqlFn, body: string | null) {\n   479→  const data = JSON.parse(body || '{}')\n   480→  const clients = data.clients || []\n   481→  let imported = 0\n   482→\n   483→  for (const c of clients) {\n   484→    if (!c.name) continue\n   485→    const inserted = await sql`\n   486→      INSERT INTO clients (name, type, status, source, source_detail, campaign, channel, date_acquired, industry)\n   487→      VALUES (${c.name}, ${c.type || 'Organization'}, ${c.status || 'Prospect'}, ${c.source || null}, ${c.source_detail || null}, ${c.campaign || null}, ${c.channel || null}, ${c.date_acquired || null}, ${c.industry || null})\n   488→      RETURNING id\n   489→    `\n   490→    const clientId = inserted[0].id\n   491→\n   492→    if (c.tags) {\n   493→      const tagNames = typeof c.tags === 'string'\n   494→        ? c.tags.split(',').map((t: string) => t.trim()).filter(Boolean)\n   495→        : c.tags\n   496→      for (const tagName of tagNames) {\n   497→        await sql`INSERT INTO tags (name) VALUES (${tagName}) ON CONFLICT (name) DO NOTHING`\n   498→        const tagRows = await sql`SELECT id FROM tags WHERE name = ${tagName}`\n   499→        if (tagRows.length) {\n   500→          await sql`INSERT INTO client_tags (client_id, tag_id) VALUES (${clientId}, ${tagRows[0].id}) ON CONFLICT DO NOTHING`\n   501→        }\n   502→      }\n   503→    }\n   504→    imported++\n   505→  }\n   506→\n   507→  return json(200, { imported })\n   508→}\n   509→\n   510→async function handleUpdate(sql: SqlFn, id: string, body: string | null) {\n   511→  const data = JSON.parse(body || '{}')\n   512→  const { name, type, status, tags, source, source_detail, campaign, channel, date_acquired, industry } = data\n   513→\n   514→  const current = await sql`SELECT * FROM clients WHERE id = ${id}`\n   515→  if (!current.length) return json(404, { error: 'Not found' })\n   516→  const oldClient = current[0]\n   517→\n   518→  const skipSourceDetail = source_detail === undefined\n   519→  const skipCampaign = campaign === undefined\n   520→  const skipChannel = channel === undefined\n   521→  const skipDateAcquired = date_acquired === undefined\n   522→  const skipIndustry = industry === undefined\n   523→\n   524→  await sql`\n   525→    UPDATE clients SET\n   526→      name = COALESCE(${name || null}, name),\n   527→      type = COALESCE(${type || null}, type),\n   528→      status = COALESCE(${status || null}, status),\n   529→      source = ${source === undefined ? null : (source || null)},\n   530→      source_detail = CASE WHEN ${skipSourceDetail}::boolean THEN source_detail ELSE ${source_detail || null} END,\n   531→      campaign = CASE WHEN ${skipCampaign}::boolean THEN campaign ELSE ${campaign || null} END,\n   532→      channel = CASE WHEN ${skipChannel}::boolean THEN channel ELSE ${channel || null} END,\n   533→      date_acquired = CASE WHEN ${skipDateAcquired}::boolean THEN date_acquired ELSE ${date_acquired || null} END,\n   534→      industry = CASE WHEN ${skipIndustry}::boolean THEN industry ELSE ${industry || null} END,\n   535→      updated_at = now()\n   536→    WHERE id = ${id}\n   537→  `\n   538→\n   539→  if (status && status !== oldClient.status) {\n   540→    await sql`\n   541→      INSERT INTO timeline_events (client_id, event_type, title, description, actor, reference_id, reference_type)\n   542→      VALUES (\n   543→        ${id},\n   544→        'status_changed',\n   545→        ${'Status Changed'},\n   546→        ${'Status changed from \"' + oldClient.status + '\" to \"' + status + '\"'},\n   547→        ${null},\n   548→        ${id},\n   549→        'client'\n   550→      )\n   551→    `\n   552→  }\n   553→\n   554→  if (name && name !== oldClient.name) {\n   555→    await sql`\n   556→      INSERT INTO timeline_events (client_id, event_type, title, description, actor, reference_id, reference_type)\n   557→      VALUES (\n   558→        ${id},\n   559→        'name_changed',\n   560→        ${'Name Changed'},\n   561→        ${'Client name changed from \"' + oldClient.name + '\" to \"' + name + '\"'},\n   562→        ${null},\n   563→        ${id},\n   564→        'client'\n   565→      )\n   566→    `\n   567→  }\n   568→\n   569→  if (!skipCampaign || !skipChannel || !skipSourceDetail || source !== undefined) 