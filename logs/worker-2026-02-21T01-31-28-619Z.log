rson = await res.json() as { id: string }\n    94→        onSave({\n    95→          related_individual_id: newPerson.id,\n    96→          relationship_type: relationshipType,\n    97→        })\n    98→        resetForm()\n    99→      } catch {\n   100→        setSaving(false)\n   101→      }\n   102→      return\n   103→    }\n   104→\n   105→    if (!selectedId) return\n   106→    onSave({\n   107→      related_individual_id: selectedId,\n   108→      relationship_type: relationshipType,\n   109→    })\n   110→    resetForm()\n   111→  }\n   112→\n   113→  function handleSelect(person: IndividualOption) {\n   114→    setSelectedId(person.id)\n   115→    setSelectedName(person.name)\n   116→    setSearchQuery(person.name)\n   117→    setShowDropdown(false)\n   118→  }\n   119→\n   120→  function handleCreateNew() {\n   121→    setCreatingNew(true)\n   122→    setNewPersonName(searchQuery)\n   123→    setSelectedId('')\n   124→    setSelectedName('')\n   125→    setShowDropdown(false)\n   126→  }\n   127→\n   128→  function handleBackToSearch() {\n   129→    setCreatingNew(false)\n   130→    setNewPersonName('')\n   131→  }\n   132→\n   133→  const canSave = creatingNew ? newPersonName.trim().length > 0 : !!selectedId\n   134→\n   135→  return (\n   136→    <div data-testid=\"add-relationship-modal\" className=\"fixed inset-0 z-50 flex items-center justify-center\">\n   137→      <div className=\"absolute inset-0 bg-black/30\" onClick={onClose} />\n   138→      <div className=\"relative bg-surface rounded-[8px] shadow-[var(--shadow-elevation-2)] w-full max-w-[480px] mx-3 max-h-[90vh] overflow-auto\">\n   139→        <div className=\"flex items-center justify-between px-5 py-4 border-b border-border\">\n   140→          <h2 className=\"text-[14px] font-semibold text-text-primary\">Add Relationship</h2>\n   141→          <button\n   142→            onClick={onClose}\n   143→            className=\"inline-flex items-center justify-center w-7 h-7 rounded-[4px] text-text-muted hover:bg-hover transition-colors duration-100\"\n   144→            data-testid=\"add-relationship-modal-close\"\n   145→          >\n   146→            <X size={16} strokeWidth={1.75} />\n   147→          </button>\n   148→        </div>\n   149→        <div className=\"px-5 py-4 flex flex-col gap-3.5\">\n   150→          {creatingNew ? (\n   151→            <div>\n   152→              <label className=\"block text-[12px] font-medium text-text-muted mb-1\">New Person Name *</label>\n   153→              <input\n   154→                data-testid=\"relationship-new-person-name\"\n   155→                type=\"text\"\n   156→                value={newPersonName}\n   157→                onChange={(e) => setNewPersonName(e.target.value)}\n   158→                placeholder=\"Enter person name...\"\n   159→                autoFocus\n   160→                className=\"w-full h-[34px] px-3 text-[13px] text-text-primary bg-base border border-border rounded-[5px] placeholder:text-text-disabled focus:outline-none focus:border-accent\"\n   161→              />\n   162→              <button\n   163→                data-testid=\"relationship-back-to-search\"\n   164→                onClick={handleBackToSearch}\n   165→                className=\"mt-2 text-[12px] text-accent hover:underline\"\n   166→              >\n   167→                Back to search\n   168→              </button>\n   169→            </div>\n   170→          ) : (\n   171→            <div ref={dropdownRef} className=\"relative\">\n   172→              <label className=\"block text-[12px] font-medium text-text-muted mb-1\">Person *</label>\n   173→              <div className=\"relative\">\n   174→                <Search size={14} className=\"absolute left-3 top-1/2 -translate-y-1/2 text-text-disabled\" />\n   175→                <input\n   176→                  data-testid=\"relationship-person-search\"\n   177→                  type=\"text\"\n   178→                  value={showDropdown ? searchQuery : selectedName || searchQuery}\n   179→                  onChange={(e) => {\n   180→                    setSearchQuery(e.target.value)\n   181→                    setSelectedId('')\n   182→                    setSelectedName('')\n   183→                    setShowDropdown(true)\n   184→                  }}\n   185→                  onFocus={() => setShowDropdown(true)}\n   186→                  placeholder=\"Search by name...\"\n   187→                  className=\"w-full h-[34px] pl-8 pr-3 text-[13px] text-text-primary bg-base border border-border rounded-[5px] placeholder:text-text-disabled focus:outline-none focus:border-accent\"\n   188→                />\n   189→              </div>\n   190→              {showDropdown && (\n   191→                <div className=\"absolute z-10 top-full left-0 right-0 mt-1 bg-surface border border-border rounded-[5px] shadow-[var(--shadow-elevation-2)] max-h-[200px] overflow-auto\">\n   192→                  {loading && (\n   193→                    <div className=\"px-3 py-2 text-[12px] text-text-muted\">Searching...</div>\n   194→                  )}\n   195→                  {!loading && options.map((person) => (\n   196→                    <button\n   197→                      key={person.id}\n   198→                      data-testid={`person-option-${person.id}`}\n   199→                      onClick={() => handleSelect(person)}\n   200→                      className=\"w-full text-left px-3 py-2 text-[13px] text-text-primary hover:bg-hover transition-colors duration-75\"\n   201→                    >\n   202→                      <span className=\"font-medium\">{person.name}</span>\n   203→                      {person.title && <span className=\"text-text-muted ml-1.5\">— {person.title}</span>}\n   204→                    </button>\n   205→                  ))}\n   206→                  {!loading && (\n   207→                    <button\n   208→                      data-testid=\"relationship-create-new-person\"\n   209→                      onClick={handleCreateNew}\n   210→                      className=\"w-full text-left px-3 py-2 text-[13px] text-accent hover:bg-hover transition-colors duration-75 border-t border-border flex items-center gap-1.5\"\n   211→                    >\n   212→                      <UserPlus size={13} strokeWidth={1.75} />\n   213→                      <span>Create New Person</span>\n   214→                    </button>\n   215→                  )}\n   216→                </div>\n   217→              )}\n   218→            </div>\n   219→          )}\n   220→          <div>\n   221→            <label className=\"block text-[12px] font-medium text-text-muted mb-1\">Relationship Type *</label>\n   222→            <FilterSelect\n   223→              testId=\"relationship-type-select\"\n   224→              value={relationshipType}\n   225→              onChange={(val) => setRelationshipType(val as RelationshipType)}\n   226→              options={RELATIONSHIP_OPTIONS}\n   227→            />\n   228→          </div>\n   229→        </div>\n   230→        <div className=\"flex items-center justify-end gap-2 px-5 py-4 border-t border-border\">\n   231→          <button\n   232→            data-testid=\"relationship-cancel-button\"\n   233→            onClick={onClose}\n   234→            className=\"h-[34px] px-3.5 text-[13px] font-medium text-text-secondary border border-border rounded-[5px] hover:bg-hover transition-colors duration-100\"\n   235→          >\n   236→            Cancel\n   237→          </button>\n   238→          <button\n   239→            data-testid=\"relationship-save-button\"\n   240→            onClick={handleSave}\n   241→            disabled={!canSave || saving}\n   242→            className=\"h-[34px] px-3.5 text-[13px] font-medium text-white bg-accent rounded-[5px] hover:opacity-90 disabled:opacity-40 disabled:cursor-not-allowed transition-opacity duration-100\"\n   243→          >\n   244→            {saving ? 'Saving...' : 'Save'}\n   245→          </button>\n   246→        </div>\n   247→      </div>\n   248→    </div>\n   249→  )\n   250→}\n   251→"}]},"parent_tool_use_id":"toolu_01PPS3KNiSWhM1chuVTUgDyN","session_id":"5b3fb5ce-80a9-4a05-9bc7-025fe1f5c418","uuid":"edb4d9f4-207a-4798-857f-60672d166d97"}
