e().replace(/\\s+/g, \"-\")}-template.csv`;\n    51→    a.click();\n    52→    URL.revokeObjectURL(url);\n    53→  }\n    54→\n    55→  function handleFileChange(e: React.ChangeEvent<HTMLInputElement>) {\n    56→    const selected = e.target.files?.[0];\n    57→    if (!selected) return;\n    58→    setFile(selected);\n    59→    setRowErrors([]);\n    60→\n    61→    const reader = new FileReader();\n    62→    reader.onload = (ev) => {\n    63→      const text = ev.target?.result as string;\n    64→      const { rows } = parseCsvWithHeaders(text);\n    65→      setParsedRows(rows);\n    66→      setStep(\"preview\");\n    67→    };\n    68→    reader.readAsText(selected);\n    69→  }\n    70→\n    71→  async function handleImport() {\n    72→    setImporting(true);\n    73→    setRowErrors([]);\n    74→    try {\n    75→      const result = await onImport(parsedRows);\n    76→      if (result.errors && result.errors.length > 0) {\n    77→        setRowErrors(result.errors);\n    78→      } else {\n    79→        setImportCount(parsedRows.length);\n    80→        setStep(\"done\");\n    81→      }\n    82→    } finally {\n    83→      setImporting(false);\n    84→    }\n    85→  }\n    86→\n    87→  function handleClose() {\n    88→    setFile(null);\n    89→    setParsedRows([]);\n    90→    setRowErrors([]);\n    91→    setStep(\"upload\");\n    92→    setImportCount(0);\n    93→    onClose();\n    94→  }\n    95→\n    96→  return (\n    97→    <div\n    98→      className=\"import-dialog-overlay\"\n    99→      data-testid={testId ? `${testId}-overlay` : \"import-dialog-overlay\"}\n   100→      onClick={handleClose}\n   101→    >\n   102→      <div\n   103→        className=\"import-dialog\"\n   104→        data-testid={testId || \"import-dialog\"}\n   105→        onClick={(e) => e.stopPropagation()}\n   106→      >\n   107→        <div className=\"import-dialog-header\">\n   108→          <h3 className=\"import-dialog-title\">{title}</h3>\n   109→          <button\n   110→            className=\"import-dialog-close\"\n   111→            data-testid={testId ? `${testId}-close` : \"import-dialog-close\"}\n   112→            onClick={handleClose}\n   113→            type=\"button\"\n   114→          >\n   115→            <svg width=\"14\" height=\"14\" viewBox=\"0 0 14 14\" fill=\"none\">\n   116→              <path\n   117→                d=\"M11 3L3 11M3 3L11 11\"\n   118→                stroke=\"currentColor\"\n   119→                strokeWidth=\"1.5\"\n   120→                strokeLinecap=\"round\"\n   121→              />\n   122→            </svg>\n   123→          </button>\n   124→        </div>\n   125→\n   126→        <div className=\"import-dialog-body\">\n   127→          {step === \"upload\" && (\n   128→            <>\n   129→              <div className=\"import-dialog-section\">\n   130→                <h4 className=\"import-dialog-section-title\">CSV Column Format</h4>\n   131→                <table className=\"import-dialog-spec-table\" data-testid=\"import-column-spec\">\n   132→                  <thead>\n   133→                    <tr>\n   134→                      <th>Column</th>\n   135→                      <th>Required</th>\n   136→                      <th>Description</th>\n   137→                    </tr>\n   138→                  </thead>\n   139→                  <tbody>\n   140→                    {columns.map((col) => (\n   141→                      <tr key={col.key}>\n   142→                        <td>{col.header}</td>\n   143→                        <td>{col.required ? \"Yes\" : \"No\"}</td>\n   144→                        <td>{col.description || \"\"}</td>\n   145→                      </tr>\n   146→                    ))}\n   147→                  </tbody>\n   148→                </table>\n   149→              </div>\n   150→\n   151→              <div className=\"import-dialog-actions-row\">\n   152→                <button\n   153→                  className=\"btn btn--secondary\"\n   154→                  data-testid=\"import-download-template\"\n   155→                  onClick={handleDownloadTemplate}\n   156→                  type=\"button\"\n   157→                >\n   158→                  Download CSV Template\n   159→                </button>\n   160→              </div>\n   161→\n   162→              <div className=\"import-dialog-upload\">\n   163→                <input\n   164→                  ref={fileInputRef}\n   165→                  type=\"file\"\n   166→                  accept=\".csv\"\n   167→                  data-testid=\"import-file-input\"\n   168→                  onChange={handleFileChange}\n   169→                  style={{ display: \"none\" }}\n   170→                />\n   171→                <button\n   172→                  className=\"btn btn--primary\"\n   173→                  data-testid=\"import-upload-btn\"\n   174→                  onClick={() => fileInputRef.current?.click()}\n   175→                  type=\"button\"\n   176→                >\n   177→                  Choose CSV File\n   178→                </button>\n   179→                {file && (\n   180→                  <span className=\"import-dialog-filename\">{file.name}</span>\n   181→                )}\n   182→              </div>\n   183→            </>\n   184→          )}\n   185→\n   186→          {step === \"preview\" && (\n   187→            <>\n   188→              <p className=\"import-dialog-preview-info\">\n   189→                Found {parsedRows.length} row{parsedRows.length !== 1 ? \"s\" : \"\"} to import\n   190→                {file ? ` from ${file.name}` : \"\"}.\n   191→              </p>\n   192→\n   193→              {rowErrors.length > 0 && (\n   194→                <div className=\"import-dialog-errors\" data-testid=\"import-errors\">\n   195→                  <h4 className=\"import-dialog-errors-title\">Validation Errors</h4>\n   196→                  <ul className=\"import-dialog-errors-list\">\n   197→                    {rowErrors.map((re) => (\n   198→                      <li key={re.row}>\n   199→                        <strong>Row {re.row}:</strong> {re.errors.join(\"; \")}\n   200→                      </li>\n   201→                    ))}\n   202→                  </ul>\n   203→                </div>\n   204→              )}\n   205→\n   206→              <div className=\"import-dialog-footer\">\n   207→                <button\n   208→                  className=\"btn btn--secondary\"\n   209→                  onClick={() => {\n   210→                    setStep(\"upload\");\n   211→                    setFile(null);\n   212→                    setParsedRows([]);\n   213→                    setRowErrors([]);\n   214→                    if (fileInputRef.current) fileInputRef.current.value = \"\";\n   215→                  }}\n   216→                  type=\"button\"\n   217→                >\n   218→                  Back\n   219→                </button>\n   220→                <button\n   221→                  className=\"btn btn--primary\"\n   222→                  data-testid=\"import-confirm-btn\"\n   223→                  disabled={importing || parsedRows.length === 0}\n   224→                  onClick={handleImport}\n   225→                  type=\"button\"\n   226→                >\n   227→                  {importing ? \"Importing...\" : `Import ${parsedRows.length} Row${parsedRows.length !== 1 ? \"s\" : \"\"}`}\n   228→                </button>\n   229→              </div>\n   230→            </>\n   231→          )}\n   232→\n   233→          {step === \"done\" && (\n   234→            <>\n   235→              <p className=\"import-dialog-success\" data-testid=\"import-success\">\n   236→                Successfully imported {importCount} row{importCount !== 1 ? \"s\" : \"\"}.\n   237→              </p>\n   238→              <div className=\"import-dialog-footer\">\n   239→                <button\n   240→                  className=\"btn btn--primary\"\n   241→                  data-testid=\"import-done-btn\"\n   242→                  onClick={handleClose}\n   243→                  type=\"button\"\n   244→                >\n   245→                  Done\n   246→                </button>\n   247→              </div>\n   248→            </>\n   249→          )}\n   250→        </div>\n   251→      </div>\n   252→    </div>\n   253→  );\n   254→}\n   255→"}]},"parent_tool_use_id":"toolu_01CZd5yLYWw63LuwzEcs1N3m","session_id":"6fd46deb-26c3-477b-b65b-810547e0388d","uuid":"dcc91add-2485-4b86-9c3e-3d95a6ef2980"}
