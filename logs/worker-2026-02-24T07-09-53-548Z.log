rity-medium-bg: #fffbeb;\n  --color-priority-low: #22c55e;\n  --color-priority-low-bg: #f0fdf4;\n  --color-priority-normal: #6b7280;\n  --color-priority-normal-bg: #f3f4f6;\n\n  /* Colors - Deal status */\n  --color-deal-on-track: #22c55e;\n  --color-deal-on-track-bg: #f0fdf4;\n  --color-deal-needs-attention: #f59e0b;\n  --color-deal-needs-attention-bg: #fffbeb;\n  --color-deal-at-risk: #ef4444;\n  --color-deal-at-risk-bg: #fef2f2;\n  --color-deal-won: #7180ff;\n  --color-deal-won-bg: #eef0ff;\n\n  /* Spacing */\n  --sidebar-width: 244px;\n\n  /* Border radius */\n  --radius-sm: 4px;\n  --radius-md: 6px;\n  --radius-lg: 8px;\n\n  /* Shadows */\n  --shadow-sm: 0px 4px 12px rgba(0, 0, 0, 0.1);\n  --shadow-md: 0px 4px 12px rgba(0, 0, 0, 0.1), 0px 0px 0px 1px rgba(0, 0, 0, 0.2);\n}\n\n* {\n  box-sizing: border-box;\n  margin: 0;\n  padding: 0;\n}\n\nbody {\n  font-family: var(--font-sans);\n  font-size: 13px;\n  color: var(--color-text-primary);\n  background: var(--color-bg-base);\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n}\n\n/* Scrollbar styling */\n::-webkit-scrollbar {\n  width: 8px;\n  height: 8px;\n}\n::-webkit-scrollbar-track {\n  background: transparent;\n}\n::-webkit-scrollbar-thumb {\n  background: var(--color-border-default);\n  border-radius: 4px;\n}\n::-webkit-scrollbar-thumb:hover {\n  background: var(--color-text-disabled);\n}\n\n/* ===== Layout ===== */\n.app-layout {\n  display: flex;\n  height: 100vh;\n  overflow: hidden;\n}\n\n.app-main {\n  flex: 1;\n  overflow-y: auto;\n  background: var(--color-bg-base);\n}\n\n.page-content {\n  max-width: 1400px;\n}\n\n.page-header {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  margin-bottom: 20px;\n  gap: 16px;\n}\n\n.page-title {\n  font-size: 20px;\n  font-weight: 600;\n  color: var(--color-text-primary);\n}\n\n/* ===== Search & Filters ===== */\n.clients-filters {\n  display: flex;\n  flex-direction: column;\n  gap: 12px;\n}\n\n.clients-search-wrapper {\n  position: relative;\n  display: flex;\n  align-items: center;\n}\n\n.clients-search-icon {\n  position: absolute;\n  left: 10px;\n  color: var(--color-text-muted);\n  pointer-events: none;\n}\n\n.clients-search-input {\n  width: 100%;\n  height: 36px;\n  padding: 0 32px 0 34px;\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-sm);\n  background: var(--color-bg-base);\n  font-size: 13px;\n  color: var(--color-text-primary);\n  outline: none;\n  transition: border-color 0.15s ease;\n}\n\n.clients-search-input::placeholder {\n  color: var(--color-text-disabled);\n}\n\n.clients-search-input:focus {\n  border-color: var(--color-accent);\n}\n\n.clients-search-clear {\n  position: absolute;\n  right: 8px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 20px;\n  height: 20px;\n  border: none;\n  border-radius: 50%;\n  background: var(--color-bg-hover);\n  color: var(--color-text-muted);\n  cursor: pointer;\n}\n\n.clients-filter-row {\n  display: flex;\n  gap: 8px;\n  flex-wrap: wrap;\n}\n\n.filter-dropdown-wrapper {\n  position: relative;\n}\n\n.filter-trigger {\n  display: inline-flex;\n  align-items: center;\n  gap: 4px;\n  height: 28px;\n  padding: 0 8px;\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-sm);\n  background: var(--color-bg-base);\n  color: var(--color-text-secondary);\n  font-size: 12px;\n  cursor: pointer;\n  transition: background 0.1s ease;\n  white-space: nowrap;\n}\n\n.filter-trigger:hover {\n  background: var(--color-bg-hover);\n}\n\n.filter-dropdown-menu {\n  position: absolute;\n  top: calc(100% + 4px);\n  left: 0;\n  min-width: 160px;\n  background: var(--color-bg-base);\n  border-radius: var(--radius-md);\n  box-shadow: var(--shadow-md);\n  padding: 4px;\n  z-index: 40;\n}\n\n.filter-dropdown-option {\n  display: block;\n  width: 100%;\n  padding: 6px 8px;\n  border: none;\n  border-radius: var(--radius-sm);\n  background: transparent;\n  color: var(--color-text-secondary);\n  font-size: 13px;\n  text-align: left;\n  cursor: pointer;\n  transition: background 0.1s ease;\n}\n\n.filter-dropdown-option:hover {\n  background: var(--color-bg-hover);\n}\n\n.filter-dropdown-option.selected {\n  background: var(--color-bg-selected);\n  color: var(--color-text-primary);\n  font-weight: 500;\n}\n\n.filter-section-label {\n  padding: 6px 8px 2px;\n  font-size: 11px;\n  font-weight: 600;\n  text-transform: uppercase;\n  color: var(--color-text-muted);\n  letter-spacing: 0.05em;\n}\n\n.filter-section-divider {\n  height: 1px;\n  background: var(--color-border-default);\n  margin: 4px 0;\n}\n\n.filter-date-range {\n  padding: 4px 8px;\n  display: flex;\n  flex-direction: column;\n  gap: 6px;\n}\n\n.filter-date-label {\n  display: flex;\n  align-items: center;\n  gap: 6px;\n  font-size: 12px;\n  color: var(--color-text-secondary);\n}\n\n.filter-date-input {\n  flex: 1;\n  padding: 4px 6px;\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-sm);\n  background: var(--color-bg-base);\n  color: var(--color-text-primary);\n  font-size: 12px;\n}\n\n/* ===== Actions ===== */\n.clients-actions {\n  display: flex;\n  align-items: center;\n  gap: 8px;\n}\n\n/* ===== Buttons ===== */\n.btn-ghost {\n  display: inline-flex;\n  align-items: center;\n  gap: 6px;\n  height: 28px;\n  padding: 0 8px;\n  border: 1px solid var(--color-border-default);\n  border-radius: 5px;\n  background: transparent;\n  color: var(--color-text-secondary);\n  font-size: 13px;\n  font-weight: 400;\n  cursor: pointer;\n  transition: background 0.1s ease;\n  white-space: nowrap;\n}\n\n.btn-ghost:hover {\n  background: var(--color-bg-hover);\n  color: var(--color-text-primary);\n}\n\n.btn-primary {\n  display: inline-flex;\n  align-items: center;\n  gap: 6px;\n  height: 28px;\n  padding: 0 12px;\n  border: none;\n  border-radius: 5px;\n  background: var(--color-accent);\n  color: white;\n  font-size: 13px;\n  font-weight: 500;\n  cursor: pointer;\n  transition: opacity 0.1s ease;\n  white-space: nowrap;\n}\n\n.btn-primary:hover {\n  opacity: 0.9;\n}\n\n.btn-primary:disabled {\n  opacity: 0.6;\n  cursor: default;\n}\n\n.btn-secondary {\n  display: inline-flex;\n  align-items: center;\n  gap: 6px;\n  height: 28px;\n  padding: 0 12px;\n  border: 1px solid var(--color-border-default);\n  border-radius: 5px;\n  background: var(--color-bg-base);\n  color: var(--color-text-secondary);\n  font-size: 13px;\n  cursor: pointer;\n  transition: background 0.1s ease;\n  white-space: nowrap;\n}\n\n.btn-secondary:hover {\n  background: var(--color-bg-hover);\n}\n\n.btn-danger {\n  display: inline-flex;\n  align-items: center;\n  gap: 6px;\n  height: 28px;\n  padding: 0 12px;\n  border: none;\n  border-radius: 5px;\n  background: var(--color-status-churned);\n  color: white;\n  font-size: 13px;\n  font-weight: 500;\n  cursor: pointer;\n  transition: opacity 0.1s ease;\n  white-space: nowrap;\n}\n\n.btn-danger:hover {\n  opacity: 0.9;\n}\n\n/* ===== Table ===== */\n.clients-table {\n  display: grid;\n  grid-template-columns: 1fr;\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-md);\n}\n\n.clients-table-header {\n  display: grid;\n  grid-template-columns: 2fr 1fr 1fr 1.5fr 1.5fr 1fr 1.5fr 44px;\n  align-items: center;\n  height: 36px;\n  padding: 0 12px;\n  background: var(--color-bg-sidebar);\n  border-bottom: 1px solid var(--color-border-default);\n  font-size: 12px;\n  font-weight: 500;\n  color: var(--color-text-muted);\n}\n\n.clients-table-row {\n  display: grid;\n  grid-template-columns: 2fr 1fr 1fr 1.5fr 1.5fr 1fr 1.5fr 44px;\n  align-items: center;\n  height: 44px;\n  padding: 0 12px;\n  border-bottom: 1px solid var(--color-border-light);\n  transition: background 0.1s ease;\n  cursor: default;\n}\n\n.clients-table-row:last-child {\n  border-bottom: none;\n}\n\n.clients-table-row:hover {\n  background: var(--color-bg-hover);\n}\n\n.table-cell {\n  padding: 0 8px;\n  overflow: hidden;\n  text-overflow: ellipsis;\n  white-space: nowrap;\n  font-size: 13px;\n  color: var(--color-text-secondary);\n}\n\n.table-cell-action {\n  padding: 0;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  position: relative;\n  z-index: 1;\n  overflow: visible;\n}\n\n.table-cell-tags {\n  display: flex;\n  gap: 4px;\n  overflow: hidden;\n}\n\n.table-cell-contact,\n.table-cell-task {\n  font-size: 12px;\n  color: var(--color-text-muted);\n}\n\n.table-cell-type {\n  font-size: 12px;\n}\n\n.client-name-link {\n  border: none;\n  background: none;\n  color: var(--color-text-primary);\n  font-size: 13px;\n  font-weight: 500;\n  cursor: pointer;\n  padding: 0;\n  text-decoration: none;\n  transition: color 0.1s ease;\n}\n\n.client-name-link:hover {\n  color: var(--color-accent);\n}\n\n/* ===== Status Badges ===== */\n.status-badge {\n  display: inline-flex;\n  align-items: center;\n  height: 20px;\n  padding: 0 8px;\n  border-radius: 10px;\n  font-size: 11px;\n  font-weight: 500;\n  white-space: nowrap;\n}\n\n.status-active {\n  color: var(--color-status-active);\n  background: var(--color-status-active-bg);\n}\n\n.status-inactive {\n  color: var(--color-status-inactive);\n  background: var(--color-status-inactive-bg);\n}\n\n.status-prospect {\n  color: var(--color-status-prospect);\n  background: var(--color-status-prospect-bg);\n}\n\n.status-churned {\n  color: var(--color-status-churned);\n  background: var(--color-status-churned-bg);\n}\n\n/* ===== Tag Chips ===== */\n.tag-chip {\n  display: inline-flex;\n  align-items: center;\n  height: 20px;\n  padding: 0 8px;\n  border-radius: 10px;\n  font-size: 11px;\n  color: var(--color-accent-muted);\n  background: #eef0ff;\n  white-space: nowrap;\n}\n\n/* ===== Row Actions ===== */\n.row-action-wrapper {\n  position: relative;\n}\n\n.row-action-trigger {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 28px;\n  height: 28px;\n  border: none;\n  border-radius: var(--radius-sm);\n  background: transparent;\n  color: var(--color-text-muted);\n  cursor: pointer;\n  transition: background 0.1s ease;\n}\n\n.row-action-trigger:hover {\n  background: var(--color-bg-hover);\n  color: var(--color-text-primary);\n}\n\n.row-action-menu {\n  position: absolute;\n  top: calc(100% + 2px);\n  right: 0;\n  min-width: 160px;\n  background: var(--color-bg-base);\n  border-radius: var(--radius-md);\n  box-shadow: var(--shadow-md);\n  padding: 4px;\n  z-index: 40;\n}\n\n.row-action-item {\n  display: flex;\n  align-items: center;\n  gap: 8px;\n  width: 100%;\n  padding: 6px 8px;\n  border: none;\n  border-radius: var(--radius-sm);\n  background: transparent;\n  color: var(--color-text-secondary);\n  font-size: 13px;\n  text-align: left;\n  cursor: pointer;\n  transition: background 0.1s ease;\n}\n\n.row-action-item:hover {\n  background: var(--color-bg-hover);\n}\n\n.row-action-item.danger {\n  color: var(--color-status-churned);\n}\n\n.row-action-item.danger:hover {\n  background: var(--color-status-churned-bg);\n}\n\n/* ===== Empty State ===== */\n.clients-table-empty {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: center;\n  padding: 48px 24px;\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-md);\n  text-align: center;\n}\n\n.empty-title {\n  font-size: 16px;\n  font-weight: 500;\n  color: var(--color-text-primary);\n  margin-bottom: 4px;\n}\n\n.empty-subtitle {\n  font-size: 13px;\n  color: var(--color-text-muted);\n}\n\n/* ===== Pagination ===== */\n.pagination {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  gap: 16px;\n  flex-wrap: wrap;\n}\n\n.pagination-info {\n  font-size: 13px;\n  color: var(--color-text-muted);\n}\n\n.pagination-controls {\n  display: flex;\n  align-items: center;\n  gap: 4px;\n}\n\n.pagination-btn {\n  display: inline-flex;\n  align-items: center;\n  gap: 4px;\n  height: 28px;\n  padding: 0 8px;\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-sm);\n  background: var(--color-bg-base);\n  color: var(--color-text-secondary);\n  font-size: 13px;\n  cursor: pointer;\n  transition: background 0.1s ease;\n}\n\n.pagination-btn:hover:not(:disabled) {\n  background: var(--color-bg-hover);\n}\n\n.pagination-btn:disabled {\n  opacity: 0.5;\n  cursor: default;\n}\n\n.pagination-pages {\n  display: flex;\n  align-items: center;\n  gap: 2px;\n}\n\n.pagination-page {\n  display: inline-flex;\n  align-items: center;\n  justify-content: center;\n  min-width: 28px;\n  height: 28px;\n  padding: 0 6px;\n  border: 1px solid transparent;\n  border-radius: var(--radius-sm);\n  background: transparent;\n  color: var(--color-text-secondary);\n  font-size: 13px;\n  cursor: pointer;\n  transition: background 0.1s ease;\n}\n\n.pagination-page:hover {\n  background: var(--color-bg-hover);\n}\n\n.pagination-page.active {\n  border-color: var(--color-accent);\n  color: var(--color-accent);\n  font-weight: 500;\n}\n\n.pagination-ellipsis {\n  display: inline-flex;\n  align-items: center;\n  justify-content: center;\n  width: 28px;\n  height: 28px;\n  color: var(--color-text-muted);\n  font-size: 13px;\n}\n\n/* ===== Modal ===== */\n.modal-overlay {\n  position: fixed;\n  inset: 0;\n  background: rgba(0, 0, 0, 0.4);\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  z-index: 100;\n}\n\n.modal-content {\n  background: var(--color-bg-base);\n  border-radius: var(--radius-lg);\n  box-shadow: var(--shadow-md);\n  width: 440px;\n  max-height: 90vh;\n  overflow-y: auto;\n  padding: 20px;\n}\n\n.modal-content.modal-wide {\n  width: 580px;\n}\n\n.modal-content.modal-small {\n  width: 380px;\n}\n\n.modal-header {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  margin-bottom: 16px;\n}\n\n.modal-title {\n  font-size: 16px;\n  font-weight: 600;\n  color: var(--color-text-primary);\n}\n\n.modal-close {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 28px;\n  height: 28px;\n  border: none;\n  border-radius: var(--radius-sm);\n  background: transparent;\n  color: var(--color-text-muted);\n  cursor: pointer;\n  transition: background 0.1s ease;\n}\n\n.modal-close:hover {\n  background: var(--color-bg-hover);\n}\n\n.modal-body {\n  margin-bottom: 16px;\n}\n\n.modal-text {\n  font-size: 13px;\n  color: var(--color-text-secondary);\n  line-height: 1.5;\n  margin-bottom: 12px;\n}\n\n.modal-actions {\n  display: flex;\n  justify-content: flex-end;\n  gap: 8px;\n  margin-top: 16px;\n}\n\n.modal-form {\n  display: flex;\n  flex-direction: column;\n  gap: 14px;\n}\n\n/* ===== Form Fields ===== */\n.form-field {\n  display: flex;\n  flex-direction: column;\n  gap: 4px;\n}\n\n.form-label {\n  font-size: 12px;\n  font-weight: 500;\n  color: var(--color-text-muted);\n}\n\n.form-input {\n  height: 32px;\n  padding: 0 10px;\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-sm);\n  background: var(--color-bg-base);\n  font-size: 13px;\n  color: var(--color-text-primary);\n  outline: none;\n  transition: border-color 0.15s ease;\n}\n\n.form-input:focus {\n  border-color: var(--color-accent);\n}\n\n.form-input.error {\n  border-color: var(--color-status-churned);\n}\n\n.form-error {\n  font-size: 12px;\n  color: var(--color-status-churned);\n}\n\n/* Type selector */\n.type-selector {\n  display: flex;\n  gap: 8px;\n}\n\n.type-option {\n  flex: 1;\n  height: 32px;\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-sm);\n  background: var(--color-bg-base);\n  color: var(--color-text-secondary);\n  font-size: 13px;\n  cursor: pointer;\n  transition: all 0.1s ease;\n}\n\n.type-option:hover {\n  background: var(--color-bg-hover);\n}\n\n.type-option.selected {\n  border-color: var(--color-accent);\n  color: var(--color-accent);\n  background: #eef0ff;\n  font-weight: 500;\n}\n\n/* Form dropdown */\n.form-dropdown-wrapper {\n  position: relative;\n}\n\n.form-dropdown-trigger {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  width: 100%;\n  height: 32px;\n  padding: 0 10px;\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-sm);\n  background: var(--color-bg-base);\n  color: var(--color-text-primary);\n  font-size: 13px;\n  cursor: pointer;\n  text-align: left;\n}\n\n.form-dropdown-menu {\n  position: absolute;\n  top: calc(100% + 4px);\n  left: 0;\n  right: 0;\n  background: var(--color-bg-base);\n  border-radius: var(--radius-md);\n  box-shadow: var(--shadow-md);\n  padding: 4px;\n  z-index: 50;\n}\n\n.form-dropdown-option {\n  display: block;\n  width: 100%;\n  padding: 6px 10px;\n  border: none;\n  border-radius: var(--radius-sm);\n  background: transparent;\n  color: var(--color-text-secondary);\n  font-size: 13px;\n  text-align: left;\n  cursor: pointer;\n  transition: background 0.1s ease;\n}\n\n.form-dropdown-option:hover {\n  background: var(--color-bg-hover);\n}\n\n.form-dropdown-option.selected {\n  background: var(--color-bg-selected);\n  font-weight: 500;\n}\n\n/* Tag input */\n.tag-input-wrapper {\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-sm);\n  padding: 4px 6px;\n  min-height: 32px;\n  display: flex;\n  align-items: center;\n}\n\n.tag-input-tags {\n  display: flex;\n  flex-wrap: wrap;\n  gap: 4px;\n  align-items: center;\n  flex: 1;\n}\n\n.tag-input-chip {\n  display: inline-flex;\n  align-items: center;\n  gap: 2px;\n  height: 22px;\n  padding: 0 6px;\n  border-radius: 10px;\n  font-size: 11px;\n  color: var(--color-accent-muted);\n  background: #eef0ff;\n}\n\n.tag-input-remove {\n  display: flex;\n  align-items: center;\n  border: none;\n  background: none;\n  color: var(--color-accent-muted);\n  cursor: pointer;\n  padding: 0;\n  margin-left: 2px;\n}\n\n.tag-input-remove:hover {\n  color: var(--color-text-primary);\n}\n\n.tag-input-field {\n  border: none;\n  outline: none;\n  font-size: 13px;\n  color: var(--color-text-primary);\n  background: transparent;\n  min-width: 80px;\n  flex: 1;\n}\n\n.tag-input-field::placeholder {\n  color: var(--color-text-disabled);\n}\n\n/* ===== Import ===== */\n.import-instructions {\n  margin-bottom: 16px;\n  padding: 12px;\n  background: var(--color-bg-sidebar);\n  border-radius: var(--radius-md);\n}\n\n.import-instructions-title {\n  font-size: 13px;\n  font-weight: 500;\n  color: var(--color-text-primary);\n  margin-bottom: 6px;\n}\n\n.import-instructions-text {\n  font-size: 12px;\n  color: var(--color-text-muted);\n  line-height: 1.5;\n}\n\n.import-dropzone {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: center;\n  padding: 32px;\n  border: 2px dashed var(--color-border-default);\n  border-radius: var(--radius-md);\n  text-align: center;\n  cursor: pointer;\n}\n\n.import-dropzone-icon {\n  color: var(--color-text-disabled);\n  margin-bottom: 8px;\n}\n\n.import-dropzone-text {\n  font-size: 13px;\n  color: var(--color-text-muted);\n  margin-bottom: 12px;\n}\n\n.import-browse-btn {\n  cursor: pointer;\n}\n\n.import-file-input {\n  display: none;\n}\n\n.import-preview {\n  margin-top: 12px;\n}\n\n.import-preview-info {\n  font-size: 13px;\n  font-weight: 500;\n  color: var(--color-text-primary);\n  margin-bottom: 8px;\n}\n\n.import-preview-table {\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-sm);\n  overflow: hidden;\n  font-size: 12px;\n}\n\n.import-preview-header {\n  display: flex;\n  background: var(--color-bg-sidebar);\n  border-bottom: 1px solid var(--color-border-default);\n}\n\n.import-preview-row {\n  display: flex;\n  border-bottom: 1px solid var(--color-border-light);\n}\n\n.import-preview-row:last-child {\n  border-bottom: none;\n}\n\n.import-preview-cell {\n  flex: 1;\n  padding: 6px 8px;\n  overflow: hidden;\n  text-overflow: ellipsis;\n  white-space: nowrap;\n  color: var(--color-text-secondary);\n}\n\n.import-preview-cell.header {\n  font-weight: 500;\n  color: var(--color-text-muted);\n}\n\n.import-preview-more {\n  padding: 6px 8px;\n  font-size: 12px;\n  color: var(--color-text-muted);\n}\n\n.import-error {\n  margin-top: 8px;\n  font-size: 13px;\n  color: var(--color-status-churned);\n}\n\n.import-success {\n  margin-top: 8px;\n  font-size: 13px;\n  color: var(--color-status-active);\n}\n\n/* ===== Tasks Filter ===== */\n.tasks-filter {\n  display: flex;\n  align-items: center;\n  gap: 8px;\n}\n\n.tasks-filter-input-wrapper {\n  position: relative;\n  flex: 1;\n  max-width: 320px;\n}\n\n.tasks-filter-input {\n  width: 100%;\n  height: 28px;\n  padding: 0 28px 0 10px;\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-sm);\n  background: var(--color-bg-base);\n  font-size: 13px;\n  color: var(--color-text-primary);\n  outline: none;\n  transition: border-color 0.15s ease;\n}\n\n.tasks-filter-input::placeholder {\n  color: var(--color-text-disabled);\n}\n\n.tasks-filter-input:focus {\n  border-color: var(--color-accent);\n}\n\n.tasks-filter-clear {\n  position: absolute;\n  right: 4px;\n  top: 50%;\n  transform: translateY(-50%);\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 20px;\n  height: 20px;\n  border: none;\n  border-radius: 50%;\n  background: var(--color-bg-hover);\n  color: var(--color-text-muted);\n  cursor: pointer;\n}\n\n/* ===== Tasks List ===== */\n.tasks-list {\n  display: flex;\n  flex-direction: column;\n  gap: 1px;\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-md);\n}\n\n.task-card {\n  display: flex;\n  align-items: center;\n  gap: 12px;\n  padding: 12px;\n  background: var(--color-bg-base);\n  transition: background 0.1s ease;\n}\n\n.task-card:first-child {\n  border-radius: var(--radius-md) var(--radius-md) 0 0;\n}\n\n.task-card:last-child {\n  border-radius: 0 0 var(--radius-md) var(--radius-md);\n}\n\n.task-card:only-child {\n  border-radius: var(--radius-md);\n}\n\n.task-card:not(:last-child) {\n  border-bottom: 1px solid var(--color-border-light);\n}\n\n.task-card:hover {\n  background: var(--color-bg-hover);\n}\n\n/* ===== Priority Badges ===== */\n.priority-badge {\n  display: inline-flex;\n  align-items: center;\n  justify-content: center;\n  height: 20px;\n  padding: 0 8px;\n  border-radius: 10px;\n  font-size: 11px;\n  font-weight: 500;\n  white-space: nowrap;\n  flex-shrink: 0;\n  min-width: 56px;\n}\n\n.priority-high {\n  color: var(--color-priority-high);\n  background: var(--color-priority-high-bg);\n}\n\n.priority-medium {\n  color: var(--color-priority-medium);\n  background: var(--color-priority-medium-bg);\n}\n\n.priority-low {\n  color: var(--color-priority-low);\n  background: var(--color-priority-low-bg);\n}\n\n.priority-normal {\n  color: var(--color-priority-normal);\n  background: var(--color-priority-normal-bg);\n}\n\n/* ===== Task Card Elements ===== */\n.task-name {\n  flex: 1;\n  min-width: 0;\n  border: none;\n  background: none;\n  padding: 0;\n  font-size: 13px;\n  font-weight: 600;\n  color: var(--color-text-primary);\n  text-align: left;\n  cursor: pointer;\n  overflow: hidden;\n  text-overflow: ellipsis;\n  white-space: nowrap;\n  transition: color 0.1s ease;\n}\n\n.task-name:hover {\n  color: var(--color-accent);\n}\n\n.task-due-date {\n  font-size: 12px;\n  color: var(--color-text-muted);\n  white-space: nowrap;\n  flex-shrink: 0;\n}\n\n.task-assignee {\n  display: flex;\n  align-items: center;\n  gap: 6px;\n  flex-shrink: 0;\n  min-width: 120px;\n}\n\n.task-assignee-avatar {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 24px;\n  height: 24px;\n  border-radius: 50%;\n  background: var(--color-accent);\n  color: white;\n  font-size: 10px;\n  font-weight: 600;\n  flex-shrink: 0;\n}\n\n.task-assignee-name {\n  font-size: 12px;\n  color: var(--color-text-secondary);\n  white-space: nowrap;\n  overflow: hidden;\n  text-overflow: ellipsis;\n}\n\n.task-card-action {\n  flex-shrink: 0;\n  position: relative;\n  z-index: 1;\n}\n\n/* ===== Tasks Empty State ===== */\n.tasks-empty {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: center;\n  padding: 48px 24px;\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-md);\n  text-align: center;\n}\n\n/* ===== Searchable Select ===== */\n.searchable-select-search {\n  display: flex;\n  align-items: center;\n  gap: 6px;\n  padding: 6px 8px;\n  border-bottom: 1px solid var(--color-border-light);\n}\n\n.searchable-select-icon {\n  color: var(--color-text-muted);\n  flex-shrink: 0;\n}\n\n.searchable-select-input {\n  width: 100%;\n  border: none;\n  outline: none;\n  font-size: 13px;\n  color: var(--color-text-primary);\n  background: transparent;\n}\n\n.searchable-select-input::placeholder {\n  color: var(--color-text-disabled);\n}\n\n.searchable-select-options {\n  max-height: 200px;\n  overflow-y: auto;\n}\n\n.searchable-select-empty {\n  padding: 8px;\n  font-size: 13px;\n  color: var(--color-text-muted);\n  text-align: center;\n}\n\n/* ===== Success Toast ===== */\n.success-toast {\n  position: fixed;\n  top: 16px;\n  right: 16px;\n  background: var(--color-status-active);\n  color: white;\n  padding: 10px 20px;\n  border-radius: var(--radius-md);\n  font-size: 13px;\n  font-weight: 500;\n  z-index: 1000;\n  box-shadow: 0 2px 8px rgba(0, 0, 0, 0.15);\n}\n\n/* ===== Person Detail Page ===== */\n.person-header {\n  margin-bottom: 24px;\n}\n\n.person-name {\n  font-size: 24px;\n  font-weight: 700;\n  color: var(--color-text-primary);\n  margin-bottom: 4px;\n}\n\n.person-title-row {\n  font-size: 14px;\n  color: var(--color-text-secondary);\n  margin-bottom: 12px;\n}\n\n.person-title-text {\n  color: var(--color-text-secondary);\n}\n\n.person-title-separator {\n  color: var(--color-text-muted);\n}\n\n.person-title-clients {\n  color: var(--color-text-muted);\n}\n\n.person-contact-info {\n  display: flex;\n  flex-wrap: wrap;\n  gap: 16px;\n  margin-bottom: 12px;\n}\n\n.person-contact-item {\n  display: flex;\n  align-items: center;\n  gap: 6px;\n  font-size: 13px;\n  color: var(--color-text-secondary);\n}\n\n.person-contact-item svg {\n  color: var(--color-text-muted);\n  flex-shrink: 0;\n}\n\n.person-associated-clients {\n  display: flex;\n  align-items: center;\n  gap: 6px;\n  font-size: 13px;\n  color: var(--color-text-secondary);\n  flex-wrap: wrap;\n}\n\n.person-associated-label {\n  font-weight: 500;\n  color: var(--color-text-muted);\n}\n\n.person-client-link {\n  border: none;\n  background: none;\n  color: var(--color-accent);\n  font-size: 13px;\n  cursor: pointer;\n  padding: 0;\n  text-decoration: underline;\n  transition: opacity 0.1s ease;\n}\n\n.person-client-link:hover {\n  opacity: 0.8;\n}\n\n/* Person Sections */\n.person-section {\n  margin-bottom: 24px;\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-md);\n  padding: 16px;\n}\n\n.person-section-header {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  margin-bottom: 12px;\n  gap: 12px;\n}\n\n.person-section-heading {\n  display: flex;\n  align-items: center;\n  gap: 8px;\n}\n\n.person-section-heading svg {\n  color: var(--color-text-muted);\n  flex-shrink: 0;\n}\n\n.person-section-heading h2 {\n  font-size: 16px;\n  font-weight: 600;\n  color: var(--color-text-primary);\n}\n\n.person-section-actions {\n  display: flex;\n  align-items: center;\n  gap: 8px;\n}\n\n.person-section-loading {\n  font-size: 13px;\n  color: var(--color-text-muted);\n  padding: 12px 0;\n}\n\n.person-section-empty {\n  font-size: 13px;\n  color: var(--color-text-muted);\n  padding: 24px;\n  text-align: center;\n}\n\n/* Tabs */\n.person-tabs {\n  display: flex;\n  gap: 0;\n  border-bottom: 1px solid var(--color-border-default);\n  margin-bottom: 12px;\n}\n\n.person-tab {\n  padding: 8px 16px;\n  border: none;\n  border-bottom: 2px solid transparent;\n  background: transparent;\n  color: var(--color-text-muted);\n  font-size: 13px;\n  font-weight: 500;\n  cursor: pointer;\n  transition: all 0.15s ease;\n}\n\n.person-tab:hover {\n  color: var(--color-text-primary);\n}\n\n.person-tab.active {\n  color: var(--color-accent);\n  border-bottom-color: var(--color-accent);\n}\n\n/* Relationships List */\n.relationships-list {\n  display: flex;\n  flex-direction: column;\n  gap: 0;\n}\n\n.relationship-entry {\n  display: flex;\n  align-items: center;\n  flex-wrap: wrap;\n  gap: 2px;\n  padding: 10px 0;\n  font-size: 13px;\n  color: var(--color-text-secondary);\n  border-bottom: 1px solid var(--color-border-light);\n}\n\n.relationship-entry:last-child {\n  border-bottom: none;\n}\n\n.relationship-name {\n  font-weight: 600;\n  color: var(--color-text-primary);\n}\n\n.relationship-type {\n  color: var(--color-text-muted);\n}\n\n.relationship-detail {\n  color: var(--color-text-secondary);\n}\n\n.relationship-link {\n  border: none;\n  background: none;\n  color: var(--color-accent);\n  font-size: 13px;\n  cursor: pointer;\n  padding: 0;\n  margin-left: 6px;\n  text-decoration: none;\n  transition: opacity 0.1s ease;\n}\n\n.relationship-link:hover {\n  opacity: 0.8;\n  text-decoration: underline;\n}\n\n.relationship-delete {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 28px;\n  height: 28px;\n  border: none;\n  border-radius: var(--radius-sm);\n  background: transparent;\n  color: var(--color-text-muted);\n  cursor: pointer;\n  margin-left: auto;\n  flex-shrink: 0;\n  transition: background 0.1s ease;\n}\n\n.relationship-delete:hover {\n  background: var(--color-bg-hover);\n  color: var(--color-status-churned);\n}\n\n/* Relationships Graph */\n.relationships-graph-view {\n  display: flex;\n  flex-wrap: wrap;\n  gap: 16px;\n  padding: 24px;\n  justify-content: center;\n  align-items: center;\n  min-height: 200px;\n}\n\n.graph-center-node {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 80px;\n  height: 80px;\n  border-radius: 50%;\n  background: var(--color-accent);\n  color: white;\n  font-size: 14px;\n  font-weight: 600;\n}\n\n.graph-node {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  gap: 4px;\n  padding: 12px;\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-md);\n  background: var(--color-bg-base);\n}\n\n.graph-node-name {\n  font-size: 13px;\n  font-weight: 500;\n  color: var(--color-text-primary);\n}\n\n.graph-node-type {\n  font-size: 11px;\n  color: var(--color-text-muted);\n}\n\n/* Contact History */\n.contact-history-list {\n  display: flex;\n  flex-direction: column;\n  gap: 0;\n}\n\n.contact-history-entry {\n  position: relative;\n  padding: 12px 0;\n  font-size: 13px;\n  border-bottom: 1px solid var(--color-border-light);\n}\n\n.contact-history-entry:last-child {\n  border-bottom: none;\n}\n\n.contact-history-entry-header {\n  margin-bottom: 4px;\n}\n\n.contact-history-date {\n  font-weight: 500;\n  color: var(--color-text-primary);\n}\n\n.contact-history-separator {\n  color: var(--color-text-muted);\n}\n\n.contact-history-type {\n  color: var(--color-accent-muted);\n  font-weight: 500;\n}\n\n.contact-history-body {\n  color: var(--color-text-secondary);\n  margin-bottom: 2px;\n}\n\n.contact-history-label {\n  font-weight: 500;\n  color: var(--color-text-muted);\n}\n\n.contact-history-summary {\n  color: var(--color-text-secondary);\n}\n\n.contact-history-team {\n  font-size: 12px;\n  color: var(--color-text-muted);\n}\n\n.contact-history-edit-btn {\n  position: absolute;\n  top: 12px;\n  right: 0;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 28px;\n  height: 28px;\n  border: none;\n  border-radius: var(--radius-sm);\n  background: transparent;\n  color: var(--color-text-muted);\n  cursor: pointer;\n  transition: background 0.1s ease;\n}\n\n.contact-history-edit-btn:hover {\n  background: var(--color-bg-hover);\n  color: var(--color-text-primary);\n}\n\n/* Form textarea */\n.form-textarea {\n  height: auto;\n  padding: 8px 10px;\n  resize: vertical;\n  min-height: 80px;\n  font-family: var(--font-sans);\n}\n\n/* Selected person chip */\n.selected-person-chip {\n  display: inline-flex;\n  align-items: center;\n  gap: 6px;\n  height: 32px;\n  padding: 0 10px;\n  border: 1px solid var(--color-accent);\n  border-radius: var(--radius-sm);\n  background: #eef0ff;\n  color: var(--color-accent-muted);\n  font-size: 13px;\n}\n\n/* Associated Clients */\n.associated-clients-grid {\n  display: flex;\n  flex-direction: column;\n  gap: 12px;\n}\n\n.associated-client-card {\n  display: flex;\n  align-items: center;\n  gap: 12px;\n  padding: 12px;\n  border: 1px solid var(--color-border-light);\n  border-radius: var(--radius-md);\n  transition: background 0.1s ease;\n}\n\n.associated-client-card:hover {\n  background: var(--color-bg-hover);\n}\n\n.associated-client-icon {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 40px;\n  height: 40px;\n  border-radius: var(--radius-md);\n  background: var(--color-bg-sidebar);\n  color: var(--color-text-muted);\n  flex-shrink: 0;\n}\n\n.associated-client-info {\n  flex: 1;\n  min-width: 0;\n}\n\n.associated-client-name {\n  font-size: 14px;\n  font-weight: 600;\n  color: var(--color-text-primary);\n  margin-bottom: 2px;\n}\n\n.associated-client-detail {\n  font-size: 12px;\n  color: var(--color-text-muted);\n}\n\n.associated-client-label {\n  font-weight: 500;\n}\n\n.associated-client-btn {\n  flex-shrink: 0;\n}\n\n/* ===== Deal Detail Page ===== */\n\n/* Deal Header */\n.deal-header {\n  margin-bottom: 24px;\n}\n\n.deal-header-title {\n  font-size: 20px;\n  font-weight: 600;\n  color: var(--color-text-primary);\n  margin-bottom: 16px;\n  text-transform: uppercase;\n  letter-spacing: 0.01em;\n}\n\n.deal-header-fields {\n  display: flex;\n  flex-wrap: wrap;\n  gap: 16px;\n  align-items: center;\n}\n\n.deal-header-field {\n  display: flex;\n  align-items: center;\n  gap: 6px;\n  font-size: 13px;\n}\n\n.deal-header-label {\n  font-weight: 500;\n  color: var(--color-text-muted);\n  white-space: nowrap;\n}\n\n.deal-header-value-btn {\n  border: none;\n  background: none;\n  color: var(--color-text-primary);\n  font-size: 13px;\n  font-weight: 500;\n  cursor: pointer;\n  padding: 2px 6px;\n  border-radius: var(--radius-sm);\n  transition: background 0.1s ease;\n}\n\n.deal-header-value-btn:hover {\n  background: var(--color-bg-hover);\n}\n\n.deal-header-input {\n  height: 28px;\n  padding: 0 8px;\n  border: 1px solid var(--color-accent);\n  border-radius: var(--radius-sm);\n  background: var(--color-bg-base);\n  font-size: 13px;\n  color: var(--color-text-primary);\n  outline: none;\n  min-width: 100px;\n}\n\n.deal-header-stage-field {\n  margin-left: auto;\n}\n\n.deal-header-stage-controls {\n  display: flex;\n  align-items: center;\n  gap: 8px;\n}\n\n/* Deal Stage Dropdown */\n.deal-stage-dropdown-wrapper {\n  position: relative;\n}\n\n.deal-stage-dropdown-trigger {\n  display: flex;\n  align-items: center;\n  gap: 6px;\n  height: 28px;\n  padding: 0 10px;\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-sm);\n  background: var(--color-bg-base);\n  font-size: 13px;\n  color: var(--color-text-primary);\n  cursor: pointer;\n  transition: border-color 0.15s ease;\n}\n\n.deal-stage-dropdown-trigger:hover {\n  border-color: var(--color-accent);\n}\n\n.deal-stage-dropdown-menu {\n  position: absolute;\n  top: 100%;\n  left: 0;\n  margin-top: 4px;\n  min-width: 160px;\n  background: var(--color-bg-base);\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-md);\n  box-shadow: var(--shadow-md);\n  z-index: 50;\n  overflow: hidden;\n}\n\n.deal-stage-dropdown-option {\n  display: block;\n  width: 100%;\n  padding: 6px 12px;\n  border: none;\n  background: none;\n  text-align: left;\n  font-size: 13px;\n  color: var(--color-text-primary);\n  cursor: pointer;\n  transition: background 0.1s ease;\n}\n\n.deal-stage-dropdown-option:hover {\n  background: var(--color-bg-hover);\n}\n\n.deal-stage-dropdown-option.selected {\n  background: var(--color-bg-selected);\n  font-weight: 500;\n}\n\n/* Stage Progress Bar */\n.stage-progress-bar {\n  display: flex;\n  align-items: flex-start;\n  margin-bottom: 24px;\n  padding: 16px;\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-md);\n}\n\n.stage-progress-step {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  flex: 1;\n  min-width: 0;\n}\n\n.stage-progress-indicator {\n  display: flex;\n  align-items: center;\n  width: 100%;\n  position: relative;\n}\n\n.stage-progress-circle {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 24px;\n  height: 24px;\n  border-radius: 50%;\n  flex-shrink: 0;\n  margin: 0 auto;\n  position: relative;\n  z-index: 1;\n}\n\n.stage-progress-circle.completed {\n  background: var(--color-accent);\n  color: white;\n}\n\n.stage-progress-circle.incomplete {\n  background: var(--color-bg-sidebar);\n  border: 2px solid var(--color-border-default);\n}\n\n.stage-progress-dot {\n  width: 8px;\n  height: 8px;\n  border-radius: 50%;\n  background: var(--color-border-default);\n}\n\n.stage-progress-line {\n  position: absolute;\n  top: 50%;\n  left: calc(50% + 12px);\n  right: calc(-50% + 12px);\n  height: 2px;\n  transform: translateY(-50%);\n}\n\n.stage-progress-line.completed {\n  background: var(--color-accent);\n}\n\n.stage-progress-line.incomplete {\n  background: var(--color-border-default);\n}\n\n.stage-progress-label {\n  font-size: 11px;\n  margin-top: 6px;\n  text-align: center;\n  white-space: nowrap;\n  overflow: hidden;\n  text-overflow: ellipsis;\n  max-width: 100%;\n}\n\n.stage-progress-label.completed {\n  color: var(--color-accent);\n  font-weight: 500;\n}\n\n.stage-progress-label.incomplete {\n  color: var(--color-text-muted);\n}\n\n/* Deal History */\n.deal-history-list {\n  display: flex;\n  flex-direction: column;\n  gap: 0;\n}\n\n.deal-history-entry {\n  padding: 8px 0;\n  font-size: 13px;\n  border-bottom: 1px solid var(--color-border-light);\n}\n\n.deal-history-entry:last-child {\n  border-bottom: none;\n}\n\n.deal-history-date {\n  font-weight: 500;\n  color: var(--color-text-primary);\n}\n\n.deal-history-separator {\n  color: var(--color-text-muted);\n}\n\n.deal-history-text {\n  color: var(--color-text-secondary);\n}\n\n.deal-history-user {\n  color: var(--color-text-muted);\n}\n\n/* Deal Metrics */\n.deal-metrics-grid {\n  display: flex;\n  gap: 32px;\n}\n\n.deal-metrics-item {\n  display: flex;\n  align-items: center;\n  gap: 6px;\n  font-size: 13px;\n}\n\n.deal-metrics-label {\n  font-weight: 500;\n  color: var(--color-text-muted);\n}\n\n.deal-metrics-edit {\n  display: flex;\n  align-items: center;\n  gap: 4px;\n  font-size: 13px;\n  color: var(--color-text-primary);\n}\n\n/* Writeups */\n.writeups-list {\n  display: flex;\n  flex-direction: column;\n  gap: 0;\n}\n\n.writeup-entry {\n  padding: 12px 0;\n  border-bottom: 1px solid var(--color-border-light);\n}\n\n.writeup-entry:last-child {\n  border-bottom: none;\n}\n\n.writeup-entry-header {\n  margin-bottom: 4px;\n  font-size: 13px;\n}\n\n.writeup-title {\n  font-weight: 600;\n  color: var(--color-text-primary);\n}\n\n.writeup-meta {\n  color: var(--color-text-muted);\n}\n\n.writeup-author {\n  color: var(--color-text-muted);\n}\n\n.writeup-content {\n  font-size: 13px;\n  color: var(--color-text-secondary);\n  margin-bottom: 8px;\n  line-height: 1.5;\n}\n\n.writeup-actions {\n  display: flex;\n  gap: 8px;\n  align-items: center;\n}\n\n.btn-danger-text {\n  color: var(--color-status-churned) !important;\n}\n\n.btn-danger-text:hover {\n  background: var(--color-status-churned-bg) !important;\n}\n\n/* Writeup Version History */\n.writeup-version-entry {\n  padding: 12px;\n  border: 1px solid var(--color-border-light);\n  border-radius: var(--radius-sm);\n  margin-bottom: 8px;\n}\n\n.writeup-version-header {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  margin-bottom: 8px;\n}\n\n.writeup-version-label {\n  font-weight: 600;\n  font-size: 13px;\n  color: var(--color-text-primary);\n}\n\n.writeup-version-date {\n  font-size: 12px;\n  color: var(--color-text-muted);\n}\n\n.writeup-version-content {\n  font-size: 13px;\n  color: var(--color-text-secondary);\n  line-height: 1.5;\n}\n\n/* Linked Tasks */\n.linked-tasks-list {\n  display: flex;\n  flex-direction: column;\n  gap: 0;\n}\n\n.linked-task-item {\n  display: flex;\n  align-items: center;\n  gap: 10px;\n  padding: 8px 0;\n  border-bottom: 1px solid var(--color-border-light);\n}\n\n.linked-task-item:last-child {\n  border-bottom: none;\n}\n\n.linked-task-checkbox-label {\n  display: flex;\n  align-items: center;\n  flex-shrink: 0;\n}\n\n.linked-task-checkbox {\n  width: 16px;\n  height: 16px;\n  accent-color: var(--color-accent);\n  cursor: pointer;\n}\n\n.linked-task-name {\n  flex: 1;\n  min-width: 0;\n  border: none;\n  background: none;\n  font-size: 13px;\n  font-weight: 500;\n  color: var(--color-text-primary);\n  cursor: pointer;\n  text-align: left;\n  padding: 0;\n  overflow: hidden;\n  text-overflow: ellipsis;\n  white-space: nowrap;\n  transition: color 0.1s ease;\n}\n\n.linked-task-name:hover {\n  color: var(--color-accent);\n}\n\n.linked-task-name.completed {\n  text-decoration: line-through;\n  color: var(--color-text-muted);\n}\n\n.linked-task-date {\n  font-size: 12px;\n  color: var(--color-text-muted);\n  white-space: nowrap;\n  flex-shrink: 0;\n}\n\n.linked-task-remove {\n  flex-shrink: 0;\n  color: var(--color-text-muted);\n}\n\n.linked-task-remove:hover {\n  color: var(--color-status-churned);\n}\n\n/* Deal Attachments */\n.deal-attachments-list {\n  display: flex;\n  flex-direction: column;\n  gap: 0;\n}\n\n.deal-attachment-item {\n  display: flex;\n  align-items: center;\n  gap: 8px;\n  padding: 8px 0;\n  font-size: 13px;\n  border-bottom: 1px solid var(--color-border-light);\n}\n\n.deal-attachment-item:last-child {\n  border-bottom: none;\n}\n\n.deal-attachment-icon {\n  display: flex;\n  align-items: center;\n  color: var(--color-text-muted);\n  flex-shrink: 0;\n}\n\n.deal-attachment-name {\n  font-weight: 500;\n  color: var(--color-text-primary);\n}\n\n.deal-attachment-size {\n  color: var(--color-text-muted);\n}\n\n.deal-attachment-separator {\n  color: var(--color-text-disabled);\n}\n\n.deal-attachment-link {\n  border: none;\n  background: none;\n  color: var(--color-accent);\n  font-size: 13px;\n  cursor: pointer;\n  padding: 0;\n  text-decoration: none;\n  transition: opacity 0.1s ease;\n}\n\n.deal-attachment-link:hover {\n  opacity: 0.8;\n  text-decoration: underline;\n}\n\n.deal-attachment-delete {\n  color: var(--color-status-churned);\n}\n\n.deal-attachment-upload-progress {\n  margin-top: 8px;\n  font-size: 13px;\n  color: var(--color-text-muted);\n  padding: 8px 0;\n}\n\n/* Deal Contacts */\n.deal-contacts-list {\n  display: flex;\n  flex-direction: column;\n  gap: 0;\n}\n\n.deal-contact-entry {\n  display: flex;\n  align-items: center;\n  gap: 12px;\n  padding: 10px 0;\n  border-bottom: 1px solid var(--color-border-light);\n}\n\n.deal-contact-entry:last-child {\n  border-bottom: none;\n}\n\n.deal-contact-avatar {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 32px;\n  height: 32px;\n  border-radius: 50%;\n  background: var(--color-accent);\n  color: white;\n  font-size: 11px;\n  font-weight: 600;\n  flex-shrink: 0;\n}\n\n.deal-contact-info {\n  flex: 1;\n  min-width: 0;\n  font-size: 13px;\n}\n\n.deal-contact-name {\n  font-weight: 600;\n  color: var(--color-text-primary);\n}\n\n.deal-contact-meta {\n  color: var(--color-text-muted);\n}\n\n.deal-contact-actions {\n  display: flex;\n  align-items: center;\n  gap: 8px;\n  flex-shrink: 0;\n}\n\n.deal-contact-view-link {\n  border: none;\n  background: none;\n  color: var(--color-accent);\n  font-size: 13px;\n  cursor: pointer;\n  padding: 0;\n  text-decoration: none;\n  transition: opacity 0.1s ease;\n}\n\n.deal-contact-view-link:hover {\n  opacity: 0.8;\n  text-decoration: underline;\n}\n\n.deal-contact-remove {\n  color: var(--color-text-muted);\n}\n\n.deal-contact-remove:hover {\n  color: var(--color-status-churned);\n}\n\n/* Contact Search Results */\n.deal-contact-search-result {\n  display: block;\n  width: 100%;\n  padding: 8px 12px;\n  border: none;\n  background: none;\n  text-align: left;\n  font-size: 13px;\n  color: var(--color-text-primary);\n  cursor: pointer;\n  transition: background 0.1s ease;\n}\n\n.deal-contact-search-result:hover {\n  background: var(--color-bg-hover);\n}\n\n.deal-contact-search-name {\n  font-weight: 500;\n}\n\n.deal-contact-search-title {\n  color: var(--color-text-muted);\n}\n\n/* ===== Deals List Page ===== */\n\n/* Summary Cards */\n.deals-summary-cards {\n  display: grid;\n  grid-template-columns: repeat(4, 1fr);\n  gap: 12px;\n}\n\n@media (max-width: 768px) {\n  .deals-summary-cards {\n    grid-template-columns: repeat(2, 1fr);\n  }\n}\n\n.deals-summary-card {\n  display: flex;\n  align-items: center;\n  gap: 12px;\n  padding: 14px 16px;\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-md);\n  background: var(--color-bg-base);\n}\n\n.deals-summary-card-icon {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 36px;\n  height: 36px;\n  border-radius: var(--radius-md);\n  flex-shrink: 0;\n}\n\n.deals-summary-card-icon--active {\n  background: #eef0ff;\n  color: var(--color-accent);\n}\n\n.deals-summary-card-icon--pipeline {\n  background: var(--color-deal-on-track-bg);\n  color: var(--color-deal-on-track);\n}\n\n.deals-summary-card-icon--won {\n  background: #eef0ff;\n  color: var(--color-accent-purple);\n}\n\n.deals-summary-card-icon--lost {\n  background: var(--color-deal-at-risk-bg);\n  color: var(--color-deal-at-risk);\n}\n\n.deals-summary-card-content {\n  display: flex;\n  flex-direction: column;\n  gap: 2px;\n  min-width: 0;\n}\n\n.deals-summary-card-label {\n  font-size: 12px;\n  font-weight: 500;\n  color: var(--color-text-muted);\n  white-space: nowrap;\n}\n\n.deals-summary-card-value {\n  font-size: 16px;\n  font-weight: 600;\n  color: var(--color-text-primary);\n  white-space: nowrap;\n}\n\n/* View Tabs */\n.deals-view-tabs {\n  display: flex;\n  gap: 0;\n  border-bottom: 1px solid var(--color-border-default);\n}\n\n.deals-view-tab {\n  padding: 8px 16px;\n  border: none;\n  border-bottom: 2px solid transparent;\n  background: transparent;\n  color: var(--color-text-muted);\n  font-size: 13px;\n  font-weight: 500;\n  cursor: pointer;\n  transition: all 0.15s ease;\n}\n\n.deals-view-tab:hover {\n  color: var(--color-text-primary);\n}\n\n.deals-view-tab.active {\n  color: var(--color-accent);\n  border-bottom-color: var(--color-accent);\n}\n\n/* Deals Toolbar */\n.deals-toolbar {\n  margin-bottom: 12px;\n}\n\n/* Deals Filters */\n.deals-filters {\n  display: flex;\n  flex-direction: column;\n  gap: 8px;\n}\n\n.deals-filters-row {\n  display: flex;\n  gap: 8px;\n  flex-wrap: wrap;\n  align-items: center;\n}\n\n.deals-search-wrapper {\n  position: relative;\n  display: flex;\n  align-items: center;\n  flex: 1;\n  min-width: 160px;\n  max-width: 240px;\n}\n\n.deals-search-icon {\n  position: absolute;\n  left: 8px;\n  color: var(--color-text-muted);\n  pointer-events: none;\n}\n\n.deals-search-input {\n  width: 100%;\n  height: 28px;\n  padding: 0 28px 0 28px;\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-sm);\n  background: var(--color-bg-base);\n  font-size: 12px;\n  color: var(--color-text-primary);\n  outline: none;\n  transition: border-color 0.15s ease;\n}\n\n.deals-search-input::placeholder {\n  color: var(--color-text-disabled);\n}\n\n.deals-search-input:focus {\n  border-color: var(--color-accent);\n}\n\n.deals-search-clear {\n  position: absolute;\n  right: 4px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 18px;\n  height: 18px;\n  border: none;\n  border-radius: 50%;\n  background: var(--color-bg-hover);\n  color: var(--color-text-muted);\n  cursor: pointer;\n}\n\n/* Deals Table */\n.deals-table {\n  display: grid;\n  grid-template-columns: 1fr;\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-md);\n}\n\n.deals-table-header {\n  display: grid;\n  grid-template-columns: 2fr 1.2fr 1fr 1fr 1fr 1fr 1fr 44px;\n  align-items: center;\n  height: 36px;\n  padding: 0 12px;\n  background: var(--color-bg-sidebar);\n  border-bottom: 1px solid var(--color-border-default);\n  font-size: 12px;\n  font-weight: 500;\n  color: var(--color-text-muted);\n}\n\n.deals-table-row {\n  display: grid;\n  grid-template-columns: 2fr 1.2fr 1fr 1fr 1fr 1fr 1fr 44px;\n  align-items: center;\n  height: 44px;\n  padding: 0 12px;\n  border-bottom: 1px solid var(--color-border-light);\n  transition: background 0.1s ease;\n  cursor: default;\n}\n\n.deals-table-row:last-child {\n  border-bottom: none;\n}\n\n.deals-table-row:hover {\n  background: var(--color-bg-hover);\n}\n\n.deals-table-sort-btn {\n  display: inline-flex;\n  align-items: center;\n  gap: 4px;\n  border: none;\n  background: none;\n  color: var(--color-text-muted);\n  font-size: 12px;\n  font-weight: 500;\n  cursor: pointer;\n  padding: 0;\n  transition: color 0.1s ease;\n}\n\n.deals-table-sort-btn:hover {\n  color: var(--color-text-primary);\n}\n\n/* Deal Status Badges */\n.deal-status-on-track {\n  color: var(--color-deal-on-track);\n  background: var(--color-deal-on-track-bg);\n}\n\n.deal-status-needs-attention {\n  color: var(--color-deal-needs-attention);\n  background: var(--color-deal-needs-attention-bg);\n}\n\n.deal-status-at-risk {\n  color: var(--color-deal-at-risk);\n  background: var(--color-deal-at-risk-bg);\n}\n\n.deal-status-won {\n  color: var(--color-deal-won);\n  background: var(--color-deal-won-bg);\n}\n\n.deal-status-lost {\n  color: var(--color-status-churned);\n  background: var(--color-status-churned-bg);\n}\n\n/* ===== Client Detail Page ===== */\n\n/* Client Header */\n.client-header {\n  margin-bottom: 20px;\n}\n\n.client-header-top {\n  display: flex;\n  align-items: center;\n  gap: 12px;\n  margin-bottom: 12px;\n  flex-wrap: wrap;\n}\n\n.client-header-name {\n  font-size: 24px;\n  font-weight: 700;\n  color: var(--color-text-primary);\n  cursor: pointer;\n  border-radius: var(--radius-sm);\n  padding: 2px 4px;\n  margin: -2px -4px;\n  transition: background 0.1s ease;\n}\n\n.client-header-name:hover {\n  background: var(--color-bg-hover);\n}\n\n.client-header-name-input {\n  font-size: 24px;\n  font-weight: 700;\n  color: var(--color-text-primary);\n  border: 1px solid var(--color-accent);\n  border-radius: var(--radius-sm);\n  background: var(--color-bg-base);\n  padding: 2px 8px;\n  outline: none;\n  min-width: 200px;\n}\n\n.client-header-badges {\n  display: flex;\n  align-items: center;\n  gap: 8px;\n}\n\n.client-type-badge {\n  display: inline-flex;\n  align-items: center;\n  height: 22px;\n  padding: 0 10px;\n  border-radius: 10px;\n  font-size: 11px;\n  font-weight: 500;\n  color: var(--color-text-muted);\n  background: var(--color-bg-sidebar);\n  border: 1px solid var(--color-border-default);\n}\n\n.client-status-wrapper {\n  position: relative;\n}\n\n.client-status-dropdown {\n  position: absolute;\n  top: calc(100% + 4px);\n  left: 0;\n  min-width: 160px;\n  background: var(--color-bg-base);\n  border-radius: var(--radius-md);\n  box-shadow: var(--shadow-md);\n  padding: 4px;\n  z-index: 50;\n}\n\n.client-status-option {\n  display: block;\n  width: 100%;\n  padding: 6px 8px;\n  border: none;\n  border-radius: var(--radius-sm);\n  background: transparent;\n  text-align: left;\n  cursor: pointer;\n  transition: background 0.1s ease;\n}\n\n.client-status-option:hover {\n  background: var(--color-bg-hover);\n}\n\n.client-status-option.selected {\n  background: var(--color-bg-selected);\n}\n\n.client-header-meta {\n  display: flex;\n  align-items: center;\n  gap: 12px;\n  flex-wrap: wrap;\n}\n\n.client-tags-area {\n  display: flex;\n  align-items: center;\n  gap: 6px;\n  flex-wrap: wrap;\n}\n\n.client-tags-display {\n  display: flex;\n  align-items: center;\n  gap: 6px;\n  flex-wrap: wrap;\n}\n\n.client-no-tags {\n  font-size: 13px;\n  color: var(--color-text-disabled);\n}\n\n.client-tags-edit-btn {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 24px;\n  height: 24px;\n  border: none;\n  border-radius: var(--radius-sm);\n  background: transparent;\n  color: var(--color-text-muted);\n  cursor: pointer;\n  transition: background 0.1s ease;\n}\n\n.client-tags-edit-btn:hover {\n  background: var(--color-bg-hover);\n  color: var(--color-text-primary);\n}\n\n.client-tags-editor {\n  display: flex;\n  flex-direction: column;\n  gap: 8px;\n  width: 100%;\n}\n\n.client-tags-editor-actions {\n  display: flex;\n  gap: 8px;\n  justify-content: flex-end;\n}\n\n.client-source-tag {\n  display: inline-flex;\n  align-items: center;\n  gap: 4px;\n  height: 22px;\n  padding: 0 10px;\n  border-radius: 10px;\n  font-size: 11px;\n  font-weight: 500;\n  color: var(--color-text-muted);\n  background: var(--color-bg-sidebar);\n  border: 1px solid var(--color-border-default);\n}\n\n.client-source-tag svg {\n  flex-shrink: 0;\n}\n\n/* Client Detail Page Layout */\n.client-detail-top {\n  display: flex;\n  align-items: flex-start;\n  gap: 16px;\n  margin-bottom: 20px;\n}\n\n.client-detail-top .client-header {\n  flex: 1;\n  min-width: 0;\n  margin-bottom: 0;\n}\n\n.client-detail-grid {\n  display: grid;\n  grid-template-columns: 1fr 1fr;\n  gap: 0 24px;\n  align-items: start;\n}\n\n.client-detail-col {\n  display: flex;\n  flex-direction: column;\n}\n\n@media (max-width: 768px) {\n  .client-detail-top {\n    flex-direction: column;\n  }\n\n  .client-detail-grid {\n 