[2026-02-24T07:06:38.799Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01XpAdcAvaWBXpPu1eyZGmKo","type":"tool_result","content":"     1→import { useState, useEffect, useRef } from 'react'\n     2→import { SlidersHorizontal, ChevronDown, X } from 'lucide-react'\n     3→import { useAppDispatch, useAppSelector } from '@/store/hooks'\n     4→import { setFilterType, setFilterText } from '@/store/tasksSlice'\n     5→\n     6→const filterOptions = [\n     7→  { label: 'All Fields', value: 'all' },\n     8→  { label: 'Priority', value: 'priority' },\n     9→  { label: 'Assignee', value: 'assignee' },\n    10→  { label: 'Client', value: 'client' },\n    11→  { label: 'Due Date', value: 'due_date' },\n    12→]\n    13→\n    14→export default function TasksFilter() {\n    15→  const dispatch = useAppDispatch()\n    16→  const filterType = useAppSelector(s => s.tasks.filterType)\n    17→  const filterText = useAppSelector(s => s.tasks.filterText)\n    18→  const [dropdownOpen, setDropdownOpen] = useState(false)\n    19→  const [localText, setLocalText] = useState(filterText)\n    20→  const ref = useRef<HTMLDivElement>(null)\n    21→\n    22→  useEffect(() => {\n    23→    const handler = (e: MouseEvent) => {\n    24→      if (ref.current && !ref.current.contains(e.target as Node)) setDropdownOpen(false)\n    25→    }\n    26→    document.addEventListener('mousedown', handler)\n    27→    return () => document.removeEventListener('mousedown', handler)\n    28→  }, [])\n    29→\n    30→  useEffect(() => {\n    31→    const timer = setTimeout(() => {\n    32→      dispatch(setFilterText(localText))\n    33→    }, 200)\n    34→    return () => clearTimeout(timer)\n    35→  }, [localText, dispatch])\n    36→\n    37→  const [prevReduxText, setPrevReduxText] = useState(filterText)\n    38→  if (filterText !== prevReduxText) {\n    39→    setPrevReduxText(filterText)\n    40→    setLocalText(filterText)\n    41→  }\n    42→\n    43→  const selectedLabel =