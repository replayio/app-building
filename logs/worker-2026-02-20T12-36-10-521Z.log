 timeout: 10000 });\n   534→  });\n   535→\n   536→  test('CLP-FLT-09: Multiple filters can be combined', async ({ page }) => {\n   537→    await page.goto('/clients');\n   538→    await page.waitForLoadState('networkidle');\n   539→\n   540→    // Apply status filter and wait for results\n   541→    await selectFilterOption(page, 'filter-status', 'active');\n   542→    await expect(async () => {\n   543→      const badges = page.locator('[data-testid=\"status-badge-active\"]');\n   544→      const count = await badges.count();\n   545→      expect(count).toBeGreaterThan(0);\n   546→    }).toPass({ timeout: 10000 });\n   547→\n   548→    // Apply tag filter if available — open dropdown to find a non-\"All\" option\n   549→    await page.getByTestId('filter-tags-trigger').click();\n   550→    const menu = page.getByTestId('filter-tags-menu');\n   551→    await expect(menu).toBeVisible();\n   552→    const menuOptions = menu.locator('button');\n   553→    const allTexts = await menuOptions.allTextContents();\n   554→    const tagOption = allTexts.find(t => !t.includes('All'));\n   555→    if (tagOption) {\n   556→      const optionEl = menuOptions.filter({ hasText: tagOption }).first();\n   557→      const tagValue = await optionEl.getAttribute('data-option-value');\n   558→      if (tagValue) {\n   559→        await optionEl.click();\n   560→      } else {\n   561→        await page.getByTestId('filter-tags-trigger').click();\n   562→      }\n   563→    } else {\n   564→      await page.getByTestId('filter-tags-trigger').click();\n   565→    }\n   566→\n   567→    // Combined filters should show only matching clients — atomic assertion avoids nested-wait deadlock\n   568→    await expect(\n   569→      page.locator('[data-testid=\"client-status\"]').filter({\n   570→        hasNot: page.locator('[data-testid=\"status-badge-active\"]')\n   571→      })\n   572→    ).toHaveCount(0, { timeout: 10000 });\n   573→  });\n   574→});\n   575→\n   576→test.describe('ClientsLi