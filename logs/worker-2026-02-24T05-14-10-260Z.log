ing>[]>([])\n    48→  const [error, setError] = useState('')\n    49→  const [importing, setImporting] = useState(false)\n    50→  const [success, setSuccess] = useState('')\n    51→\n    52→  const handleFile = useCallback((f: File) => {\n    53→    setError('')\n    54→    setSuccess('')\n    55→    if (!f.name.endsWith('.csv')) {\n    56→      setError('Please upload a CSV file')\n    57→      return\n    58→    }\n    59→    setFile(f)\n    60→    const reader = new FileReader()\n    61→    reader.onload = (e) => {\n    62→      const text = e.target?.result as string\n    63→      const rows = parseCSV(text)\n    64→      if (rows.length === 0) {\n    65→        setError('CSV file is empty or has no data rows')\n    66→        return\n    67→      }\n    68→      const headers = Object.keys(rows[0])\n    69→      const requiredCols = ['Client Name']\n    70→      const missing = requiredCols.filter(c => !headers.some(h => h.toLowerCase() === c.toLowerCase()))\n    71→      if (missing.length) {\n    72→        setError(`Missing required column: ${missing.join(', ')}`)\n    73→        return\n    74→      }\n    75→      setPreview(rows)\n    76→    }\n    77→    reader.readAsText(f)\n    78→  }, [])\n    79→\n    80→  const handleImport = async () => {\n    81→    if (!preview.length) return\n    82→    setImporting(true)\n    83→    setError('')\n    84→    try {\n    85→      const clients = preview.map(row => ({\n    86→        name: row['Client Name'] || row['client name'] || row['Name'] || row['name'] || '',\n    87→        type: row['Type'] || row['type'] || 'Organization',\n    88→        status: row['Status'] || row['status'] || 'Prospect',\n    89→        tags: row['Tags'] || row['tags'] || '',\n    90→        source: row['Source'] || row['source'] || '',\n    91→      }))\n    92→      const result = await dispatch(importClients(clients)).unwrap()\n    93→      setSuccess(`${result.imported} clients imported successfully`)\n    94→      await dispatch(fetchClients())\n    95→      setTimeout(() => {\n    96→        onClose()\n    97→        setFile(null)\n    98→        setPreview([])\n    99→        setSuccess('')\n   100→      }, 1500)\n   101→    } catch {\n   102→      setError('Failed to import clients')\n   103→    } finally {\n   104→      setImporting(false)\n   105→    }\n   106→  }\n   107→\n   108→  const reset = () => {\n   109→    setFile(null)\n   110→    setPreview([])\n   111→    setError('')\n   112→    setSuccess('')\n   113→    onClose()\n   114→  }\n   115→\n   116→  if (!open) return null\n   117→\n   118→  return (\n   119→    <div className=\"modal-overlay\" onClick={reset} data-testid=\"import-modal\">\n   120→      <div className=\"modal-content modal-wide\" onClick={(e) => e.stopPropagation()} data-testid=\"import-modal-content\">\n   121→        <div className=\"modal-header\">\n   122→          <h2 className=\"modal-title\">Import Clients</h2>\n   123→          <button className=\"modal-close\" onClick={reset} data-testid=\"import-modal-close\">\n   124→            <X size={18} />\n   125→          </button>\n   126→        </div>\n   127→        <div className=\"modal-body\">\n   128→          <div className=\"import-instructions\">\n   129→            <p className=\"import-instructions-title\">Expected CSV Format</p>\n   130→            <p className=\"import-instructions-text\">\n   131→              Required columns: <strong>Client Name</strong>, <strong>Type</strong>, <strong>Status</strong>\n   132→            </p>\n   133→            <p className=\"import-instructions-text\">\n   134→              Optional columns: Tags, Source, Primary Contact\n   135→            </p>\n   136→          </div>\n   137→\n   138→          {!preview.length && (\n   139→            <div\n   140→              className=\"import-dropzone\"\n   141→              onDragOver={(e) => { e.preventDefault(); e.stopPropagation() }}\n   142→              onDrop={(e) => { e.preventDefault(); e.stopPropagation(); if (e.dataTransfer.files[0]) handleFile(e.dataTransfer.files[0]) }}\n   143→              data-testid=\"import-dropzone\"\n   144→            >\n   145→              <UploadIcon size={32} className=\"import-dropzone-icon\" />\n   146→              <p className=\"import-dropzone-text\">Drag and drop a CSV file here, or click to browse</p>\n   147→              <label className=\"btn-secondary import-browse-btn\">\n   148→                Browse Files\n   149→                <input\n   150→                  type=\"file\"\n   151→                  accept=\".csv\"\n   152→                  className=\"import-file-input\"\n   153→                  onChange={(e) => { if (e.target.files?.[0]) handleFile(e.target.files[0]) }}\n   154→                  data-testid=\"import-file-input\"\n   155→                />\n   156→              </label>\n   157→            </div>\n   158→          )}\n   159→\n   160→          {preview.length > 0 && (\n   161→            <div className=\"import-preview\" data-testid=\"import-preview\">\n   162→              <p className=\"import-preview-info\">\n   163→                {file?.name} — {preview.length} client{preview.length !== 1 ? 's' : ''} found\n   164→              </p>\n   165→              <div className=\"import-preview-table\">\n   166→                <div className=\"import-preview-header\">\n   167→                  {Object.keys(preview[0]).slice(0, 5).map(key => (\n   168→                    <div key={key} className=\"import-preview-cell header\">{key}</div>\n   169→                  ))}\n   170→                </div>\n   171→                {preview.slice(0, 5).map((row, i) => (\n   172→                  <div key={i} className=\"import-preview-row\">\n   173→                    {Object.keys(preview[0]).slice(0, 5).map(key => (\n   174→                      <div key={key} className=\"import-preview-cell\">{row[key] || '—'}</div>\n   175→                    ))}\n   176→                  </div>\n   177→                ))}\n   17