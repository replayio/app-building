ht: 600;\n  color: var(--color-text-primary);\n}\n\n.modal-close {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 28px;\n  height: 28px;\n  border: none;\n  border-radius: var(--radius-sm);\n  background: transparent;\n  color: var(--color-text-muted);\n  cursor: pointer;\n  transition: background 0.1s ease;\n}\n\n.modal-close:hover {\n  background: var(--color-bg-hover);\n}\n\n.modal-body {\n  margin-bottom: 16px;\n}\n\n.modal-text {\n  font-size: 13px;\n  color: var(--color-text-secondary);\n  line-height: 1.5;\n  margin-bottom: 12px;\n}\n\n.modal-actions {\n  display: flex;\n  justify-content: flex-end;\n  gap: 8px;\n  margin-top: 16px;\n}\n\n.modal-form {\n  display: flex;\n  flex-direction: column;\n  gap: 14px;\n}\n\n/* ===== Form Fields ===== */\n.form-field {\n  display: flex;\n  flex-direction: column;\n  gap: 4px;\n}\n\n.form-label {\n  font-size: 12px;\n  font-weight: 500;\n  color: var(--color-text-muted);\n}\n\n.form-input {\n  height: 32px;\n  padding: 0 10px;\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-sm);\n  background: var(--color-bg-base);\n  font-size: 13px;\n  color: var(--color-text-primary);\n  outline: none;\n  transition: border-color 0.15s ease;\n}\n\n.form-input:focus {\n  border-color: var(--color-accent);\n}\n\n.form-input.error {\n  border-color: var(--color-status-churned);\n}\n\n.form-error {\n  font-size: 12px;\n  color: var(--color-status-churned);\n}\n\n/* Type selector */\n.type-selector {\n  display: flex;\n  gap: 8px;\n}\n\n.type-option {\n  flex: 1;\n  height: 32px;\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-sm);\n  background: var(--color-bg-base);\n  color: var(--color-text-secondary);\n  font-size: 13px;\n  cursor: pointer;\n  transition: all 0.1s ease;\n}\n\n.type-option:hover {\n  background: var(--color-bg-hover);\n}\n\n.type-option.selected {\n  border-color: var(--color-accent);\n  color: var(--color-accent);\n  background: #eef0ff;\n  font-weight: 500;\n}\n\n/* Form dropdown */\n.form-dropdown-wrapper {\n  position: relative;\n}\n\n.form-dropdown-trigger {\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  width: 100%;\n  height: 32px;\n  padding: 0 10px;\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-sm);\n  background: var(--color-bg-base);\n  color: var(--color-text-primary);\n  font-size: 13px;\n  cursor: pointer;\n  text-align: left;\n}\n\n.form-dropdown-menu {\n  position: absolute;\n  top: calc(100% + 4px);\n  left: 0;\n  right: 0;\n  background: var(--color-bg-base);\n  border-radius: var(--radius-md);\n  box-shadow: var(--shadow-md);\n  padding: 4px;\n  z-index: 50;\n}\n\n.form-dropdown-option {\n  display: block;\n  width: 100%;\n  padding: 6px 10px;\n  border: none;\n  border-radius: var(--radius-sm);\n  background: transparent;\n  color: var(--color-text-secondary);\n  font-size: 13px;\n  text-align: left;\n  cursor: pointer;\n  transition: background 0.1s ease;\n}\n\n.form-dropdown-option:hover {\n  background: var(--color-bg-hover);\n}\n\n.form-dropdown-option.selected {\n  background: var(--color-bg-selected);\n  font-weight: 500;\n}\n\n/* Tag input */\n.tag-input-wrapper {\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-sm);\n  padding: 4px 6px;\n  min-height: 32px;\n  display: flex;\n  align-items: center;\n}\n\n.tag-input-tags {\n  display: flex;\n  flex-wrap: wrap;\n  gap: 4px;\n  align-items: center;\n  flex: 1;\n}\n\n.tag-input-chip {\n  display: inline-flex;\n  align-items: center;\n  gap: 2px;\n  height: 22px;\n  padding: 0 6px;\n  border-radius: 10px;\n  font-size: 11px;\n  color: var(--color-accent-muted);\n  background: #eef0ff;\n}\n\n.tag-input-remove {\n  display: flex;\n  align-items: center;\n  border: none;\n  background: none;\n  color: var(--color-accent-muted);\n  cursor: pointer;\n  padding: 0;\n  margin-left: 2px;\n}\n\n.tag-input-remove:hover {\n  color: var(--color-text-primary);\n}\n\n.tag-input-field {\n  border: none;\n  outline: none;\n  font-size: 13px;\n  color: var(--color-text-primary);\n  background: transparent;\n  min-width: 80px;\n  flex: 1;\n}\n\n.tag-input-field::placeholder {\n  color: var(--color-text-disabled);\n}\n\n/* ===== Import ===== */\n.import-instructions {\n  margin-bottom: 16px;\n  padding: 12px;\n  background: var(--color-bg-sidebar);\n  border-radius: var(--radius-md);\n}\n\n.import-instructions-title {\n  font-size: 13px;\n  font-weight: 500;\n  color: var(--color-text-primary);\n  margin-bottom: 6px;\n}\n\n.import-instructions-text {\n  font-size: 12px;\n  color: var(--color-text-muted);\n  line-height: 1.5;\n}\n\n.import-dropzone {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: center;\n  padding: 32px;\n  border: 2px dashed var(--color-border-default);\n  border-radius: var(--radius-md);\n  text-align: center;\n  cursor: pointer;\n}\n\n.import-dropzone-icon {\n  color: var(--color-text-disabled);\n  margin-bottom: 8px;\n}\n\n.import-dropzone-text {\n  font-size: 13px;\n  color: var(--color-text-muted);\n  margin-bottom: 12px;\n}\n\n.import-browse-btn {\n  cursor: pointer;\n}\n\n.import-file-input {\n  display: none;\n}\n\n.import-preview {\n  margin-top: 12px;\n}\n\n.import-preview-info {\n  font-size: 13px;\n  font-weight: 500;\n  color: var(--color-text-primary);\n  margin-bottom: 8px;\n}\n\n.import-preview-table {\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-sm);\n  overflow: hidden;\n  font-size: 12px;\n}\n\n.import-preview-header {\n  display: flex;\n  background: var(--color-bg-sidebar);\n  border-bottom: 1px solid var(--color-border-default);\n}\n\n.import-preview-row {\n  display: flex;\n  border-bottom: 1px solid var(--color-border-light);\n}\n\n.import-preview-row:last-child {\n  border-bottom: none;\n}\n\n.import-preview-cell {\n  flex: 1;\n  padding: 6px 8px;\n  overflow: hidden;\n  text-overflow: ellipsis;\n  white-space: nowrap;\n  color: var(--color-text-secondary);\n}\n\n.import-preview-cell.header {\n  font-weight: 500;\n  color: var(--color-text-muted);\n}\n\n.import-preview-more {\n  padding: 6px 8px;\n  font-size: 12px;\n  color: var(--color-text-muted);\n}\n\n.import-error {\n  margin-top: 8px;\n  font-size: 13px;\n  color: var(--color-status-churned);\n}\n\n.import-success {\n  margin-top: 8px;\n  font-size: 13px;\n  color: var(--color-status-active);\n}\n\n/* ===== Tasks Filter ===== */\n.tasks-filter {\n  display: flex;\n  align-items: center;\n  gap: 8px;\n}\n\n.tasks-filter-input-wrapper {\n  position: relative;\n  flex: 1;\n  max-width: 320px;\n}\n\n.tasks-filter-input {\n  width: 100%;\n  height: 28px;\n  padding: 0 28px 0 10px;\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-sm);\n  background: var(--color-bg-base);\n  font-size: 13px;\n  color: var(--color-text-primary);\n  outline: none;\n  transition: border-color 0.15s ease;\n}\n\n.tasks-filter-input::placeholder {\n  color: var(--color-text-disabled);\n}\n\n.tasks-filter-input:focus {\n  border-color: var(--color-accent);\n}\n\n.tasks-filter-clear {\n  position: absolute;\n  right: 4px;\n  top: 50%;\n  transform: translateY(-50%);\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 20px;\n  height: 20px;\n  border: none;\n  border-radius: 50%;\n  background: var(--color-bg-hover);\n  color: var(--color-text-muted);\n  cursor: pointer;\n}\n\n/* ===== Tasks List ===== */\n.tasks-list {\n  display: flex;\n  flex-direction: column;\n  gap: 1px;\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-md);\n}\n\n.task-card {\n  display: flex;\n  align-items: center;\n  gap: 12px;\n  padding: 12px;\n  background: var(--color-bg-base);\n  transition: background 0.1s ease;\n}\n\n.task-card:first-child {\n  border-radius: var(--radius-md) var(--radius-md) 0 0;\n}\n\n.task-card:last-child {\n  border-radius: 0 0 var(--radius-md) var(--radius-md);\n}\n\n.task-card:only-child {\n  border-radius: var(--radius-md);\n}\n\n.task-card:not(:last-child) {\n  border-bottom: 1px solid var(--color-border-light);\n}\n\n.task-card:hover {\n  background: var(--color-bg-hover);\n}\n\n/* ===== Priority Badges ===== */\n.priority-badge {\n  display: inline-flex;\n  align-items: center;\n  justify-content: center;\n  height: 20px;\n  padding: 0 8px;\n  border-radius: 10px;\n  font-size: 11px;\n  font-weight: 500;\n  white-space: nowrap;\n  flex-shrink: 0;\n  min-width: 56px;\n}\n\n.priority-high {\n  color: var(--color-priority-high);\n  background: var(--color-priority-high-bg);\n}\n\n.priority-medium {\n  color: var(--color-priority-medium);\n  background: var(--color-priority-medium-bg);\n}\n\n.priority-low {\n  color: var(--color-priority-low);\n  background: var(--color-priority-low-bg);\n}\n\n.priority-normal {\n  color: var(--color-priority-normal);\n  background: var(--color-priority-normal-bg);\n}\n\n/* ===== Task Card Elements ===== */\n.task-name {\n  flex: 1;\n  min-width: 0;\n  border: none;\n  background: none;\n  padding: 0;\n  font-size: 13px;\n  font-weight: 600;\n  color: var(--color-text-primary);\n  text-align: left;\n  cursor: pointer;\n  overflow: hidden;\n  text-overflow: ellipsis;\n  white-space: nowrap;\n  transition: color 0.1s ease;\n}\n\n.task-name:hover {\n  color: var(--color-accent);\n}\n\n.task-due-date {\n  font-size: 12px;\n  color: var(--color-text-muted);\n  white-space: nowrap;\n  flex-shrink: 0;\n}\n\n.task-assignee {\n  display: flex;\n  align-items: center;\n  gap: 6px;\n  flex-shrink: 0;\n  min-width: 120px;\n}\n\n.task-assignee-avatar {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  width: 24px;\n  height: 24px;\n  border-radius: 50%;\n  background: var(--color-accent);\n  color: white;\n  font-size: 10px;\n  font-weight: 600;\n  flex-shrink: 0;\n}\n\n.task-assignee-name {\n  font-size: 12px;\n  color: var(--color-text-secondary);\n  white-space: nowrap;\n  overflow: hidden;\n  text-overflow: ellipsis;\n}\n\n.task-card-action {\n  flex-shrink: 0;\n  position: relative;\n  z-index: 1;\n}\n\n/* ===== Tasks Empty State ===== */\n.tasks-empty {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: center;\n  padding: 48px 24px;\n  border: 1px solid var(--color-border-default);\n  border-radius: var(--radius-md);\n  text-align: center;\n}\n\n/* ===== Searchable Select ===== */\n.searchable-select-search {\n  display: flex;\n  align-items: center;\n  gap: 6px;\n  padding: 6px 8px;\n  border-bottom: 1px solid var(--color-border-light);\n}\n\n.searchable-select-icon {\n  color: var(--color-text-muted);\n  flex-shrink: 0;\n}\n\n.searchable-select-input {\n  width: 100%;\n  border: none;\n  outline: none;\n  font-size: 13px;\n  color: var(--color-text-primary);\n  background: transparent;\n}\n\n.searchable-select-input::placeholder {\n  color: var(--color-text-disabled);\n}\n\n.searchable-select-options {\n  max-height: 200px;\n  overflow-y: auto;\n}\n\n.searchable-select-empty {\n  padding: 8px;\n  font-size: 13px;\n  color: var(--color-text-muted);\n  text-align: center;\n}\n\n/* ===== Success Toast ===== */\n.success-toast {\n  position: fixed;\n  top: 16px;\n  right: 16px;\n  background: var(--color-status-active);\n  color: white;\n  padding: 10px 20px;\n  border-radius: var(--radius-md);\n  font-size: 13px;\n  font-weight: 500;\n  z-index: 1000;\n  box-shadow: 0 2px 8px rgba(0, 0, 0, 0.15);\n}\n\n/* ===== Person Detail Page ===== */\n.person-header {\n  margin-bottom: 24px;\n}\n\n.person-name {\n  font-size: 24px;\n  font-weight: 700;\n  color: var(--color-text-primary);\n  margin-bottom: 4px;\n}\n\n.person-title-row {\n  font-size: 14px;\n  color: var(--color-text-secondary);\n  margin-bottom: 12px;\n}\n\n.person-title-text {\n  color: var(--color-text-secondary);\n}\n\n.person-title-separator {\n  color: var(--color-text-muted);\n}\n\n.per