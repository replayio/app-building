svg\n    43→            width=\"16\"\n    44→            height=\"16\"\n    45→            viewBox=\"0 0 16 16\"\n    46→            fill=\"none\"\n    47→            xmlns=\"http://www.w3.org/2000/svg\"\n    48→          >\n    49→            <path\n    50→              d=\"M6 4L10 8L6 12\"\n    51→              stroke=\"currentColor\"\n    52→              strokeWidth=\"1.5\"\n    53→              strokeLinecap=\"round\"\n    54→              strokeLinejoin=\"round\"\n    55→            />\n    56→          </svg>\n    57→        </span>\n    58→        <span className=\"category-section-title\" data-testid={`category-title-${title.toLowerCase()}`}>{title}</span>\n    59→        {total !== undefined && (\n    60→          <span className=\"category-section-total\" data-testid={`category-total-${title.toLowerCase()}`}>{total}</span>\n    61→        )}\n    62→      </button>\n    63→\n    64→      {expanded && (\n    65→        <div className=\"category-section-content\" data-testid={`category-content-${title.toLowerCase()}`}>\n    66→          {items.map((item, index) => (\n    67→            <React.Fragment\n    68→              key={getItemKey ? getItemKey(item, index) : index}\n    69→            >\n    70→              {renderItem(item, index)}\n    71→            </React.Fragment>\n    72→          ))}\n    73→        </div>\n    74→      )}\n    75→    </div>\n    76→  );\n    77→}\n    78→\n    79→export interface CategoryGroupProps<T> {\n    80→  /** Ordered list of categories to display. */\n    81→  categories: Array<{\n    82→    title: string;\n    83→    items: T[];\n    84→    total?: string;\n    85→    defaultExpanded?: boolean;\n    86→  }>;\n    87→  /** Render function for each item. */\n    88→  renderItem: (item: T, index: number) => React.ReactNode;\n    89→  /** Optional key extractor for items. */\n    90→  getItemKey?: (item: T, index: number) => string | number;\n    91→}\n    92→\n    93→/**\n    94→ * Renders multiple CategorySection components for a list of categories.\n    95→ * Provides the overall grouped layout for accounts or similar entities.\n    96→ */\n    97→export function CategoryGroup<T>({\n    98→  categories,\n    99→  renderItem,\n   100→  getItemKey,\n   101→}: CategoryGroupProps<T>) {\n   102→  return (\n   103→    <div className=\"category-group\">\n   104→      {categories.map((cat) => (\n   105→        <CategorySection\n   106→          key={cat.title}\n   107→          title={cat.title}\n   108→          items={cat.items}\n   109→          total={cat.total}\n   110→          defaultExpanded={cat.defaultExpanded ?? true}\n   111→          renderItem={renderItem}\n   112→          getItemKey={getItemKey}\n   113→        />\n   114→      ))}\n   115→    </div>\n   116→  );\n   117→}\n   118→"}]},"parent_tool_use_id":"toolu_01VuXa3rndmrfJYo7jqmCZ16","session_id":"ca254283-9e3b-4262-9b55-038a3d781a39","uuid":"b7a06141-7c2d-4fc3-8bc8-422d917604cc"}
