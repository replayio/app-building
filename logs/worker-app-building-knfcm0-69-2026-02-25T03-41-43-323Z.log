seInt(availableUnits, 10) || 0,\n   180→    });\n   181→    setSubmitting(false);\n   182→  };\n   183→\n   184→  return (\n   185→    <div className=\"modal-overlay\" data-testid=\"edit-equipment-modal\" onClick={onClose}>\n   186→      <div className=\"modal-content\" onClick={(e) => e.stopPropagation()}>\n   187→        <div className=\"modal-header\">\n   188→          <h2 className=\"modal-title\">Edit Equipment</h2>\n   189→          <button\n   190→            className=\"modal-close-btn\"\n   191→            data-testid=\"edit-equipment-modal-close\"\n   192→            onClick={onClose}\n   193→          >\n   194→            <svg\n   195→              width=\"16\"\n   196→              height=\"16\"\n   197→              viewBox=\"0 0 24 24\"\n   198→              fill=\"none\"\n   199→              stroke=\"currentColor\"\n   200→              strokeWidth=\"2\"\n   201→              strokeLinecap=\"round\"\n   202→              strokeLinejoin=\"round\"\n   203→            >\n   204→              <line x1=\"18\" y1=\"6\" x2=\"6\" y2=\"18\" />\n   205→              <line x1=\"6\" y1=\"6\" x2=\"18\" y2=\"18\" />\n   206→            </svg>\n   207→          </button>\n   208→        </div>\n   209→        <div className=\"form-group\">\n   210→          <label className=\"form-label\">Title</label>\n   211→          <input\n   212→            className=\"form-input\"\n   213→            data-testid=\"edit-equipment-title-input\"\n   214→            type=\"text\"\n   215→            value={title}\n   216→            onChange={(e) => setTitle(e.target.value)}\n   217→            placeholder=\"Equipment name\"\n   218→          />\n   219→        </div>\n   220→        <div className=\"form-group\">\n   221→          <label className=\"form-label\">Description</label>\n   222→          <textarea\n   223→            className=\"form-textarea\"\n   224→            data-testid=\"edit-equipment-description-input\"\n   225→            value={description}\n   226→    