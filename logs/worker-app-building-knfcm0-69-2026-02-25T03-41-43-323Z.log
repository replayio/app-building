sum, li) => sum + (parseFloat(li.amount) || 0), 0);\n    39→  const isBalanced = Math.abs(totalDebits - totalCredits) < 0.005;\n    40→\n    41→  const updateLineItem = (index: number, field: keyof LineItem, value: string) => {\n    42→    setLineItems((prev) => prev.map((li, i) => (i === index ? { ...li, [field]: value } : li)));\n    43→  };\n    44→\n    45→  const addLineItem = () => {\n    46→    setLineItems((prev) => [...prev, { account_id: \"\", entry_type: \"debit\", amount: \"\" }]);\n    47→  };\n    48→\n    49→  const removeLineItem = (index: number) => {\n    50→    if (lineItems.length <= 2) return;\n    51→    setLineItems((prev) => prev.filter((_, i) => i !== index));\n    52→  };\n    53→\n    54→  const handleTagKeyDown = (e: React.KeyboardEvent) => {\n    55→    if (e.key === \"Enter\" && tagInput.trim()) {\n    56→      e.preventDefault();\n    57→      if (!tags.includes(tagInput.trim())) {\n    58→        setTags((prev) => [...prev, tagInput.trim()]);\n    59→      }\n    60→      setTagInput(\"\");\n    61→    }\n    62→  };\n    63→\n    64→  const handleSubmit = async () => {\n    65→    if (!description.trim() || !isBalanced) return;\n    66→    setSubmitting(true);\n    67→    try {\n    68→      await dispatch(\n    69→        createTransaction({\n    70→          date,\n    71→          description,\n    72→          currency,\n    73→          entries: lineItems\n    74→            .filter((li) => li.account_id && parseFloat(li.amount) > 0)\n    75→            .map((li) => ({\n    76→              account_id: li.account_id,\n    77→              entry_type: li.entry_type,\n    78→              amount: parseFloat(li.amount),\n    79→            })),\n    80→          tags,\n    81→        })\n    82→      ).unwrap();\n    83→      await dispatch(fetchAccounts());\n    84→      onClose();\n    85→    } catch {\n    86→      setSubmitting(false);\n    87→    }\n    88→  };\n    89→\n    90→  return (\n    91→    <div className=\"modal-overlay\" data-testid=\"new-transaction-modal\" onClick={onClose}>\n    92→      <div className=\"modal-content\" onClick={(e) => e.stopPropagation()}>\n    93→        <div className=\"modal-header\">\n    94→          <h2 className=\"modal-title\">New Transaction</h2>\n    95→          <button className=\"modal-close-btn\" data-testid=\"modal-close-btn\" onClick={onClose}>\n    96→            <svg width=\"16\" height=\"16\" viewBox=\"0 0 16 16\" fill=\"none\">\n    97→              <path d=\"M4 4L12 12M12 4L4 12\" stroke=\"currentColor\" strokeWidth=\"1.5\" strokeLinecap=\"round\" />\n    98→            </svg>\n    99→          </button>\n   100→        </div>\n   101→\n   102→        <div className=\"form-group\">\n   103→          <label className=\"form-label\">Date</label>\n   104→          <input\n   105→            type=\"date\"\n   106→            className=\"form-input\"\n   107→            data-testid=\"transaction-date\"\n   108→            value={date}\n   109→            onChange={(e) => setDate(e.target.value)}\n   110→          />\n   111→        </div>\n   112→\n   113→        <div className=\"form-group\">\n   114→          <label className=\"form-label\">Description</label>\n   115→          <input\n   116→            type=\"text\"\n   117→            className=\"form-input\"\n   118→            data-testid=\"transaction-description\"\n   119→            placeholder=\"Enter description...\"\n   120→            value={description}\n   121→            onChange={(e) => setDescription(e.target.value)}\n   122→          />\n   123→        </div>\n   124→\n   125→        <div className=\"form-group\">\n   126→          <label className=\"form-label\">Line Items</label>\n   127→          <div className=\"line-items-table\">\n   128→            <div className=\"line-items-header\">\n   129→              <span>Account</span>\n   130→              <span>Type</span>\n   131→              <span>Amount</span>\n   132→              <span></span>\n   133→           