[2026-02-24T00:26:18.846Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01PH56Dg82DFoSGQMfZFrd4u","type":"tool_result","content":"     1→import type { Context } from '@netlify/functions'\n     2→import { neon } from '@neondatabase/serverless'\n     3→\n     4→const ITEMS_PER_PAGE = 20\n     5→\n     6→export default async function handler(req: Request, _context: Context) {\n     7→  const databaseUrl = process.env.DATABASE_URL\n     8→  if (!databaseUrl) {\n     9→    return Response.json({ error: 'Database not configured' }, { status: 500 })\n    10→  }\n    11→\n    12→  const sql = neon(databaseUrl)\n    13→  const url = new URL(req.url)\n    14→  const segments = url.pathname.split('/').filter(Boolean)\n    15→  const resourceId = segments[3] || null\n    16→\n    17→  // GET requests\n    18→  if (req.method === 'GET') {\n    19→    // CSV export\n    20→    if (url.searchParams.get('export') === 'csv') {\n    21→      const rows = await sql`\n    22→        SELECT t.title, t.description, t.due_date, t.priority, t.status,\n    23→               c.name as client_name, u.name as assignee\n    24→        FROM tasks t\n    25→        LEFT JOIN clients c ON t.client_id = c.id\n    26→        LEFT JOIN users u ON t.assignee_id = u.id\n    27→        ORDER BY t.due_date ASC NULLS LAST\n    28→      `\n    29→      const header = 'Title,Description,Due Date,Priority,Client Name,Assignee'\n    30→      const csvRows = rows.map((r: Record<string, unknown>) => {\n    31→        const fields = [r.title, r.description || '', r.due_date || '', r.priority, r.client_name || '', r.assignee || '']\n    32→        return fields.map((f) => `\"${String(f).replace(/\"/g, '\"\"')}\"`).join(',')\n    33→      })\n    34→      const csv = [header, ...csvRows].join('\\n')\n    35→      return new Response(csv, {\n    36→        status: 200,\n    37→        headers: {\n    38→          'Content-Type': 'text/csv',\n    39→          'Content-Disposition': 'attachment; filename=\"tasks.csv\"',\n    40→        },\n    41→      })\n    42→    }\n    43→\n    44→    // Single task by ID\n    45→    if (resourceId) {\n    46→      const rows = await sql`\n    47→        SELECT t.*, c.name as client_name, d.name as deal_name,\n    48→               u.name as assignee, u.avatar_url as assignee_avatar, u.role as assignee_role\n    49→        FROM tasks t\n    50→        LEFT JOIN clients c ON t.client_id = c.id\n    51→        LEFT JOIN deals d ON t.deal_id = d.id\n    52→        LEFT JOIN users u ON t.assignee_id = u.id\n    53→        WHERE t.id = ${resourceId}\n    54→      `\n    55→      if (rows.length === 0) {\n    56→        return Response.json({ error: 'Task not found' }, { status: 404 })\n    57→      }\n    58→      return Response.json(rows[0])\n    59→    }\n    60→\n    61→    // List tasks with filters\n    62→    const search = url.searchParams.get('search') || ''\n    63→    const priority = url.searchParams.get('priority') || ''\n    64→    const status = url.searchParams.get('status') || ''\n    65→    const sortBy = url.searchParams.get('sortBy') || 'due_date'\n    66→    const sortDir = url.searchParams.get('sortDir') || 'asc'\n    67→    const page = Math.max(1, parseInt(url.searchParams.get('page') || '1', 10))\n    68→    const offset = (page - 1) * ITEMS_PER_PAGE\n    69→\n    70→    const allowedSortColumns = ['due_date', 'title', 'priority', 'created_at']\n    71→    const safeSortBy = allowedSortColumns.includes(sortBy) ? sortBy : 'due_date'\n    72→    const safeSortDir = sortDir === 'desc' ? 'desc' : 'asc'\n    73→\n    74→    const countResult = await sql`\n    75→      SELECT COUNT(*) as count\n    76→      FROM tasks t\n    77→      LEFT JOIN clients c ON t.client_id = c.id\n    78→      WHERE (${search} = '' OR t.title ILIKE ${'%' + search + '%'})\n    79→        AND (${priority} = '' OR t.priority = ${priority})\n    80→        AND (${status} = '' OR t.status = ${status})\n    81→    `\n    82→    const total = parseInt(String(countResult[0].count), 10)\n    83→\n    84→    const rows = await sql`\n    85→      SELECT t.*, c.name as client_name, d.name as deal_name,\n    86→             u.name as assignee, u.avatar_url as assignee_avatar, u.role as assignee_role\n    87→      FROM tasks t\n    88→      LEFT JOIN clients c ON t.client_id = c.id\n    89→      LEFT JOIN deals d ON t.deal_id = d.id\n    90→      LEFT JOIN users u ON t.assignee_id = u.id\n    91→      WHERE (${search} = '' OR t.title ILIKE ${'%' + search + '%'})\n    92→        AND (${priority} = '' OR t.priority = ${priority})\n    93→        AND (${status} = '' OR t.status = ${status})\n    94→      ORDER BY\n    95→        CASE WHEN ${safeSortBy} = 'due_date' AND ${safeSortDir} = 'asc' THEN t.due_date END ASC NULLS LAST,\n    96→        CASE WHEN ${safeSortBy} = 'due_date' AND ${safeSortDir} = 'desc' THEN t.due_date END DESC NULLS LAST,\n    97→        CASE WHEN ${safeSortBy} = 'title' AND ${safeSortDir} = 'asc' THEN t.title END ASC,\n    98→        CASE WHEN ${safeSortBy} = 'title' AND ${safeSortDir} = 'desc' THEN t.title END DESC,\n    99→        CASE WHEN ${safeSortBy} = 'priority' AND ${safeSortDir} = 'asc' THEN t.priority END ASC,\n   100→        CASE WHEN ${safeSortBy} = 'priority' AND ${safeSortDir} = 'desc' THEN t.priority END DESC,\n   101→        CASE WHEN ${safeSortBy} = 'created_at' AND ${safeSortDir} = 'asc' THEN t.created_at END ASC,\n   102→        CASE WHEN ${safeSortBy} = 'created_at' AND ${safeSortDir} = 'desc' THEN t.created_at END DESC,\n   103→        t.due_date ASC NULLS LAST\n   104→      LIMIT ${ITEMS_PER_PAGE} OFFSET ${offset}\n   105→    `\n   106→\n   107→    return Response.json({ tasks: rows, total, page, perPage: ITEMS_PER_PAGE })\n   108→  }\n   109→\n   110→  // POST - create or import\n   111→  if (req.method === 'POST') {\n   112→    if (resourceId === 'import') {\n   113→      const body = await req.json()\n   114→      const { tasks: taskRows } = body as { tasks: Array<Record<string, string>> }\n   115→      if (!Array.isArray(taskRows)) {\n   116→        return Response.json({ error: 'tasks array required' }, { status: 400 })\n   117→      }\n   118→\n   119→      const errors: Array<{ row: number; error: string }> = []\n   120→      let imported = 0\n   121→\n   122→      for (let i = 0; i < taskRows.length; i++) {\n   123→        const row = taskRows[i]\n   124→        const title = (row.Title || row.title || '').trim()\n   125→        if (!title) {\n   126→          errors.push({ row: i + 1, error: 'Title is required' })\n   127→          continue\n   128→        }\n   129→\n   130→        const description = (row.Description || row.description || '').trim() || null\n   131→        const dueDateStr = (row['Due Date'] || row.due_date || '').trim()\n   132→        const dueDate = dueDateStr || null\n   133→        const priorityRaw = (row.Priority || row.priority || 'medium').trim().toLowerCase()\n   134→        const validPriorities = ['high', 'medium', 'normal', 'low']\n   135→        if (!validPriorities.includes(priorityRaw)) {\n   136→          errors.push({ row: i + 1, error: `Invalid priority '${row.Priority || row.priority}'` })\n   137→          continue\n   138→        }\n   139→\n   140→        let clientId: string | null = null\n   141→        const clientName = (row['Client Name'] || row.client_name || '').trim()\n   142→        if (clientName) {\n   143→          const clients = await sql`SELECT id FROM clients WHERE LOWER(name) = LOWER(${clientName}) LIMIT 1`\n   144→          if (clients.length > 0) {\n   145→            clientId = clients[0].id\n   146→          }\n   147→        }\n   148→\n   149→        let assigneeId: string | null = null\n   150→        const assigneeName = (row.Assignee || row.assignee || '').trim()\n   151→        if (assigneeName) {\n   152→          const users = await sql`SELECT id FROM users WHERE LOWER(name) ILIKE LOWER(${assigneeName + '%'}) LIMIT 1`\n   153→          if (users.length > 0) {\n   154→            assigneeId = users[0].id\n   155→          }\n   156→        }\n   157→\n   158→        await sql`\n   159→          INSERT INTO tasks (title, description, due_date, priority, status, client_id, assignee_id)\n   160→          VALUES (${title}, ${description}, ${dueDate || null}, ${priorityRaw}, 'open', ${clientId}, ${assigneeId})\n   161→        `\n   162→        imported++\n   163→      }\n   164→\n   165→      return Response.json({ imported, errors, total: taskRows.length })\n   166→    }\n   167→\n   168→    // Single create\n   169→    const body = await req.json()\n   170→    const { title, description, due_date, priority, client_id, deal_id, assignee_id } = body as {\n   171→      title?: string\n   172→      description?: string\n   173→      due_date?: string\n   174→      priority?: string\n   175→      client_id?: string\n   176→      deal_id?: string\n   177→      assignee_id?: string\n   178→    }\n   179→\n   180→    if (!title?.trim()) {\n   181→      return Response.json({ error: 'Title is required' }, { status: 400 })\n   182→    }\n   183→\n   184→    const validPriorities = ['high', 'medium', 'normal', 'low']\n   185→    const safePriority = validPriorities.includes(priority || '') ? priority : 'medium'\n   186→\n   187→    const rows = await sql`\n   188→      INSERT INTO tasks (title, description, due_date, priority, status, client_id, deal_id, assignee_id)\n   189→      VALUES (\n   190→        ${title.trim()},\n   191→        ${description?.trim() || null},\n   192→        ${due_date || null},\n   193→        ${safePriority},\n   194→        'open',\n   195→        ${client_id || null},\n   196→        ${deal_id || null},\n   197→        ${assignee_id || null}\n   198→      )\n   199→      RETURNING *\n   200→    `\n   201→\n   202→    const task = rows[0]\n   203→\n   204→    // Fetch joined data for response\n   205→    const fullTask = await sql`\n   206→      SELECT t.*, c.name as client_name, d.name as deal_name,\n   207→             u.name as assignee, u.avatar_url as assignee_avatar, u.role as assignee_role\n   208→      FROM tasks t\n   209→      LEFT JOIN clients c ON t.client_id = c.id\n   210→      LEFT JOIN deals d ON t.deal_id = d.id\n   211→      LEFT JOIN users u ON t.assignee_id = u.id\n   212→      WHERE t.id = ${task.id}\n   213→    `\n   214→\n   215→    // Create timeline event if associated with a client\n   216→    if (task.client_id) {\n   217→      await sql`\n   218→        INSERT INTO timeline_events (client_id, task_id, event_type, description, actor_name)\n   219→        VALUES (${task.client_id}, ${task.id}, 'task_created', ${'Task created: ' + title.trim()}, 'System')\n   220→      `\n   221→    }\n   222→\n   223→    return Response.json(fullTask[0], { status: 201 })\n   224→  }\n   225→\n   226→  // PUT - update\n   227→  if (req.method === 'PUT') {\n   228→    if (!resourceId) {\n   229→      return Response.json({ error: 'Task ID required' }, { status: 400 })\n   230→    }\n   231→\n   232→    const body = await req.json()\n   233→    const { title, description, due_date, priority, status, client_id, deal_id, assignee_id } = body as {\n   234→      title?: string\n   235→      description?: string\n   236→      due_date?: string\n   237→      priority?: string\n   238→      status?: string\n   239→      client_id?: string\n   240→      deal_id?: string\n   241→      assignee_id?: string\n   242→    }\n   243→\n   244→    // Fetch current task for status change detection\n   245→    const current = await sql`SELECT * FROM tasks WHERE id = ${resourceId}`\n   246→    if (current.length === 0) {\n   247→      return Response.json({ error: 'Task not found' }, { status: 404 })\n   248→    }\n   249→\n   250→    const validPriorities = ['high', 'medium', 'normal', 'low']\n   251→    const validStatuses = ['open', 'completed', 'cancelled']\n   252→    const safePriority = priority && validPriorities.includes(priority) ? priority : current[0].priority\n   253→    const safeStatus = status && validStatuses.includes(status) ? status : current[0].status\n   254→\n   255→    const rows = await sql`\n   256→      UPDATE tasks SET\n   257→        title = ${title?.trim() || current[0].title},\n   258→        description = ${description !== undefined ? (description?.trim() || null) : current[0].description},\n   259→        due_date = ${due_date !== undefined ? (due_date || null) : current[0].due_date},\n   260→        priority = ${safePriority},\n   261→        status = ${safeStatus},\n   262→        client_id = ${client_id !== undefined ? (client_id || null) : current[0].client_id},\n   263→        deal_id = ${deal_id !== undefined ? (deal_id || null) : current[0].deal_id},\n   264→        assignee_id = ${assignee_id !== undefined ? (assignee_id || null) : current[0].assignee_id},\n   265→        updated_at = NOW()\n   266→      WHERE id = ${resourceId}\n   267→      RETURNING *\n   268→    `\n   269→\n   270→    // Create timeline event for status changes\n   271→    if (status && status !== current[0].status && current[0].client_id) {\n   272→      const description = status === 'completed'\n   273→        ? `Task completed: ${rows[0].title}`\n   274→        : status === 'cancelled'\n   275→          ? `Task cancelled: ${rows[0].title}`\n   276→          : `Task status changed to ${status}: ${rows[0].title}`\n   277→      await sql`\n   278→        INSERT INTO timeline_events (client_id, task_id, event_type, description, actor_name)\n   279→        VALUES (${current[0].client_id}, ${resourceId}, 'task_status_changed', ${description}, 'System')\n   280→      `\n   281→    }\n   282→\n   283→    const fullTask = await sql`\n   284→      SELECT t.*, c.name as client_name, d.name as deal_name,\n   285→             u.name as assignee, u.avatar_url as assignee_avatar, u.role as assignee_role\n   286→      FROM tasks t\n   287→      LEFT JOIN clients c ON t.client_id = c.id\n   288→      LEFT JOIN deals d ON