ength > 0) {\n   193→        const resetToken = randomBytes(32).toString('hex')\n   194→        await sql`\n   195→          INSERT INTO email_tokens (user_id, token, type, expires_at)\n   196→          VALUES (${rows[0].id}, ${resetToken}, 'reset', NOW() + INTERVAL '1 hour')\n   197→        `\n   198→      }\n   199→\n   200→      return new Response(\n   201→        JSON.stringify({ message: 'If the email exists, a reset link has been sent.' }),\n   202→        { status: 200, headers: { 'Content-Type': 'application/json' } },\n   203→      )\n   204→    }\n   205→\n   206→    if (action === 'confirm-email') {\n   207→      const { token } = body as { token?: string }\n   208→      if (!token) {\n   209→        return new Response(JSON.stringify({ error: 'Token required' }), {\n   210→          status: 400,\n   211→          headers: { 'Content-Type': 'application/json' },\n   212→        })\n   213→      }\n   214→\n   215→      const rows = await sql`\n   216→        SELECT id, user_id FROM email_tokens\n   217→        WHERE token = ${token} AND type = 'confirmation' AND used_at IS NULL AND expires_at > NOW()\n   218→      `\n   219→\n   220→      if (rows.length === 0) {\n   221→        return new Response(JSON.stringify({ error: 'Invalid or expired token' }), {\n   222→          status: 400,\n   223→          headers: { 'Content-Type': 'application/json' },\n   224→        })\n   225→      }\n   226→\n   227→      await sql`UPDATE email_tokens SET used_at = NOW() WHERE id = ${rows[0].id}`\n   228→\n   229→      return new Response(\n   230→        JSON.stringify({ message: 'Email confirmed successfully.' }),\n   231→        { status: 200, headers: { 'Content-Type': 'application/json' } },\n   232→      )\n   233→    }\n   234→\n   235→    if (action === 'reset-password') {\n   236→      const { token, newPassword } = body as { token?: string; newPassword?: string }\n   237→      if (!token || !newPassword) {\n   238→        return new Response(JSON.stringify({ error: 'Token and new password required' }), {\n   239→          status: 400,\n   240→          headers: { 'Content-Type': 'application/json' },\n   241→        })\n   242→      }\n   243→\n   244→      const rows = await sql`\n   245→        SELECT id, user_id FROM email_tokens\n   246→        WHERE token = ${token} AND type = 'reset' AND used_at IS NULL AND expires_at > NOW()\n   247→      `\n   248→\n   249→      if (rows.length === 0) {\n   250→        return new Response(JSON.stringify({ error: 'Invalid or expired token' }), {\n   251→          status: 400,\n   252→          headers: { 'Content-Type': 'application/json' },\n   253→        })\n   254→      }\n   255→\n   256→      const passwordHash = hashPassword(newPassword)\n   257→      await sql`UPDATE users SET password_hash = ${passwordHash}, updated_at = NOW() WHERE id = ${rows[0].user_id}`\n   258→      await sql`UPDATE email_tokens SET used_at = NOW() WHERE id = ${rows[0].id}`\n   259→\n   260→      return new Response(\n   261→        JSON.stringify({ message: 'Password reset successfully.' }),\n   262→        { status: 200, headers: { 'Content-Type': 'application/json' } },\n   263→      )\n   264→    }\n   265→\n   266→    return new Response(JSON.stringify({ error: 'Unknown action' }), {\n   267→      status: 400,\n   268→      headers: { 'Content-Type': 'application/json' },\n   269→    })\n   270→  }\n   271→\n   272→  return new Response(JSON.stringify({ error: 'Method not allowed' }), {\n   273→    status: 405,\n   274→    headers: { 'Content-Type': 'application/json' },\n   275→  })\n   276→}\n   277→"}]},"parent_tool_use_id":"toolu_01Y577ScU6v941jfcV4TkgzA","session_id":"9358be0e-1d5d-4025-8e62-a8c8aedcc7f4","uuid":"0f20d8fc-4a43-4da5-a41e-42167a026469"}
