[2026-02-24T00:26:16.763Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01XMgpk19TMqY97DC2sLiAhf","type":"tool_result","content":"     1→import { useState, useRef, useEffect } from 'react'\n     2→import { useNavigate } from 'react-router-dom'\n     3→import { MoreHorizontal } from 'lucide-react'\n     4→import type { Task } from '../../store/slices/tasksSlice'\n     5→import { formatDueDate, getInitials } from '../../lib/utils'\n     6→\n     7→const priorityConfig: Record<string, { label: string; color: string; bg: string }> = {\n     8→  high: { label: 'High', color: 'var(--color-priority-high)', bg: 'var(--color-priority-high-bg)' },\n     9→  medium: { label: 'Medium', color: 'var(--color-priority-medium)', bg: 'var(--color-priority-medium-bg)' },\n    10→  normal: { label: 'Normal', color: 'var(--color-priority-normal)', bg: 'var(--color-priority-normal-bg)' },\n    11→  low: { label: 'Low', color: 'var(--color-priority-low)', bg: 'var(--color-priority-low-bg)' },\n    12→}\n    13→\n    14→function formatAssigneeName(name: string): string {\n    15→  const parts = name.trim().split(' ')\n    16→  if (parts.length <= 1) return name\n    17→  return `${parts[0]} ${parts[parts.length - 1][0]}.`\n    18→}\n    19→\n    20→interface TaskCardProps {\n    21→  task: Task\n    22→  onStatusChange: (taskId: string, status: 'completed' | 'cancelled') => void\n    23→  onDelete: (taskId: string) => void\n    24→}\n    25→\n    26→export default function TaskCard({ task, onStatusChange, onDelete }: TaskCardProps) {\n    27→  const navigate = useNavigate()\n    28→  const [menuOpen, setMenuOpen] = useState(false)\n    29→  const [confirmDelete, setConfirmDelete] = useState(false)\n    30→  const menuRef = useRef<HTMLDivElement>(null)\n    31→\n    32→  useEffect(() => {\n    33→    function handleClickOutside(e: MouseEvent) {\n    34→      if (menuRef.current && !menuRef.current.contains(e.target as Node)) {\n    35→        setMenuOpen(false)\n    36→        setConfirmDelete(false)\n    37→      }\n    38→    }\n    39→    if (menuOpen) {\n    40→      document.addEventListener('mousedown', handleClickOutside)\n    41→      return () => document.removeEventListener('mousedown', handleClickOutside)\n    42→    }\n    43→  }, [menuOpen])\n    44→\n    45→  const priority = priorityConfig[task.priority] || priorityConfig.medium\n    46→  const dueDateStr = formatDueDate(task.due_date)\n    47→\n    48→  function handleCardClick(e: React.MouseEvent) {\n    49→    const target = e.target as HTMLElement\n    50→    if (target.closest('[data-menu-area]')) return\n    51→    navigate(`/tasks/${task.id}`)\n    52→  }\n    53→\n    54→  function handleDelete() {\n    55→    if (!confirmDelete) {\n    56→      setConfirmDelete(true)\n    57→      return\n    58→    }\n    59→    onDelete(task.id)\n    60→    setMenuOpen(false)\n    61→    setConfirmDelete(false)\n    62→  }\n    63→\n    64→  return (\n    65→    <div\n    66→      data-testid={`task-card-${task.id}`}\n    67→      className=\"flex items-center gap-3 h-[44px] px-3 rounded hover:bg-[var(--color-hover)] transition-colors duration-100 cursor-pointer group\"\n    68→      onClick={handleCardClick}\n    69→    >\n    70→      {/* Priority badge */}\n    71→      <span\n    72→        data-testid=\"task-priority-badge\"\n    73→        className=\"shrink-0 inline-flex items-center h-5 px-1.5 rounded text-[11px] font-medium\"\n    74→        style={{ color: priority.color, backgroundColor: priority.bg }}\n    75→      >\n    76→        {priority.label}\n    77→      </span>\n    78→\n    79→      {/* Title */}\n    80→      <span\n    81→        data-testid=\"task-title\"\n    82→        className=\"flex-1 min-w-0 truncate text-[13px] font-medium text-[var(--color-text-primary)]\"\n    83→      >\n    84→        {task.title}\n    85→      </span>\n    86→\n    87→      {/* Due date */}\n    88→      {dueDateStr && (\n    89→        <span data-testid=\"task