         setClients(items.map((c) => ({ value: c.id, label: c.name })))\n    49→        })\n    50→        .catch(() => {})\n    51→\n    52→      fetch('/.netlify/functions/users')\n    53→        .then((r) => r.json())\n    54→        .then((data) => {\n    55→          const items = (data.users || data || []) as Array<{ id: string; name: string }>\n    56→          setUsers(items.map((u) => ({ value: u.id, label: u.name })))\n    57→        })\n    58→        .catch(() => {})\n    59→    }\n    60→  }, [open])\n    61→\n    62→  if (!open) return null\n    63→\n    64→  function resetForm() {\n    65→    setTitle('')\n    66→    setDescription('')\n    67→    setDueDate('')\n    68→    setPriority('medium')\n    69→    setClientId('')\n    70→    setAssigneeId('')\n    71→    setError('')\n    72→  }\n    73→\n    74→  function handleClose() {\n    75→    resetForm()\n    76→    onClose()\n    77→  }\n    78→\n    79→  async function handleSubmit(e: React.FormEvent) {\n    80→    e.preventDefault()\n    81→    if (!title.trim()) {\n    82→      setError('Title is required')\n    83→      return\n    84→    }\n    85→    setError('')\n    86→    setSubmitting(true)\n    87→    try {\n    88→      await onSubmit({\n    89→        title: title.trim(),\n    90→        description: description.trim(),\n    91→        due_date: dueDate,\n    92→        priority,\n    93→        client_id: clientId,\n    94→        assignee_id: assigneeId,\n    95→      })\n    96→      resetForm()\n    97→      onClose()\n    98→    } catch {\n    99→      setError('Failed to create task')\n   100→    } finally {\n   101→      setSubmitting(false)\n   102→    }\n   103→  }\n   104→\n   105→  return (\n   106→    <div className=\"fixed inset-0 z-50 flex items-center justify-center\" data-testid=\"create-task-modal\">\n   107→      <div className=\"absolute inset-0 bg-black/30\" data-testid=\"modal-overlay\" onClick={handleClose} />\n   108→      <div className=\"relative bg-[var(--color