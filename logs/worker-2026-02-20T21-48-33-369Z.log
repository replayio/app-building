[2026-02-20T21:36:07.434Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01UzB2kFWFZLFFDS25p4PNQW","type":"tool_result","content":"     1→import { getDb, getDbUrl } from '../utils/db'\n     2→import { optionalAuth, type OptionalAuthRequest } from '../utils/auth'\n     3→import { notifyClientFollowers } from '../utils/notifications'\n     4→\n     5→async function handler(req: OptionalAuthRequest) {\n     6→  const sql = getDb()\n     7→  const url = new URL(req.url)\n     8→  const pathParts = url.pathname.split('/').filter(Boolean)\n     9→  // Path: /deals or /deals/:dealId\n    10→  const dealId = pathParts.length >= 4 ? pathParts[3] : null\n    11→\n    12→  // GET /deals — list all deals with filters and pagination\n    13→  if (req.method === 'GET' && !dealId) {\n    14→    const page = parseInt(url.searchParams.get('page') ?? '1', 10)\n    15→    const pageSize = parseInt(url.searchParams.get('pageSize') ?? '20', 10)\n    16→    const search = url.searchParams.get('search') ?? ''\n    17→    const stage = url.searchParams.get('stage') ?? ''\n    18→    const client = url.searchParams.get('client') ?? ''\n    19→    const status = url.searchParams.get('status') ?? ''\n    20→    const sort = url.searchParams.get('sort') ?? 'close_date_desc'\n    21→    const dateFrom = url.searchParams.get('dateFrom') ?? ''\n    22→    const dateTo = url.searchParams.get('dateTo') ?? ''\n    23→\n    24→    const offset = (page - 1) * pageSize\n    25→\n    26→    const searchPattern = search ? '%' + search + '%' : null\n    27→    const stageFilter = stage || null\n    28→    const clientFilter = client || null\n    29→    const dateFromFilter = dateFrom || null\n    30→    const dateToFilter = dateTo || null\n    31→\n    32→    // Normalize status into stage-based or status-based filter\n    33→    const statusIsActive = status === 'active'\n    34→    const statusIsWon = status === 'won'\n    35→    const statusIsLost = status === 'lost'\n    36→    const statusOther = (status && !statusIsActive && !statusIsWon && !statusIsLost) ? status : null\n    37→\n    38→    // Count total matching deals\n    39→    const countResult = await sql`\n    40→      SELECT COUNT(*) as count FROM deals d\n    41→      JOIN clients c ON d.client_id = c.id\n    42→      WHERE (${searchPattern}::text IS NULL OR (d.name ILIKE ${searchPattern} OR c.name ILIKE ${searchPattern}))\n    43→      AND (${stageFilter}::text IS NULL OR d.stage = ${stageFilter})\n    44→      AND (${clientFilter}::text IS NULL OR d.client_id = ${clientFilter}::uuid)\n    45→      AND (NOT ${statusIsActive}::boolean OR d.stage NOT IN ('closed_won', 'closed_lost'))\n    46→      AND (NOT ${statusIsWon}::boolean OR d.stage = 'closed_won')\n    47→      AND (NOT ${statusIsLost}::boolean OR d.stage = 'closed_lost')\n    48→      AND (${statusOther}::text IS NULL OR d.status = ${statusOther})\n    49→      AND (${dateFromFilter}::date IS NULL OR d.expected_close_date >= ${dateFromFilter}::date)\n    50→      AND (${dateToFilter}::date IS NULL OR d.expected_close_date <= ${dateToFilter}::date)\n    51→    `\n    52→    const total = parseInt(String(countResult[0].count), 10)\n    53→\n    54→    // Map sort parameter to safe sort key\n    55→    const sortMap: Record<string, string> = {\n    56→      'close_date_asc': 'close_date_asc',\n    57→      'close_date_desc': 'close_date_desc',\n    58→      'name_asc': 'name_asc',\n    59→      'name_desc': 'name_desc',\n    60→      'value_desc': 'value_desc',\n    61→      'value_asc': 'value_asc',\n    62→      'updated_at': 'updated_at',\n    63→    }\n    64→    const sortKey = sortMap[sort] ?? 'close_date_desc'\n    65→\n    66→    // Fetch deals with CASE-based ORDER BY to avoid dynamic SQL\n    67→    const deals = await sql`\n    68→      SELECT d.*, c.name as client_name FROM deals d\n    69→      JOIN clients c ON d.client_id = c.id\n    70→      WHERE (${searchPattern}::text IS NULL OR (d.name ILIKE ${searchPattern} OR c.name ILIKE ${searchPattern}))\n    71→      AND (${stageFilter}::text IS NULL OR d.stage = ${stageFilter})\n    72→      AND (${clientFilter}::text IS NULL OR d.client_id = ${clientFilter}::uuid)\n    73→      AND (NOT ${statusIsActive}::boolean OR d.stage NOT IN ('closed_won', 'closed_lost'))\n    74→      AND (NOT ${statusIsWon}::boolean OR d.stage = 'closed_won')\n    75→      AND (NOT ${statusIsLost}::boolean OR d.stage = 'closed_lost')\n    76→      AND (${statusOther}::text IS NULL OR d.status = ${statusOther})\n    77→      AND (${dateFromFilter}::date IS NULL OR d.expected_close_date >= ${dateFromFilter}::date)\n    78→      AND (${dateToFilter}::date IS NULL OR d.expected_close_date <= ${dateToFilter}::date)\n    79→      ORDER BY\n    80→        CASE WHEN ${sortKey} = 'close_date_asc' THEN d.expected_close_date END ASC NULLS LAST,\n    81→        CASE WHEN ${sortKey} = 'close_date_desc' THEN d.expected_close_date END DESC NULLS LAST,\n    82→        CASE WHEN ${sortKey} = 'name_asc' THEN d.name END ASC,\n    83→        CASE WHEN ${sortKey} = 'name_desc' THEN d.name END DESC,\n    84→        CASE WHEN ${sortKey} = 'value_desc' THEN d.value END DESC,\n    85→        CASE WHEN ${sortKey} = 'value_asc' THEN d.value END ASC,\n    86→        CASE WHEN ${sortKey} = 'updated_at' THEN d.updated_at END DESC,\n    87→        d.expected_close_date DESC NULLS LAST\n    88→      LIMIT ${pageSize} OFFSET ${offset}\n    89→    `\n    90→\n    91→    // Get summary metrics\n    92→    const metrics = await sql`\n    93→      SELECT\n    94→        COUNT(*) FILTER (WHERE status = 'active') as total_active,\n    95→        COALESCE(SUM(value) FILTER (WHERE status = 'active'), 0) as pipeline_value,\n    96→        COUNT(*) FILTER (WHERE stage = 'closed_won' AND updated_at >= date_trunc('quarter', CURRENT_DATE)) as won_count,\n    97→        COALESCE(SUM(value) FILTER (WHERE stage = 'closed_won' AND updated_at >= date_trunc('quarter', CURRENT_DATE)), 0) as won_value,\n    98→        COUNT(*) FILTER (WHERE stage = 'closed_lost' AND updated_at >= date_trunc('quarter', CURRENT_DATE)) as lost_count,\n    99→        COALESCE(SUM(value) FILTER (WHERE stage = 'closed_lost' AND updated_at >= date_trunc('quarter', CURRENT_DATE)), 0) as lost_value\n   100→      FROM deals\n   101→    `\n   102→\n   103→    // Get unique clients for filter dropdown\n   104→    const clients = await sql`\n   105→      SELECT DISTINCT c.id, c.name FROM clients c\n   106→      JOIN deals d ON d.client_id = c.id\n   107→      ORDER BY c.name ASC\n   108→    `\n   109→\n   110→    return Response.json({\n   111→      deals,\n   112→      total,\n   113→      metrics: metrics[0],\n   114→      clients,\n   115→    })\n   116→  }\n   117→\n   118→  // GET /deals/:dealId — get single deal with full details\n   119→  if (req.method === 'GET' && dealId) {\n   120→    const rows = await sql`\n   121→      SELECT d.*, c.name as client_name FROM deals d\n   122→      JOIN clients c ON d.client_id = c.id\n   123→      WHERE d.id = ${dealId}::uuid\n   124→    `\n   125→    if (rows.length === 0) {\n   126→      return new Response(JSON.stringify({ error: 'Deal not found' }), { status: 404 })\n   127→    }\n   128→    return Response.json(rows[0])\n   129→  }\n   130→\n   131→  // POST /deals?action=import (bulk CSV import)\n   132→  if (req.method === 'POST' && !dealId && url.searchParams.get('action') === 'import') {\n   133→    const body = await req.json() as {\n   134→      deals: Array<{\n   135→        name: string\n   136→        client_name?: string\n   137→        value?: string\n   138→        stage?: string\n   139→        owner?: string\n   140→        probability?: string\n   141→        expected_close_date?: string\n   142→        status?: string\n   143→      }>\n   144→    }\n   145→\n   146→    const results = { imported: 0, errors: [] as string[] }\n   147→    const validStages = ['lead', 'qualification', 'discovery', 'proposal', 'negotiation', 'closed_won', 'closed_lost']\n   148→    const validStatuses = ['active', 'on_track', 'at_risk', 'stalled']\n   149→\n   150→    for (let i = 0; i < body.deals.length; i++) {\n   151→      const row = body.deals[i]\n   152→      if (!row.name?.trim()) {\n   153→        results.errors.push(`Row ${i + 1}: Name is required`)\n   154→        continue\n   155→      }\n   156→      if (!row.client_name?.trim()) {\n   157→        results.errors.push(`Row ${i + 1}: Client Name is required`)\n   158→        continue\n   159→      }\n   160→      // Look up client by name\n   161→      const clientRows = await sql`SELECT id FROM clients WHERE LOWER(name) = LOWER(${row.client_name.trim()}) LIMIT 1`\n   162→      if (clientRows.length === 0) {\n   163→        results.errors.push(`Row ${i + 1}: Client \"${row.client_name.trim()}\" not found`)\n   164→        continue\n   165→      }\n   166→      const clientId = clientRows[0].id\n   167→      const stage = row.stage?.trim().toLowerCase() || 'lead'\n   168→      if (!validStages.includes(stage)) {\n   169→        results.errors.push(`Row ${i + 1}: Stage must be one of: ${validStages.join(', ')}`)\n   170→        continue\n   171→      }\n   172→      const status = row.status?.trim().toLowerCase() || 'active'\n   173→      if (!validStatuses.includes(status)) {\n   174→        results.errors.push(`Row ${i + 1}: Status must be one of: ${validStatuses.join(', ')}`)\n   175→        continue\n   176→      }\n   177→      const value = row.value ? parseFloat(row.value) : 0\n   178→      if (isNaN(value)) {\n   179→        results.errors.push(`Row ${i + 1}: Value must be a number`)\n   180→        continue\n   181→      }\n   182→      const probability = row.probability ? parseInt(row.probability) : 0\n   183→      if (isNaN(probability)) {\n   184→        results.errors.push(`Row ${i + 1}: Probability must be a number`)\n   185→        continue\n   186→      }\n   187→      try {\n   188→        await sql`\n   189→          INSERT INTO deals (name, client_id, value, stage, owner, probability, expected_close_date, status)\n   190→          VALUES (${row.name.trim()}, ${clientId}::uuid, ${value}, ${stage}, ${row.owner?.trim() || null}, ${probability}, ${row.expected_close_date?.trim() || null}, ${status})\n   191→        `\n   192→        results.imported++\n   193→      } catch (err) {\n   194→        results.errors.push(`Row ${i + 1}: ${err instanceof Error ? err.message : 'Database error'}`)\n   195→      }\n   196→    }\n   197→\n   198→    return Response.json(results, { status: 200 })\n   199→  }\n   200→\n   201→  // POST /deals — create a new deal\n   202→  if (req.method === 'POST' && !dealId) {\n   203→    const body = await req.json() as {\n   204→      name: string\n   205→      client_id: string\n   206→      value?: number\n   207→      stage?: string\n   208→      owner?: string\n   209→      probability?: number\n   210→      expected_close_date?: string\n   211→      status?: string\n   212→    }\n   213→\n   214→    const rows = await sql`\n   215→      INSERT INTO deals (name, client_id, value, stage, owner, probability, expected_close_date, status)\n   216→      VALUES (\n   217→        ${body.name},\n   218→        ${body.client_id}::uuid,\n   219→        ${body.value ?? 0},\n   220→        ${body.stage ?? 'lead'},\n   221→        ${body.owner ?? null},\n   222→        ${body.probability ?? 0},\n   223→        ${body.expected_close_date || null},\n   224→        ${body.status ?? 'active'}\n   225→      )\n   226→      RETURNING *\n   227→    `\n   228→\n   229→    const deal = rows[0]\n   230→\n   231→    // Create deal history entry for initial stage\n   232→    await sql`\n   233→      INSERT INTO deal_history (deal_id, old_stage, new_stage, changed_by)\n   234→      VALUES (${deal.id}::uuid, 'none', ${deal.stage}, ${req.user?.name ?? 'System'})\n   235→    `\n   236→\n   237→    // Create timeline event\n   238→    const dealCreateDesc = 'Deal Created: \\'' + body.name + '