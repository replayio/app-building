value: 'on_track', label: 'On Track' },\n    54→    { value: 'needs_attention', label: 'Needs Attention' },\n    55→    { value: 'at_risk', label: 'At Risk' },\n    56→    { value: 'won', label: 'Won' },\n    57→    { value: 'lost', label: 'Lost' },\n    58→    { value: 'on_hold', label: 'On Hold' },\n    59→  ]\n    60→\n    61→  const sortOptions = [\n    62→    { value: 'close_date_desc', label: 'Close Date (Newest)' },\n    63→    { value: 'close_date_asc', label: 'Close Date (Oldest)' },\n    64→    { value: 'name_asc', label: 'Name A-Z' },\n    65→    { value: 'name_desc', label: 'Name Z-A' },\n    66→    { value: 'value_desc', label: 'Value (High-Low)' },\n    67→    { value: 'value_asc', label: 'Value (Low-High)' },\n    68→  ]\n    69→\n    70→  return (\n    71→    <div className=\"flex items-center gap-2 mb-4 flex-wrap\" data-testid=\"deals-filter-controls\">\n    72→      <FilterSelect label=\"Stage\" value={stage} options={stageOptions} onChange={onStageChange} testId=\"deals-filter-stage\" />\n    73→      <FilterSelect label=\"Client\" value={client} options={clientOptions} onChange={onClientChange} testId=\"deals-filter-client\" searchable />\n    74→      <FilterSelect label=\"Status\" value={status} options={statusOptions} onChange={onStatusChange} testId=\"deals-filter-status\" />\n    75→      <FilterSelect label=\"Sort by\" value={sort} options={sortOptions} onChange={onSortChange} testId=\"deals-filter-sort\" />\n    76→      <div className=\"deals-date-range flex items-center gap-1.5\">\n    77→        <input\n    78→          type=\"date\"\n    79→          data-testid=\"deals-filter-date-from\"\n    80→          value={dateFrom}\n    81→          onChange={(e) => onDateFromChange(e.target.value)}\n    82→          className=\"h-[34px] px-2 text-[13px] text-text-secondary bg-surface border border-border rounded-[5px] focus:outline-none focus:border-accent transition-colors duration-100\"\n    83→          placeholder=\"From\"\n    84→        