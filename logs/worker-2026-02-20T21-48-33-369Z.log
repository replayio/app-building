tDateTo] = useState('')\n    31→  const [createModalOpen, setCreateModalOpen] = useState(false)\n    32→  const [deleteConfirm, setDeleteConfirm] = useState<string | null>(null)\n    33→  const [importDialogOpen, setImportDialogOpen] = useState(false)\n    34→  const [availableUsers, setAvailableUsers] = useState<{ name: string }[]>([])\n    35→\n    36→  useEffect(() => {\n    37→    fetch('/.netlify/functions/users')\n    38→      .then(r => r.json())\n    39→      .then((data: { users: { name: string }[] }) => setAvailableUsers(data.users))\n    40→      .catch(() => {})\n    41→  }, [])\n    42→\n    43→  const loadDeals = useCallback(() => {\n    44→    dispatch(\n    45→      fetchDeals({\n    46→        page,\n    47→        search: search || undefined,\n    48→        stage: stageFilter || undefined,\n    49→        client: clientFilter || undefined,\n    50→        status: statusFilter || undefined,\n    51→        sort,\n    52→        dateFrom: dateFrom || undefined,\n    53→        dateTo: dateTo || undefined,\n    54→      })\n    55→    )\n    56→  }, [dispatch, page, search, stageFilter, clientFilter, statusFilter, sort, dateFrom, dateTo])\n    57→\n    58→  useEffect(() => {\n    59→    loadDeals()\n    60→  }, [loadDeals])\n    61→\n    62→  // Debounce search input\n    63→  useEffect(() => {\n    64→    const timer = setTimeout(() => {\n    65→      setSearch(searchInput)\n    66→      dispatch(setDealsPage(1))\n    67→    }, 300)\n    68→    return () => clearTimeout(timer)\n    69→  }, [searchInput, dispatch])\n    70→\n    71→  function handleCreateDeal(data: {\n    72→    name: string\n    73→    client_id: string\n    74→    value: number\n    75→    stage: DealStage\n    76→    owner: string\n    77→    expected_close_date: string | null\n    78→  }) {\n    79→    dispatch(createDeal({ ...data, expected_close_date: data.expected_close_date ?? undefined })).then(() => {\n    80→      setCreateModalOpen(false)\n    81→      return dispatch(fetchDeals({\n    82→        page,\n    83→        search: search || undefined,\n    84→        stage: stageFilter || undefined,\n    85→        client: clientFilter || undefined,\n    86→        status: statusFilter || undefined,\n    87→        sort,\n    88→        dateFrom: dateFrom || undefined,\n    89→        dateTo: dateTo || undefined,\n    90→      }))\n    91→    })\n    92→  }\n    93→\n    94→  function handleDeleteDeal() {\n    95→    if (!deleteConfirm) return\n    96→    dispatch(deleteDeal(deleteConfirm)).then(() => {\n    97→      setDeleteConfirm(null)\n    98→      loadDeals()\n    99→    })\n   100→  }\n   101→\n   102→  function handlePageChange(newPage: number) {\n   103→    dispatch(setDealsPage(newPage))\n   104→  }\n   105→\n   106→  // Also fetch all clients for the create modal (not just ones with deals)\n   107→  const [allClients, setAllClients] = useState<{ id: string; name: string }[]>([])\n   108→  useEffect(() => {\n   109→    fetch('/.netlify/functions/clients?pageSize=1000')\n   110→      .then((res) => res.json())\n   111→      .then((data: { clients: { id: string; name: string }[] }) => {\n   112→        setAllClients(data.clients.map((c) => ({ id: c.id, name: c.name })))\n   113→      })\n   114→      .catch(() => {})\n   115→  }, [])\n   116→\n   117→  return (\n   118→    <div className=\"p-6 max-sm:p-3\" data-testid=\"deals-list-page\">\n   119→      <DealsPageHeader\n   120→        searchValue={searchInput}\n   121→        onSearchChange={setSearchInput}\n   122→        onCreateDeal={() => setCreateModalOpen(true)}\n   123→        onImport={() => setImportDialogOpen(true)}\n   124→      />\n   125→\n   126→      <DealsSummaryCards\n   127→        totalActive={Number(metrics.total_active)}\n   128→        pipelineValue={Number(metrics.pipeline_value)}\n   129→        wonCount={Number(metrics.won_count)}\n   130→        wonValue={Number(metrics.won_value)}\n   131→        lostCount={Number(metrics.lost_count)}\n   132→        lo