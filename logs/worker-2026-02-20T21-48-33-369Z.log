9→          />\n   180→        </div>\n   181→      )}\n   182→\n   183→      <CreateDealModal\n   184→        open={createModalOpen}\n   185→        availableClients={allClients}\n   186→        availableUsers={availableUsers}\n   187→        onClose={() => setCreateModalOpen(false)}\n   188→        onSave={handleCreateDeal}\n   189→      />\n   190→\n   191→      <ConfirmDialog\n   192→        open={deleteConfirm !== null}\n   193→        title=\"Delete Deal\"\n   194→        message=\"Are you sure you want to delete this deal? This action cannot be undone.\"\n   195→        confirmLabel=\"Delete\"\n   196→        onConfirm={handleDeleteDeal}\n   197→        onCancel={() => setDeleteConfirm(null)}\n   198→      />\n   199→\n   200→      {importDialogOpen && (\n   201→        <ImportDialog\n   202→          entityName=\"Deal\"\n   203→          entityNamePlural=\"Deals\"\n   204→          columns={DEAL_CSV_COLUMNS}\n   205→          headerMap={DEAL_HEADER_MAP}\n   206→          templateFilename=\"deals-import-template.csv\"\n   207→          templateExample=\"Enterprise License,Acme Corp,50000,proposal,Jane Smith,75,2024-06-15,active\"\n   208→          apiEndpoint=\"/.netlify/functions/deals?action=import\"\n   209→          apiBodyKey=\"deals\"\n   210→          onClose={() => setImportDialogOpen(false)}\n   211→          onImported={loadDeals}\n   212→        />\n   213→      )}\n   214→    </div>\n   215→  )\n   216→}\n   217→\n   218→const DEAL_CSV_COLUMNS = [\n   219→  { name: 'Name', required: true, description: 'Deal name' },\n   220→  { name: 'Client Name', required: true, description: 'Name of an existing client' },\n   221→  { name: 'Value', required: false, description: 'Deal value in dollars (default: 0)' },\n   222→  { name: 'Stage', required: false, description: '\"lead\", \"qualification\", \"discovery\", \"proposal\", \"negotiation\", \"closed_won\", or \"closed_lost\" (default: lead)' },\n   223→  { name: 'Owner', required: false, description: 'De