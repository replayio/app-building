[2026-02-23T18:48:12.580Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01V3Bd2EgqqecELUGSFs2xwG","type":"tool_result","content":"     1→/**\n     2→ * deploy script: Creates and/or syncs the production Neon database,\n     3→ * creates and/or updates the Netlify site, builds, and deploys.\n     4→ *\n     5→ * Project info (NEON_PROJECT_ID, DATABASE_URL, NETLIFY_SITE_ID) is stored\n     6→ * in .env so subsequent runs reuse existing resources.\n     7→ *\n     8→ * Usage: npm run deploy\n     9→ */\n    10→\n    11→import { execSync } from 'child_process'\n    12→import { readFileSync, writeFileSync, existsSync } from 'fs'\n    13→import { resolve, dirname } from 'path'\n    14→import { fileURLToPath } from 'url'\n    15→import { initSchema } from './schema.ts'\n    16→import { runMigrations } from './migrate-db.ts'\n    17→\n    18→const __filename = fileURLToPath(import.meta.url)\n    19→const __dirname = dirname(__filename)\n    20→const APP_DIR = resolve(__dirname, '..')\n    21→const ENV_PATH = resolve(APP_DIR, '.env')\n    22→\n    23→function readEnvFile(): Record<string, string> {\n    24→  const result: Record<string, string> = {}\n    25→  if (!existsSync(ENV_PATH)) return result\n    26→  const content = readFileSync(ENV_PATH, 'utf-8')\n    27→  for (const line of content.split('\\n')) {\n    28→    const match = line.match(/^([^#=]+)=(.*)$/)\n    29→    if (match) result[match[1].trim()] = match[2].trim()\n    30→  }\n    31→  return result\n    32→}\n    33→\n    34→function writeEnvValue(key: string, value: string) {\n    35→  const content = existsSync(ENV_PATH) ? readFileSync(ENV_PATH, 'utf-8') : ''\n    36→  const regex = new RegExp(`^${key}=.*$`, 'm')\n    37→  if (regex.test(content)) {\n    38→    writeFileSync(ENV_PATH, content.replace(regex, `${key}=${value}`))\n    39→  } else {\n    40→    writeFileSync(ENV_PATH, content + (content.endsWith('\\n') ? '' : '\\n') + `${key}=${value}\\n`)\n    41→  }\n    42→}\n    43→\n    44→function requireEnv(name: string): string {\n    45→  const val = process.env[name]\n    46→  if (!val) {\n    47→    console.error(`Error: ${name} environment variable is required`)\n    48→    process.exit(1)\n    49→  }\n    50→  return val\n    51→}\n    52→\n    53→async function ensureNeonProject(neonApiKey: string): Promise<{ projectId: string; databaseUrl: string }> {\n    54→  const envVars = readEnvFile()\n    55→\n    56→  if (envVars.NEON_PROJECT_ID && envVars.DATABASE_URL) {\n    57→    console.log(`Using existing Neon project: ${envVars.NEON_PROJECT_ID}`)\n    58→    return { projectId: envVars.NEON_PROJECT_ID, databaseUrl: envVars.DATABASE_URL }\n    59→  }\n    60→\n    61→  console.log('Creating new Neon project...')\n    62→  const res = await fetch('https://console.neon.tech/api/v2/projects', {\n    63→    method: 'POST',\n    64→    headers: {\n    65→      'Authorization': `Bearer ${neonApiKey}`,\n    66→      'Content-Type': 'application/json',\n    67→    },\n    68→    body: JSON.stringify({\n    69→      project: {\n    70→        name: `sales-crm-${Date.now()}`,\n    71→        pg_version: 16,\n    72→      },\n    73→    }),\n    74→  })\n    75→\n    76→  if (!res.ok) {\n    77→    const err = await res.text()\n    78→    console.error(`Failed to create Neon project: ${res.status} ${err}`)\n    79→    process.exit(1)\n    80→  }\n    81→\n    82→  const data = await res.json() as {\n    83→    project: { id: string }\n    84→    connection_uris: Array<{ connection_uri: string }>\n    85→  }\n    86→\n    87→  const projectId = data.project.id\n    88→  const databaseUrl = data.connection_uris[0].connection_uri\n    89→\n    90→  writeEnvValue('NEON_PROJECT_ID', projectId)\n    91→  writeEnvValue('DATABASE_URL', databaseUrl)\n    92→  console.log(`Created Neon project: ${projectId}`)\n    93→\n    94→  return { projectId, databaseUrl }\n    95→}\n    96→\n    97→async function ensureNetlifySite(netlifyAccountSlug: string): Promise<string> {\n    98→  const envVars = readEnvFile()\n    99→\n   100→  if (envVars.NETLIFY_SITE_ID) {\n   101→    console.log(`Using existing Netlify site: ${envVars.NETLIFY_SITE_ID}`)\n   102→    return envVars.NETLIFY_SITE_ID\n   103→  }\n   104→\n   105→  console.log('Creating new Netlify site...')\n   106→  const netlifyToken = process.env.NETLIFY_AUTH_TOKEN!\n   107→  const res = await fetch('https://api.netlify.com/api/v1/sites', {\n   108→    method: 'POST',\n   109→    headers: {\n   110→      'Authorization': `Bearer ${netlifyToken}`,\n   111→      'Content-Type': 'application/json',\n   112→    },\n   113→    body: JSON.stringify({\n   114→      name: `sales-crm-${Date.now()}`,\n   115→      account_slug: netlifyAccountSlug,\n   116→    }),\n   117→  })\n   118→\n   119→  if (!res.ok) {\n   120→    const err = await res.text()\n   121→    console.error(`Failed to create Netlify site: ${res.status} ${err}`)\n   122→    process.exit(1)\n   123→  }\n   124→\n   125→  const siteData = await res.json() as { id: string; ssl_url?: string; url?: string }\n   126→  const siteId = siteData.id\n   127→\n   128→  writeEnvValue('NETLIFY_SITE_ID', siteId)\n   129→  console.log(`Created Netlify site: ${siteId}`)\n   130→\n   131→  return siteId\n   132→}\n   133→\n   134→async function main() {\n   135→  const neonApiKey = requireEnv('NEON_API_KEY')\n   136→  requireEnv('NETLIFY_AUTH_TOKEN')\n   137→  const netlifyAccountSlug = requireEnv('NETLIFY_ACCOUNT_SLUG')\n   138→\n   139→  // Step 1-2: Database setup and schema sync\n   140→  const { databaseUrl } = await ensureNeonProject(neonApiKey)\n   141→\n   142→  console.log('Syncing production database schema...')\n   143→  await initSchema(databaseUrl)\n   144→  await runMigrations(databaseUrl)\n   145→  console.log('Schema sync complete.')\n   146→\n   147→  // Step 3: Netlify site setup\n   148→  const siteId = await ensureNetlifySite(netlifyAccountSlug)\n   149→\n   150→  // Step 4: Build\n   151→  console.log('Building app...')\n   152→  execSync('npx vite build', { stdio: 'inherit', cwd: APP_DIR })\n   153→\n   154→  // Step 5: Deploy\n   155→  console.log('Deploying to Netlify...')\n   156→  const deployOutput = execSync(\n   157→    `npx netlify deploy --prod --dir dist --functions ./netlify/functions --site ${siteId} --json`,\n   158→    { cwd: APP_DIR, encoding: 'utf-8' },\n   159→  )\n   160→\n   161→  const deployData = JSON.parse(deployOutput) as { ssl_url?: string; deploy_ssl_url?: string; url?: string }\n   162→  const deployedUrl = deployData.ssl_url || deployData.deploy_ssl_url || deployData.url || ''\n   163→\n   164→  // Step 6: Write deployment.txt\n   165→  const deploymentInfo = [\n   166→    `url=${deployedUrl}`,\n   167→    `site_id=${siteId}`,\n   168→    `deployed_at=${new Date().toISOString()}`,\n   169→  ].join('\\n') + '\\n'\n   170→\n   171→  writeFileSync(resolve(APP_DIR, 'deployment.txt'), deploymentInfo)\n   172→  console.log(`\\nDeployment complete: ${deployedUrl}`)\n   173→  console.log('Deployment info written to deployment.txt')\n   174→}\n   175→\n   176→main().catch((err) => {\n   177→  console.error('Deployment failed:', err)\n   178→  process.exit(1)\n   179→})\n   180→"}]},"parent_tool_use_id":"toolu_01AGKFNkxXcJ6DnyUXSq7Adn","session_id":"43e5740d-520f-43b8-8281-b08f938db847","uuid":"3512e428-76c5-49e5-b470-898ac6242fbf"}
