med || trimmed.startsWith(\"#\")) continue;\n    49→    const eqIndex = trimmed.indexOf(\"=\");\n    50→    if (eqIndex === -1) continue;\n    51→    const key = trimmed.slice(0, eqIndex).trim();\n    52→    let value = trimmed.slice(eqIndex + 1).trim();\n    53→    if ((value.startsWith('\"') && value.endsWith('\"')) || (value.startsWith(\"'\") && value.endsWith(\"'\"))) {\n    54→      value = value.slice(1, -1);\n    55→    }\n    56→    vars[key] = value;\n    57→  }\n    58→  return vars;\n    59→}\n    60→\n    61→function loadRequiredEnvVars(projectRoot: string): string[] {\n    62→  const examplePath = resolve(projectRoot, \".env.example\");\n    63→  if (!existsSync(examplePath)) return [];\n    64→  const content = readFileSync(examplePath, \"utf-8\");\n    65→  return content\n    66→    .split(\"\\n\")\n    67→    .map((line) => line.trim())\n    68→    .filter((line) => line && !line.startsWith(\"#\") && !line.includes(\"# optional\"))\n    69→    .map((line) => line.split(\"=\")[0].trim())\n    70→    .filter((key) => key.length > 0);\n    71→}\n    72→\n    73→function findFreePort(): number {\n    74→  let port = 3100;\n    75→  try {\n    76→    // With --network host, docker doesn't track port mappings.\n    77→    // Check actual listening ports on the host via ss.\n    78→    const out = execFileSync(\"ss\", [\"-tlnH\"], {\n    79→      encoding: \"utf-8\",\n    80→      timeout: 5000,\n    81→    });\n    82→    const usedPorts = new Set<number>();\n    83→    for (const match of out.matchAll(/:(\\d+)\\s/g)) {\n    84→      usedPorts.add(parseInt(match[1], 10));\n    85→    }\n    86→    while (usedPorts.has(port)) port++;\n    87→  } catch {\n    88→    // ss not available, just use default\n    89→  }\n    90→  return port;\n    91→}\n    92→\n    93→function registerContainer(state: AgentState): void {\n    94→  logContainer(state);\n    95→}\n    96→\n    97→export interface StartContainerOptions {\n    98→  repoUrl: string;\n    99→  cloneB