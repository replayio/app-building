\n    52→function setupMockApps(apps: AppEntry[]) {\n    53→  return async (page: import('@playwright/test').Page) => {\n    54→    await page.route('**/.netlify/functions/apps', (route) => {\n    55→      route.fulfill({\n    56→        status: 200,\n    57→        contentType: 'application/json',\n    58→        body: JSON.stringify(apps),\n    59→      })\n    60→    })\n    61→  }\n    62→}\n    63→\n    64→// ──────────────────────────────────────────────────────────────────────────────\n    65→// StatusFilter\n    66→// ──────────────────────────────────────────────────────────────────────────────\n    67→\n    68→test.describe('StatusFilter', () => {\n    69→  test('StatusFilter: Default tab is Building', async ({ page }) => {\n    70→    await setupMockApps(mockApps)(page)\n    71→    await page.goto('/')\n    72→\n    73→    // StatusFilter is visible\n    74→    await expect(page.getByTestId('status-filter')).toBeVisible({ timeout: 10000 })\n    75→\n    76→    // Three tabs are present\n    77→    await expect(page.getByTestId('status-filter-tab-building')).toBeVisible()\n    78→    await expect(page.getByTestId('status-filter-tab-finished')).toBeVisible()\n    79→    await expect(page.getByTestId('status-filter-tab-queued')).toBeVisible()\n    80→\n    81→    // Building tab is active (has the active class)\n    82→    await expect(page.getByTestId('status-filter-tab-building')).toHaveClass(\n    83→      /status-filter__tab--active/\n    84→    )\n    85→\n    86→    // Finished and Queued are NOT active\n    87→    await expect(page.getByTestId('status-filter-tab-finished')).not.toHaveClass(\n    88→      /status-filter__tab--active/\n    89→    )\n    90→    await expect(page.getByTestId('status-filter-tab-queued')).not.toHaveClass(\n    91→      /status-filter__tab--active/\n    92→    )\n    93→\n    94→    // Grid shows only building apps\n    95→    const cards = page.getByTestId('app-card')\n    96→    await expect(cards).toHaveCount(2, { timeout: 10000 })\n    97→  })\n    98→\n    99→  test('StatusFilter: Switch to Finished tab', async ({ page }) => {\n   100→    await setupMockApps(mockApps)(page)\n   101→    await page.goto('/')\n   102→    await expect(page.getByTestId('status-filter')).toBeVisible({ timeout: 10000 })\n   103→\n   104→    // Initially building is active\n   105→    await expect(page.getByTestId('status-filter-tab-building')).toHaveClass(\n   106→      /status-filter__tab--active/\n   107→    )\n   108→\n   109→    // Click Finished tab\n   110→    await page.getByTestId('status-filter-tab-finished').click()\n   111→\n   112→    // Finished is now active, Building is not\n   113→    await expect(page.getByTestId('status-filter-tab-finished')).toHaveClass(\n   114→      /status-filter__tab--active/\n   115→    )\n   116→    await expect(page.getByTestId('status-filter-tab-building')).not.toHaveClass(\n   117→      /status-filter__tab--active/\n   118→    )\n   119→\n   120→    // Grid shows only finished apps\n   121→    const cards = page.getByTestId('app-card')\n   122→    await expect(cards).toHaveCount(2, { timeout: 10000 })\n   123→\n   124→    // Verify the cards are finished apps (not building ones)\n   125→    await expect(page.getByTestId('app-card-title').first()).toContainText('Analytics Portal')\n   126→  })\n   127→\n   128→  test('StatusFilter: Switch to Queued tab', async ({ page }) => {\n   129→    // Include a queued app for this test\n   130→    const appsWithQueued: AppEntry[] = [\n   131→      ...mockApps,\n   132→      {\n   133→        id: 'queued-app',\n   134→        name: 'Queued App',\n   135→        description: 'An app waiting to be built.',\n   136→        status: 'queued',\n   137→        progress: 0,\n   138→        created_at: '2026-02-28T00:00:00Z',\n   139→        model: null,\n   140→        deployment_url: null,\n   141→        source_url: null,\n   142→      },\n   143→    ]\n   144→    await setupMockApps(appsWithQueued)(page)\n   145→    await page.