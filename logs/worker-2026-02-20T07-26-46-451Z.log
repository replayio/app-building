n   315→        break;\n   316→      }\n   317→    }\n   318→\n   319→    if (!sourceStage || !dealId) return;\n   320→\n   321→    // Pick a target column that's different from source\n   322→    const targetStage = stages.find((s) => s !== sourceStage) ?? 'proposal';\n   323→    const targetColumn = page.getByTestId(`pipeline-column-${targetStage}`);\n   324→    const sourceCard = page.getByTestId(`pipeline-deal-card-${dealId}`);\n   325→\n   326→    // Perform drag and drop with retry — dragTo can be unreliable\n   327→    await expect(async () => {\n   328→      // Re-locate the source card (it may have moved after a reload)\n   329→      const card = page.getByTestId(`pipeline-deal-card-${dealId}`);\n   330→      const cardInTarget = page.getByTestId(`pipeline-column-${targetStage}`).getByTestId(`pipeline-deal-card-${dealId}`);\n   331→\n   332→      if (await cardInTarget.count() === 0) {\n   333→        await card.dragTo(targetColumn);\n   334→        await page.waitForLoadState('networkidle');\n   335→      }\n   336→\n   337→      await expect(cardInTarget).toBeVisible();\n   338→    }).toPass({ timeout: 15000 });\n   339→  });\n   340→});\n   341→\n   342→test.describe('DealsListPage - FilterControls (DLP-FLT)', () => {\n   343→  test('DLP-FLT-01: Stage filter shows all stage options', async ({ page }) => {\n   344→    await page.goto('/deals');\n   345→    await page.waitForLoadState('networkidle');\n   346→\n   347→    const stageFilter = page.getByTestId('deals-filter-stage');\n   348→    await expect(stageFilter).toBeVisible();\n   349→\n   350→    // Open the dropdown menu\n   351→    await page.getByTestId('deals-filter-stage-trigger').click();\n   352→    const menu = page.getByTestId('deals-filter-stage-menu');\n   353→    await expect(menu).toBeVisible();\n   354→\n   355→    // Check that all stage options are present\n   356→    const optionTexts = await menu.locator('button').allTextContents();\n   357→\n   358→    expect(optionTexts.join(','))