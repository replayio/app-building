Import Test Deal')).toBeVisible({ timeout: 5000 });\n   161→  });\n   162→});\n   163→\n   164→test.describe('DealsListPage - SummaryCards (DLP-SUM)', () => {\n   165→  test('DLP-SUM-01: Summary cards display correct metrics', async ({ page }) => {\n   166→    await page.goto('/deals');\n   167→    await page.waitForLoadState('networkidle');\n   168→\n   169→    const summaryCards = page.getByTestId('deals-summary-cards');\n   170→    await expect(summaryCards).toBeVisible();\n   171→\n   172→    // Four summary cards should be visible\n   173→    await expect(page.getByTestId('summary-card-total-active-deals')).toBeVisible();\n   174→    await expect(page.getByTestId('summary-card-pipeline-value')).toBeVisible();\n   175→    await expect(page.getByTestId('summary-card-won-quarter')).toBeVisible();\n   176→    await expect(page.getByTestId('summary-card-lost-quarter')).toBeVisible();\n   177→\n   178→    // Labels are correct\n   179→    await expect(page.getByTestId('summary-card-label-total-active-deals')).toHaveText('Total Active Deals');\n   180→    await expect(page.getByTestId('summary-card-label-pipeline-value')).toHaveText('Pipeline Value');\n   181→    await expect(page.getByTestId('summary-card-label-won-quarter')).toHaveText('Won (Quarter)');\n   182→    await expect(page.getByTestId('summary-card-label-lost-quarter')).toHaveText('Lost (Quarter)');\n   183→\n   184→    // Values are present (not empty)\n   185→    const totalActiveValue = await page.getByTestId('summary-card-value-total-active-deals').textContent();\n   186→    expect(totalActiveValue!.length).toBeGreaterThan(0);\n   187→\n   188→    const pipelineValue = await page.getByTestId('summary-card-value-pipeline-value').textContent();\n   189→    expect(pipelineValue).toContain('$');\n   190→  });\n   191→\n   192→  test('DLP-SUM-02: Summary cards update when deals are added', async ({ page }) => {\n   193→    await page.goto('/deals');\n   194→    await page.waitForLoadState('networkidle');\n 