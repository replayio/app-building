tion.css\";\n@import \"@shared/components/ConfirmDialog.css\";\n@import \"@shared/components/DocumentsSection.css\";\n@import \"@shared/components/FilterSelect.css\";\n\n/* App-specific variables */\n:root {\n  /* Supplier status colors */\n  --status-active-bg: #e6ede8;\n  --status-active-text: var(--status-success);\n  --status-inactive-bg: #f0eeea;\n  --status-inactive-text: var(--text-muted);\n  --status-on-hold-bg: #f5f0e3;\n  --status-on-hold-text: var(--status-warning);\n  --status-suspended-bg: #f5ece6;\n  --status-suspended-text: var(--status-error);\n\n  /* Order status colors */\n  --status-pending-bg: #f5f0e3;\n  --status-pending-text: var(--status-warning);\n  --status-in-transit-bg: #e3ecf0;\n  --status-in-transit-text: var(--status-info);\n  --status-fulfilled-bg: #e6ede8;\n  --status-fulfilled-text: var(--status-success);\n  --status-processing-bg: #f5ece6;\n  --status-processing-text: #b07a3e;\n  --status-completed-bg: #e6ede8;\n  --status-completed-text: var(--status-success);\n  --status-cancelled-bg: #f5ece6;\n  --status-cancelled-text: var(--status-error);\n  --status-approved-bg: #e6ede8;\n  --status-approved-text: var(--status-success);\n  --status-shipped-bg: #e3ecf0;\n  --status-shipped-text: var(--status-info);\n  --status-delivered-bg: #f0eeea;\n  --status-delivered-text: var(--text-muted);\n\n  /* Card styles */\n  --card-bg: #ffffff;\n  --card-border: var(--bg-border-color-light);\n}\n\n/* App shell layout (top nav + main content) */\n.app-shell {\n  display: flex;\n  flex-direction: column;\n  min-height: 100vh;\n}\n\n.app-main {\n  flex: 1;\n  min-width: 0;\n  display: flex;\n  flex-direction: column;\n}\n\n/* Page content */\n.page-content {\n  flex: 1;\n  padding: 24px;\n}\n\n@media (max-width: 640px) {\n  .page-content {\n    padding: 12px;\n  }\n}\n\n/* Page header */\n.page-header {\n  display: flex;\n  justify-content: space-between;\n  align-items: flex-start;\n  margin-bottom: 24px;\n}\n\n.page-title {\n  font-size: 24px;\n  fo