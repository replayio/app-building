yTestId('deal-header-change-stage-button');\n   440→    await expect(changeStageBtn).not.toBeDisabled({ timeout: 5000 });\n   441→    await changeStageBtn.click();\n   442→    await page.waitForLoadState('networkidle');\n   443→    // Wait for the stage change to be processed\n   444→    await page.waitForTimeout(500);\n   445→\n   446→    // Navigate back to client detail to check timeline\n   447→    await gotoClientDetail(page, clientId);\n   448→\n   449→    // Wait for timeline entries to load and verify at least one stage change entry exists\n   450→    await expect(\n   451→      page.locator('[data-testid^=\"timeline-entry-\"]').filter({ hasText: /stage/i }).first()\n   452→    ).toBeVisible({ timeout: 10000 });\n   453→  });\n   454→\n   455→  test('ATOM-03: Single status change produces exactly one timeline entry', async ({ page }) => {\n   456→    await navigateToFirstClientDetail(page);\n   457→\n   458→    // Verify the client detail page loaded (navigateToFirstClientDetail handles retries)\n   459→    await expect(page.getByTestId('status-dropdown-button')).toBeVisible({ timeout: 10000 });\n   460→\n   461→    // Get current status and determine the target status\n   462→    const currentStatus = await page.getByTestId('client-header-status-badge').textContent();\n   463→    const statusMap: Record<string, string> = {\n   464→      'Active': 'inactive',\n   465→      'Inactive': 'active',\n   466→      'Prospect': 'active',\n   467→      'Churned': 'active',\n   468→    };\n   469→    const targetStatus = statusMap[currentStatus?.trim() || 'Active'] || 'inactive';\n   470→\n   471→    // Change client status\n   472→    await page.getByTestId('status-dropdown-button').click();\n   473→    await expect(page.getByTestId('status-dropdown-options')).toBeVisible();\n   474→    await page.getByTestId(`status-dropdown-option-${targetStatus}`).click();\n   475→    await page.waitForLoadState('networkidle');\n   476→\n   477→    // Wait a moment for any potential duplicate API calls\n   478→    await page.waitForTimeout(500);\n   479→\n   480→    // Reload to get fresh timeline data\n   481→    await page.reload();\n   482→    await page.waitForLoadState('networkidle');\n   483→\n   484→    // Verify a timeline entry references this specific status change\n   485→    // Timeline format: \"Status Changed: from 'active' to 'inactive'\"\n   486→    const matchingEntries = page.locator('[data-testid^=\"timeline-entry-\"]', {\n   487→      hasText: new RegExp(`Status Changed:.*from '${currentStatus?.trim().toLowerCase()}' to '${targetStatus}'`)\n   488→    });\n   489→\n   490→    await expect(matchingEntries.first()).toBeVisible({ timeout: 10000 });\n   491→  });\n   492→});\n   493→"}]},"parent_tool_use_id":"toolu_013TjSBQi1c9MQVRidjGMQyz","session_id":"8402aee7-3fe8-4bee-a470-0a89bff784ad","uuid":"0fc73853-f7f2-48eb-95fc-36d86c1cfdf7"}
