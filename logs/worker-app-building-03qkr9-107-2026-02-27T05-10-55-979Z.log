→    await expect(value).toHaveText(/[A-Z][a-z]{2} \\d{1,2}, \\d{4}/);\n    78→  });\n    79→\n    80→  test(\"Display Expected Delivery Date\", async ({ page }) => {\n    81→    await navigateToFirstOrder(page);\n    82→\n    83→    const deliveryField = page.getByTestId(\"order-field-expected-delivery\");\n    84→    await expect(deliveryField).toBeVisible();\n    85→\n    86→    // Should display label \"Expected Delivery\"\n    87→    await expect(deliveryField.locator(\".order-summary-field-label\")).toHaveText(\"Expected Delivery\");\n    88→\n    89→    // Should display a formatted date value\n    90→    const value = deliveryField.locator(\".order-summary-field-value\");\n    91→    await expect(value).not.toBeEmpty();\n    92→    await expect(value).toHaveText(/[A-Z][a-z]{2} \\d{1,2}, \\d{4}/);\n    93→  });\n    94→\n    95→  test(\"Display Status Badge\", async ({ page }) => {\n    96→    await navigateToFirstOrder(page);\n    97→\n    98→    const statusField = page.getByTestId(\"order-field-status\");\n    99→    await expect(statusField).toBeVisible();\n   100→    await expect(statusField.locator(\".order-summary-field-label\")).toHaveText(\"Status\");\n   101→\n   102→    const badge = page.getByTestId(\"order-status-badge\");\n   103→    await expect(badge).toBeVisible();\n   104→\n   105→    // Badge should contain an SVG icon\n   106→    await expect(badge.locator(\"svg\")).toBeVisible();\n   107→\n   108→    // Badge text should be one of the known statuses\n   109→    const badgeText = await badge.textContent();\n   110→    const validStatuses = [\"Pending\", \"Approved\", \"Shipped\", \"Delivered\", \"Cancelled\", \"In Transit\", \"Fulfilled\", \"Processing\"];\n   111→    const found = validStatuses.some((s) => badgeText?.includes(s));\n   112→    expect(found).toBeTruthy();\n   113→  });\n   114→\n   115→  test(\"Display Overall Cost\", async ({ page }) => {\n   116→    await navigateToFirstOrder(page);\n   117→\n   118→    const costField = 