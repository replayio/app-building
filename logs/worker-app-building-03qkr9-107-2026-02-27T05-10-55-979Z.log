[2026-02-27T04:55:43.792Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01SpRDW5oB43AJQ4frogSM3b","type":"tool_result","content":"     1→import { neon } from \"@neondatabase/serverless\";\n     2→import type { Context } from \"@netlify/functions\";\n     3→\n     4→export default async function handler(req: Request, _context: Context) {\n     5→  const dbUrl = process.env.DATABASE_URL;\n     6→  if (!dbUrl) {\n     7→    return new Response(JSON.stringify({ error: \"DATABASE_URL not set\" }), {\n     8→      status: 500,\n     9→      headers: { \"Content-Type\": \"application/json\" },\n    10→    });\n    11→  }\n    12→\n    13→  const sql = neon(dbUrl);\n    14→  const url = new URL(req.url);\n    15→  const segments = url.pathname.split(\"/\").filter(Boolean);\n    16→  const orderId = segments[3] || null;\n    17→\n    18→  if (req.method === \"GET\") {\n    19→    try {\n    20→      if (orderId) {\n    21→        const rows = await sql`\n    22→          SELECT o.*, s.name AS supplier_name\n    23→          FROM orders o\n    24→          LEFT JOIN suppliers s ON s.id = o.supplier_id\n    25→          WHERE o.id = ${orderId}::uuid\n    26→          LIMIT 1\n    27→        `;\n    28→        if (rows.length === 0) {\n    29→          return new Response(JSON.stringify({ error: \"Order not found\" }), {\n    30→            status: 404,\n    31→            headers: { \"Content-Type\": \"application/json\" },\n    32→          });\n    33→        }\n    34→        return new Response(JSON.stringify(rows[0]), {\n    35→          status: 200,\n    36→          headers: { \"Content-Type\": \"application/json\" },\n    37→        });\n    38→      }\n    39→\n    40→      const supplierId = url.searchParams.get(\"supplierId\");\n    41→\n    42→      if (supplierId) {\n    43→        const rows = await sql`\n    44→          SELECT o.*, s.name AS supplier_name\n    45→          FROM orders o\n    46→          LEFT JOIN suppliers s ON s.id = o.supplier_id\n    47→          WHERE o.supplier_id = ${supplierId}::uuid\n    48→          ORDER BY o.order_date DESC\n    49→        `;\n    50→        return new Response(JSON.stringify(rows), {\n    51→          status: 200,\n    52→          headers: { \"Content-Type\": \"application/json\" },\n    53→        });\n    54→      }\n    55→\n    56→      const rows = await sql`\n    57→        SELECT o.*, s.name AS supplier_name\n    58→        FROM orders o\n    59→        LEFT JOIN suppliers s ON s.id = o.supplier_id\n    60→        ORDER BY o.order_date DESC\n    61→      `;\n    62→      return new Response(JSON.stringify(rows), {\n    63→        status: 200,\n    64→        headers: { \"Content-Type\": \"application/json\" },\n    65→      });\n    66→    } catch (err) {\n    67→      console.error(\"Orders GET error:\", err);\n    68→      return new Response(JSON.stringify({ error: \"Internal server error\" }), {\n    69→        status: 500,\n    70→        headers: { \"Content-Type\": \"application/json\" },\n    71→      });\n    72→    }\n    73→  }\n    74→\n    75→  if (req.method === \"POST\") {\n    76→    try {\n    77→      const body = await req.json();\n    78→      const { supplier_id, order_date, expected_delivery, line_items } = body;\n    79→\n    80→      if (!supplier_id) {\n    81→        return new Response(JSON.stringify({ error: \"supplier_id is required\" }), {\n    82→          status: 400,\n    83→          headers: { \"Content-Type\": \"application/json\" },\n    84→        });\n    85→      }\n    86→\n    87→      // Generate a unique order ID (PO-XXXXXX format)\n    88→      const countRows = await sql`SELECT COUNT(*)::int AS cnt FROM orders`;\n    89→      const count = (countRows[0]?.cnt as number) || 0;\n    90→      const generatedOrderId = `PO-${String(count + 1).padStart(6, \"0\")}`;\n    91→\n    92→      // Calculate subtotal from line items\n    93→      const items = Array.isArray(line_items) ? line_items : [];\n    94→      let subtotal = 0;\n    95→      for (const item of items) {\n    96→        subtotal += (item.quantity || 0) * (item.unit_price || 0);\n    97→      }\n    98→      const totalCost = subtotal;\n    99→\n   100→      const orderRows = await sql`\n   101→        INSERT INTO orders (order_id, supplier_id, order_date, expected_delivery, subtotal, total_cost)\n   102→        VALUES (\n   103→          ${generatedOrderId},\n   104→          ${supplier_id}::uuid,\n   105→          ${order_date || null},\n   106→          ${expected_delivery || null},\n   107→          ${subtotal},\n   108→          ${totalCost}\n   109→        )\n   110→        RETURNING *\n   111→      `;\n   112→      const order = orderRows[0];\n   113→\n   114→      // Insert line items\n   115→      for (const item of items) {\n   116→        const lineTotal = (item.quantity || 0) * (item.unit_price || 0);\n   117→        await sql`\n   118→          INSERT INTO order_line_items (order_id, sku, item_name, quantity, unit_price, line_total)\n   119→          VALUES (\n   120→            ${order.id}::uuid,\n   121→            ${item.sku || \"\"},\n   122→            ${item.item_name || \"\"},\n   123→            ${item.quantity || 0},\n   124→            ${item.unit_price || 0},\n   125→            ${lineTotal}\n   126→          )\n   127→        `;\n   128→      }\n   129→\n   130→      // Add history entry for creation\n   131→      await sql`\n   132→        INSERT INTO order_history (order_id, event_type, description, actor)\n   133→        VALUES (\n   134→          ${order.id}::uuid,\n   135→          'created',\n   136→          ${\"Order \" + generatedOrderId + \" created\"},\n   137→          'System'\n   138→        )\n   139→      `;\n   140→\n   141→      // Re-fetch with supplier name\n   142→      const result = await sql`\n   143→        SELECT o.*, s.name AS supplier_name\n   144→        FROM orders o\n   145→        LEFT JOIN suppliers s ON s.id = o.supplier_id\n   146→        WHERE o.id = ${order.id}::uuid\n   147→        LIMIT 1\n   148→      `;\n   149→\n   150→      return new Response(JSON.stringify(result[0]), {\n   151→        status: 201,\n   152→        headers: { \"Content-Type\": \"application/json\" },\n   153→      });\n   154→    } catch (err) {\n   155→      console.error(\"Orders POST error:\", err);\n   156→      return new Response(JSON.stringify({ error: \"Internal server error\" }), {\n   157→        status: 500,\n   158→        headers: { \"Content-Type\": \"application/json\" },\n   159→      });\n   160→    }\n   161→  }\n   162→\n   163→  if (req.method === \"PUT\") {\n   164→    if (!orderId) {\n   165→      return new Response(JSON.stringify({ error: \"Order ID required\" }), {\n   166→        status: 400,\n   167→        headers: { \"Content-Type\": \"application/json\" },\n   168→      });\n   169→    }\n   170→\n   171→    try {\n   172→      const body = await req.json();\n   173→      const { status, expected_delivery } = body;\n   174→\n   175→      // If status is changing, record it in history\n   176→      if (status) {\n   177→        const existing = await sql`\n   178→          SELECT status, order_id FROM orders WHERE id = ${orderId}::uuid LIMIT 1\n   179→        `;\n   180→        if (existing.length > 0 && existing[0].status !== status) {\n   181→          await sql`\n   182→            INSERT INTO order_history (order_id, event_type, description, actor)\n   183→            VALUES (\n   184→              ${orderId}::uuid,\n   185→              'status_change',\n   186→              ${\"Status changed from \" + existing[0].status + \" to \" + status},\n   187→              'System'\n   188→            )\n   189→          `;\n   190→        }\n   191→      }\n   192→\n   193→      const rows = await sql`\n   194→        UPDATE orders SET\n   195→          status = COALESCE(${status ?? null}, status),\n   196→          expected_delivery = COALESCE(${expected_delivery || null}, expected_delivery),\n   1