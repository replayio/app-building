ounts (category)`;\n    40→\n    41→  // Transactions: each financial transaction (header)\n    42→  await sql`\n    43→    CREATE TABLE IF NOT EXISTS transactions (\n    44→      id UUID PRIMARY KEY DEFAULT gen_random_uuid(),\n    45→      date DATE NOT NULL,\n    46→      description VARCHAR(500) NOT NULL,\n    47→      currency VARCHAR(10) NOT NULL DEFAULT 'USD',\n    48→      created_at TIMESTAMPTZ NOT NULL DEFAULT NOW(),\n    49→      updated_at TIMESTAMPTZ NOT NULL DEFAULT NOW()\n    50→    )\n    51→  `;\n    52→\n    53→  await sql`CREATE INDEX IF NOT EXISTS idx_transactions_date ON transactions (date DESC)`;\n    54→\n    55→  // Transaction entries: double-entry line items for each transaction\n    56→  await sql`\n    57→    CREATE TABLE IF NOT EXISTS transaction_entries (\n    58→      id UUID PRIMARY KEY DEFAULT gen_random_uuid(),\n    59→      transaction_id UUID NOT NULL REFERENCES transactions(id) ON DELETE CASCADE,\n    60→      account_id UUID NOT NULL REFERENCES accounts(id),\n    61→      entry_type VARCHAR(10) NOT NULL CHECK (entry_type IN ('debit', 'credit')),\n    62→      amount NUMERIC(15, 2) NOT NULL CHECK (amount > 0),\n    63→      created_at TIMESTAMPTZ NOT NULL DEFAULT NOW()\n    64→    )\n    65→  `;\n    66→\n    67→  await sql`CREATE INDEX IF NOT EXISTS idx_transaction_entries_transaction ON transaction_entries (transaction_id)`;\n    68→  await sql`CREATE INDEX IF NOT EXISTS idx_transaction_entries_account ON transaction_entries (account_id)`;\n    69→\n    70→  // Tags: free-form labels for transactions\n    71→  await sql`\n    72→    CREATE TABLE IF NOT EXISTS tags (\n    73→      id UUID PRIMARY KEY DEFAULT gen_random_uuid(),\n    74→      name VARCHAR(100) NOT NULL UNIQUE\n    75→    )\n    76→  `;\n    77→\n    78→  // Transaction-tag junction table\n    79→  await sql`\n    80→    CREATE TABLE IF NOT EXISTS transaction_tags (\n    81→      transaction_id UUID NOT NULL REFERENCES transactions(id) ON DELETE CASCADE,\n    82→   