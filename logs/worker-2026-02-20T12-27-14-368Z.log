ealRes.json();\n   633→    const clientId = dealData.client_id;\n   634→\n   635→    // Open add task modal\n   636→    await page.getByTestId('deal-linked-tasks-add-button').click();\n   637→    const modal = page.getByTestId('add-deal-task-modal');\n   638→    await expect(modal).toBeVisible();\n   639→\n   640→    const taskTitle = `Test Task ${Date.now()}`;\n   641→    await page.getByTestId('add-deal-task-title').fill(taskTitle);\n   642→    await page.getByTestId('add-deal-task-save').click();\n   643→\n   644→    await expect(modal).not.toBeVisible();\n   645→    await page.waitForLoadState('networkidle');\n   646→\n   647→    // Task should appear in the linked tasks list\n   648→    const tasksSection = page.getByTestId('deal-linked-tasks-section');\n   649→    await expect(tasksSection).toContainText(taskTitle);\n   650→\n   651→    // Verify exactly one timeline entry was created for this task (side effect)\n   652→    const timelineRes = await page.request.get(`/.netlify/functions/client-timeline?clientId=${clientId}`);\n   653→    const timelineData = await timelineRes.json() as { events: { description: string; event_type: string }[] };\n   654→    const matchingEntries = timelineData.events.filter(\n   655→      (e) => e.event_type === 'task_created' && e.description.includes(taskTitle)\n   656→    );\n   657→    expect(matchingEntries).toHaveLength(1);\n   658→  });\n   659→\n   660→  test('DDP-LTK-04: Checking a task marks it complete', async ({ page }) => {\n   661→    await navigateToFirstDealDetail(page);\n   662→\n   663→    // Create a task to toggle\n   664→    await page.getByTestId('deal-linked-tasks-add-button').click();\n   665→    const modal = page.getByTestId('add-deal-task-modal');\n   666→    await expect(modal).toBeVisible();\n   667→\n   668→    const taskTitle = `Toggle Task ${Date.now()}`;\n   669→    await page.getByTestId('add-deal-task-title').fill(taskTitle);\n   670→    await page.getByTestId('add-deal-task-save').click();\n  