 === \"On Track\") {\n    42→    return (\n    43→      <svg className=\"calendar-event-icon\" width=\"10\" height=\"10\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" strokeWidth=\"3\" strokeLinecap=\"round\" strokeLinejoin=\"round\">\n    44→        <polyline points=\"20 6 9 17 4 12\" />\n    45→      </svg>\n    46→    );\n    47→  }\n    48→  if (status === \"Material Shortage\") {\n    49→    return (\n    50→      <svg className=\"calendar-event-icon\" width=\"10\" height=\"10\" viewBox=\"0 0 24 24\" fill=\"currentColor\">\n    51→        <path d=\"M12 2L1 21h22L12 2zm0 4l7.5 13h-15L12 6z\" />\n    52→        <rect x=\"11\" y=\"10\" width=\"2\" height=\"4\" />\n    53→        <rect x=\"11\" y=\"16\" width=\"2\" height=\"2\" />\n    54→      </svg>\n    55→    );\n    56→  }\n    57→  return null;\n    58→}\n    59→\n    60→function formatEventLabel(run: ProductionRun): string {\n    61→  const product = run.product_name || \"Unknown\";\n    62→  const recipe = run.recipe_name || \"No Recipe\";\n    63→  return `${product} (${recipe}) | ${parseFloat(String(run.planned_quantity))} ${run.unit} | Status: ${run.status}`;\n    64→}\n    65→\n    66→function isSameDay(d1: Date, d2: Date): boolean {\n    67→  return d1.getFullYear() === d2.getFullYear() &&\n    68→    d1.getMonth() === d2.getMonth() &&\n    69→    d1.getDate() === d2.getDate();\n    70→}\n    71→\n    72→function getDaysBetween(start: Date, end: Date): number {\n    73→  const s = new Date(start.getFullYear(), start.getMonth(), start.getDate());\n    74→  const e = new Date(end.getFullYear(), end.getMonth(), end.getDate());\n    75→  return Math.round((e.getTime() - s.getTime()) / (1000 * 60 * 60 * 24));\n    76→}\n    77→\n    78→function getMonthGrid(date: Date): Date[] {\n    79→  const year = date.getFullYear();\n    80→  const month = date.getMonth();\n    81→  const firstDay = new Date(year, month, 1);\n    82→  const startOffset = firstDay.getDay();\n    83→  const gridStart = new Date(year, month, 1 - startOffset);\n    84→  const cells: Date[] = [];\n    85→  for (let i = 0; i < 42; i++) {\n    86→    const d = new Date(gridStart);\n    87→    d.setDate(gridStart.getDate() + i);\n    88→    cells.push(d);\n    89→  }\n    90→  return cells;\n    91→}\n    92→\n    93→function getWeekDates(date: Date): Date[] {\n    94→  const dayOfWeek = date.getDay();\n    95→  const start = new Date(date);\n    96→  start.setDate(date.getDate() - dayOfWeek);\n    97→  const days: Date[] = [];\n    98→  for (let i = 0; i < 7; i++) {\n    99→    const d = new Date(start);\n   100→    d.setDate(start.getDate() + i);\n   101→    days.push(d);\n   102→  }\n   103→  return days;\n   104→}\n   105→\n   106→function formatHour(h: number): string {\n   107→  if (h === 0) return \"12 AM\";\n   108→  if (h < 12) return `${h} AM`;\n   109→  if (h === 12) return \"12 PM\";\n   110→  return `${h - 12} PM`;\n   111→}\n   112→\n   113→function getRunsForDay(runs: ProductionRun[], date: Date): ProductionRun[] {\n   114→  return runs.filter((run) => {\n   115→    const start = new Date(run.start_date);\n   116→    const end = new Date(run.end_date);\n   117→    const startDay = new Date(start.getFullYear(), start.getMonth(), start.getDate());\n   118→    const endDay = new Date(end.getFullYear(), end.getMonth(), end.getDate());\n   119→    const targetDay = new Date(date.getFullYear(), date.getMonth(), date.getDate());\n   120→    return targetDay >= startDay && targetDay <= endDay;\n   121→  });\n   122→}\n   123→\n   124→function getRunsForHour(runs: ProductionRun[], date: Date, hour: number): ProductionRun[] {\n   125→  return runs.filter((run) => {\n   126→    const start = new Date(run.start_date);\n   127→    const end = new Date(run.end_date);\n   128→    const slotStart = new Date(date.getFullYear(), date.getMonth(), date.getDate(), hour);\n   129→    const slotEnd = new Date(date.getFullYear(), date.getMonth(), date.getDate(), hour + 1);\n   130→    return start < slotEnd && end > slotStart;\n   131→  });\n   132→}\n   133→\n   134→interface RescheduleState {\n   135→  runId: string;\n   136→  daysDelta: number;\n   137→  x: number;\n   138→  y: number;\n   139→  targetDate: Date;\n   140→}\n   141→\n   142→export function CalendarGrid({\n   143→  currentDate,\n   144→  view,\n   145→  runs,\n   146→  statusFilter,\n   147→  onEventClick,\n   148→  onReschedule,\n   149→}: CalendarGridProps) {\n   150→  const [, setDragRunId] = useState<string | null>(null);\n   151→  const [dragOverDate, setDragOverDate] = useState<string | null>(null);\n   152→  const [rescheduleConfirm, setRescheduleConfirm] = useState<RescheduleState | null>(null);\n   153→\n   154→  const filteredRuns = statusFilter === \"All\"\n   155→    ? runs\n   156→    : runs.filter((r) => r.status === statusFilter);\n   157→\n   158→  const today = new Date();\n   159→\n   160→  const handleDragStart = useCallback((e: React.DragEvent, runId: string) => {\n   161→    setDragRunId(runId);\n   162→    e.dataTransfer.effectAllowed = \"move\";\n   163→    e.dataTransfer.setData(\"text/plain\", runId);\n   164→  }, []);\n   165→\n   166→  const handleDragOver = useCallback((e: React.DragEvent, dateKey: string) => {\n   167→    e.preventDefault();\n   168→    e.dataTransfer.dropEffect = \"move\";\n   169→    setDragOverDate(dateKey);\n   170→  }, []);\n   171→\n   172→  const handleDragLeave = useCallback(() => {\n   173→    setDragOverDate(null);\n   174→  }, []);\n   175→\n   176→  const handleDrop = useCallback(\n   177→    (e: React.DragEvent, targetDate: Date) => {\n   178→      e.preventDefault();\n   179→      setDragOverDate(null);\n   180→      const runId = e.dataTransfer.getData(\"text/plain\");\n   181→      if (!runId) return;\n   182→\n   183→      const run = runs.find((r) => r.id === runId);\n   184→      if (!run) return;\n   185→\n   186→      const startDate = new Date(run.start_date);\n   187→      const startDay = new Date(startDate.getFullYear(), startDate.getMonth(), startDate.getDate());\n   188→      const target = new Date(targetDate.getFullYear(), targetDate.getMonth(), targetDate.getDate());\n   189→      const daysDelta = getDaysBetween(startDay, target);\n   190→\n   191→      if (daysDelta === 0) {\n   192→        setDragRunId(null);\n   193→        return;\n   194→      }\n   195→\n   196→      setRescheduleConfirm({\n   197→        runId,\n   198→        daysDelta,\n   199→        x: e.clientX,\n   200→        y: e.clientY,\n   201→        targetDate,\n   202→      });\n   203→      setDragRunId(null);\n   204→    },\n   205→    [runs]\n   206→  );\n   207→\n   208→  const confirmReschedule = useCallback(() => {\n   209→    if (!rescheduleConfirm) return;\n   210→    onReschedule(rescheduleConfirm.runId, rescheduleConfirm.daysDelta);\n   211→    setRescheduleConfirm(null);\n   212→  }, [rescheduleConfirm, onReschedule]);\n   213→\n   214→  const cancelReschedule = useCallback(() => {\n   215→    setRescheduleConfirm(null);\n   216→  }, []);\n   217→\n   218→  const formatRescheduleDate = (date: Date): string => {\n   219→    const months = [\"January\",\"February\",\"March\",\"April\",\"May\",\"June\",\"July\",\"August\",\"September\",\"October\",\"November\",\"December\"];\n   220→    return `${months[date.getMonth()]} ${date.getDate()}`;\n   221→  };\n   222→\n   223→  if (view === \"Monthly\") {\n   224→    const cells = getMonthGrid(currentDate);\n   225→    const month = currentDate.getMonth();\n   226→\n   227→    return (\n   228→      <div data-testid=\"calendar-grid\">\n   229→        <div className=\"calendar-grid-monthly\">\n   230→          <div className=\"calendar-grid-header\">\n   231→            {DAY_NAMES.map((name) => (\n   232→              <div key={name} className=\"calendar-day-header\" data-testid={`calendar-header-${name.toLowerCase()}`}>\n   233→                {name}\n   234→              </div>\n   235→            ))}\n   236→          </div>\n   237→          <div className=\"calendar-grid-body\">\n   238→            {cells.map((cellDate, idx) => {\n   239→              const dateKey = `${cellDate.getFullYear()}-${cellDate.getMonth()}-${cellDate.getDate()}`;\n   240→              const isDimmed = cellDate.getMonth() !== month;\n   241→              const isToday = isSameDay(cellDate, today);\n   242→              const isDragOver = dragOverDate === dateKey;\n   243→              const dayRuns = getRunsForDay(filteredRuns, cellDate);\n   244→\n   245→              return (\n   246→                <div\n   247→                  key={idx}\n   248→                  className={`calendar-day-cell${isDimmed ? \" calendar-day-cell--dimmed\" : \"\"}${isToday ? \" calendar-day-cell--today\" : \"\"}${isDragOver ? \" calendar-day-cell--drag-over\" : \"\"}`}\n   249→                  data-testid={`calendar-day-${cellDate.getFullYear()}-${String(cellDate.getMonth() + 1).padStart(2, \"0\")}-${String(cellDate.getDate()).padStart(2, \"0\")}`}\n   250→                  onDragOver={(e) => handleDragOver(e, dateKey)}\n   251→                  onDragLeave={handleDragLeave}\n   252→                  onDrop={(e) => handleDrop(e, cellDate)}\n   253→                >\n   254→                  <div className={`calendar-day-number${isToday ? \" calendar-day-number--today\" : \"\"}`}>\n   255→                    {cellDate.getDate()}\n   256→                  </div>\n   257→                  {dayRuns.map((run) => {\n   258→                    const startDate = new Date(run.start_date);\n   259→                    const endDate = new Date(run.end_date);\n   260→                    const spanDays = getDaysBetween(startDate, endDate) + 1;\n   261→                    const isStart = isSameDay(cellDate, startDate);\n   262→                    const isSpanning = spanDays > 1;\n   263→\n   264→                    // For multi-day events, calculate span width within the current week row\n   265→                    let spanStyle: React.CSSProperties = {};\n   266→                    if (isSpanning && isStart) {\n   267→                      const dayOfWeek = cellDate.getDay();\n   268→                      const remainingInWeek = 7 - dayOfWeek;\n   269→                      const visibleSpan = Math.min(spanDays, remainingInWeek);\n   270→                      spanStyle = {\n   271→                        width: `calc(${visibleSpan * 100}% + ${(visibleSpan - 1) * 1}px)`,\n   272→                        position: \"relative\",\n   273→                        zIndex: 2,\n   274→                      };\n   275→                    }\n   276→\n   277→                    // Only render event on start day (for multi-day) or every day for single-day\n   278→                    if (isSpanning && !isStart) {\n   279→                      // For multi-day, check if this is the start of a week row continuation\n   280→                      if (cellDate.getDay() === 0) {\n   281→                        const endDay = new Date(endDate.getFullYear(), endDate.getMonth(), endDate.getDate());\n   282→                        const cellDay = new Date(cellDate.getFullYear(), cellDate.getMonth(), cellDate.getDate());\n   283→                        const remainingDays = getDaysBetween(cellDay, endDay) + 1;\n   284→                        const visibleSpan = Math.min(remainingDays, 7);\n   285→                        spanStyle = {\n   286→                          width: `calc(${visibleSpan * 100}% + ${(visibleSpan - 1) * 1}px)`,\n   287→                          position: \"relative\",\n   288→                          zIndex: 2,\n   289→                        };\n   290→                      } else {\n   291→                        return null;\n   292→                      }\n   293→                    }\n   294→\n   295→                    return (\n   296→                      <div\n   297→                        key={run.id}\n   298→                        className={`${getEventClass(run.status)}${isSpanning ? \" calendar-event-card--span\" : 