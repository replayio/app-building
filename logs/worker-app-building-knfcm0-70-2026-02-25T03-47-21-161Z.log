cts\n    47→          WHERE recipe_id = ${recipeId}::uuid\n    48→          ORDER BY product_name\n    49→        `;\n    50→\n    51→        const equipment = await sql`\n    52→          SELECT re.id, re.recipe_id, re.equipment_id, e.name AS equipment_name\n    53→          FROM recipe_equipment re\n    54→          JOIN equipment e ON e.id = re.equipment_id\n    55→          WHERE re.recipe_id = ${recipeId}::uuid\n    56→          ORDER BY e.name\n    57→        `;\n    58→\n    59→        return new Response(\n    60→          JSON.stringify({\n    61→            ...recipe,\n    62→            materials,\n    63→            products,\n    64→            equipment,\n    65→          }),\n    66→          {\n    67→            status: 200,\n    68→            headers: { \"Content-Type\": \"application/json\" },\n    69→          }\n    70→        );\n    71→      }\n    72→\n    73→      const rows = await sql`\n    74→        SELECT id, name, product, version, status, description,\n    75→               instructions, created_at, updated_at\n    76→        FROM recipes\n    77→        ORDER BY name\n    78→      `;\n    79→      return new Response(JSON.stringify(rows), {\n    80→        status: 200,\n    81→        headers: { \"Content-Type\": \"application/json\" },\n    82→      });\n    83→    } catch (err) {\n    84→      console.error(\"Recipes GET error:\", err);\n    85→      return new Response(JSON.stringify({ error: \"Internal server error\" }), {\n    86→        status: 500,\n    87→        headers: { \"Content-Type\": \"application/json\" },\n    88→      });\n    89→    }\n    90→  }\n    91→\n    92→  if (req.method === \"POST\") {\n    93→    try {\n    94→      const body = await req.json();\n    95→      const { name, product, version, status, description, instructions } = body;\n    96→\n    97→      if (!name) {\n    98→        return new Response(JSON.stringify({ error: \"name is required\" }), {\n    99→          status: 400,\n   100→        