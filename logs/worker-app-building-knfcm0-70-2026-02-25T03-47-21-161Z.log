        updated_at = NOW()\n   150→        WHERE id = ${recipeId}::uuid\n   151→        RETURNING *\n   152→      `;\n   153→      if (rows.length === 0) {\n   154→        return new Response(JSON.stringify({ error: \"Recipe not found\" }), {\n   155→          status: 404,\n   156→          headers: { \"Content-Type\": \"application/json\" },\n   157→        });\n   158→      }\n   159→\n   160→      // If materials were provided, replace them\n   161→      if (materials !== undefined) {\n   162→        await sql`DELETE FROM recipe_materials WHERE recipe_id = ${recipeId}::uuid`;\n   163→        for (const m of materials) {\n   164→          await sql`\n   165→            INSERT INTO recipe_materials (recipe_id, material_name, quantity, unit)\n   166→            VALUES (${recipeId}::uuid, ${m.material_name}, ${m.quantity}, ${m.unit || \"kg\"})\n   167→          `;\n   168→        }\n   169→      }\n   170→\n   171→      // If products were provided, replace them\n   172→      if (products !== undefined) {\n   173→        await sql`DELETE FROM recipe_products WHERE recipe_id = ${recipeId}::uuid`;\n   174→        for (const p of products) {\n   175→          await sql`\n   176→            INSERT INTO recipe_products (recipe_id, product_name, amount)\n   177→            VALUES (${recipeId}::uuid, ${p.product_name}, ${p.amount || \"\"})\n   178→          `;\n   179→        }\n   180→      }\n   181→\n   182→      // If equipment were provided, replace them\n   183→      if (equipment !== undefined) {\n   184→        await sql`DELETE FROM recipe_equipment WHERE recipe_id = ${recipeId}::uuid`;\n   185→        for (const e of equipment) {\n   186→          await sql`\n   187→            INSERT INTO recipe_equipment (recipe_id, equipment_id)\n   188→            VALUES (${recipeId}::uuid, ${e.equipment_id}::uuid)\n   189→          `;\n   190→        }\n   191→      }\n   192→\n   193→      // Re-fetch full recipe with related data\n   194→      const updatedMaterials = await sql`\n   195→        SELECT id, recipe_id, material_name, quantity, unit\n   196→        FROM recipe_materials WHERE recipe_id = ${recipeId}::uuid ORDER BY material_name\n   197→      `;\n   198→      const updatedProducts = await sql`\n   199→        SELECT id, recipe_id, product_name, amount\n   200→        FROM recipe_products WHERE recipe_id = ${recipeId}::uuid ORDER BY product_name\n   201→      `;\n   202→      const updatedEquipment = await sql`\n   203→        SELECT re.id, re.recipe_id, re.equipment_id, e.name AS equipment_name\n   204→        FROM recipe_equipment re\n   205→        JOIN equipment e ON e.id = re.equipment_id\n   206→        WHERE re.recipe_id = ${recipeId}::uuid ORDER BY e.name\n   207→      `;\n   208→\n   209→      return new Response(\n   210→        JSON.stringify({\n   211→          ...rows[0],\n   212→          materials: updatedMaterials,\n   213→          products: updatedProducts,\n   214→          equipment: updatedEquipment,\n   215→        }),\n   216→        {\n   217→          status: 200,\n   218→          headers: { \"Content-Type\": \"application/json\" },\n   219→        }\n   220→      );\n   221→    } catch (err) {\n   222→      console.error(\"Recipes PUT error:\", err);\n   223→      return new Response(JSON.stringify({ error: \"Internal server error\" }), {\n   224→        status: 500,\n   225→        headers: { \"Content-Type\": \"application/json\" },\n   226→      });\n   227→    }\n   228→  }\n   229→\n   230→  if (req.method === \"DELETE\") {\n   231→    if (!recipeId) {\n   232→      return new Response(JSON.stringify({ error: \"Recipe ID required\" }), {\n   233→        status: 400,\n   234→        headers: { \"Content-Type\": \"application/json\" },\n   235→      });\n   236→    }\n   237→\n   238→    try {\n   239→      await sql`DELETE FROM recipes WHERE id = ${recipeId}::uuid`;\n   240→      return new Response(JSON.stringify({ success: true }), {\n   241→        status: 200,\n   242→        headers: { \"Content-Type\": \"application/json\" },\n   243→      });\n   244→    } catch (err) {\n   245→      console.error(\"Recipes DELETE error:\", err);\n   246→      return new Response(JSON.stringify({ error: \"Internal server error\" }), {\n   247→        status: 500,\n   248→        headers: { \"Content-Type\": \"application/json\" },\n   249→      });\n   250→    }\n   251→  }\n   252→\n   253→  return new Response(JSON.stringify({ error: \"Method not allowed\" }), {\n   254→    status: 405,\n   255→    headers: { \"Content-Type\": \"application/json\" },\n   256→  });\n   257→}\n   258→"}]},"parent_tool_use_id":"toolu_018oNKgcA5k63gNsvHt4f3ND","session_id":"b1a44c46-9df2-43cc-a087-896791d4866f","uuid":"c5f53531-b17a-4fbc-a91e-37999fb2d027"}
