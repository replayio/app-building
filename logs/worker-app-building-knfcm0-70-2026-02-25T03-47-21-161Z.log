  49→          setSearchText(e.target.value);\n    50→          setShowDropdown(true);\n    51→        }}\n    52→        onFocus={() => {\n    53→          setSearchText(\"\");\n    54→          setShowDropdown(true);\n    55→        }}\n    56→        onBlur={() => setTimeout(() => setShowDropdown(false), 150)}\n    57→      />\n    58→      {showDropdown && filtered.length > 0 && (\n    59→        <div className=\"searchable-select-dropdown\">\n    60→          {filtered.map((o) => (\n    61→            <button\n    62→              key={o.id}\n    63→              className=\"searchable-select-option\"\n    64→              onMouseDown={() => {\n    65→                onChange(o.id);\n    66→                setShowDropdown(false);\n    67→              }}\n    68→            >\n    69→              {o.label}\n    70→            </button>\n    71→          ))}\n    72→        </div>\n    73→      )}\n    74→    </div>\n    75→  );\n    76→}\n    77→"}]},"parent_tool_use_id":null,"session_id":"b1a44c46-9df2-43cc-a087-896791d4866f","uuid":"f11f7683-47f1-4a57-8c61-839b3e1dd6fe","tool_use_result":{"type":"text","file":{"filePath":"/repo/apps/shared/components/SearchableSelect.tsx","content":"import { useState } from \"react\";\n\nexport interface SearchableSelectOption {\n  id: string;\n  label: string;\n}\n\nexport interface SearchableSelectProps {\n  /** Available options to search and select from. */\n  options: SearchableSelectOption[];\n  /** Currently selected option ID, or empty string if none selected. */\n  value: string;\n  /** Called when an option is selected. */\n  onChange: (id: string) => void;\n  /** Placeholder text shown when no option is selected. */\n  placeholder?: string;\n  /** data-testid attribute for the input element. */\n  testId?: string;\n}\n\n/**\n * A searchable dropdown select component.\n * Used by Accounting (account selection in transactions) and InventoryTracker\n * (account/material/batch selection in transactions).\n */\nexport 