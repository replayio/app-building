ions under sb-<hostname-prefix>-auth-token\n    52→  // For https://auth.nut.new, hostname.split('.')[0] = 'auth'\n    53→  const supabaseStorageKey = 'sb-auth-auth-token'\n    54→\n    55→  const supabaseSession = {\n    56→    access_token: testJWT,\n    57→    refresh_token: 'test-refresh-token',\n    58→    expires_in: 86400,\n    59→    expires_at: Math.floor(Date.now() / 1000) + 86400,\n    60→    token_type: 'bearer',\n    61→    user: {\n    62→      id: TEST_USER_AUTH_ID,\n    63→      email: 'test@example.com',\n    64→      user_metadata: {\n    65→        full_name: 'Test User',\n    66→      },\n    67→      app_metadata: {\n    68→        provider: 'email',\n    69→      },\n    70→      role: 'authenticated',\n    71→      aud: 'authenticated',\n    72→    },\n    73→  }\n    74→\n    75→  // Write storageState file for Playwright\n    76→  const storageState = {\n    77→    cookies: [],\n    78→    origins: [\n    79→      {\n    80→        origin: 'http://localhost:8888',\n    81→        localStorage: [\n    82→          {\n    83→            name: supabaseStorageKey,\n    84→            value: JSON.stringify(supabaseSession),\n    85→          },\n    86→        ],\n    87→      },\n    88→    ],\n    89→  }\n    90→\n    91→  const storageStatePath = resolve(__dirname, 'test-storage-state.json')\n    92→  writeFileSync(storageStatePath, JSON.stringify(storageState, null, 2))\n    93→  console.log('Test auth storage state written to', storageStatePath)\n    94→}\n    95→"}]},"parent_tool_use_id":"toolu_01VZANeNjjhkxvqLtZNSZcfe","session_id":"193d30c5-bd37-4e94-a126-4421a0813e64","uuid":"ed27ec4e-b932-4fe1-91ed-de1445d4fdbe"}
