loading, setLoading] = useState(true)\n    48→\n    49→  useEffect(() => {\n    50→    auth.getSession().then(({ data: { session: s } }) => {\n    51→      setSession(s)\n    52→      if (!s) {\n    53→        setHasStoredToken(getStoredSession())\n    54→      }\n    55→      setLoading(false)\n    56→    })\n    57→\n    58→    const { data: { subscription } } = auth.onAuthStateChange((_event, s) => {\n    59→      setSession(s)\n    60→      if (!s) {\n    61→        setHasStoredToken(getStoredSession())\n    62→      }\n    63→      setLoading(false)\n    64→    })\n    65→\n    66→    return () => subscription.unsubscribe()\n    67→  }, [])\n    68→\n    69→  // Listen for auth tokens written to localStorage by the popup window.\n    70→  // The popup can't use postMessage because window.opener is nullified\n    71→  // by the cross-origin navigation during OAuth.\n    72→  useEffect(() => {\n    73→    function handleStorage(event: StorageEvent) {\n    74→      if (event.key !== AUTH_CALLBACK_KEY || !event.newValue) return\n    75→      try {\n    76→        const { access_token, refresh_token } = JSON.parse(event.newValue)\n    77→        if (access_token && refresh_token) {\n    78→          auth.setSession({ access_token, refresh_token }).then(({ data: { session: s } }) => {\n    79→            setSession(s)\n    80→          })\n    81→        }\n    82→      } catch {\n    83→        // ignore parse errors\n    84→      } finally {\n    85→        localStorage.removeItem(AUTH_CALLBACK_KEY)\n    86→      }\n    87→    }\n    88→    window.addEventListener('storage', handleStorage)\n    89→    return () => window.removeEventListener('storage', handleStorage)\n    90→  }, [])\n    91→\n    92→  const isLoggedIn = !!session || hasStoredToken\n    93→  const { data: userInfo } = useGetUserInfoQuery(undefined, { skip: !isLoggedIn })\n    94→\n    95→  const signIn = useCallback(async () => {\n    96→    const callbackUrl = `${window.location.origin}/auth/callback`\n    97→    const { data, error } = await auth.signInWithOAuth({\n    98→      provider: 'google',\n    99→      options: {\n   100→        redirectTo: callbackUrl,\n   101→        skipBrowserRedirect: true,\n   102→      },\n   103→    })\n   104→    if (error || !data.url) return\n   105→    const width = 500\n   106→    const height = 600\n   107→    const left = window.screenX + (window.innerWidth - width) / 2\n   108→    const top = window.screenY + (window.innerHeight - height) / 2\n   109→    const popup = window.open(data.url, 'auth-popup', `width=${width},height=${height},left=${left},top=${top}`)\n   110→\n   111→    // Poll for the popup closing — when it does, check if we got tokens\n   112→    // via the storage event or via Supabase session\n   113→    if (popup) {\n   114→      const interval = setInterval(() => {\n   115→        if (popup.closed) {\n   116→          clearInterval(interval)\n   117→          auth.getSession().then(({ data: { session: s } }) => {\n   118→            if (s) setSession(s)\n   119→          })\n   120→        }\n   121→      }, 500)\n   122→    }\n   123→  }, [])\n   124→\n   125→  async function signOut() {\n   126→    await auth.signOut()\n   127→    localStorage.removeItem(SUPABASE_STORAGE_KEY)\n   128→    setSession(null)\n   129→    setHasStoredToken(false)\n   130→  }\n   131→\n   132→  return (\n   133→    <AuthContext.Provider\n   134→      value={{\n   135→        isLoggedIn,\n   136→        loading,\n   137→        user: userInfo ?? null,\n   138→        signIn,\n   139→        signOut,\n   140→      }}\n   141→    >\n   142→      {children}\n   143→    </AuthContext.Provider>\n   144→  )\n   145→}\n   146→\n   147→export function useAuth() {\n   148→  return useContext(AuthContext)\n   149→}\n   150→"}]},"parent_tool_use_id":"toolu_013RRhphhuCsGTQA4mzj14XV","session_id":"193d30c5-bd37-4e94-a126-4421a0813e64","uuid":"ef98b982-1b40-4538-af45-7cb60dcac9b6"}
