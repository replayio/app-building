/.netlify/functions/clients/${clientId}`)\n    48→    if (!res.ok) throw new Error('Failed to fetch client')\n    49→    return res.json() as Promise<Client>\n    50→  }\n    51→)\n    52→\n    53→export const createClient = createAsyncThunk(\n    54→  'clients/createClient',\n    55→  async (data: {\n    56→    name: string\n    57→    type: string\n    58→    status?: string\n    59→    tags?: string[]\n    60→    source_type?: string\n    61→    source_detail?: string\n    62→    campaign?: string\n    63→    channel?: string\n    64→    date_acquired?: string\n    65→  }) => {\n    66→    const res = await fetch('/.netlify/functions/clients', {\n    67→      method: 'POST',\n    68→      headers: { 'Content-Type': 'application/json' },\n    69→      body: JSON.stringify(data),\n    70→    })\n    71→    if (!res.ok) throw new Error('Failed to create client')\n    72→    return res.json() as Promise<Client>\n    73→  }\n    74→)\n    75→\n    76→export const updateClient = createAsyncThunk(\n    77→  'clients/updateClient',\n    78→  async ({ clientId, data }: { clientId: string; data: Record<string, unknown> }) => {\n    79→    const res = await fetch(`/.netlify/functions/clients/${clientId}`, {\n    80→      method: 'PUT',\n    81→      headers: { 'Content-Type': 'application/json' },\n    82→      body: JSON.stringify(data),\n    83→    })\n    84→    if (!res.ok) throw new Error('Failed to update client')\n    85→    return res.json() as Promise<Client>\n    86→  }\n    87→)\n    88→\n    89→export const deleteClient = createAsyncThunk(\n    90→  'clients/deleteClient',\n    91→  async (clientId: string) => {\n    92→    const res = await fetch(`/.netlify/functions/clients/${clientId}`, {\n    93→      method: 'DELETE',\n    94→    })\n    95→    if (!res.ok) throw new Error('Failed to delete client')\n    96→    return clientId\n    97→  }\n    98→)\n    99→\n   100→export const clientsSlice = createSlice({\n   101→  name: 'clients',\n   102→  initialState,\n   103→  reducers: {\n   104→    setPage(state, action) {\n   105→      state.page = action.payload\n   106→    },\n   107→  },\n   108→  extraReducers: (builder) => {\n   109→    builder\n   110→      .addCase(fetchClients.pending, (state) => {\n   111→        if (state.items.length === 0) {\n   112→          state.loading = true\n   113→        }\n   114→        state.error = null\n   115→      })\n   116→      .addCase(fetchClients.fulfilled, (state, action) => {\n   117→        state.loading = false\n   118→        state.items = action.payload.clients\n   119→        state.total = action.payload.total\n   120→        state.availableTags = action.payload.tags ?? []\n   121→        state.availableSources = action.payload.sources ?? []\n   122→      })\n   123→      .addCase(fetchClients.rejected, (state, action) => {\n   124→        state.loading = false\n   125→        state.error = action.error.message ?? 'Unknown error'\n   126→      })\n   127→      .addCase(fetchClient.pending, (state) => {\n   128→        state.loading = true\n   129→        state.error = null\n   130→      })\n   131→      .addCase(fetchClient.fulfilled, (state, action) => {\n   132→        state.loading = false\n   133→        state.currentClient = action.payload\n   134→      })\n   135→      .addCase(fetchClient.rejected, (state, action) => {\n   136→        state.loading = false\n   137→        state.error = action.error.message ?? 'Unknown error'\n   138→      })\n   139→      .addCase(createClient.fulfilled, (state, action) => {\n   140→        state.items.unshift(action.payload)\n   141→        state.total += 1\n   142→      })\n   143→      .addCase(updateClient.fulfilled, (state, action) => {\n   144→        state.currentClient = action.payload\n   145→        const idx = state.items.findIndex((c) => c.id === action.payload.id)\n   146→        if (idx !== -1) state.items[idx] = action.payload\n   147→      })\n   148→      .addCase(deleteClient.fulfilled, (state, action) => {\n   149→        state.items = state.items.filter((c) => c.id !== action.payload)\n   150→        state.total -= 1\n   151→      })\n   152→  },\n   153→})\n   154→\n   155→export const { setPage } = clientsSlice.actions\n   156→"}]},"parent_tool_use_id":"toolu_01UVjm2zretyP5m5VjTMEGu4","session_id":"193d30c5-bd37-4e94-a126-4421a0813e64","uuid":"7ec506da-6781-499a-883b-65a7e41108db"}
