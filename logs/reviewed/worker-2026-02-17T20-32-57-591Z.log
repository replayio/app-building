[2026-02-17T18:55:00.864Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_0128Vwk9KUskTNn81RihPnsM","type":"tool_result","content":"     1→import { neon } from '@neondatabase/serverless'\n     2→import { optionalAuth, type OptionalAuthRequest } from '../utils/auth'\n     3→\n     4→function getDb() {\n     5→  const url = process.env.DATABASE_URL\n     6→  if (!url) throw new Error('DATABASE_URL not set')\n     7→  return neon(url)\n     8→}\n     9→\n    10→async function handler(req: OptionalAuthRequest) {\n    11→  const sql = getDb()\n    12→  const url = new URL(req.url)\n    13→  const pathParts = url.pathname.split('/').filter(Boolean)\n    14→  const resourceId = pathParts.length >= 4 ? pathParts[3] : null\n    15→\n    16→  // GET /clients/:id\n    17→  if (req.method === 'GET' && resourceId) {\n    18→    const rows = await sql`\n    19→      SELECT c.*,\n    20→        (SELECT COUNT(*) FROM deals d WHERE d.client_id = c.id AND d.stage NOT IN ('closed_won', 'closed_lost')) as open_deals_count,\n    21→        COALESCE((SELECT SUM(d.value) FROM deals d WHERE d.client_id = c.id AND d.stage NOT IN ('closed_won', 'closed_lost')), 0) as open_deals_value,\n    22→        (SELECT i.name FROM client_individuals ci JOIN individuals i ON ci.individual_id = i.id WHERE ci.client_id = c.id AND ci.is_primary = true LIMIT 1) as primary_contact_name,\n    23→        (SELECT ci.role FROM client_individuals ci WHERE ci.client_id = c.id AND ci.is_primary = true LIMIT 1) as primary_contact_role,\n    24→        (SELECT t.title FROM tasks t WHERE t.client_id = c.id AND t.completed = false ORDER BY t.due_date ASC NULLS LAST LIMIT 1) as next_task_title,\n    25→        (SELECT t.due_date FROM tasks t WHERE t.client_id = c.id AND t.completed = false ORDER BY t.due_date ASC NULLS LAST LIMIT 1) as next_task_due\n    26→      FROM clients c WHERE c.id = ${resourceId}::uuid\n    27→    `\n    28→    if (rows.length === 0) {\n    29→      return new Response(JSON.stringify({ error: 'Not found' }), { status: 404 })\n    30→    }\n    31→    return Response.json(rows[0])\n    32→  }\n    33→\n    34→  // GET /clients\n    35→  if (req.method === 'GET') {\n    36→    const page = parseInt(url.searchParams.get('page') ?? '1')\n    37→    const pageSizeParam = url.searchParams.get('pageSize')\n    38→    const pageSize = pageSizeParam ? parseInt(pageSizeParam) : 50\n    39→    const offset = (page - 1) * pageSize\n    40→    const search = url.searchParams.get('search') ?? ''\n    41→    const status = url.searchParams.get('status') ?? ''\n    42→    const tag = url.searchParams.get('tag') ?? ''\n    43→    const source = url.searchParams.get('source') ?? ''\n    44→    const sort = url.searchParams.get('sort') ?? 'updated_at'\n    45→\n    46→    const searchPattern = search ? '%' + search + '%' : null\n    47→\n    48→    // Count query using conditional WHERE with tagged template\n    49→    const countResult = await sql`\n    50→      SELECT COUNT(*) as count FROM clients c\n    51→      WHERE (${searchPattern}::text IS NULL OR (\n    52→        c.name ILIKE ${searchPattern}\n    53→        OR EXISTS (SELECT 1 FROM unnest(c.tags) AS tg WHERE tg ILIKE ${searchPattern})\n    54→        OR EXISTS (SELECT 1 FROM client_individuals ci JOIN individuals i ON ci.individual_id = i.id WHERE ci.client_id = c.id AND i.name ILIKE ${searchPattern})\n    55→      ))\n    56→      AND (${status || null}::text IS NULL OR c.status = ${status || null})\n    57→      AND (${tag || null}::text IS NULL OR ${tag || null} = ANY(c.tags))\n    58→      AND (${source || null}::text IS NULL OR c.source_type = ${source || null})\n    59→    `\n    60→\n    61→    // Map sort parameter to safe sort key\n    62→    const sortMap: Record<string, string> = {\n    63→      'name_asc': 'name_asc',\n    64→      'name_desc': 'name_desc',\n    65→      'status': 'status',\n    66→      'updated_at': 'updated_at',\n    67→    }\n    68→    const sortKey = sortMap[sort] ?? 'updated_at'\n    69→\n    70→    // Data query — use CASE-based ORDER BY to avoid dynamic SQL\n    71→    const dataResult = await sql`\n    72→      SELECT c.*,\n    73→        (SELECT COUNT(*) FROM deals d WHERE d.client_id = c.id AND d.stage NOT IN ('closed_won', 'closed_lost')) as open_deals_count,\n    74→        COALESCE((SELECT SUM(d.value) FROM deals d WHERE d.client_id = c.id AND d.stage NOT IN ('closed_won', 'closed_lost')), 0) as open_deals_value,\n    75→        (SELECT i.name FROM client_individuals ci JOIN individuals i ON ci.individual_id = i.id WHERE ci.client_id = c.id AND ci.is_primary = true LIMIT 1) as primary_contact_name,\n    76→        (SELECT ci.role FROM client_individuals ci WHERE ci.client_id = c.id AND ci.is_primary = true LIMIT 1) as primary_contact_role,\n    77→        (SELECT t.title FROM tasks t WHERE t.client_id = c.id AND t.completed = false ORDER BY t.due_date ASC NULLS LAST LIMIT 1) as next_task_title,\n    78→        (SELECT t.due_date FROM tasks t WHERE t.client_id = c.id AND t.completed = false ORDER BY t.due_date ASC NULLS LAST LIMIT 1) as next_task_due\n    79→      FROM clients c\n    80→      WHERE (${searchPattern}::text IS NULL OR (\n    81→        c.name ILIKE ${searchPattern}\n    82→        OR EXISTS (SELECT 1 FROM unnest(c.tags) AS tg WHERE tg ILIKE ${searchPattern})\n    83→        OR EXISTS (SELECT 1 FROM client_individuals ci JOIN individuals i ON ci.individual_id = i.id WHERE ci.client_id = c.id AND i.name ILIKE ${searchPattern})\n    84→      ))\n    85→      AND (${status || null}::text IS NULL OR c.status = ${status || null})\n    86→      AND (${tag || null}::text IS NULL OR ${tag || null} = ANY(c.tags))\n    87→      AND (${source || null}::text IS NULL OR c.source_type = ${source || null})\n    88→      ORDER BY\n    89→        CASE WHEN ${sortKey} = 'name_asc' THEN c.name END ASC,\n    90→        CASE WHEN ${sortKey} = 'name_desc' THEN c.name END DESC,\n    91→        CASE WHEN ${sortKey} = 'status' THEN c.status END ASC,\n    92→        CASE WHEN ${sortKey} = 'updated_at' THEN c.updated_at END DESC,\n    93→        c.updated_at DESC\n    94→      LIMIT ${pageSize} OFFSET ${offset}\n    95→    `\n    96→\n    97→    // Fetch distinct tags and sources for filter dropdowns\n    98→    const [tagsResult, sourcesResult] = await Promise.all([\n    99→      sql`SELECT DISTINCT unnest(tags) as tag FROM clients ORDER BY tag`,\n   100→      sql`SELECT DISTINCT source_type FROM clients WHERE source_type IS NOT NULL ORDER BY source_type`,\n   101→    ])\n   102→\n   103→    return Response.json({\n   104→      clients: dataResult,\n   105→      total: parseInt(String(countResult[0].count)),\n   106→      tags: tagsResult.map((r: Record<string, unknown>) => r.tag as string),\n   107→      sources: sourcesResult.map((r: Record<string, unknown>) => r.source_type as string),\n   108→    })\n   109→  }\n   110→\n   111→  // POST /clients\n   112→  if (req.method === 'POST') {\n   113→    const body = await req.json() as {\n   114→      name: string\n   115→      type: string\n   116→      status?: string\n   117→      tags?: string[]\n   118→      source_type?: string\n   119→      source_detail?: string\n   120→      campaign?: string\n   121→      channel?: string\n   122→      date_acquired?: string\n   123→    }\n   124→\n   125→    const rows = await sql`\n   126→      INSERT INTO clients (name, type, status, tags, source_type, source_detail, campaign, channel, date_acquired)\n   127→      VALUES (${body.name}, ${body.type}, ${body.status ?? 'prospect'}, ${body.tags ?? []}, ${body.source_type ?? null}, ${body.source_detail ?? null}, ${body.campaign ?? null}, ${body.channel ?? null}, ${body.date_acquired || null})\n   128→      RETURNING *\n   129→    `\n   130→\n   131→    return Response.json(rows[0], { status: 201 })\n   132→  }\n   133→\n   134→  // DELETE /clients/:id\n   135→  if (req.method === 'DELETE' && resourceId) {\n   136→    await sql`DELETE FROM clients WHERE id = ${resourceId}`\n   137→    return Response.json({ success: true })\n   138→  }\n   139→\n   140→  // PUT /clients/:id\n   141→  if (req.method === 'PUT' && resourceId) {\n   142→    const body = await req.json() as Record<string, unknown>\n   143→\n   144→    // If status is changing, get old status for timeline event\n   145→    let oldStatus: string | null = null\n   146→    if (body.status) {\n   147→      const existing = await sql`SELECT status FROM clients WHERE id = ${resourceId}::uuid`\n   148→      if (existing.length > 0 && existing[0].status !== body.status) {\n   149→        oldStatus = existing[0].status as string\n   150→      }\n   151→    }\n   152→\n   153→    const rows = await sql`\n   154→      UPDATE clients SET\n   155→        name = COALESCE(${(body.name as string) ?? null}, name),\n   156→        type = COALESCE(${(body.type as string) ?? null}, type),\n   157→        status = COALESCE(${(body.status as string) ?? null}, status),\n   158→        tags = COALESCE(${(body.tags as string[]) ?? null}, tags),\n   159→        source_type = COALESCE(${(body.source_type as string) ?? null}, source_type),\n   160→        source_detail = COALESCE(${(body.source_detail as string) ?? null}, source_detail),\n   161→        campaign = COALESCE(${(body.campaign as string) ?? null}, campaign),\n   162→        channel = COALESCE(${(body.channel as string) ?? null}, channel),\n   163→        updated_at = NOW()\n   164→      WHERE id = ${resourceId}::uuid\n   165→      RETURNING *\n   166→    `\n   167→\n   168→    if (rows.length === 0) {\n   169→      return new Response(JSON.stringify({ error: 'Not found' }), { status: 404 })\n   170→    }\n   171→\n   172→    // Create timeline event for status change\n   173→    if (oldStatus) {\n   174→      await sql`\n   175→        INSERT INTO timeline_events (client_id, event_type, description, user_name)\n   176→        VALUES (${resourceId}::uuid, 'status_changed', ${'Status Changed: from \\'' + oldStatus + '\\' to \\'' + (body.status as string) + '\\''}, ${req.user?.name ?? 'System'})\n   177→      `\n   178→    }\n   179→\n   180→    return Response.json(rows[0])\n   181→  }\n   182→\n   183→  return new Response(JSON.stringify({ error: 'Method not allowed' }), { status: 405 })\n   184→}\n   185→\n   186→export default optionalAuth(handler)\n   187→"}]},"parent_tool_use_id":"toolu_01UVjm2zretyP5m5VjTMEGu4","session_id":"193d30c5-bd37-4e94-a126-4421a0813e64","uuid":"1d58cab9-8ae9-4a27-bf54-3155e280c2a0"}
