2→}\n    43→\n    44→export function ClientsTable({ clients, onDeleteClient }: ClientsTableProps) {\n    45→  const navigate = useNavigate()\n    46→\n    47→  if (clients.length === 0) {\n    48→    return (\n    49→      <div className=\"text-center py-12 text-text-muted text-[13px]\">\n    50→        No clients found.\n    51→      </div>\n    52→    )\n    53→  }\n    54→\n    55→  return (\n    56→    <div data-testid=\"clients-table\" className=\"w-full\">\n    57→      {/* Header */}\n    58→      <div data-testid=\"clients-table-header\" className=\"grid grid-cols-[1.2fr_0.8fr_0.7fr_1.2fr_1.1fr_0.9fr_1.3fr_40px] items-center h-[36px] px-4 text-[12px] font-medium text-text-muted border-b border-border\">\n    59→        <span>Client Name</span>\n    60→        <span>Type</span>\n    61→        <span>Status</span>\n    62→        <span>Tags</span>\n    63→        <span>Primary Contact</span>\n    64→        <span>Open Deals</span>\n    65→        <span>Next Task</span>\n    66→        <span></span>\n    67→      </div>\n    68→\n    69→      {/* Rows */}\n    70→      {clients.map((client) => (\n    71→        <div\n    72→          key={client.id}\n    73→          data-testid={`client-row-${client.id}`}\n    74→          onClick={() => navigate(`/clients/${client.id}`)}\n    75→          className=\"grid grid-cols-[1.2fr_0.8fr_0.7fr_1.2fr_1.1fr_0.9fr_1.3fr_40px] items-center h-[44px] px-4 cursor-pointer hover:bg-hover transition-colors duration-100 border-b border-border/50\"\n    76→        >\n    77→          <span data-testid=\"client-name\" className=\"text-[13px] font-medium text-text-primary truncate pr-2\">\n    78→            {client.name}\n    79→          </span>\n    80→          <span data-testid=\"client-type\" className=\"text-[13px] text-text-secondary\">\n    81→            {client.type === 'organization' ? 'Organization' : 'Individual'}\n    82→          </span>\n    83→          <span data-testid=\"client-status\">\n    84→            <StatusBadge status={client.status} />\n    85→          </span>\n    86→          <div data-testid=\"client-tags\" className=\"flex items-center gap-1 overflow-hidden\">\n    87→            {client.tags.map((tag) => (\n    88→              <TagBadge key={tag} tag={tag} />\n    89→            ))}\n    90→          </div>\n    91→          <span data-testid=\"client-primary-contact\" className=\"text-[13px] text-text-secondary truncate pr-2\">\n    92→            {formatPrimaryContact(client)}\n    93→          </span>\n    94→          <span data-testid=\"client-open-deals\" className=\"text-[13px] text-text-secondary\">\n    95→            {formatOpenDeals(client)}\n    96→          </span>\n    97→          <span data-testid=\"client-next-task\" className=\"text-[13px] text-text-secondary truncate pr-2\">\n    98→            {formatNextTask(client)}\n    99→          </span>\n   100→          <ClientRowActionMenu\n   101→            onView={() => navigate(`/clients/${client.id}`)}\n   102→            onEdit={() => navigate(`/clients/${client.id}`)}\n   103→            onDelete={() => onDeleteClient(client.id)}\n   104→          />\n   105→        </div>\n   106→      ))}\n   107→    </div>\n   108→  )\n   109→}\n   110→"}]},"parent_tool_use_id":"toolu_01UVjm2zretyP5m5VjTMEGu4","session_id":"193d30c5-bd37-4e94-a126-4421a0813e64","uuid":"9e643c22-2db4-4d4d-9ed3-d98b39a8edd2"}
