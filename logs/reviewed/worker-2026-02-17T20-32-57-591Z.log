[2026-02-17T19:25:46.734Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01Y1ZCHCfZYh4mq1kaBh63Nu","type":"tool_result","content":"     1→import { createContext, useContext, useEffect, useState, useCallback, type ReactNode } from 'react'\n     2→import { auth } from '../lib/auth'\n     3→import { useGetUserInfoQuery } from '../store/authSlice'\n     4→import type { Session } from '@supabase/supabase-js'\n     5→\n     6→// The Supabase storage key — matches the one in auth.ts\n     7→const supabaseUrl = import.meta.env.VITE_AUTH_SUPABASE_URL\n     8→const SUPABASE_STORAGE_KEY = `sb-${new URL(supabaseUrl).hostname.split('.')[0]}-auth-token`\n     9→\n    10→interface AuthContextValue {\n    11→  isLoggedIn: boolean\n    12→  loading: boolean\n    13→  user: { id: string; email: string; name: string; avatar_url: string } | null\n    14→  signIn: (email: string, password: string) => Promise<{ error: string | null }>\n    15→  signUp: (email: string, password: string) => Promise<{ error: string | null }>\n    16→  signOut: () => Promise<void>\n    17→}\n    18→\n    19→const AuthContext = createContext<AuthContextValue>({\n    20→  isLoggedIn: false,\n    21→  loading: true,\n    22→  user: null,\n    23→  signIn: async () => ({ error: null }),\n    24→  signUp: async () => ({ error: null }),\n    25→  signOut: async () => {},\n    26→})\n    27→\n    28→/**\n    29→ * Check localStorage for a session token (used as fallback in test mode\n    30→ * where tokens aren't set via real Supabase auth).\n    31→ */\n    32→function getStoredSession(): boolean {\n    33→  try {\n    34→    const stored = localStorage.getItem(SUPABASE_STORAGE_KEY)\n    35→    if (stored) {\n    36→      const parsed = JSON.parse(stored)\n    37→      return !!parsed.access_token\n    38→    }\n    39→  } catch {\n    40→    // ignore\n    41→  }\n    42→  return false\n    43→}\n    44→\n    45→export function AuthProvider({ children }: { children: ReactNode }) {\n    46→  const [session, setSession] = useState<Session | null>(null)\n    47→  const [hasStoredToken, setHasStoredToken] = useState(false)\n    48→  const [loading, setLoading] = useState(true)\n    49→\n    50→  useEffect(() => {\n    51→    auth.getSession().then(({ data: { session: s } }) => {\n    52→      setSession(s)\n    53→      if (!s) {\n    54→        setHasStoredToken(getStoredSession())\n    55→      }\n    56→      setLoading(false)\n    57→    })\n    58→\n    59→    const { data: { subscription } } = auth.onAuthStateChange((_event, s) => {\n    60→      setSession(s)\n    61→      if (!s) {\n    62→        setHasStoredToken(getStoredSession())\n    63→      }\n    64→      setLoading(false)\n    65→    })\n    66→\n    67→    return () => subscription.unsubscribe()\n    68→  }, [])\n    69→\n    70→  const isLoggedIn = !!session || hasStoredToken\n    71→  const { data: userInfo } = useGetUserInfoQuery(undefined, { skip: !isLoggedIn })\n    72→\n    73→  const signIn = useCallback(async (email: string, password: string): Promise<{ error: string | null }> => {\n    74→    const { data, error } = await auth.signInWithPassword({ email, password })\n    75→    if (error) {\n    76→      return { error: error.message }\n    77→    }\n    78→    if (data.session) {\n    79→      setSession(data.session)\n    80→    }\n    81→    return { error: null }\n    82→  }, [])\n    83→\n    84→  const signUp = useCallback(async (email: string, password: string): Promise<{ error: string | null }> => {\n    85→    const { data, error } = await auth.signUp({ email, password })\n    86→    if (error) {\n    87→      return { error: error.message }\n    88→    }\n    89→    if (data.session) {\n    90→      setSession(data.session)\n    91→    }\n    92→    return { error: null }\n    93→  }, [])\n    94→\n    95→  async function signOut() {\n    96→    await auth.signOut()\n    97→    localStorage.removeItem(SUPABASE_STORAGE_KEY)\n    98→    setSession(null)\n    99→    setHasStoredToken(false)\n   100→  }\n   101→\n   102→  return (\n   103→    <AuthContext.Provider\n   104→      value={{\n   105→        isLoggedIn,\n   106→        loading,\n   107→        user: userInfo ?? null,\n   108→        signIn,\n   109→        signUp,\n   110→        signOut,\n   111→      }}\n   112→    >\n   113→      {children}\n   114→    </AuthContext.Provider>\n   115→  )\n   116→}\n   117→\n   118→export function useAuth() {\n   119→  return useContext(AuthContext)\n   120→}\n   121→"}]},"parent_tool_use_id":"toolu_01VZANeNjjhkxvqLtZNSZcfe","session_id":"193d30c5-bd37-4e94-a126-4421a0813e64","uuid":"9685553e-03b7-40a7-8b61-fd95fd944fc1"}
