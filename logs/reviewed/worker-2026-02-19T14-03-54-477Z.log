oads(sys.stdin.read())\nexcept Exception:\n    sys.exit(0)\n\n# Print full metadata block for ALL recordings\nprint('=== REPLAY RECORDINGS METADATA ===')\nfor r in data:\n    test_meta = r.get('metadata', {}).get('test', {})\n    entry = {\n        'id': r.get('id', ''),\n        'date': r.get('date', ''),\n        'duration': r.get('duration', 0),\n        'uri': r.get('metadata', {}).get('uri', ''),\n        'recordingStatus': r.get('recordingStatus', ''),\n        'host': r.get('metadata', {}).get('host', ''),\n        'testResult': test_meta.get('result', ''),\n        'testTitle': test_meta.get('source', {}).get('title', ''),\n        'specFile': test_meta.get('source', {}).get('path', ''),\n    }\n    print(json.dumps(entry))\nprint('=== END REPLAY RECORDINGS METADATA ===')\nprint()\n\n# Only upload recordings where test result is failed or timedOut\nfor r in data:\n    if r.get('recordingStatus') != 'finished':\n        continue\n    test_result = r.get('metadata', {}).get('test', {}).get('result', '')\n    if test_result in ('failed', 'timedOut'):\n        print('UPLOAD:' + r['id'])\n\" 2>&1)\n\n    echo \"$RECORDINGS\"\n    echo \"\"\n\n    # Upload only the failing recordings.\n    UPLOAD_COUNT=0\n    while IFS= read -r line; do\n      if [[ \"$line\" == UPLOAD:* ]]; then\n        REC_ID=\"${line#UPLOAD:}\"\n        echo \"Uploading recording: $REC_ID\"\n        npx [REDACTED]io upload \"$REC_ID\" 2>&1\n        echo \"REPLAY UPLOADED: $REC_ID\"\n        echo \"\"\n        UPLOAD_COUNT=$((UPLOAD_COUNT + 1))\n      fi\n    done <<< \"$RECORDINGS\"\n\n    if [ $UPLOAD_COUNT -eq 0 ]; then\n      echo \"WARNING: No failed recordings found to upload.\"\n    else\n      echo \"Uploaded $UPLOAD_COUNT recording(s).\"\n    fi\n\n    # Clean up local recordings after upload.\n    echo \"Cleaning up local recordings...\"\n    npx [REDACTED]io remove --all 2>/dev/null || true\n  fi\nfi\n\nexit $PW_EXIT\n"}}
