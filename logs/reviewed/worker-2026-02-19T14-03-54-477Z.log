]; then\n  echo \"\"\n  echo \"=== Tests failed — uploading a Replay recording ===\"\n\n  # Find the most recent recording with a non-trivial duration.\n  # `[REDACTED]io list` output columns: ID, Title, Process, Date, Duration, Status\n  # Filter out 1.0ms stubs and \"Recording\" (in-progress) entries.\n  RECORDING_ID=$(npx [REDACTED]io list 2>&1 \\\n    | grep -v '1\\.0ms' \\\n    | grep 'Recorded' \\\n    | head -1 \\\n    | awk '{print $1}' \\\n    | tr -d '…')\n\n  if [ -n \"$RECORDING_ID\" ]; then\n    echo \"Uploading recording: $RECORDING_ID\"\n    npx [REDACTED]io upload \"$RECORDING_ID\" 2>&1\n    echo \"\"\n    echo \"REPLAY UPLOADED: $RECORDING_ID\"\n  else\n    echo \"WARNING: Could not find a Replay recording to upload.\"\n  fi\nfi\n\nexit $PW_EXIT\n","numLines":35,"startLine":1,"totalLines":35}}}
