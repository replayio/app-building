  className=\"flex items-center gap-1 w-full h-[34px] pl-3 pr-7 text-[13px] text-text-secondary bg-surface border border-border rounded-[5px] cursor-pointer hover:bg-hover focus:outline-none focus:border-accent transition-colors duration-100 whitespace-nowrap text-left\"\n    55→      >\n    56→        <span className=\"truncate\">{displayText}</span>\n    57→        <ChevronDown\n    58→          size={12}\n    59→          strokeWidth={2}\n    60→          className=\"absolute right-2 top-1/2 -translate-y-1/2 text-text-disabled\"\n    61→        />\n    62→      </button>\n    63→      {open && (\n    64→        <div\n    65→          data-testid={testId ? `${testId}-menu` : undefined}\n    66→          className=\"absolute top-full left-0 mt-1 min-w-full w-max max-h-[240px] overflow-y-auto bg-surface border border-border rounded-[6px] shadow-lg z-50\"\n    67→        >\n    68→          {options.map((opt) => (\n    69→            <button\n    70→              key={opt.value}\n    71→              type=\"button\"\n    72→              data-testid={testId ? `${testId}-option-${opt.value || 'all'}` : undefined}\n    73→              data-option-value={opt.value}\n    74→              onClick={() => {\n    75→                onChange(opt.value)\n    76→                setOpen(false)\n    77→              }}\n    78→              className={`flex items-center gap-2 w-full px-3 py-1.5 text-[13px] text-left hover:bg-hover transition-colors duration-100 ${\n    79→                opt.value === value ? 'text-accent font-medium' : 'text-text-secondary'\n    80→              }`}\n    81→            >\n    82→              <span className=\"w-3.5 flex-shrink-0\">\n    83→                {opt.value === value && <Check size={12} strokeWidth={2.5} />}\n    84→              </span>\n    85→              {opt.label}\n    86→            </button>\n    87→          ))}\n    88→        </div>\n    89→      )}\n    90→    </div>\n    91→  )\n    92→}\n    93→"}]},"parent_tool_use_id"