e'\n    39→    const statusIsWon = status === 'won'\n    40→    const statusIsLost = status === 'lost'\n    41→    const statusOther = (status && !statusIsActive && !statusIsWon && !statusIsLost) ? status : null\n    42→\n    43→    // Count total matching deals\n    44→    const countResult = await sql`\n    45→      SELECT COUNT(*) as count FROM deals d\n    46→      JOIN clients c ON d.client_id = c.id\n    47→      WHERE (${searchPattern}::text IS NULL OR (d.name ILIKE ${searchPattern} OR c.name ILIKE ${searchPattern}))\n    48→      AND (${stageFilter}::text IS NULL OR d.stage = ${stageFilter})\n    49→      AND (${clientFilter}::text IS NULL OR d.client_id = ${clientFilter}::uuid)\n    50→      AND (NOT ${statusIsActive}::boolean OR d.stage NOT IN ('closed_won', 'closed_lost'))\n    51→      AND (NOT ${statusIsWon}::boolean OR d.stage = 'closed_won')\n    52→      AND (NOT ${statusIsLost}::boolean OR d.stage = 'closed_lost')\n    53→      AND (${statusOther}::text IS NULL OR d.status = ${statusOther})\n    54→      AND (${dateFromFilter}::date IS NULL OR d.expected_close_date >= ${dateFromFilter}::date)\n    55→      AND (${dateToFilter}::date IS NULL OR d.expected_close_date <= ${dateToFilter}::date)\n    56→    `\n    57→    const total = parseInt(String(countResult[0].count), 10)\n    58→\n    59→    // Map sort parameter to safe sort key\n    60→    const sortMap: Record<string, string> = {\n    61→      'close_date_asc': 'close_date_asc',\n    62→      'close_date_desc': 'close_date_desc',\n    63→      'name_asc': 'name_asc',\n    64→      'name_desc': 'name_desc',\n    65→      'value_desc': 'value_desc',\n    66→      'value_asc': 'value_asc',\n    67→      'updated_at': 'updated_at',\n    68→    }\n    69→    const sortKey = sortMap[sort] ?? 'close_date_desc'\n    70→\n    71→    // Fetch deals with CASE-based ORDER BY to avoid dynamic SQL\n    72→    const deals = await sql`\n    73→      SELECT d.*, c.name as client_name FROM deals d\n    74→      J