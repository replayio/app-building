 => {\n    48→      setSession(s)\n    49→      // If Supabase didn't find a session, check localStorage directly\n    50→      if (!s) {\n    51→        setHasStoredToken(getStoredSession())\n    52→      }\n    53→      setLoading(false)\n    54→    })\n    55→\n    56→    const { data: { subscription } } = auth.onAuthStateChange((_event, s) => {\n    57→      setSession(s)\n    58→      if (!s) {\n    59→        setHasStoredToken(getStoredSession())\n    60→      }\n    61→      setLoading(false)\n    62→    })\n    63→\n    64→    return () => subscription.unsubscribe()\n    65→  }, [])\n    66→\n    67→  // User is logged in if Supabase has a session OR we have a stored token\n    68→  const isLoggedIn = !!session || hasStoredToken\n    69→  const { data: userInfo } = useGetUserInfoQuery(undefined, { skip: !isLoggedIn })\n    70→\n    71→  async function signOut() {\n    72→    await auth.signOut()\n    73→    localStorage.removeItem(SUPABASE_STORAGE_KEY)\n    74→    setSession(null)\n    75→    setHasStoredToken(false)\n    76→  }\n    77→\n    78→  return (\n    79→    <AuthContext.Provider\n    80→      value={{\n    81→        isLoggedIn,\n    82→        loading,\n    83→        user: userInfo ?? null,\n    84→        signOut,\n    85→      }}\n    86→    >\n    87→      {children}\n    88→    </AuthContext.Provider>\n    89→  )\n    90→}\n    91→\n    92→export function useAuth() {\n    93→  return useContext(AuthContext)\n    94→}\n    95→"}]},"parent_tool_use_id":"toolu_0112Ziy6izM6ZuYDgDGovW1T","session_id":"ea780c21-dd76-4158-a0db-c55297864d38","uuid":"62cdea26-7c46-4165-a2e4-633532c03f65"}
