46→      {user && (\n    47→        <div className=\"border-t border-border pt-3 mt-3\">\n    48→          <div className=\"flex items-center gap-2 px-2 mb-2\">\n    49→            {user.avatar_url ? (\n    50→              <img\n    51→                src={user.avatar_url}\n    52→                alt={user.name}\n    53→                className=\"w-6 h-6 rounded-full\"\n    54→                data-testid=\"sidebar-user-avatar\"\n    55→              />\n    56→            ) : (\n    57→              <div className=\"w-6 h-6 rounded-full bg-accent/20 flex items-center justify-center text-[11px] font-medium text-accent\" data-testid=\"sidebar-user-avatar\">\n    58→                {user.name.charAt(0).toUpperCase()}\n    59→              </div>\n    60→            )}\n    61→            <span className=\"text-[12px] text-text-secondary truncate\" data-testid=\"sidebar-user-name\">\n    62→              {user.name}\n    63→            </span>\n    64→          </div>\n    65→          <button\n    66→            data-testid=\"sidebar-sign-out\"\n    67→            onClick={signOut}\n    68→            className=\"flex items-center gap-2.5 h-7 px-2 w-full rounded-[4px] text-[13px] font-[450] text-text-secondary hover:bg-sidebar-hover transition-colors duration-100\"\n    69→          >\n    70→            <LogOut size={16} strokeWidth={1.75} />\n    71→            Sign out\n    72→          </button>\n    73→        </div>\n    74→      )}\n    75→    </aside>\n    76→  )\n    77→}\n    78→"}]},"parent_tool_use_id":"toolu_0112Ziy6izM6ZuYDgDGovW1T","session_id":"ea780c21-dd76-4158-a0db-c55297864d38","uuid":"b02243db-cb0c-4c60-a291-59b21f8695dd"}
