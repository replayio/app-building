notes (task_id, content, author)\n    43→      VALUES (${taskId}::uuid, ${body.content}, ${req.user?.name ?? 'System'})\n    44→      RETURNING *\n    45→    `\n    46→    return Response.json(rows[0], { status: 201 })\n    47→  }\n    48→\n    49→  // DELETE /tasks/:taskId/notes/:noteId — delete a note\n    50→  if (req.method === 'DELETE' && taskId && subResource === 'notes') {\n    51→    const noteId = pathParts.length >= 6 ? pathParts[5] : null\n    52→    if (noteId) {\n    53→      await sql`DELETE FROM task_notes WHERE id = ${noteId}::uuid AND task_id = ${taskId}::uuid`\n    54→      return Response.json({ success: true })\n    55→    }\n    56→  }\n    57→\n    58→  // GET /tasks — list all upcoming (uncompleted) tasks with filters\n    59→  if (req.method === 'GET' && !taskId) {\n    60→    const search = url.searchParams.get('search') ?? ''\n    61→    const priority = url.searchParams.get('priority') ?? ''\n    62→    const assignee = url.searchParams.get('assignee') ?? ''\n    63→    const clientId = url.searchParams.get('clientId') ?? ''\n    64→    const includeCompleted = url.searchParams.get('includeCompleted') === 'true'\n    65→\n    66→    const searchPattern = search ? '%' + search + '%' : null\n    67→    const priorityFilter = priority || null\n    68→    const assigneePattern = assignee ? '%' + assignee + '%' : null\n    69→    const clientIdFilter = clientId || null\n    70→\n    71→    const tasks = await sql`\n    72→      SELECT t.*, c.name as client_name, d.name as deal_name\n    73→      FROM tasks t\n    74→      LEFT JOIN clients c ON t.client_id = c.id\n    75→      LEFT JOIN deals d ON t.deal_id = d.id\n    76→      WHERE (${includeCompleted}::boolean OR t.completed = false)\n    77→      AND (${searchPattern}::text IS NULL OR t.title ILIKE ${searchPattern})\n    78→      AND (${priorityFilter}::text IS NULL OR t.priority = ${priorityFilter})\n    79→      AND (${assigneePattern}::text IS NULL OR t.assignee_name ILIKE ${assigneePattern})\n    80→      AND (${clientIdFilter}::text IS NULL OR t.client_id = ${clientIdFilter}::uuid)\n    81→      ORDER BY t.due_date ASC NULLS LAST, t.created_at DESC\n    82→    `\n    83→\n    84→    const countResult = await sql`\n    85→      SELECT COUNT(*) as count\n    86→      FROM tasks t\n    87→      LEFT JOIN clients c ON t.client_id = c.id\n    88→      LEFT JOIN deals d ON t.deal_id = d.id\n    89→      WHERE (${includeCompleted}::boolean OR t.completed = false)\n    90→      AND (${searchPattern}::text IS NULL OR t.title ILIKE ${searchPattern})\n    91→      AND (${priorityFilter}::text IS NULL OR t.priority = ${priorityFilter})\n    92→      AND (${assigneePattern}::text IS NULL OR t.assignee_name ILIKE ${assigneePattern})\n    93→      AND (${clientIdFilter}::text IS NULL OR t.client_id = ${clientIdFilter}::uuid)\n    94→    `\n    95→    const total = parseInt(String(countResult[0].count), 10)\n    96→\n    97→    // Get unique assignees for filter dropdown\n    98→    const assignees = await sql`\n    99→      SELECT DISTINCT assignee_name, assignee_role\n   100→      FROM tasks\n   101→      WHERE assignee_name IS NOT NULL AND assignee_name != ''\n   102→      ORDER BY assignee_name ASC\n   103→    `\n   104→\n   105→    // Get unique clients for filter dropdown\n   106→    const clients = await sql`\n   107→      SELECT DISTINCT c.id, c.name\n   108→      FROM clients c\n   109→      JOIN tasks t ON t.client_id = c.id\n   110→      ORDER BY c.name ASC\n   111→    `\n   112→\n   113→    return Response.json({ tasks, total, assignees, clients })\n   114→  }\n   115→\n   116→  // POST /tasks — create a new task\n   117→  if (req.method === 'POST' && !taskId) {\n   118→    const body = await req.json() as {\n   119→      title: string\n   120→      description?: string\n   121→      due_date?: string\n   122→      priority?: string\n   123→      client_id?: string\n   124→      deal_id?: string\n   125→      assignee_name?: string\n   126→      assignee_role?: string\n   127→    }\n   128→\n   129→    const rows = await sql`\n   130→      INSERT INTO tasks (title, description, due_date, priority, client_id, deal_id, assignee_name, assignee_role)\n   131→      VALUES (\n   132→        ${body.title},\n   133→        ${body.description ?? null},\n   134→        ${body.due_date || null},\n   135→        ${body.priority ?? 'normal'},\n   136→        ${body.client_id || null},\n   137→        ${body.deal_id || null},\n   138→        ${body.assignee_name ?? null},\n   139→        ${body.assignee_role ?? null}\n   140→      )\n   141→      RETURNING *\n   142→    `\n   143→\n   144→    const task = rows[0]\n   145→\n   146→    // Create timeline event if client_id is provided\n   147→    if (body.client_id) {\n   148→      await sql`\n   149→        INSERT INTO timeline_events (client_id, event_type, description, user_name, related_entity_id, related_entity_type)\n   150→        VALUES (${body.client_id}::uuid, 'task_created', ${'Task Created: \\'' + body.title + '\\''}, ${req.user?.name ?? 'System'}, ${task.id}::uuid, 'task')\n   151→      `\n   152→    }\n   153→\n   154→    // Fetch related names\n   155→    if (task.client_id) {\n   156→      const clientRows = await sql`SELECT name FROM clients WHERE id = ${task.client_id}::uuid`\n   157→      task.client_name = clientRows.length > 0 ? clientRows[0].name : null\n   158→    }\n   159→    if (task.deal_id) {\n   160→      const dealRows = await sql`SELECT name FROM deals WHERE id = ${task.deal_id}::uuid`\n   161→      task.deal_name = dealRows.length > 0 ? dealRows[0].name : null\n   162→    }\n   163→\n   164→    return Response.json(task, { status: 201 })\n   165→  }\n   166→\n   167→  // PUT /tasks/:taskId — update a task\n   168→  if (req.method === 'PUT' && taskId) {\n   169→    const body = await req.json() as {\n   170→      completed?: boolean\n   171→      title?: string\n   172→      description?: string\n   173→      due_date?: string\n   174→      priority?: string\n   175→      assignee_name?: string\n   176→      assignee_role?: string\n   177→    }\n   178→\n   179→    const rows = await sql`\n   180→      UPDATE tasks SET\n   181→        completed = COALESCE(${body.completed !== undefined ? body.completed : null}, completed),\n   182→        completed_at = CASE\n   183→          WHEN ${body.completed !== undefined ? body.completed : null}::boolean = true THEN NOW()\n   184→          WHEN ${body.completed !== undefined ? body.completed : null}::boolean = false THEN NULL\n   185→          ELSE completed_at\n   186→        END,\n   187→        title = COALESCE(${body.title ?? null}, title),\n   188→        description = COALESCE(${body.description ?? null}, description),\n   189→        due_date = COALESCE(${body.due_date !== undefined ? (body.due_date || null) : null}, due_date),\n   190→        priority = COALESCE(${body.priority ?? null}, priority),\n   191→        assignee_name = COALESCE(${body.assignee_name ?? null}, assignee_name),\n   192→        assignee_role = COALESCE(${body.assignee_role ?? null}, assignee_role),\n   193→        updated_at = NOW()\n   194→      WHERE id = ${taskId}::uuid\n   195→      RETURNING *\n   196→    `\n   197→    if (rows.length === 0) {\n   198→      return new Response(JSON.stringify({ error: 'Task not found' }), { status: 404 })\n   199→    }\n   200→\n   201→    const task = rows[0]\n   202→\n   203→    // Create timeline event for completion\n   204→    if (body.completed && task.client_id) {\n   205→      await sql`\n   206→        INSERT INTO timeline_events (client_id, event_type, description, user_name, related_entity_id, related_entity_type)\n   207→        VALUES (${task.client_id}::uuid, 'task_completed', ${'Task Completed: \\'' + task.title + '\\''}, ${req.user?.name ?? 'System'}, ${taskId}::uuid, 'task')\n   208→      `\n   209→    }\n   210→\n   211→    // Fetch related names\n   212→    if (task.client_id) {\n   213→      const clientRows = await sql`SELECT name FROM clients WHERE id = ${task.client_id}::uuid`\n   214→      task.client_name = clientRows.length > 0 ? clientRows[0].name : null\n   215→    }\n   216→    if (task.deal_id) {\n   217→      const dealRows = await sql`SELECT name FROM deals WHERE id = ${task.deal_id}::uuid`\n   218→      task.deal_name = dealRows.length > 0 ? dealRows[0].name : null\n   219→    }\n   220→\n   221→    return Response.json(task)\n   222→  }\n   223→\n   224→  // DELETE /tasks/:taskId — delete a task\n   225→  if (req.method === 'DELETE' && taskId) {\n   226→    await sql`DELETE FROM tasks WHERE id = ${taskId}::uuid`\n   227→    return Response.json({ success: true })\n   228→  }\n   229→\n   230→  return new Response(JSON.stringify({ error: 'Method not allowed' }), { status: 405 })\n   231→}\n   232→\n   233→export default optionalAuth(handler)\n   234→"}]},"parent_tool_use_id":"toolu_0124zDNe7otxBhqQ1hi4thwM","session_id":"ea780c21-dd76-4158-a0db-c55297864d38","uuid":"81050d9a-8748-477d-84d7-a06c05231e7c"}
