] : null\n    57→\n    58→  // GET /individuals — list/search all individuals\n    59→  if (req.method === 'GET' && !individualId) {\n    60→    const search = url.searchParams.get('search') ?? ''\n    61→    const searchPattern = search ? '%' + search + '%' : null\n    62→    const rows = await sql`\n    63→      SELECT id, name, title FROM individuals\n    64→      WHERE (${searchPattern}::text IS NULL OR name ILIKE ${searchPattern})\n    65→      ORDER BY name ASC\n    66→      LIMIT 50\n    67→    ` as { id: string; name: string; title: string | null }[]\n    68→    return Response.json({ individuals: rows })\n    69→  }\n    70→\n    71→  // GET /individuals/:id — fetch individual with associations, relationships, and contact history\n    72→  if (req.method === 'GET' && individualId && !subResource) {\n    73→    const rows = await sql`\n    74→      SELECT * FROM individuals WHERE id = ${individualId}\n    75→    ` as IndividualRow[]\n    76→\n    77→    if (rows.length === 0) {\n    78→      return new Response(JSON.stringify({ error: 'Individual not found' }), { status: 404 })\n    79→    }\n    80→\n    81→    const individual = rows[0]\n    82→\n    83→    // Fetch client associations\n    84→    const clientAssociations = await sql`\n    85→      SELECT ci.client_id, c.name AS client_name, c.status AS client_status, ci.industry, ci.role\n    86→      FROM client_individuals ci\n    87→      JOIN clients c ON ci.client_id = c.id\n    88→      WHERE ci.individual_id = ${individualId}\n    89→      ORDER BY c.name ASC\n    90→    ` as ClientAssociationRow[]\n    91→\n    92→    // Fetch relationships\n    93→    const relationships = await sql`\n    94→      SELECT ir.id, ir.related_individual_id, i.name AS related_individual_name,\n    95→             ir.relationship_type, ci.role, ci.industry AS company\n    96→      FROM individual_relationships ir\n    97→      JOIN individuals i ON ir.related_individual_id = i.id\n    98→      LEFT JOIN client_individu