[5] : null\n    57→\n    58→  // GET /individuals — list/search all individuals\n    59→  if (req.method === 'GET' && !individualId) {\n    60→    const search = url.searchParams.get('search') ?? ''\n    61→    const searchPattern = search ? '%' + search + '%' : null\n    62→    const rows = await sql`\n    63→      SELECT id, name, title FROM individuals\n    64→      WHERE (${searchPattern}::text IS NULL OR name ILIKE ${searchPattern})\n    65→      ORDER BY name ASC\n    66→      LIMIT 50\n    67→    ` as { id: string; name: string; title: string | null }[]\n    68→    return Response.json({ individuals: rows })\n    69→  }\n    70→\n    71→  // GET /individuals/:id — fetch individual with associations, relationships, and contact history\n    72→  if (req.method === 'GET' && individualId && !subResource) {\n    73→    const rows = await sql`\n    74→      SELECT * FROM individuals WHERE id = ${individualId}\n    75→    ` as IndividualRow[]\n    76→\n    77→    if (rows.length === 0) {\n    78→      return new Response(JSON.stringify({ error: 'Individual not found' }), { status: 404 })\n    79→    }\n    80→\n    81→    const individual = rows[0]\n    82→\n    83→    // Fetch client associations\n    84→    const clientAssociations = await sql`\n    85→      SELECT ci.client_id, c.name AS client_name, c.status AS client_status, ci.industry, ci.role\n    86→      FROM client_individuals ci\n    87→      JOIN clients c ON ci.client_id = c.id\n    88→      WHERE ci.individual_id = ${individualId}\n    89→      ORDER BY c.name ASC\n    90→    ` as ClientAssociationRow[]\n    91→\n    92→    // Fetch relationships\n    93→    const relationships = await sql`\n    94→      SELECT ir.id, ir.related_individual_id, i.name AS related_individual_name,\n    95→             ir.relationship_type, ci.role, ci.industry AS company\n    96→      FROM individual_relationships ir\n    97→      JOIN individuals i ON ir.related_individual_id = i.id\n    98→      LEFT JOIN client_individuals ci ON ci.individual_id = ir.related_individual_id\n    99→        AND ci.client_id = (\n   100→          SELECT ci2.client_id FROM client_individuals ci2\n   101→          WHERE ci2.individual_id = ${individualId} LIMIT 1\n   102→        )\n   103→      WHERE ir.individual_id = ${individualId}\n   104→      ORDER BY i.name ASC\n   105→    ` as RelationshipRow[]\n   106→\n   107→    // Fetch contact history\n   108→    const contactHistory = await sql`\n   109→      SELECT * FROM contact_history\n   110→      WHERE individual_id = ${individualId}\n   111→      ORDER BY date DESC\n   112→    ` as ContactHistoryRow[]\n   113→\n   114→    return Response.json({\n   115→      ...individual,\n   116→      client_associations: clientAssociations,\n   117→      relationships,\n   118→      contact_history: contactHistory,\n   119→    })\n   120→  }\n   121→\n   122→  // PUT /individuals/:id — update individual info\n   123→  if (req.method === 'PUT' && individualId && !subResource) {\n   124→    const body = await req.json() as Partial<{\n   125→      name: string\n   126→      title: string\n   127→      email: string\n   128→      phone: string\n   129→      location: string\n   130→    }>\n   131→\n   132→    const rows = await sql`\n   133→      UPDATE individuals SET\n   134→        name = COALESCE(${body.name ?? null}, name),\n   135→        title = COALESCE(${body.title ?? null}, title),\n   136→        email = COALESCE(${body.email ?? null}, email),\n   137→        phone = COALESCE(${body.phone ?? null}, phone),\n   138→        location = COALESCE(${body.location ?? null}, location),\n   139→        updated_at = NOW()\n   140→      WHERE id = ${individualId}\n   141→      RETURNING *\n   142→    ` as IndividualRow[]\n   143→\n   144→    if (rows.length === 0) {\n   145→      return new Response(JSON.stringify({ error: 'Individual not found' }), { status: 404 })\n   146→    }\n   147→\n   148→    return Response.json(rows[0])\n   149→  }\n   150→\n   151→  // POST /individuals/:id/relationships — add a relationship\n   152→  if (req.method === 'POST' && individualId && subResource === 'relationships') {\n   153→    const body = await req.json() as {\n   154→      related_individual_id: string\n   155→      relationship_type: string\n   156→    }\n   157→\n   158→    const rows = await sql`\n   159→      INSERT INTO individual_relationships (individual_id, related_individual_id, relationship_type)\n   160→      VALUES (${individualId}, ${body.related_individual_id}, ${body.relationship_type})\n   161→      RETURNING *\n   162→    `\n   163→\n   164→    return Response.json(rows[0], { status: 201 })\n   165→  }\n   166→\n   167→  // DELETE /individuals/:id/relationships/:relId — delete a relationship\n   168→  if (req.method === 'DELETE' && individualId && subResource === 'relationships' && subId) {\n   169→    await sql`DELETE FROM individual_relationships WHERE id = ${subId} AND individual_id = ${individualId}`\n   170→    return Response.json({ success: true })\n   171→  }\n   172→\n   173→  // POST /individuals/:id/contact-history — add a contact history entry\n   174→  if (req.method === 'POST' && individualId && subResource === 'contact-history') {\n   175→    const body = await req.json() as {\n   176→      date: string\n   177→      type: string\n   178→      summary: string\n   179→      team_member: string\n   180→    }\n   181→\n   182→    const rows = await sql`\n   183→      INSERT INTO contact_history (individual_id, date, type, summary, team_member)\n   184→      VALUES (${individualId}, ${body.date}, ${body.type}, ${body.summary}, ${body.team_member || null})\n   185→      RETURNING *\n   186→    ` as ContactHistoryRow[]\n   187→\n   188→    return Response.json(rows[0], { status: 201 })\n   189→  }\n   190→\n   191→  // PUT /individuals/:id/contact-history/:entryId — update a contact history entry\n   192→  if (req.method === 'PUT' && individualId && subResource === 'contact-history' && subId) {\n   193→   