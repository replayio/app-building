[2026-02-17T16:17:21.209Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_012hrzd3ZpjFbHiWCvPm8FbR","type":"tool_result","content":"     1→import { createContext, useContext, useEffect, useState, useCallback, type ReactNode } from 'react'\n     2→import { auth } from '../lib/auth'\n     3→import { useGetUserInfoQuery } from '../store/authSlice'\n     4→import type { Session } from '@supabase/supabase-js'\n     5→\n     6→// The Supabase storage key — matches the one in auth.ts\n     7→const supabaseUrl = import.meta.env.VITE_AUTH_SUPABASE_URL\n     8→const SUPABASE_STORAGE_KEY = `sb-${new URL(supabaseUrl).hostname.split('.')[0]}-auth-token`\n     9→\n    10→interface AuthContextValue {\n    11→  isLoggedIn: boolean\n    12→  loading: boolean\n    13→  user: { id: string; email: string; name: string; avatar_url: string } | null\n    14→  signIn: () => void\n    15→  signOut: () => Promise<void>\n    16→}\n    17→\n    18→const AuthContext = createContext<AuthContextValue>({\n    19→  isLoggedIn: false,\n    20→  loading: true,\n    21→  user: null,\n    22→  signIn: () => {},\n    23→  signOut: async () => {},\n    24→})\n    25→\n    26→/**\n    27→ * Check localStorage for a session token (used as fallback in test mode\n    28→ * where tokens aren't set via real Supabase auth).\n    29→ */\n    30→function getStoredSession(): boolean {\n    31→  try {\n    32→    const stored = localStorage.getItem(SUPABASE_STORAGE_KEY)\n    33→    if (stored) {\n    34→      const parsed = JSON.parse(stored)\n    35→      return !!parsed.access_token\n    36→    }\n    37→  } catch {\n    38→    // ignore\n    39→  }\n    40→  return false\n    41→}\n    42→\n    43→export function AuthProvider({ children }: { children: ReactNode }) {\n    44→  const [session, setSession] = useState<Session | null>(null)\n    45→  const [hasStoredToken, setHasStoredToken] = useState(false)\n    46→  const [loading, setLoading] = useState(true)\n    47→\n    48→  useEffect(() => {\n    49→    auth.getSession().then(({ data: { session: s } }) => {\n    50→      setSession(s)\n    51→      if (!s) {\n    52→        setHasStoredToken(getStoredSession())\n    53→      }\n    54→      setLoading(false)\n    55→    })\n    56→\n    57→    const { data: { subscription } } = auth.onAuthStateChange((_event, s) => {\n    58→      setSession(s)\n    59→      if (!s) {\n    60→        setHasStoredToken(getStoredSession())\n    61→      }\n    62→      setLoading(false)\n    63→    })\n    64→\n    65→    return () => subscription.unsubscribe()\n    66→  }, [])\n    67→\n    68→  // Listen for auth-callback messages from popup window\n    69→  useEffect(() => {\n    70→    function handleMessage(event: MessageEvent) {\n    71→      if (event.origin !== window.location.origin) return\n    72→      if (event.data?.type === 'auth-callback') {\n    73→        // Popup completed auth — refresh session\n    74→        auth.getSession().then(({ data: { session: s } }) => {\n    75→          setSession(s)\n    76→          if (!s) {\n    77→            setHasStoredToken(getStoredSession())\n    78→          }\n    79→        })\n    80→      }\n    81→    }\n    82→    window.addEventListener('message', handleMessage)\n    83→    return () => window.removeEventListener('message', handleMessage)\n    84→  }, [])\n    85→\n    86→  const isLoggedIn = !!session || hasStoredToken\n    87→  const { data: userInfo } = useGetUserInfoQuery(undefined, { skip: !isLoggedIn })\n    88→\n    89→  const signIn = useCallback(() => {\n    90→    const callbackUrl = `${window.location.origin}/auth/callback`\n    91→    const authUrl = `https://auth.nut.new/functions/v1/oauth/start?provider=google&redirect_to=${encodeURIComponent(callbackUrl)}`\n    92→    const width = 500\n    93→    const height = 600\n    94→    const left = window.screenX + (window.innerWidth - width) / 2\n    95→    const top = window.screenY + (window.innerHeight - height) / 2\n    96→    window.open(authUrl, 'auth-popup', `width=${width},height=${height},left=${left},top=${top}`)\n    97→  }, [])\n    98→\n    99→  async function signOut() {\n   100→    await auth.signOut()\n   101→    localStorage.removeItem(SUPABASE_STORAGE_KEY)\n   102→    setSession(null)\n   103→    setHasStoredToken(false)\n   104→  }\n   105→\n   106→  return (\n   107→    <AuthContext.Provider\n   108→      value={{\n   109→        isLoggedIn,\n   110→        loading,\n   111→        user: userInfo ?? null,\n   112→        signIn,\n   113→        signOut,\n   114→      }}\n   115→    >\n   116→      {children}\n   117→    </AuthContext.Provider>\n   118→  )\n   119→}\n   120→\n   121→export function useAuth() {\n   122→  return useContext(AuthContext)\n   123→}\n   124→"}]},"parent_tool_use_id":"toolu_0124zDNe7otxBhqQ1hi4thwM","session_id":"ea780c21-dd76-4158-a0db-c55297864d38","uuid":"d8ebeebd-08db-4355-ad32-a4d339a4791b"}
