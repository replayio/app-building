[2026-02-19T23:25:56.805Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01F4B9mnE6aGyoWRivWpUFFB","type":"tool_result","content":"     1→import { getDb } from '../utils/db'\n     2→import { optionalAuth, type OptionalAuthRequest } from '../utils/auth'\n     3→\n     4→const VALID_EVENTS = [\n     5→  'client_created',\n     6→  'client_updated',\n     7→  'deal_created',\n     8→  'deal_stage_changed',\n     9→  'deal_closed_won',\n    10→  'deal_closed_lost',\n    11→  'task_created',\n    12→  'task_completed',\n    13→  'contact_created',\n    14→  'note_added',\n    15→]\n    16→\n    17→async function handler(req: OptionalAuthRequest) {\n    18→  const sql = getDb()\n    19→  const url = new URL(req.url)\n    20→  const pathParts = url.pathname.split('/').filter(Boolean)\n    21→  const webhookId = pathParts.length >= 4 ? pathParts[3] : null\n    22→\n    23→  // GET /webhooks — list all webhooks\n    24→  if (req.method === 'GET' && !webhookId) {\n    25→    const webhooks = await sql`\n    26→      SELECT * FROM webhooks ORDER BY created_at DESC\n    27→    `\n    28→    return Response.json({ webhooks, availableEvents: VALID_EVENTS })\n    29→  }\n    30→\n    31→  // POST /webhooks — create a new webhook\n    32→  if (req.method === 'POST' && !webhookId) {\n    33→    const body = await req.json() as {\n    34→      name: string\n    35→      url: string\n    36→      events: string[]\n    37→      enabled?: boolean\n    38→    }\n    39→\n    40→    if (!body.name?.trim()) {\n    41→      return new Response(JSON.stringify({ error: 'Name is required' }), { status: 400 })\n    42→    }\n    43→    if (!body.url?.trim()) {\n    44→      return new Response(JSON.stringify({ error: 'URL is required' }), { status: 400 })\n    45→    }\n    46→    if (!body.events || body.events.length === 0) {\n    47→      return new Response(JSON.stringify({ error: 'At least one event is required' }), { status: 400 })\n    48→    }\n    49→\n    50→    const rows = await sql`\n    51→      INSERT INTO webhooks (name, url, events, enabled)\n    52→      VALUES (${body.name.trim()}, ${body.url.trim()}, ${body.events}, ${body.enabled !== false})\n    53→      RETURNING *\n    54→    `\n    55→    return Response.json(rows[0], { status: 201 })\n    56→  }\n    57→\n    58→  // PUT /webhooks/:id — update a webhook\n    59→  if (req.method === 'PUT' && webhookId) {\n    60→    const body = await req.json() as {\n    61→      name?: string\n    62→      url?: string\n    63→      events?: string[]\n    64→      enabled?: boolean\n    65→    }\n    66→\n    67→    const existing = await sql`SELECT * FROM webhooks WHERE id = ${webhookId}::uuid`\n    68→    if (existing.length === 0) {\n    69→      return new Response(JSON.stringify({ error: 'Webhook not found' }), { status: 404 })\n    70→    }\n    71→\n    72→    const rows = await sql`\n    73→      UPDATE webhooks SET\n    74→        name = ${body.name?.trim() ?? existing[0].name},\n    75→        url = ${body.url?.trim() ?? existing[0].url},\n    76→        events = ${body.events ?? existing[0].events},\n    77→        enabled = ${body.enabled !== undefined ? body.enabled : existing[0].enabled},\n    78→        updated_at = NOW()\n    79→      WHERE id = ${webhookId}::uuid\n    80→      RETURNING *\n    81→    `\n    82→    return Response.json(rows[0])\n    83→  }\n    84→\n    85→  // DELETE /webhooks/:id — delete a webhook\n    86→  if (req.method === 'DELETE' && webhookId) {\n    87→    const rows = await sql`DELETE FROM webhooks WHERE id = ${webhookId}::uuid RETURNING id`\n    88→    if (rows.length === 0) {\n    89→      return new Response(JSON.stringify({ error: 'Webhook not found' }), { status: 404 })\n    90→    }\n    91→    return Response.json({ deleted: true })\n    92→  }\n    93→\n    94→  return new Response(JSON.stringify({ error: 'Not found' }), { status: 404 })\n    95→}\n    96→\n    97→export default optionalAuth(handler)\n    98→"}]},"pare