[2026-02-19T23:26:03.042Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01VxPkfTfiGw6B3UumrdWoCm","type":"tool_result","content":"     1→import { useState, useEffect, useCallback } from 'react'\n     2→import { SettingsPageHeader } from '../components/settings/SettingsPageHeader'\n     3→import { ImportExportSection } from '../components/settings/ImportExportSection'\n     4→import { WebhookSection } from '../components/settings/WebhookSection'\n     5→import { WebhookModal } from '../components/settings/WebhookModal'\n     6→import { ImportDialog } from '../components/shared/ImportDialog'\n     7→import { ConfirmDialog } from '../components/shared/ConfirmDialog'\n     8→\n     9→interface Webhook {\n    10→  id: string\n    11→  name: string\n    12→  url: string\n    13→  events: string[]\n    14→  enabled: boolean\n    15→  created_at: string\n    16→}\n    17→\n    18→export function SettingsPage() {\n    19→  const [webhooks, setWebhooks] = useState<Webhook[]>([])\n    20→  const [availableEvents, setAvailableEvents] = useState<string[]>([])\n    21→  const [webhookModalOpen, setWebhookModalOpen] = useState(false)\n    22→  const [editingWebhook, setEditingWebhook] = useState<Webhook | null>(null)\n    23→  const [deleteConfirm, setDeleteConfirm] = useState<string | null>(null)\n    24→  const [importType, setImportType] = useState<string | null>(null)\n    25→\n    26→  const loadWebhooks = useCallback(async () => {\n    27→    try {\n    28→      const res = await fetch('/.netlify/functions/webhooks')\n    29→      const data = await res.json() as { webhooks: Webhook[]; availableEvents: string[] }\n    30→      setWebhooks(data.webhooks)\n    31→      setAvailableEvents(data.availableEvents)\n    32→    } catch {\n    33→      // ignore\n    34→    }\n    35→  }, [])\n    36→\n    37→  useEffect(() => {\n    38→    loadWebhooks()\n    39→  }, [loadWebhooks])\n    40→\n    41→  async function handleSaveWebhook(data: { name: string; url: string; events: string[]; enabled: boolean }) {\n    42→    try {\n    43→      if (editingWebhook) {\n    44→        await fetch(`/.netlify/functions/webhooks/${editingWebhook.id}`, {\n    45→          method: 'PUT',\n    46→          headers: { 'Content-Type': 'application/json' },\n    47→          body: JSON.stringify(data),\n    48→        })\n    49→      } else {\n    50→        await fetch('/.netlify/functions/webhooks', {\n    51→          method: 'POST',\n    52→          headers: { 'Content-Type': 'application/json' },\n    53→          body: JSON.stringify(data),\n    54→        })\n    55→      }\n    56→      setWebhookModalOpen(false)\n    57→      setEditingWebhook(null)\n    58→      loadWebhooks()\n    59→    } catch {\n    60→      // ignore\n    61→    }\n    62→  }\n    63→\n    64→  async function handleDeleteWebhook() {\n    65→    if (!deleteConfirm) return\n    66→    try {\n    67→      await fetch(`/.netlify/functions/webhooks/${deleteConfirm}`, { method: 'DELETE' })\n    68→      setDeleteConfirm(null)\n    69→      loadWebhooks()\n    70→    } catch {\n    71→      // ignore\n    72→    }\n    73→  }\n    74→\n    75→  async function handleToggleWebhook(id: string, enabled: boolean) {\n    76→    try {\n    77→      await fetch(`/.netlify/functions/webhooks/${id}`, {\n    78→        method: 'PUT',\n    79→        headers: { 'Content-Type': 'application/json' },\n    80→        body: JSON.stringify({ enabled }),\n    81→      })\n    82→      loadWebhooks()\n    83→    } catch {\n    84→      // ignore\n    85→    }\n    86→  }\n    87→\n    88→  function downloadCSV(filename: string, headers: string[], rows: string[][]) {\n    89→    const csv = [headers.join(','), ...rows.map(r => r.map(c => `\"${(c ?? '').replace(/\"/g, '\"\"')}\"`).join(','))].join('\\n')\n    90→    const blob = new Blob([csv], { type: 'text/csv' })\n    91→    const url = URL.createObjectURL(blob)\n    92→    const a = document.createElement('a')\n    93→    a.href = url\n    94→    a.download = filename\n    95→    a.click()\n    96→    URL.revokeObjectURL(url)\n    97→  }\n    98→\n    99→  async function handleExportClients() {\n   100→    const res = await fetch('/.netlify/functions/clients?pageSize=10000')\n   101→    const data = await res.json() as { clients: Record<string, unknown>[] }\n   102→    downloadCSV('clients-export.csv',\n   103→      ['Name', 'Type', 'Status', 'Tags', 'Primary Contact', 'Open Deals'],\n   104→      data.clients.map((c: Record<string, unknown>) => [\n   105→        String(c.name ?? ''), String(c.type ?? ''), String(c.status ?? ''),\n   106→        Array.isArray(c.tags) ? c.tags.join('; ') : '',\n   107→        String(c.primary_contact ?? ''), String(c.open_deals_count ?? '0'),\n   108→      ])\n   109→    )\n   110→  }\n   111→\n   112→  async function handleExportDeals() {\n   113→    const res = await fetch('/.netlify/functions/deals?pageSize=10000')\n   114→    const data = await res.json() as { deals: Record<string, unknown>[] }\n   115→    downloadCSV('deals-export.csv',\n   116→      ['Name', 'Client', 'Value', 'Stage', 'Owner', 'Status'],\n   117→      data.deals.map((d: Record<string, unknown>) => [\n   118→        String(d.name ?? ''), String(d.client_name ?? ''), String(d.value ?? '0'),\n   119→        String(d.stage ?? ''), String(d.owner ?? ''), String(d.status ?? ''),\n   120→      ])\n   121→    )\n   122→  }\n   123→\n   124→  async function handleExportTasks() {\n   125→    const res = await fetch('/.netlify/functions/tasks')\n   126→    const data = await res.json() as { tasks: Record<string, unknown>[] }\n   127→    downloadCSV('tasks-export.csv',\n   128→      ['Title', 'Description', 'Priority', 'Due Date', 'Assignee', 'Client', 'Completed'],\n   129→      data.tasks.map((t: Record<string, unknown>) => [\n   130→        String(t.title ?? ''), String(t.description ?? ''), String(t.priority ?? ''),\n   131→        String(t.due_date ?? ''), String(t.assignee_name ?? ''), String(t.client_name ?? ''),\n   132→        t.completed ? 'Yes' : 'No',\n   133→      ])\n   134→    )\n   135→  }\n   136→\n   137→  return (\n   138→    <div className=\"p-6 max-w-[900px]\">\n   139→      <SettingsPageHeader />\n   140→\n   141→      <div className=\"flex flex-col gap-6\">\n   142→        <ImportExportSection\n   143→          onImportClients={() => setImportType('clients')}\n   144→          onImportDeals={() => setImportType('deals')}\n   145→          onImportTasks={() => setImportType('tasks')}\n   146→          onImportContacts={() => setImportType('contacts')}\n   147→          onExportClients={handleExportClients}\n   148→          onExportDeals={handleExportDeals}\n   149→          onExportTasks={handleExportTasks}\n   150→        />\n   151→\n   152→        <WebhookSection\n   153→          webhooks={webhooks}\n   154→          onAdd={() => { setEditingWebhook(null); setWebhookModalOpen(true) }}\n   155→          onEdit={(w) => { setEditingWebhook(w); setWebhookModalOpen(true) }}\n   156→          onDelete={(id) => setDeleteConfirm(id)}\n   157→          onToggle={handleToggleWebhook}\n   158→        />\n   159→      </div>\n   160→\n   161→      <WebhookModal\n   162→        open={webhookModalOpen}\n   163→        webhook={editingWebhook}\n   164→        availableEvents={availableEvents}\n   165→        onClose={() => { setWebhookModalOpen(false); setEditingWebhook(null) }}\n   166→        onSave={handleSaveWebhook}\n   167→      />\n   168→\n   169→      <ConfirmDialog\n   170→        open={deleteConfirm !== null}\n   171→        title=\"Delete Webhook\"\n   172→        message=\"Are you sure you want to delete this webhook? This action cannot be undone.\"\n   173→        confirmLabel=\"Delete\"\n   174→        onConfirm={handleDeleteWebhook}\n   175→        onCancel={() => setDeleteConfirm(null)}\n   176→      />\n   177→\n   178→      {importType === 'clients' && (\n   179→        <ImportDialog\n   180→          entityName=\"Client\"\n   181→          entityNamePlural=\"Clients\"\n   182→          columns={CLIENT_CSV_COLUMNS}\n   183→          headerMap={CLIENT_HEADER_MAP}\n   184→          templateFilename=\"clients-import-template.csv\"\n   185→          templateExample='Acme Corp,organization,active,\"Enterprise; SaaS\",Referral,John Smith,Q1 Campaign,Direct Sales,2024-01-15'\n   186→          apiEndpoint=\"/.netlify/functions/clients?action=import\"\n   187→          apiBodyKey=\"clients\"\n   188→          onClose={() => setImportType(null)}\n   189→          onImported={() => {}}\n   190→        />\n   191→      )}\n   192→\n   193→      {importType === 'deals' && (\n   194→        <ImportDialog\n   195→          entityName=\"Deal\"\n   196→          entityNamePlural=\"Deals\"\n   197→          columns={DEAL_CSV_COLUMNS}\n   198→          headerMap={DEAL_HEADER_MAP}\n   199→          templateFilename=\"deals-import-template.csv\"\n   200→          templateExample=\"Enterprise License,Acme Corp,50000,proposal,Jane Smith,75,2024-06-15,active\"\n   201→          apiEndpoint=\"/.netlify/functions/deals?action=import\"\n   202→          apiBodyKey=\"deals\"\n   203→          onClose={() => setImportType(null)}\n   204→          onImported={() => {}}\n   205→        />\n   206→      )}\n   207→\n   208→      {importType === 'tasks' && (\n   209→        <ImportDialog\n   210→          entityName=\"Task\"\n   211→          entityNamePlural=\"Tasks\"\n   212→          columns={TASK_CSV_COLUMNS}\n   213→          headerMap={TASK_HEADER_MAP}\n   214→          templateFilename=\"tasks-import-template.csv\"\n   215→          templateExample=\"Follow up with client,Review Q1 proposal details,2024-03-15,high,Acme Corp,Jane Smith\"\n   216→          apiEndpoint=\"/.netlify/functions/tasks?action=import\"\n   217→          apiBodyKey=\"tasks\"\n   218→          onClose={() => setImportType(null)}\n   219→          onImported={() => {}}\n   220→        />\n   221→      )}\n   222→\n   223→      {importTyp