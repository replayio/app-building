tion loadUser() {\n    61→      try {\n    62→        const res = await fetch(`/.netlify/functions/users/${userId}`)\n    63→        if (!res.ok) {\n    64→          navigate('/users')\n    65→          return\n    66→        }\n    67→        const data = await res.json() as { user: UserDetail; deals: UserDeal[]; tasks: UserTask[]; activity: UserActivity[] }\n    68→        setUser(data.user)\n    69→        setDeals(data.deals)\n    70→        setTasks(data.tasks)\n    71→        setActivity(data.activity)\n    72→      } catch {\n    73→        navigate('/users')\n    74→      }\n    75→    }\n    76→    if (userId) loadUser()\n    77→  }, [userId, navigate])\n    78→\n    79→  if (!user) return null\n    80→\n    81→  const activeDeals = deals.filter(d => d.stage !== 'closed_won' && d.stage !== 'closed_lost')\n    82→  const openTasks = tasks.filter(t => !t.completed)\n    83→\n    84→  function formatDate(dateStr: string) {\n    85→    return new Date(dateStr).toLocaleDateString('en-US', { month: 'short', day: 'numeric', year: 'numeric' })\n    86→  }\n    87→\n    88→  function formatRelativeDate(dateStr: string) {\n    89→    const now = new Date()\n    90→    const date = new Date(dateStr)\n    91→    const diffMs = now.getTime() - date.getTime()\n    92→    const diffDays = Math.floor(diffMs / (1000 * 60 * 60 * 24))\n    93→    if (diffDays === 0) return 'Today'\n    94→    if (diffDays === 1) return 'Yesterday'\n    95→    if (diffDays < 7) return `${diffDays} days ago`\n    96→    if (diffDays < 30) return `${Math.floor(diffDays / 7)} week${Math.floor(diffDays / 7) > 1 ? 's' : ''} ago`\n    97→    return formatDate(dateStr)\n    98→  }\n    99→\n   100→  return (\n   101→    <div className=\"p-6 max-w-[900px]\" data-testid=\"user-detail-page\">\n   102→      {/* Header */}\n   103→      <div data-testid=\"user-detail-header\" className=\"mb-6\">\n   104→        <button\n   105→          data-testid=\"user-detail-back\"\n   106→          onClick={() => nav