ion_date = COALESCE(${installation_date || null}, installation_date),\n   149→          photo_url = COALESCE(${photo_url}, photo_url),\n   150→          updated_at = NOW()\n   151→        WHERE id = ${equipmentId}::uuid\n   152→        RETURNING *\n   153→      `;\n   154→      if (rows.length === 0) {\n   155→        return new Response(JSON.stringify({ error: \"Equipment not found\" }), {\n   156→          status: 404,\n   157→          headers: { \"Content-Type\": \"application/json\" },\n   158→        });\n   159→      }\n   160→      return new Response(JSON.stringify(rows[0]), {\n   161→        status: 200,\n   162→        headers: { \"Content-Type\": \"application/json\" },\n   163→      });\n   164→    } catch (err) {\n   165→      console.error(\"Equipment PUT error:\", err);\n   166→      return new Response(JSON.stringify({ error: \"Internal server error\" }), {\n   167→        status: 500,\n   168→        headers: { \"Content-Type\": \"application/json\" },\n   169→      });\n   170→    }\n   171→  }\n   172→\n   173→  if (req.method === \"DELETE\") {\n   174→    if (!equipmentId) {\n   175→      return new Response(JSON.stringify({ error: \"Equipment ID required\" }), {\n   176→        status: 400,\n   177→        headers: { \"Content-Type\": \"application/json\" },\n   178→      });\n   179→    }\n   180→\n   181→    try {\n   182→      await sql`DELETE FROM equipment WHERE id = ${equipmentId}::uuid`;\n   183→      return new Response(JSON.stringify({ success: true }), {\n   184→        status: 200,\n   185→        headers: { \"Content-Type\": \"application/json\" },\n   186→      });\n   187→    } catch (err) {\n   188→      console.error(\"Equipment DELETE error:\", err);\n   189→      return new Response(JSON.stringify({ error: \"Internal server error\" }), {\n   190→        status: 500,\n   191→        headers: { \"Content-Type\": \"application/json\" },\n   192→      });\n   193→    }\n   194→  }\n   195→\n   196→  return new Response(JSON.string