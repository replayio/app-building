         <line x1=\"22\" y1=\"11\" x2=\"16\" y2=\"11\" />\n    43→        </svg>\n    44→        Add New Supplier\n    45→      </button>\n    46→\n    47→      <button\n    48→        className=\"quick-action-btn\"\n    49→        data-testid=\"quick-action-create-order\"\n    50→        onClick={() => setShowOrderModal(true)}\n    51→      >\n    52→        <svg width=\"20\" height=\"20\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" strokeWidth=\"2\" strokeLinecap=\"round\" strokeLinejoin=\"round\">\n    53→          <path d=\"M14 2H6a2 2 0 0 0-2 2v16a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V8z\" />\n    54→          <polyline points=\"14 2 14 8 20 8\" />\n    55→          <line x1=\"12\" y1=\"18\" x2=\"12\" y2=\"12\" />\n    56→          <line x1=\"9\" y1=\"15\" x2=\"15\" y2=\"15\" />\n    57→        </svg>\n    58→        Create Purchase Order\n    59→      </button>\n    60→\n    61→      {showSupplierModal && (\n    62→        <AddSupplierModal\n    63→          onClose={() => setShowSupplierModal(false)}\n    64→          onSave={async (data) => {\n    65→            await dispatch(createSupplier(data)).unwrap();\n    66→            await dispatch(fetchSuppliers());\n    67→            setShowSupplierModal(false);\n    68→          }}\n    69→        />\n    70→      )}\n    71→\n    72→      {showOrderModal && (\n    73→        <CreateOrderModal\n    74→          suppliers={suppliers}\n    75→          onClose={() => setShowOrderModal(false)}\n    76→          onSave={async (data) => {\n    77→            await dispatch(createOrder(data)).unwrap();\n    78→            await dispatch(fetchOrders());\n    79→            setShowOrderModal(false);\n    80→          }}\n    81→        />\n    82→      )}\n    83→    </div>\n    84→  );\n    85→}\n    86→\n    87→/* Add Supplier Modal */\n    88→\n    89→interface AddSupplierForm {\n    90→  name: string;\n    91→  address: string;\n    92→  contact_name: string;\n    93→  phone: string;\n    94→  email: string;\n    95→  description: string;\n    96→  status: string;\n    97→}\n    98→\n    99→function AddSupplierModal({\n   100→  onClose,\n   101→  onSave,\n   102→}: {\n   103→  onClose: () => void;\n   104→  onSave: (data: AddSupplierForm) => Promise<void>;\n   105→}) {\n   106→  const [form, setForm] = useState<AddSupplierForm>({\n   107→    name: \"\",\n   108→    address: \"\",\n   109→    contact_name: \"\",\n   110→    phone: \"\",\n   111→    email: \"\",\n   112→    description: \"\",\n   113→    status: \"Active\",\n   114→  });\n   115→  const [errors, setErrors] = useState<Record<string, string>>({});\n   116→  const [saving, setSaving] = useState(false);\n   117→  const [statusOpen, setStatusOpen] = useState(false);\n   118→\n   119→  const update = (field: keyof AddSupplierForm, value: string) => {\n   120→    setForm((f) => ({ ...f, [field]: value }));\n   121→    if (errors[field]) setErrors((e) => ({ ...e, [field]: \"\" }));\n   122→  };\n   123→\n   124→  const handleSave = async () => {\n   125→    const newErrors: Record<string, string> = {};\n   126→    if (!form.name.trim()) newErrors.name = \"Supplier name is required\";\n   127→    if (Object.keys(newErrors).length > 0) {\n   128→      setErrors(newErrors);\n   129→      return;\n   130→    }\n   131→    setSaving(true);\n   132→    try {\n   133→      await onSave(form);\n   134→    } finally {\n   135→      setSaving(false);\n   136→    }\n   137→  };\n   138→\n   139→  return (\n   140→    <div className=\"modal-overlay\" data-testid=\"add-supplier-modal\" onClick={onClose}>\n   141→      <div className=\"modal-content\" onClick={(e) => e.stopPropagation()}>\n   142→        <div className=\"modal-header\">\n   143→          <h2 className=\"modal-title\">Add New Supplier</h2>\n   144→          <button className=\"modal-close-btn\" data-testid=\"add-supplier-modal-close\" onClick={onClose}>\n   145→            <svg width=\"16\" height=\"16\" viewBox=\"0 0 24 24\" fill=\"none\" stroke=\"currentColor\" strokeWidth=\"2\">\n   146→              <line x1=\"18\" y1=\"6\" x2=\"6\" y2=\"18\" /><line x1=\"6\" y1=\"6\" x2=\"18\" y2=\"18\" />\n   147→            </svg>\n   148→          </button>\n   149→        </div>\n   150→\n   151→        <div className=\"form-group\">\n   152→          <label className=\"form-label\">Supplier Name *</label>\n   153→          <input\n   154→            className=\"form-input\"\n   155→            data-testid=\"supplier-form-name\"\n   156→            value={form.name}\n   157→            onChange={(e) => update(\"name\", e.target.value)}\n   158→            placeholder=\"Enter supplier name\"\n   159→          />\n   160→          {errors.name && <div className=\"form-error\" data-testid=\"supplier-form-name-error\">{errors.name}</div>}\n   161→        </div>\n   162→\n   163→        <div className=\"form-group\">\n   164→          <label className=\"form-label\">Address</label>\n   165→          <textarea\n   166→            className=\"form-textarea\"\n   167→            data-testid=\"supplier-form-address\"\n   168→            value={form.address}\n   169→            onChange={(e) => update(\"address\", e.target.value)}\n   170→            placeholder=\"Enter address\"\n   171→          />\n   172→        </div>\n   173→\n   174→        <div className=\"form-row\">\n   175→          <div className=\"form-group\">\n   176→            <label className=\"form-label\">Contact Name</label>\n   177→            <input\n   178→              className=\"form-input\"\n   179→              data-testid=\"supplier-form-contact-name\"\n   180→              value={form.contact_name}\n   181→              onChange={(e) => update(\"contact_name\", e.target.value)}\n   182→              placeholder=\"Enter contact name\"\n   183→            />\n   184→          </div>\n   185→          <div className=\"form-group\">\n   186→            <label className=\"form-label\">Phone</label>\n   187→            <input\n   188→              className=\"form-input\"\n   189→              data-testid=\"supplier-form-phone\"\n   190→              value={form.phone}\n   191→              onChange={(e) => update(\"phone\", e.target.value)}\n   192→              placeholder=\"Enter phone number\"\n   193→            />\n   194→          </div>\n   195→        </div>\n   196→\n   197→        <div className=\"form-group\">\n   198→          <label className=\"form-label\">Email</label>\n   199→          <input\n   200→            className=\"form-input\"\n   201→            data-testid=\"supplier-form-email\"\n   202→            value={form.email}\n   203→            onChange={(e) => update(\"email\", e.target.value)}\n   204→            placeholder=\"Enter email\"\n   205→            type=\"email\"\n   206→          />\n   207→        </div>\n   208→\n   209→        <div className=\"form-group\">\n   210→          <label className=\"form-label\">Description</label>\n   211→          <textarea\n   212→            className=\"form-textarea\"\n   213→            data-testid=\"supplier-form-description\"\n   214→            value={form.description}\n   215→            onChange={(e) => update(\"description\", e.target.value)}\n   216→            placeholder=\"Enter description\"\n   217→          />\n   218→        </div>\n   219→\n   220→        <div className=\"form-group\">\n   221→          <label className=\"form-label\">Status</label>\n   222→          <div className=\"filter-select\" style={{ position: \"relative\" }}>\n   223→            <button\n   224→              className=\"filter-select-trigger\"\n   225→              data-testid=\"supplier-form-status\"\n   226→              type=\"button\"\n   227→              onClick={() => setStatusOpen(!statusOpen)}\n   228→            >\n   229→              <span className=\"filter-select-value\">{form.status}</span>\n   230→              <svg className={`filter-select-chevron${statusOpen ? \" filter-select-chevron--open\" : \"\"}`} width=\"12\" height=\"12\" viewBox=\"0 0 12 12\" fill=\"none\">\n   231→                <path d=\"M3 4.5L6 7.5L9 4.5\" stroke=\"currentColor\" strokeWidth=\"1.5\" strokeLinecap=\"round\" strokeLinejoin=\"round\" />\n   232→              </svg>\n   233→            </button>\n   234→            {statusOpen && (\n   235→              <div className=\"filter-select-dropdown\" data-testid=\"supplier-form-status-dropdown\">\n   236→                <div className=\"filter-select-options\">\n   237→                  {SUPPLIER_STATUSES.map((s) => (\n   238→                    <button\n   239→                      key={s}\n   240→                      className={`filter-select-option${s === form.status ? \" filter-select-option--selected\" : \"\"}`}\n   241→                      data-testid={`supplier-form-status-option-${s.toLowerCase().replace(/ /g, \"-\")}`}\n   242→                      type=\"button\"\n   243→                      onClick={() => {\n   244→                        update(\"status\", s);\n   245→                        setStatusOpen(false);\n   246→                      }}\n   247→                    >\n   248→                      {s}\n   249→                    </button>\n   250→                  ))}\n   251→                </div>\n   252→              </div>\n   253→            )}\n   254→          </div>\n   255→        </div>\n   256→\n   257→        <div className=\"modal-footer\">\n   258→          <button className=\"btn-secondary\" data-testid=\"add-supplier-cancel\" onClick={onClose}>\n   259→            Cancel\n   260→          </button>\n   261→          <button className=\"btn-primary\" data-testid=\"add-supplier-save\" onClick={handleSave} disabled={saving}>\n   262→            {saving ? \"Saving...\" : \"Save\"}\n   263→          </button>\n   264→        </div>\n   265→      </div>\n   266→    </div>\n   267→  );\n   268→}\n   269→\n   270→/* Create Order Modal */\n   271→\n   272→interface CreateOrderData {\n   273→  supplier_id: string;\n   274→  order_date: string;\n   275→  expected_delivery: string;\n   276→  line_items: { sku: string; item_name: string; quantity: number; unit_price: number }[];\n   277→}\n   278→\n   279→function CreateOrderModal({\n   280→  suppliers,\n   281→  onClose,\n   282→  onSave,\n   283→}: {\n   284→  suppliers: { id: string; name: string }[];\n   285→  onClose: () => void;\n   286→  onSave: (data: CreateOrderData) => Promise<void>;\n   287→}) {\n   288→  const today = new Date().toISOString().split(\"T\")[0];\n   289→  const [supplierId, setSupplierId] = useState(\"\");\n   290→  const [orderDate, setOrderDate] = useState(today);\n   291→  const [expectedDelivery, setExpectedDelivery] = useState(\"\");\n   292→  const [lineItems, setLineItems] = useState<LineItemInput[]>([emptyLineItem()]);\n   293→  const [errors, setErrors] = useState<Record<string, string>>({});\n   294→  const [saving, setSaving] = useState(false);\n   295→\n   296→  const supplierOptions = suppliers.map((s) => ({ id: s.id, label: s.name }));\n   297→\n   298→  const updateLineItem = (index: number, field: keyof LineItemInput, value: string) => {\n   299→    setLineItems((items) => {\n   300→      const next = [...items];\n   301→      next[index] = { ...next[index], [field]: value };\n   302→      return next;\n   303→    });\n   304→  };\n   305→\n   306→  const removeLineItem = (index: number) => {\n   307→    if (lineItems.length <= 1) return;\n   308→    setLineItems((items) => items.filter((_, i) => i !== index));\n   309→  };\n   310→\n   311→  const addLineItem = () => {\n   312→    setLineItems((items) => [...items, emptyLineItem()]);\n   313→  };\n   314→\n   315→  const handleSave = async () => {\n   316→    const newErrors: Record<string, string> = {};\n   317→    if (!supplierId) newErrors.supplier = \"Supplier is required\";\n   318→    if (Object.keys(newErrors).length > 0) {\n   319→      setErrors(newErrors);\n   320→      return;\n   321→    }\n   322→\n   323→    const parsedLineItems = lineItems\n   324→      .filter