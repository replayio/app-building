0→}\n   211→\n   212→function countResults(suites: SuiteData[], counts: { passed: number; failed: number; skipped: number }) {\n   213→  for (const suite of suites) {\n   214→    for (const spec of suite.specs || []) {\n   215→      for (const test of spec.tests || []) {\n   216→        for (const result of test.results || []) {\n   217→          if (result.status === 'passed') counts.passed++\n   218→          else if (result.status === 'skipped') counts.skipped++\n   219→          else counts.failed++\n   220→        }\n   221→      }\n   222→    }\n   223→    if (suite.suites) {\n   224→      countResults(suite.suites, counts)\n   225→    }\n   226→  }\n   227→}\n   228→\n   229→function parsePlaywrightResults(): PlaywrightResults {\n   230→  const resultsPath = join(appDir, 'test-results', 'results.json')\n   231→  if (!existsSync(resultsPath)) return { passed: 0, failed: 0, skipped: 0 }\n   232→  const data = JSON.parse(readFileSync(resultsPath, 'utf-8')) as { suites?: SuiteData[] }\n   233→  const counts = { passed: 0, failed: 0, skipped: 0 }\n   234→  countResults(data.suites || [], counts)\n   235→  return counts\n   236→}\n   237→\n   238→async function main() {\n   239→  let branchId = ''\n   240→  let playwrightExitCode = 1\n   241→\n   242→  try {\n   243→    // Step 1: Kill stale processes\n   244→    killStaleProcesses()\n   245→\n   246→    // Step 2: Clean up stale Neon branches\n   247→    await cleanupStaleBranches()\n   248→\n   249→    // Step 3: Create ephemeral Neon branch\n   250→    appendLog(`Creating Neon branch: ${branchName}`)\n   251→    const branch = await createBranch()\n   252→    branchId = branch.branchId\n   253→    const branchDatabaseUrl = buildDatabaseUrl(branch.host)\n   254→    appendLog(`Branch created: ${branchId} (host: ${branch.host})`)\n   255→\n   256→    // Step 4: Initialize schema, run migrations, seed data\n   257→    appendLog('Initializing schema...')\n   258→    await initSchema(branchDatabaseUrl)\n   259→    app