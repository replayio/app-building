[2026-02-27T06:36:54.839Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01U3xhzRJzPEdxrwbBM9nNG4","type":"tool_result","content":"     1→import { useState, useMemo } from \"react\";\n     2→import { useNavigate } from \"react-router-dom\";\n     3→import { FilterSelect } from \"@shared/components/FilterSelect\";\n     4→import type { Batch } from \"../types\";\n     5→\n     6→interface AllBatchesTableProps {\n     7→  batches: Batch[];\n     8→  unitOfMeasure: string;\n     9→}\n    10→\n    11→function formatDate(dateStr: string): string {\n    12→  const d = new Date(dateStr);\n    13→  return d.toLocaleDateString(\"en-US\", {\n    14→    month: \"short\",\n    15→    day: \"2-digit\",\n    16→    year: \"numeric\",\n    17→  });\n    18→}\n    19→\n    20→function formatQuantity(value: number): string {\n    21→  const num = Number(value);\n    22→  return num.toLocaleString(\"en-US\");\n    23→}\n    24→\n    25→function getDateRangeStart(range: string): Date | null {\n    26→  const now = new Date();\n    27→  switch (range) {\n    28→    case \"7d\":\n    29→      return new Date(now.getTime() - 7 * 24 * 60 * 60 * 1000);\n    30→    case \"30d\":\n    31→      return new Date(now.getTime() - 30 * 24 * 60 * 60 * 1000);\n    32→    case \"90d\":\n    33→      return new Date(now.getTime() - 90 * 24 * 60 * 60 * 1000);\n    34→    default:\n    35→      return null;\n    36→  }\n    37→}\n    38→\n    39→export function AllBatchesTable({\n    40→  batches,\n    41→  unitOfMeasure,\n    42→}: AllBatchesTableProps) {\n    43→  const navigate = useNavigate();\n    44→  const [accountFilter, setAccountFilter] = useState(\"\");\n    45→  const [dateFilter, setDateFilter] = useState(\"\");\n    46→\n    47→  // Build account options from the batches data\n    48→  const accountOptions = useMemo(() => {\n    49→    const accountMap = new Map<string, string>();\n    50→    for (const b of batches) {\n    51→      if (b.account_id && b.account_name) {\n    52→        accountMap.set(b.account_id, b.account_name);\n    53→      }\n    54→    }\n    55→    const options = [{ value: \"\", label: \"[All Accounts]\" }];\n    56→    for (const [id, name] of accountMap) {\n    57→      options.push({ value: id, label: name });\n    58→    }\n    59→    return options;\n    60→  }, [batches]);\n    61→\n    62→  const dateOptions = [\n    63→    { value: \"\", label: \"[All Dates]\" },\n    64→    { value: \"7d\", label: \"Last 7 Days\" },\n    65→    { value: \"30d\", label: \"Last 30 Days\" },\n    66→    { value: \"90d\", label: \"Last 90 Days\" },\n    67→  ];\n    68→\n    69→  const filteredBatches = useMemo(() => {\n    70→    let result = batches;\n    71→\n    72→    if (accountFilter) {\n    73→      result = result.filter((b) => b.account_id === accountFilter);\n    74→    }\n    75→\n    76→    if (dateFilter) {\n    77→      const rangeStart = getDateRangeStart(dateFilter);\n    78→      if (rangeStart) {\n    79→        result = result.filter(\n    80→          (b) => new Date(b.created_at) >= rangeStart\n    81→        );\n    82→      }\n    83→    }\n    84→\n    85→    return result;\n    86→  }, [batches, accountFilter, dateFilter]);\n    87→\n    88→  return (\n    89→    <div data-testid=\"all-batches-section\">\n    90→      <div className=\"section-card\">\n    91→        <div className=\"section-card-header\">\n    92→          <h2 className=\"section-card-title\">\n    93→            <svg\n    94→              className=\"section-card-title-icon\"\n    95→              viewBox=\"0 0 24 24\"\n    96→              fill=\"none\"\n    97→              stroke=\"currentColor\"\n    98→              strokeWidth=\"2\"\n    99→              strokeLinecap=\"round\"\n   100→              strokeLinejoin=\"round\""}]},"parent_tool_use_id":"toolu_019k4pAvr2CJytzXjnz5ByG6","session_id":"803982d4-1eb8-4b0e-90aa-3e51546ed787","uuid":"244d7730-7f25-4ef0-b906-e8fe6cb4184b"}
