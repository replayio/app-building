ND b.account_id = ${accountId}\n    40→          ORDER BY b.created_at DESC\n    41→        `;\n    42→      } else if (materialId) {\n    43→        rows = await sql`\n    44→          SELECT b.id, b.material_id, m.name as material_name,\n    45→                 b.account_id, a.name as account_name,\n    46→                 b.quantity, b.unit, b.location, b.lot_number,\n    47→                 b.expiration_date, b.quality_grade, b.storage_condition,\n    48→                 b.status, b.originating_transaction_id,\n    49→                 b.created_at, b.updated_at\n    50→          FROM batches b\n    51→          LEFT JOIN materials m ON b.material_id = m.id\n    52→          LEFT JOIN accounts a ON b.account_id = a.id\n    53→          WHERE b.material_id = ${materialId}\n    54→          ORDER BY b.created_at DESC\n    55→        `;\n    56→      } else if (accountId) {\n    57→        rows = await sql`\n    58→          SELECT b.id, b.material_id, m.name as material_name,\n    59→                 b.account_id, a.name as account_name,\n    60→                 b.quantity, b.unit, b.location, b.lot_number,\n    61→                 b.expiration_date, b.quality_grade, b.storage_condition,\n    62→                 b.status, b.originating_transaction_id,\n    63→                 b.created_at, b.updated_at\n    64→          FROM batches b\n    65→          LEFT JOIN materials m ON b.material_id = m.id\n    66→          LEFT JOIN accounts a ON b.account_id = a.id\n    67→          WHERE b.account_id = ${accountId}\n    68→          ORDER BY b.created_at DESC\n    69→        `;\n    70→      } else {\n    71→        rows = await sql`\n    72→          SELECT b.id, b.material_id, m.name as material_name,\n    73→                 b.account_id, a.name as account_name,\n    74→                 b.quantity, b.unit, b.location, b.lot_number,\n    75→                 b.expiration_date, b.quality_grade, b.storage_condition,\n    76→                 b.status, b.originating_transac