[2026-02-20T20:15:11.988Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01RY4YotNN3f5Rh7BZFp3Vut","type":"tool_result","content":"     1→import { useState, useEffect, useCallback } from 'react'\n     2→import { useAppDispatch, useAppSelector } from '../store/hooks'\n     3→import { fetchClients, createClient, deleteClient, setPage } from '../store/clientsSlice'\n     4→import { ClientsPageHeader } from '../components/clients/ClientsPageHeader'\n     5→import { ClientsSearchBar } from '../components/clients/ClientsSearchBar'\n     6→import { ClientsFilterControls } from '../components/clients/ClientsFilterControls'\n     7→import { ClientsTable } from '../components/clients/ClientsTable'\n     8→import { ClientsPagination } from '../components/clients/ClientsPagination'\n     9→import { AddClientModal } from '../components/clients/AddClientModal'\n    10→import { ConfirmDialog } from '../components/shared/ConfirmDialog'\n    11→import { ImportDialog } from '../components/shared/ImportDialog'\n    12→import type { ClientType, ClientStatus } from '../types'\n    13→\n    14→export function ClientsListPage() {\n    15→  const dispatch = useAppDispatch()\n    16→  const { items, total, page, pageSize, loading, availableTags, availableSources } = useAppSelector(\n    17→    (state) => state.clients\n    18→  )\n    19→\n    20→  const [search, setSearch] = useState('')\n    21→  const [statusFilter, setStatusFilter] = useState('')\n    22→  const [tagFilter, setTagFilter] = useState('')\n    23→  const [sourceFilter, setSourceFilter] = useState('')\n    24→  const [sort, setSort] = useState('updated_at')\n    25→  const [addModalOpen, setAddModalOpen] = useState(false)\n    26→  const [deleteConfirm, setDeleteConfirm] = useState<string | null>(null)\n    27→  const [importDialogOpen, setImportDialogOpen] = useState(false)\n    28→  const [importContactsDialogOpen, setImportContactsDialogOpen] = useState(false)\n    29→\n    30→  const loadClients = useCallback(() => {\n    31→    dispatch(\n    32→      fetchClients({\n    33→        page,\n    34→        search: search || undefined,\n    35→        status: statusFilter || undefined,\n    36→        tag: tagFilter || undefined,\n    37→        source: sourceFilter || undefined,\n    38→        sort,\n    39→      })\n    40→    )\n    41→  }, [dispatch, page, search, statusFilter, tagFilter, sourceFilter, sort])\n    42→\n    43→  useEffect(() => {\n    44→    loadClients()\n    45→  }, [loadClients])\n    46→\n    47→  // Debounce search input\n    48→  const [searchInput, setSearchInput] = useState('')\n    49→  useEffect(() => {\n    50→    const timer = setTimeout(() => {\n    51→      setSearch(searchInput)\n    52→      dispatch(setPage(1))\n    53→    }, 300)\n    54→    return () => clearTimeout(timer)\n    55→  }, [searchInput, dispatch])\n    56→\n    57→  function handleAddClient(data: {\n    58→    name: string\n    59→    type: ClientType\n    60→    status: ClientStatus\n    61→    tags: string[]\n    62→    source_type: string\n    63→    source_detail: string\n    64→    campaign: string\n    65→    channel: string\n    66→  }) {\n    67→    dispatch(createClient(data)).then(() => {\n    68→      setAddModalOpen(false)\n    69→      loadClients()\n    70→    })\n    71→  }\n    72→\n    73→  function handleDeleteClient() {\n    74→    if (!deleteConfirm) return\n    75→    dispatch(deleteClient(deleteConfirm)).then(() => {\n    76→      setDeleteConfirm(null)\n    77→    })\n    78→  }\n    79→\n    80→  function handlePageChange(newPage: number) {\n    81→    dispatch(setPage(newPage))\n    82→  }\n    83→\n    84→  function handleExport() {\n    85→    const headers = ['Name', 'Type', 'Status', 'Tags', 'Primary Contact', 'Open Deals']\n    86→    const rows = items.map((c) => [\n    87→      c.name,\n    88→      c.type,\n    89→      c.status,\n    90→      c.tags.join('; '),\n    91→      c.primary_contact_name ?? '',\n    92→      String(c.open_deals_count ?? 0),\n    93→    ])\n    94→    const csv = [headers, ...rows].map((row) => row.map((v) => `\"${v}\"`).join(',')).join('\\n')\n    95→    const blob = new Blob([csv], { type: 'text/csv' })\n    96→    const csvUrl = URL.createObjectURL(blob)\n    97→    const a = document.createElement('a')\n    98→    a.href = csvUrl\n    99→    a.download = 'clients-export.csv'\n   100→    a.click()\n   101→    URL.revokeObjectURL(csvUrl)\n   102→  }\n   103→\n   104→  return (\n   105→    <div className=\"p-3 sm:p-6\" data-testid=\"clients-list-page\">\n   106→      <ClientsPageHeader\n   107→        onAddClient={() => setAddModalOpen(true)}\n   108→        onImport={() => setImportDialogOpen(true)}\n   109→        onImportContacts={() => setImportContactsDialogOpen(true)}\n   110→        onExport={handleExport}\n   111→      />\n   112→\n   113→      {/* Search + Filters row */}\n   114→      <div className=\"flex flex-wrap items-center gap-3 mb-4\">\n   115→        <ClientsSearchBar value={searchInput} onChange={setSearchInput} />\n   116→        <ClientsFilterControls\n   117→          status={statusFilter}\n   118→          tag={tagFilter}\n   119→          source={sourceFilter}\n   120→          sort={sort}\n   121→          availableTags={availableTags}\n   122→          availableSources={availableSources}\n   123→          onStatusChange={(v) => { setStatusFilter(v); dispatch(setPage(1)) }}\n   124→          onTagChange={(v) => { setTagFilter(v); dispatch(setPage(1)) }}\n   125→          onSourceChange={(v) => { setSourceFilter(v); dispatch(setPage(1)) }}\n   126→          onSortChange={setSort}\n   127→        />\n   128→      </div>\n   129→\n   130→      {/* Table */}\n   131→      <div className=\"border border-border rounded-[6px] bg-surface overflow-x-auto\">\n   132→        {loading ? (\n   133→          <div className=\"flex items-center justify-center py-12 text-[13px] text-text-muted\">\n   134→            Loading clients...\n   135→          </div>\n   136→        ) : (\n   137→          <ClientsTable clients={items} onDeleteClient={(id) => setDeleteConfirm(id)} />\n   138→        )}\n   139→        <ClientsPagination\n   140→          page={page}\n   141→          pageSize={pageSize}\n   142→          total={total}\n   143→          onPageChange={handlePageChange}\n   144→        />\n   145→      </div>\n   146→\n   147→      {/* Add Client Modal */}\n   148→      <AddClientModal\n   149→        open={addModalOpen}\n   150→        onClose={() => setAddModalOpen(false)}\n   151→        onSave={handleAddClient}\n   152→      />\n   153→\n   154→      {/* Delete Confirmation */}\n   155→      <ConfirmDialog\n   156→        open={deleteConfirm !== null}\n   157→        title=\"Delete Client\"\n   158→        message=\"Are you sure you want to delete this client? This action cannot be undone.\"\n   159→        confirmLabel=\"Delete\"\n   160→        onConfirm={handleDeleteClient}\n   161→        onCancel={() => setDeleteConfirm(null)}\n   162→      />\n   163→\n   164→      {/* Import Dialog */}\n   165→      {importDialogOpen && (\n   166→        <ImportDialog\n   167→          entityName=\"Client\"\n   168→          entityNamePlural=\"Clients\"\n   169→          columns={CLIENT_CSV_COLUMNS}\n   170→          headerMap={CLIENT_HEADER_MAP}\n   171→          templateFilename=\"clients-import-template.csv\"\n   172→          templateExample='Acme Corp,organization,active,\"Enterprise; SaaS\",Referral,John Smith,Q1 Campaign,Direct Sales,2024-01-15'\n   173→          apiEndpoint=\"/.netlify/functions/clients?action=import\"\n   174→          apiBodyKey=\"clients\"\n   175→          onClose={() => setImportDialogOpen(false)}\n   176→          onImported={loadClients}\n   177→        />\n   178→      )}\n   179→\n   180→      {importContactsDialogOpen && (\n   181→        <ImportDialog\n   182→          entityName=\"Contact\"\n   183→          entityNamePlural=\"Contacts\"\n   184→          columns={CONTACT_CSV_COLUMNS}\n   185→          headerMap={CONTACT_HEADER_MAP}\n   186→          templateFilename=\"contacts-import-template.csv\"\n   187→          templateExample=\"Sarah Johnson,CEO,sarah@acmecorp.com,+1 555-100-0001,New York,Acme Corp\"\n   188→          apiEndpoint=\"/.netlify/functions/individuals?action=import\"\n   189→          apiBodyKey=\"contacts\"\n   190→          onClose={() => setImportContactsDialogOpen(false)}\n   191→          onImported={loadClients}\n   192→        />\n   193→      )}\n   194→    </div>\n   195→  )\n   196→}\n   197→\n   198→const CLIENT_CSV_COLUMNS = [\n   199→  { name: 'Name', required: true, description: 'Client name' },\n   200→  { name: 'Type', required: false, description: '\"organization\" or \"individual\" (default: organization)' },\n   201→  { name: 'Status', required: false, description: '\"active\", \"inactive\", \"prospect\", or \"churned\" (default: prospect)' },\n   202→  { name: 'Tags', required: false, description: 'Semicolon-separated tags, e.g. \"Enterprise; SaaS\"' },\n   203→  { name: 'Source Type', required: false, description: 'Acquisition source type, e.g. \"Referral\", \"Campaign\"' },\n   204→  { name: 'Source Detail', required: false, description: 'Source details' },\n   205→  { name: 'Campaign', required: false, description: 'Campaign name' },\n   206→  { name: 'Channel', required: false, description: 'Acquisition channel' },\n   207→  { name: 'Date Acquired', required: false, description: 'Date in YYYY-MM-DD format' },\n   208→]\n   209→\n   210→const CLIENT_HEADER_MAP: Record<string, string> = {\n   211→  'name': 'name',\n   212→  'type': 'type',\n   213→  'status': 'status',\n   214→  'tags': 'tags',\n   215→  'source type': 'source_type',\n   216→  'source_type': 'source_type',\n   217→  'source detail': 'source_detail',\n   218→  'source_detail': 'source_detail',\n   219→  'campaign': 'campaign',\n   220→  'channel': 'channel',\n   221→  'date acquired': 'date_acquired',\n   222→  'date_acquired': 'date_acquired',\n   223→}\n   224→\n   225→const CONTACT_CSV_COLUMNS = [\n   226→  { name: 'Name', required: true, description: 'Contact name' },\n   227→  { name: 'Title', required: false, description: 'Job title, e.g. \"CEO\", \"VP Engineering\"' },\n   228→  { name: 'Email', required: false, description: 'Email address' },\n   229→  { name: 'Phone', required: false, description: 'Phone number' },\n   230→  { name: 'Location', required: false, description: 'City, state, or address' },\n   231→  { name: 'Client Name', required: false, description: 'Name of an existing client to associate with' },\n   232→]\n   233→\n   234→const CONTACT_HEADER_MAP: Record<string, string> = {\n   235→  'name': 'name',\n   236→  'title': 'title',\n   237→  'email': 'email',\n   238→  'phone': 'phone',\n   239→  'location': 'location',\n   240→  'client name': 'client_name',\n   241→  'client_name': 'client_name',\n   242→  'client': 'client_name',\n   243→}\n   244→"}]},"parent_tool_use_id":"toolu_01WR9mHgzLUHpBMbTc7Bbm1G","session_id":"03f3a0a3-8c2a-4560-abbb-1538d56ad28c","uuid":"e8f9f162-ffb9-40f2-a908-cc5a07d12532"}
