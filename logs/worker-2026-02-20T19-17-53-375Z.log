[2026-02-20T19:07:22.197Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_011NXTmTnxGHfwu6Gi2E3qHT","type":"tool_result","content":"     1→import { getDb } from '../utils/db'\n     2→import { optionalAuth, type OptionalAuthRequest } from '../utils/auth'\n     3→\n     4→interface IndividualRow {\n     5→  id: string\n     6→  name: string\n     7→  title: string | null\n     8→  email: string | null\n     9→  phone: string | null\n    10→  location: string | null\n    11→  created_at: string\n    12→  updated_at: string\n    13→}\n    14→\n    15→interface ClientAssociationRow {\n    16→  client_id: string\n    17→  client_name: string\n    18→  client_status: string\n    19→  industry: string | null\n    20→  role: string | null\n    21→}\n    22→\n    23→interface RelationshipRow {\n    24→  id: string\n    25→  related_individual_id: string\n    26→  related_individual_name: string\n    27→  relationship_type: string\n    28→  role: string | null\n    29→  company: string | null\n    30→}\n    31→\n    32→interface ContactHistoryRow {\n    33→  id: string\n    34→  individual_id: string\n    35→  date: string\n    36→  type: string\n    37→  summary: string\n    38→  team_member: string | null\n    39→  created_at: string\n    40→  updated_at: string\n    41→}\n    42→\n    43→async function handler(req: OptionalAuthRequest) {\n    44→  const sql = getDb()\n    45→  const url = new URL(req.url)\n    46→  const pathParts = url.pathname.split('/').filter(Boolean)\n    47→  // Path: /.netlify/functions/individuals/:individualId/:subResource/:subId\n    48→  const individualId = pathParts.length >= 4 ? pathParts[3] : null\n    49→  const subResource = pathParts.length >= 5 ? pathParts[4] : null\n    50→  const subId = pathParts.length >= 6 ? pathParts[5] : null\n    51→\n    52→  // POST /individuals?action=import (bulk CSV import)\n    53→  if (req.method === 'POST' && !individualId && url.searchParams.get('action') === 'import') {\n    54→    const body = await req.json() as {\n    55→      contacts: Array<{\n    56→        name: string\n    57→        title?: string\n    58→        email?: string\n    59→        phone?: string\n    60→        location?: string\n    61→        client_name?: string\n    62→      }>\n    63→    }\n    64→\n    65→    const results = { imported: 0, errors: [] as string[] }\n    66→\n    67→    for (let i = 0; i < body.contacts.length; i++) {\n    68→      const row = body.contacts[i]\n    69→      if (!row.name?.trim()) {\n    70→        results.errors.push(`Row ${i + 1}: Name is required`)\n    71→        continue\n    72→      }\n    73→      try {\n    74→        const individualRows = await sql`\n    75→          INSERT INTO individuals (name, title, email, phone, location)\n    76→          VALUES (${row.name.trim()}, ${row.title?.trim() || null}, ${row.email?.trim() || null}, ${row.phone?.trim() || null}, ${row.location?.trim() || null})\n    77→          RETURNING id\n    78→        `\n    79→        const individualId = individualRows[0].id\n    80→        // Associate with client if client_name provided\n    81→        if (row.client_name?.trim()) {\n    82→          const clientRows = await sql`SELECT id FROM clients WHERE LOWER(name) = LOWER(${row.client_name.trim()}) LIMIT 1`\n    83→          if (clientRows.length > 0) {\n    84→            await sql`\n    85→              INSERT INTO client_individuals (client_id, individual_id)\n    86→              VALUES (${clientRows[0].id}::uuid, ${individualId}::uuid)\n    87→            `\n    88→          } else {\n    89→            results.errors.push(`Row ${i + 1}: Contact imported but client \"${row.client_name.trim()}\" not found — not associated`)\n    90→          }\n    91→        }\n    92→        results.imported++\n    93→      } catch (err) {\n    94→        results.errors.push(`Row ${i + 1}: ${err instanceof Error ? err.message : 'Database error'}`)\n    95→      }\n    96→    }\n    97→\n    98→    return Response.json(results, { status: 200 })\n    99→  }\n   100→\n   101→  // GET /individuals — list/search all individuals\n   102→  if (req.method === 'GET' && !individualId) {\n   103→    const search = url.searchParams.get('search') ?? ''\n   104→    const searchPattern = search ? '%' + search + '%' : null\n   105→    const rows = await sql`\n   106→      SELECT id, name, title FROM individuals\n   107→      WHERE (${searchPattern}::text IS NULL OR name ILIKE ${searchPattern})\n   108→      ORDER BY name ASC\n   109→      LIMIT 50\n   110→    ` as { id: string; name: string; title: string | null }[]\n   111→    return Response.json({ individuals: rows })\n   112→  }\n   113→\n   114→  // GET /individuals/:id — fetch individual with associations, relationships, and contact history\n   115→  if (req.method === 'GET' && individualId && !subResource) {\n   116→    const rows = await sql`\n   117→      SELECT * FROM individuals WHERE id = ${individualId}\n   118→    ` as IndividualRow[]\n   119→\n   120→    if (rows.length === 0) {\n   121→      return new Response(JSON.stringify({ error: 'Individual not found' }), { status: 404 })\n   122→    }\n   123→\n   124→    const individual = rows[0]\n   125→\n   126→    // Fetch client associations\n   127→    const clientAssociations = await sql`\n   128→      SELECT ci.client_id, c.name AS client_name, c.status AS client_status, ci.industry, ci.role\n   129→      FROM client_individuals ci\n   130→      JOIN clients c ON ci.client_id = c.id\n   131→      WHERE ci.individual_id = ${individualId}\n   132→      ORDER BY c.name ASC\n   133→    ` as ClientAssociationRow[]\n   134→\n   135→    // Fetch relationships\n   136→    const relationships = await sql`\n   137→      SELECT ir.id, ir.related_individual_id, i.name AS related_individual_name,\n   138→             ir.relationship_type, ci.role, ci.industry AS company\n   139→      FROM individual_relationships ir\n   140→      JOIN individuals i ON ir.related_individual_id = i.id\n   141→      LEFT JOIN client_individuals ci ON ci.individual_id = ir.related_individual_id\n   142→        AND ci.client_id = (\n   143→          SELECT ci2.client_id FROM client_individuals ci2\n   144→          WHERE ci2.individual_id = ${individualId} LIMIT 1\n   145→        )\n   146→      WHERE ir.individual_id = ${individualId}\n   147→      ORDER BY i.name ASC\n   148→    ` as RelationshipRow[]\n   149→\n   150→    // Fetch contact history\n   151→    const contactHistory = await sql`\n   152→      SELECT * FROM contact_history\n   153→      WHERE individual_id = ${individualId}\n   154→      ORDER BY date DESC\n   155→    ` as ContactHistoryRow[]\n   156→\n   157→    return Response.json({\n   158→      ...individual,\n   159→      client_associations: clientAssociations,\n   160→      relationships,\n   161→      contact_history: contactHistory,\n   162→    })\n   163→  }\n   164→\n   165→  // PUT /individuals/:id — update individual info\n   166→  if (req.method === 'PUT' && individualId && !subResource) {\n   167→    const body = await req.json() as Partial<{\n   168→      name: string\n   169→      title: string\n   170→      email: string\n   171→      phone: string\n   172→      location: string\n   173→    }>\n   174→\n   175→    const rows = await sql`\n   176→      UPDATE individuals SET\n   177→        name = COALESCE(${body.name ?? null}, name),\n   178→        title = COALESCE(${body.title ?? null}, title),\n   179→        email = COALESCE(${body.email ?? null}, email),\n   180→        phone = COALESCE(${body.phone ?? null}, phone),\n   181→        location = COALESCE(${body.location ?? null}, location),\n   182→        updated_at = NOW()\n   183→      WHERE id = ${individualId}\n   184→      RETURNING *\n   185→    ` as IndividualRow[]\n   186→\n   187→    if (rows.length === 0) {\n   188→      return new Response(JSON.stringify({ error: 'Individual not found' }), { status: 404 })\n   189→    }\n   190→\n   191→    return Response.json(rows[0])\n   192→  }\n   193→\n   194→  // POST /individuals/:id/relationships — add a relationship\n   195→  if (req.method === 'POST' && individualId && subResource === 'relationships') {\n   196→    const body = await req.json() as {\n   197→      related_individual_id: string\n   198→      relationship_type: string\n   199→    }\n   200→\n   201→    const rows = await sql`\n   202→      INSERT INTO individual_relationships (individual_id, related_individual_id, relationship_type)\n   203→      VALUES (${individualId}, ${body.related_individual_id}, ${body.relationship_type})\n   204→      RETURNING *\n   205→    `\n   206→\n   207→    return Response.json(rows[0], { status: 201 })\n   208→  }\n   209→\n   210→  // DELETE /individuals/:id/relationships/:relId — delete a relationship\n   211→  if (req.method === 'DELETE' && individualId && subResource === 'relationships' && subId) {\n   212→    await sql`DELETE FROM individual_relationships WHERE id = ${subId} AND individual_id = ${individualId}`\n   213→    return Response.json({ success: true })\n   214→  }\n   215→\n   216→  // POST /individuals/:id/contact-history — add a contact history entry\n   217→  if (req.method === 'POST' && individualId && subResource === 'contact-history') {\n   218→    const body = await req.json() as {\n   219→      date: string\n   220→      type: string\n   221→      summary: string\n   222→      team_member: string\n   223→    }\n   224→\n   225→    const rows = await sql`\n   226→      INSERT INTO contact_history (individual_id, date, type, summary, team_member)\n   227→      VALUES (${individualId}, ${body.date || null}, ${body.type}, ${body.summary}, ${body.team_member || null})\n   228→      RETURNING *\n   229→    ` as ContactHistoryRow[]\n   230→\n   231→    return Response.json(rows[0], { status: 201 })\n   232→  }\n   233→\n   234→  // PUT /individuals/:id/contact-history/:entryId — update a contact history entry\n   235→  if (req.method === 'PUT' && individualId && subResource === 'contact-history' && subId) {\n   236→    const body = await req.json() as Partial<{\n   237→      date: string\n   238→      type: string\n   239→      summary: string\n   240→      team_member: string\n   241→    }>\n   242→\n   243→    const rows = await sql`\n   244→      UPDATE contact_history SET\n   245→        date = COALESCE(${body.date || null}, date),\n   246→        type = COALESCE(${body.type ?? null}, type),\n   247→        summary = COALESCE(${body.summary ?? null}, summary),\n   248→        team_member = COALESCE(${body.team_member ?? null}, team_member),\n   249→        updated_at = NOW()\n   250→      WHERE id = ${subId} AND individual_id = ${individualId}\n   251→      RETURNING *\n   252→    ` as ContactHistoryRow[]\n   253→\n   254→    if (rows.length === 0) {\n   255→      return new Response(JSON.stringify({ error: 'Entry not found' }), { status: 404 })\n   256→    }\n   257→\n   258→    return Response.json(rows[0])\n   259→  }\n   260→\n   261→  // DELETE /individuals/:id/contact-history/:entryId — delete a contact history entry\n   262→  if (req.method === 'DELETE' && individualId && subResource === 'contact-history' && subId) {\n   263→    await sql`DELETE FROM contact_history WHERE id = ${subId} AND individual_id = ${individualId}`\n   264→    return Response.json({ success: true })\n   265→  }\n   266→\n   267→  return new Response(JSON.stringify({ error: 'Method not allowed' }), { status: 405 })\n   268→}\n   269→\n   270→export default optionalAuth(handler)\n   271→"}]},"parent_tool_use_id":"toolu_01FGhPRdhXc2vgSJAJ9BigCH","session_id":"3da3a56b-30bd-428a-b01f-c4bcb1c5eb57","uuid":"d70c8e74-c55c-45dc-8a44-50b1d379c022"}
