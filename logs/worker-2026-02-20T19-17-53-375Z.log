)\n    27→  }\n    28→\n    29→  // GET /clients\n    30→  if (req.method === 'GET') {\n    31→    const page = parseInt(url.searchParams.get('page') ?? '1')\n    32→    const pageSizeParam = url.searchParams.get('pageSize')\n    33→    const pageSize = pageSizeParam ? parseInt(pageSizeParam) : 50\n    34→    const offset = (page - 1) * pageSize\n    35→    const search = url.searchParams.get('search') ?? ''\n    36→    const status = url.searchParams.get('status') ?? ''\n    37→    const tag = url.searchParams.get('tag') ?? ''\n    38→    const source = url.searchParams.get('source') ?? ''\n    39→    const sort = url.searchParams.get('sort') ?? 'updated_at'\n    40→\n    41→    const searchPattern = search ? '%' + search + '%' : null\n    42→\n    43→    // Count query using conditional WHERE with tagged template\n    44→    const countResult = await sql`\n    45→      SELECT COUNT(*) as count FROM clients c\n    46→      WHERE (${searchPattern}::text IS NULL OR (\n    47→        c.name ILIKE ${searchPattern}\n    48→        OR EXISTS (SELECT 1 FROM unnest(c.tags) AS tg WHERE tg ILIKE ${searchPattern})\n    49→        OR EXISTS (SELECT 1 FROM client_individuals ci JOIN individuals i ON ci.individual_id = i.id WHERE ci.client_id = c.id AND i.name ILIKE ${searchPattern})\n    50→      ))\n    51→      AND (${status || null}::text IS NULL OR c.status = ${status || null})\n    52→      AND (${tag || null}::text IS NULL OR ${tag || null} = ANY(c.tags))\n    53→      AND (${source || null}::text IS NULL OR c.source_type = ${source || null})\n    54→    `\n    55→\n    56→    // Map sort parameter to safe sort key\n    57→    const sortMap: Record<string, string> = {\n    58→      'name_asc': 'name_asc',\n    59→      'name_desc': 'name_desc',\n    60→      'status': 'status',\n    61→      'updated_at': 'updated_at',\n    62→      'most_deals': 'most_deals',\n    63→    }\n    64→    const sortKey = sortMap[sort] ?? 'updated_at'\n    65→\n    66→    // Data query — use CASE-based ORDER BY to avoid dynamic SQL\n    67→    const dataResult = await sql`\n    68→      SELECT c.*,\n    69→        (SELECT COUNT(*) FROM deals d WHERE d.client_id = c.id AND d.stage NOT IN ('closed_won', 'closed_lost')) as open_deals_count,\n    70→        COALESCE((SELECT SUM(d.value) FROM deals d WHERE d.client_id = c.id AND d.stage NOT IN ('closed_won', 'closed_lost')), 0) as open_deals_value,\n    71→        (SELECT i.name FROM client_individuals ci JOIN individuals i ON ci.individual_id = i.id WHERE ci.client_id = c.id AND ci.is_primary = true LIMIT 1) as primary_contact_name,\n    72→        (SELECT ci.role FROM client_individuals ci WHERE ci.client_id = c.id AND ci.is_primary = true LIMIT 1) as primary_contact_role,\n    73→        (SELECT t.title FROM tasks t WHERE t.client_id = c.id AND t.completed = false ORDER BY t.due_date ASC NULLS LAST LIMIT 1) as next_task_title,\n    74→        (SELECT t.due_date FROM tasks t WHERE t.client_id = c.id AND t.completed = false ORDER BY t.due_date ASC NULLS LAST LIMIT 1) as next_task_due\n    75→      FROM clients c\n    76→      WHERE (${searchPattern}::text IS NULL OR (\n    77→        c.name ILIKE ${searchPattern}\n    78→        OR EXISTS (SELECT 1 FROM unnest(c.tags) AS tg WHERE tg ILIKE ${searchPattern})\n    79→        OR EXISTS (SELECT 1 FROM client_individuals ci JOIN individuals i ON ci.individual_id = i.id WHERE ci.client_id = c.id AND i.name ILIKE ${searchPattern})\n    80→      ))\n    81→      AND (${status || null}::text IS NULL OR c.status = ${status || null})\n    82→      AND (${tag || null}::text IS NULL OR ${tag || null} = ANY(c.tags))\n    83→      AND (${source || null}::text IS NULL OR c.source_type = ${source || null})\n    84→      ORDER BY\n    85→        CASE WHEN ${sortKey} = 'name_asc' THEN c.name END ASC,\n    86→        CASE WHEN ${sortKey} = 'name_desc' THEN c.name END DESC,\n    87→        CASE WHEN ${sortKey} = 'status' THEN c.status END ASC,\n    88→        CASE WHEN ${sortKey