or(\"URL is required\");\n        return;\n      }\n      setSaving(true);\n      setError(\"\");\n      try {\n        await dispatch(\n          createAttachmentLink({\n            filename: linkName.trim() || linkUrl.trim(),\n            url: linkUrl.trim(),\n            clientId,\n            dealId: dealId || undefined,\n          })\n        ).unwrap();\n        resetAndClose();\n      } catch (err) {\n        setError(err instanceof Error ? err.message : \"Failed to create link\");\n      } finally {\n        setSaving(false);\n      }\n    }\n  };\n\n  const resetAndClose = () => {\n    setMode(\"file\");\n    setFile(null);\n    setLinkName(\"\");\n    setLinkUrl(\"\");\n    setDealId(\"\");\n    setError(\"\");\n    onClose();\n  };\n\n  const dealOptions = [\n    { value: \"\", label: \"None\" },\n    ...deals.map((d) => ({ value: d.id, label: d.name })),\n  ];\n\n  return (\n    <div className=\"modal-overlay\" data-testid=\"add-attachment-modal\" onClick={resetAndClose}>\n      <div className=\"modal\" onClick={(e) => e.stopPropagation()}>\n        <div className=\"modal-header\">\n          <h2 className=\"modal-title\">Add Attachment</h2>\n          <button className=\"modal-close\" onClick={resetAndClose} type=\"button\" data-testid=\"add-attachment-close\">\n            <svg width=\"16\" height=\"16\" viewBox=\"0 0 16 16\" fill=\"none\">\n              <path d=\"M4 4L12 12M12 4L4 12\" stroke=\"currentColor\" strokeWidth=\"1.5\" strokeLinecap=\"round\" />\n            </svg>\n          </button>\n        </div>\n        <div className=\"modal-body\">\n          {error && <div className=\"modal-error\" data-testid=\"add-attachment-error\">{error}</div>}\n\n          <div className=\"attachment-mode-toggle\" data-testid=\"attachment-mode-toggle\">\n            <button\n              className={`attachment-mode-btn ${mode === \"file\" ? \"attachment-mode-btn--active\" : \"\"}`}\n              data-testid=\"attachment-mode-file\"\n              onClick={() => setMode(\"file\")}\n              type=\"button\"\n            >\n              File Upload\n            </button>\n            <button\n              className={`attachment-mode-btn ${mode === \"link\" ? \"attachment-mode-btn--active\" : \"\"}`}\n              data-testid=\"attachment-mode-link\"\n              onClick={() => setMode(\"link\")}\n              type=\"button\"\n            >\n              Link URL\n            </button>\n          </div>\n\n          {mode === \"file\" ? (\n            <>\n              <div\n                className=\"file-upload-area\"\n                data-testid=\"file-upload-area\"\n                onClick={() => fileInputRef.current?.click()}\n              >\n                <input\n                  ref={fileInputRef}\n                  type=\"file\"\n                  style={{ display: \"none\" }}\n                  data-testid=\"file-upload-input\"\n                  onChange={(e) => {\n                    const f = e.target.files?.[0];\n                    if (f) setFile(f);\n                  }}\n                />\n                <div className=\"file-upload-label\">\n                  <strong>Click to browse</strong> or drag and drop\n                </div>\n                {file && (\n                  <div className=\"file-upload-selected\" data-testid=\"file-upload-selected\">\n                    Selected: {file.name} ({(file.size / 1024).toFixed(1)} KB)\n                  </div>\n                )}\n              </div>\n            </>\n          ) : (\n            <>\n              <div className=\"form-group\">\n                <label className=\"form-label\">Display Name</label>\n                <input\n                  className=\"form-input\"\n                  data-testid=\"add-attachment-link-name\"\n                  value={linkName}\n                  onChange={(e) => setLinkName(e.target.value)}\n                  placeholder=\"e.g., Client Portal\"\n                />\n              </div>\n              <div className=\"form-group\">\n                <label className=\"form-label\">URL *</label>\n                <input\n                  className=\"form-input\"\n                  data-testid=\"add-attachment-link-url\"\n                  value={linkUrl}\n                  onChange={(e) => setLinkUrl(e.target.value)}\n                  placeholder=\"https://example.com\"\n                />\n              </div>\n            </>\n          )}\n\n          <div className=\"form-group\">\n            <label className=\"form-label\">Link to Deal (optional)</label>\n            <FilterSelect\n              options={dealOptions}\n              value={dealId}\n              onChange={setDealId}\n              placeholder=\"Select a deal...\"\n              searchable\n              testId=\"add-attachment-deal\"\n            />\n          </div>\n        </div>\n        <div className=\"modal-footer\">\n          <button\n            className=\"btn btn--secondary\"\n            data-testid=\"add-attachment-cancel\"\n            onClick={resetAndClose}\n            type=\"button\"\n          >\n            Cancel\n          </button>\n          <button\n            className=\"btn btn--primary\"\n            data-testid=\"add-attachment-submit\"\n            onClick={handleSubmit}\n            disabled={saving}\n            type=\"button\"\n          >\n            {saving ? \"Uploading...\" : mode === \"file\" ? \"Upload\" : \"Add Link\"}\n          </button>\n        </div>\n      </div>\n    </div>\n  );\n}\n"},"caller":{"type":"direct"}}],"stop_reason":null,"stop_sequence":null,"usage":{"input_tokens":1,"cache_creation_input_tokens":7457,"cache_read_input_tokens":124828,"cache_creation":{"ephemeral_5m_input_tokens":7457,"ephemeral_1h_input_tokens":0},"output_tokens":3,"service_tier":"standard","inference_geo":"global"},"context_management":null},"parent_tool_use_id":null,"session_id":"20dd06ca-9916-4e95-99e2-6242a9e6f0be","uuid":"efc65b42-b9ac-4521-8ad5-6