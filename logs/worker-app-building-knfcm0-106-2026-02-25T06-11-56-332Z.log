ounts (category)`;\n    40→\n    41→  // Transactions: each financial transaction (header)\n    42→  await sql`\n    43→    CREATE TABLE IF NOT EXISTS transactions (\n    44→      id UUID PRIMARY KEY DEFAULT gen_random_uuid(),\n    45→      date DATE NOT NULL,\n    46→      description VARCHAR(500) NOT NULL,\n    47→      currency VARCHAR(10) NOT NULL DEFAULT 'USD',\n    48→      created_at TIMESTAMPTZ NOT NULL DEFAULT NOW(),\n    49→      updated_at TIMESTAMPTZ NOT NULL DEFAULT NOW()\n    50→    )\n    51→  `;\n    52→\n    53→  await sql`CREATE INDEX IF NOT EXISTS idx_transactions_date ON transactions (date DESC)`;\n    54→\n    55→  // Transaction entries: double-entry line items for each transaction\n    56→  await sql`\n    57→    CREATE TABLE IF NOT EXISTS transaction_entries (\n    58→      id UUID PRIMARY KEY DEFAULT gen_random_uuid(),\n    59→      transaction_id UUID NOT NULL REFERENCES transactions(id) ON DELETE CASCADE,\n    60→      account_id UUID NOT NULL REFERENCES accounts(id),\n    61→      entry_type VARCHAR(10) NOT NULL CHECK (entry_type IN ('debit', 'credit')),\n    62→      amount NUMERIC(15, 2) NOT NULL CHECK (amount > 0),\n    63→      created_at TIMESTAMPTZ NOT NULL DEFAULT NOW()\n    64→    )\n    65→  `;\n    66→\n    67→  await sql`CREATE INDEX IF NOT EXISTS idx_transaction_entries_transaction ON transaction_entries (transaction_id)`;\n    68→  await sql`CREATE INDEX IF NOT EXISTS idx_transaction_entries_account ON transaction_entries (account_id)`;\n    69→\n    70→  // Tags: free-form labels for transactions\n    71→  await sql`\n    72→    CREATE TABLE IF NOT EXISTS tags (\n    73→      id UUID PRIMARY KEY DEFAULT gen_random_uuid(),\n    74→      name VARCHAR(100) NOT NULL UNIQUE\n    75→    )\n    76→  `;\n    77→\n    78→  // Transaction-tag junction table\n    79→  await sql`\n    80→    CREATE TABLE IF NOT EXISTS transaction_tags (\n    81→      transaction_id UUID NOT NULL REFERENCES transactions(id) ON DELETE CASCADE,\n    82→      tag_id UUID NOT NULL REFERENCES tags(id) ON DELETE CASCADE,\n    83→      PRIMARY KEY (transaction_id, tag_id)\n    84→    )\n    85→  `;\n    86→\n    87→  // Budgets: per-account budget items (e.g., Rent: $1,500/month for Checking)\n    88→  await sql`\n    89→    CREATE TABLE IF NOT EXISTS budgets (\n    90→      id UUID PRIMARY KEY DEFAULT gen_random_uuid(),\n    91→      account_id UUID NOT NULL REFERENCES accounts(id) ON DELETE CASCADE,\n    92→      name VARCHAR(255) NOT NULL,\n    93→      amount NUMERIC(15, 2) NOT NULL,\n    94→      actual_amount NUMERIC(15, 2) NOT NULL DEFAULT 0,\n    95→      period VARCHAR(20) NOT NULL DEFAULT 'monthly',\n    96→      created_at TIMESTAMPTZ NOT NULL DEFAULT NOW()\n    97→    )\n    98→  `;\n    99→\n   100→  await sql`CREATE INDEX IF NOT EXISTS idx_budgets_account ON budgets (account_id)`;\n   101→\n   102→  // Reports: generated report configurations and metadata\n   103→  await sql`\n   104→    CREATE TABLE IF NOT EXISTS reports (\n   105→      id UUID PRIMARY KEY DEFAULT gen_random_uuid(),\n   106→      name VARCHAR(255) NOT NULL,\n   107→      report_type VARCHAR(50) NOT NULL CHECK (report_type IN ('summary', 'detailed', 'budget_vs_actual')),\n   108→      date_range_start DATE NOT NULL,\n   109→      date_range_end DATE NOT NULL,\n   110→      accounts_included TEXT NOT NULL DEFAULT 'all',\n   111→      categories_filter TEXT,\n   112→      include_zero_balance BOOLEAN NOT NULL DEFAULT FALSE,\n   113→      status VARCHAR(20) NOT NULL DEFAULT 'complete',\n   114→      created_at TIMESTAMPTZ NOT NULL DEFAULT NOW()\n   115→    )\n   116→  `;\n   117→\n   118→  await sql`CREATE INDEX IF NOT EXISTS idx_reports_type ON reports (report_type)`;\n   119→  await sql`CREATE INDEX IF NOT EXISTS idx_reports_created ON reports (created_at DESC)`;\n   120→\n   121→  // Report items: individual rows in a generated report\n   122→  await sql`\n   123→    CREATE TABLE IF NOT EXISTS report_items (\n   124→      id UUID PRIMARY KEY