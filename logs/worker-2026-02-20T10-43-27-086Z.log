[2026-02-20T10:33:54.369Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01DKzT2L1zNEck9qyKzYU4yo","type":"tool_result","content":"     1→import { getDb } from '../utils/db'\n     2→import { optionalAuth, type OptionalAuthRequest } from '../utils/auth'\n     3→\n     4→async function handler(req: OptionalAuthRequest) {\n     5→  const sql = getDb()\n     6→  const url = new URL(req.url)\n     7→  const pathParts = url.pathname.split('/').filter(Boolean)\n     8→  const attachmentId = pathParts.length >= 4 ? pathParts[3] : null\n     9→\n    10→  // GET /client-attachments?clientId=...\n    11→  if (req.method === 'GET' && !attachmentId) {\n    12→    const clientId = url.searchParams.get('clientId')\n    13→    if (!clientId) {\n    14→      return new Response(JSON.stringify({ error: 'clientId required' }), { status: 400 })\n    15→    }\n    16→\n    17→    const attachments = await sql`\n    18→      SELECT a.*, d.name as deal_name\n    19→      FROM attachments a\n    20→      LEFT JOIN deals d ON a.deal_id = d.id\n    21→      WHERE a.client_id = ${clientId}\n    22→      ORDER BY a.created_at DESC\n    23→    `\n    24→\n    25→    return Response.json({ attachments })\n    26→  }\n    27→\n    28→  // POST /client-attachments — create a new attachment\n    29→  if (req.method === 'POST') {\n    30→    const body = await req.json() as {\n    31→      filename: string\n    32→      type: string\n    33→      url: string\n    34→      size?: number\n    35→      client_id: string\n    36→      deal_id?: string\n    37→    }\n    38→\n    39→    const rows = await sql`\n    40→      INSERT INTO attachments (filename, type, url, size, client_id, deal_id)\n    41→      VALUES (\n    42→        ${body.filename},\n    43→        ${body.type},\n    44→        ${body.url},\n    45→        ${body.size ?? null},\n    46→        ${body.client_id},\n    47→        ${body.deal_id || null}\n    48→      )\n    49→      RETURNING *\n    50→    `\n    51→\n    52→    // Create timeline event\n    53→    await sql`\n    54→      INSERT INTO timeline_events (client_id, event_type, description, user_name, related_entity_id, related_entity_type)\n    55→      VALUES (${body.client_id}, 'attachment_added', ${'Attachment Added: \\'' + body.filename + '\\''}, ${req.user?.name ?? 'System'}, ${rows[0].id}, 'attachment')\n    56→    `\n    57→\n    58→    // Fetch deal_name if deal_id provided\n    59→    const attachment = rows[0]\n    60→    if (body.deal_id) {\n    61→      const dealRows = await sql`SELECT name FROM deals WHERE id = ${body.deal_id}`\n    62→      attachment.deal_name = dealRows.length > 0 ? dealRows[0].name : null\n    63→    }\n    64→\n    65→    return Response.json(attachment, { status: 201 })\n    66→  }\n    67→\n    68→  // DELETE /client-attachments/:attachmentId\n    69→  if (req.method === 'DELETE' && attachmentId) {\n    70→    await sql`DELETE FROM attachments WHERE id = ${attachmentId}`\n    71→    return Response.json({ success: true })\n    72→  }\n    73→\n    74→  return new Response(JSON.stringify({ error: 'Method not allowed' }), { status: 405 })\n    75→}\n    76→\n    77→export default optionalAuth(handler)\n    78→"}]},"parent_tool_use_id":"toolu_017wrsKFYcNdGuaESDAUHWTj","session_id":"351c7274-5e41-434e-b128-bd6636f90849","uuid":"144f3e86-e170-4635-a15f-c9c409595e5a"}
