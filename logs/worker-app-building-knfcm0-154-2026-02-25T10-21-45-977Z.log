    return [\n    36→      { value: \"\", label: \"Tags: All\" },\n    37→      ...sorted.map((t) => ({ value: t, label: t })),\n    38→    ];\n    39→  }, [clients]);\n    40→\n    41→  // Derive unique source options from data\n    42→  const sourceOptions = useMemo(() => {\n    43→    const sourceSet = new Set<string>();\n    44→    clients.forEach((c) => {\n    45→      if (c.sourceType) sourceSet.add(c.sourceType);\n    46→    });\n    47→    const sorted = Array.from(sourceSet).sort();\n    48→    return [\n    49→      { value: \"\", label: \"Source: All\" },\n    50→      ...sorted.map((s) => ({ value: s, label: s })),\n    51→    ];\n    52→  }, [clients]);\n    53→\n    54→  // Filter clients\n    55→  const filtered = useMemo(() => {\n    56→    let result = clients;\n    57→\n    58→    // Search filter\n    59→    if (search.trim()) {\n    60→      const q = search.toLowerCase();\n    61→      result = result.filter(\n    62→        (c) =>\n    63→          c.name.toLowerCase().includes(q) ||\n    64→          c.tags.some((t) => t.toLowerCase().includes(q)) ||\n    65→          (c.primaryContact && c.primaryContact.name.toLowerCase().includes(q))\n    66→      );\n    67→    }\n    68→\n    69→    // Status filter\n    70→    if (statusFilter) {\n    71→      result = result.filter((c) => c.status === statusFilter);\n    72→    }\n    73→\n    74→    // Tag filter\n    75→    if (tagFilter) {\n    76→      result = result.filter((c) => c.tags.includes(tagFilter));\n    77→    }\n    78→\n    79→    // Source filter\n    80→    if (sourceFilter) {\n    81→      result = result.filter((c) => c.sourceType === sourceFilter);\n    82→    }\n    83→\n    84→    // Sort\n    85→    result = [...result].sort((a, b) => {\n    86→      switch (sortBy) {\n    87→        case \"name_asc\":\n    88→          return a.name.localeCompare(b.name);\n    89→        case \"name_desc\":\n    90→          return b.name.localeCompare(a.name);\n    91→        case \"created_desc\":\n    92→          return new Date(b.createdAt).getTime() - new Date(a.createdAt).getTime();\n    93→        case \"created_asc\":\n    94→          return new Date(a.createdAt).getTime() - new Date(b.createdAt).getTime();\n    95→        case \"updated_desc\":\n    96→        default:\n    97→          return new Date(b.updatedAt).getTime() - new Date(a.updatedAt).getTime();\n    98→      }\n    99→    });\n   100→\n   101→    return result;\n   102→  }, [clients, search, statusFilter, tagFilter, sourceFilter, sortBy]);\n   103→\n   104→  // Reset to page 1 when filters change\n   105→  useEffect(() => {\n   106→    setCurrentPage(1);\n   107→  }, [search, statusFilter, tagFilter, sourceFilter, sortBy]);\n   108→\n   109→  // Paginate\n   110→  const totalPages = Math.max(1, Math.ceil(filtered.length / PAGE_SIZE));\n   111→  const paginated = filtered.slice(\n   112→    (currentPage - 1) * PAGE_SIZE,\n   113→    currentPage * PAGE_SIZE\n   114→  );\n   115→\n   116→  if (loading && clients.length === 0) {\n   117→    return (\n   118→      <div className=\"clients-page p-6 max-sm:p-3\" data-testid=\"clients-list-page\">\n   119→        <div className=\"page-header\">\n   120→          <h1 className=\"page-title\">Clients</h1>\n   121→        </div>\n   122→        <div className=\"clients-loading\">Loading clients...</div>\n   123→      </div>\n   124→    );\n   125→  }\n   126→\n   127→  return (\n   128→    <div className=\"clients-page p-6 max-sm:p-3\" data-testid=\"clients-list-page\">\n   129→      <ClientsListHeader onAddClient={() => setAddModalOpen(true)} />\n   130→\n   131→      <ClientsSearchAndFilters\n   132→        search={search}\n   133→        onSearchChange={setSearch}\n   134→        status={statusFilter}\n   135→        onStatusChange={setStatusFilter}\n   136→        tag={tagFilter}\n   137→        onTagChange={setTagFilter}\n   138→        source={sourceFilter}\n   139→        onSourceChange={setSourceFilter}\n   140→        sort={sortBy