onst body = await req.json();\n    90→      const { name, description } = body as {\n    91→        name?: string;\n    92→        description?: string;\n    93→      };\n    94→      if (!name || !name.trim()) {\n    95→        return new Response(\n    96→          JSON.stringify({ error: \"Account Name is required\" }),\n    97→          { status: 400, headers: { \"Content-Type\": \"application/json\" } }\n    98→        );\n    99→      }\n   100→      const rows = await sql`\n   101→        UPDATE accounts\n   102→        SET name = ${name.trim()},\n   103→            description = ${(description || \"\").trim()},\n   104→            updated_at = NOW()\n   105→        WHERE id = ${accountId}\n   106→        RETURNING id, name, account_type, description, is_default, status, created_at, updated_at\n   107→      `;\n   108→      if (rows.length === 0) {\n   109→        return new Response(JSON.stringify({ error: \"Account not found\" }), {\n   110→          status: 404,\n   111→          headers: { \"Content-Type\": \"application/json\" },\n   112→        });\n   113→      }\n   114→      return new Response(JSON.stringify(rows[0]), {\n   115→        status: 200,\n   116→        headers: { \"Content-Type\": \"application/json\" },\n   117→      });\n   118→    }\n   119→\n   120→    if (req.method === \"DELETE\" && accountId) {\n   121→      // Archive the account (soft delete)\n   122→      const rows = await sql`\n   123→        UPDATE accounts\n   124→        SET status = 'archived', updated_at = NOW()\n   125→        WHERE id = ${accountId}\n   126→        RETURNING id\n   127→      `;\n   128→      if (rows.length === 0) {\n   129→        return new Response(JSON.stringify({ error: \"Account not found\" }), {\n   130→          status: 404,\n   131→          headers: { \"Content-Type\": \"application/json\" },\n   132→        });\n   133→      }\n   134→      return new Response(JSON.stringify({ success: true }), {\n   135→        status: 200,\n   136→        