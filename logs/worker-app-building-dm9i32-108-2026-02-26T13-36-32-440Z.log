IN quantity_transfers qt ON t.id = qt.transaction_id\n    39→          LEFT JOIN batches_created bc ON t.id = bc.transaction_id\n    40→          WHERE (qt.material_id = ${materialId} OR bc.material_id = ${materialId})\n    41→            ${transactionType ? sql`AND t.transaction_type = ${transactionType}` : sql``}\n    42→            ${dateFrom ? sql`AND t.date >= ${dateFrom}::date` : sql``}\n    43→            ${dateTo ? sql`AND t.date <= ${dateTo}::date` : sql``}\n    44→            ${search ? sql`AND (t.reference_id ILIKE ${\"%\" + search + \"%\"} OR t.description ILIKE ${\"%\" + search + \"%\"})` : sql``}\n    45→          ORDER BY t.date DESC, t.created_at DESC\n    46→        `;\n    47→        rows = baseRows;\n    48→      } else if (accountId) {\n    49→        const baseRows = await sql`\n    50→          SELECT DISTINCT t.id, t.date, t.reference_id, t.description,\n    51→                 t.transaction_type, t.status, t.created_by,\n    52→                 t.created_at, t.updated_at\n    53→          FROM transactions t\n    54→          LEFT JOIN quantity_transfers qt ON t.id = qt.transaction_id\n    55→          WHERE (qt.source_account_id = ${accountId} OR qt.destination_account_id = ${accountId})\n    56→            ${transactionType ? sql`AND t.transaction_type = ${transactionType}` : sql``}\n    57→            ${dateFrom ? sql`AND t.date >= ${dateFrom}::date` : sql``}\n    58→            ${dateTo ? sql`AND t.date <= ${dateTo}::date` : sql``}\n    59→            ${search ? sql`AND (t.reference_id ILIKE ${\"%\" + search + \"%\"} OR t.description ILIKE ${\"%\" + search + \"%\"})` : sql``}\n    60→          ORDER BY t.date DESC, t.created_at DESC\n    61→        `;\n    62→        rows = baseRows;\n    63→      } else {\n    64→        rows = await sql`\n    65→          SELECT t.id, t.date, t.reference_id, t.description,\n    66→                 t.transaction_type, t.status, t.created_by,\n    67→                 t.created_at, t.updated_at\n    68→          FROM transactions t\n    69→          WHERE 1=1\n    70→            ${transactionType ? sql`AND t.transaction_type = ${transactionType}` : sql``}\n    71→            ${dateFrom ? sql`AND t.date >= ${dateFrom}::date` : sql``}\n    72→            ${dateTo ? sql`AND t.date <= ${dateTo}::date` : sql``}\n    73→            ${search ? sql`AND (t.reference_id ILIKE ${\"%\" + search + \"%\"} OR t.description ILIKE ${\"%\" + search + \"%\"})` : sql``}\n    74→          ORDER BY t.date DESC, t.created_at DESC\n    75→        `;\n    76→      }\n    77→\n    78→      // For each transaction, fetch transfers and batches_created\n    79→      const result = await Promise.all(\n    80→        rows.map(async (t: Record<string, unknown>) => {\n    81→          const transfers = await sql`\n    82→            SELECT qt.id, qt.transaction_id,\n    83→                   qt.source_account_id, sa.name as source_account_name,\n    84→                   qt.destination_account_id, da.name as destination_account_name,\n    85→                   qt.material_id, m.name as material_name,\n    86→                   qt.amount, qt.unit, qt.source_batch_id,\n    87→                   sb.lot_number as source_batch_label\n    88→            FROM quantity_transfers qt\n    89→            LEFT JOIN accounts sa ON qt.source_account_id = sa.id\n    90→            LEFT JOIN accounts da ON qt.destination_account_id = da.id\n    91→            LEFT JOIN materials m ON qt.material_id = m.id\n    92→            LEFT JOIN batches sb ON qt.source_batch_id = sb.id\n    93→            WHERE qt.transaction_id = ${t.id as string}\n    94→          `;\n    95→\n    96→          const batchesCreated = await sql`\n    97→            SELECT bc.id, bc.transaction_id, bc.batch_id,\n    98→                   bc.material_id, m.name as material_name,\n    99→                   bc.quantity, bc.unit\n   100→            FROM batches_created bc\n   101→            LEFT JOIN materials m ON bc.material_id = m.id\n   102→            WHERE bc.transaction_id = ${t.id as string}\n   103→          `;\n   104→\n   105→          return {\n   106→            ...t,\n   107→            transfers,\n   108→            batches_created: batchesCreated,\n   109→          };\n   110→        })\n   111→      );\n   112→\n   113→      return new Response(JSON.stringify(result), {\n   114→        status: 200,\n   115→        headers: { \"Content-Type\": \"application/json\" },\n   116→      });\n   117→    }\n   118→\n   119→    if (req.method === \"GET\" && transactionId) {\n   120→      const rows = await sql`\n   121→        SELECT t.id, t.date, t.reference_id, t.description,\n   122→               t.transaction_type, t.status, t.created_by,\n   123→               t.created_at, t.updated_at\n   124→        FROM transactions t\n   125→        WHERE t.id = ${transactionId}\n   126→      `;\n   127→      if (rows.length === 0) {\n   128→        return new Response(\n   129→          JSON.stringify({ error: \"Transaction not found\" }),\n   130→          { status: 404, headers: { \"Content-Type\": \"application/json\" } }\n   131→        );\n   132→      }\n   133→\n   134→      const transfers = await sql`\n   135→        SELECT qt.id, qt.transaction_id,\n   136→               qt.source_account_id, sa.name as source_account_name,\n   137→               qt.destination_account_id, da.name as destination_account_name,\n   138→               qt.material_id, m.name as material_name,\n   139→               qt.amount, qt.unit, qt.source_batch_id,\n   140→               sb.lot_number as source_batch_label\n   141→        FROM quantity_transfers qt\n   142→        LEFT JOIN accounts sa ON qt.source_account_id = sa.id\n   143→        LEFT JOIN accounts da ON qt.destination_account_id = da.id\n   144→        LEFT JOIN materials m ON qt.material_id = m.id\n   145→        LEFT JOIN batches sb ON qt.source_batch_id = sb.id\n   146→        WHERE qt.transaction_id = ${transactionId}\n   147→      `;\n   148→\n   149→      const batchesCreated = await sql`\n   150→        SELECT bc.id, bc.transaction_id, bc.batch_id,\n   151→               bc.material_id, m.name as material_name,\n   152→               bc.quantity, bc.unit\n   153→        FROM batches_created bc\n   154→        LEFT JOIN materials m ON bc.material_id = m.id\n   155→        WHERE bc.transaction_id = ${transactionId}\n   156→      `;\n   157→\n   158→      return new Response(\n   159→        JSON.stringify({\n   160→          ...rows[0],\n   161→          transfers,\n   162→          batches_created: batchesCreated,\n   163→        }),\n   164→        {\n   165→          status: 200,\n   166→          headers: { \"Content-Type\": \"application/json\" },\n   167→        }\n   168→      );\n   169→    }\n   170→\n   171→    if (req.method === \"POST\") {\n   172→      const body = await req.json();\n   173→      const { date, reference_id, description, transaction_type, transfers, batches_created } =\n   174→        body as {\n   175→          date: string;\n   176→          reference_id: string;\n   177→          description: string;\n   178→          transaction_type: string;\n   179→          transfers: {\n   180→            source_account_id: string;\n   181→            destination_account_id: string;\n   182→            material_id: string;\n   183→            amount: number;\n   184→            unit: string;\n   185→            source_batch_id?: string;\n   186→          }[];\n   187→          batches_created: {\n   188→            material_id: string;\n   189→            quantity: number;\n   190→            unit: string;\n   191→          }[];\n   192→        };\n   193→\n   194→      if (!date) {\n   195→        return new Response(\n   196→          JSON.stringify({ error: \"Date is required\" }),\n   197→          { status: 400, headers: { \"Content-Type\": \"application/json\" } }\n   198→        );\n   199→      }\n   200→      if (!transaction_type) {\n   201→        return new Response(\n   202→          JSON.stringify({ error: \"Transaction type is required\" }),\n   203→          { status: 400, headers: { \"Content-Type\": \"application/json\" } }\n   204→        );\n   205→      }\n   206→\n   207→      // Create the transaction\n   208→      const txnRows = await sql`\n   209→        INSERT INTO transactions (date, reference_id, description, transaction_type)\n   210→        VALUES (${date}, ${reference_id || \"\"}, ${description || \"\"}, ${transaction_type})\n   211→        RETURNING *\n   212→      `;\n   213→      const txn = txnRows[0];\n   214→      const txnId = txn.id as string;\n   215→\n   216→      // Create quantity transfers\n   217→      const createdTransfers = [];\n   218→      if (transfers && transfers.length > 0) {\n   219→        for (const t of transfers) {\n   220→          const rows = await sql`\n   221→            INSERT INTO quantity_transfers (transaction_id, source_account_id, destination_account_id, material_id, amount, unit, source_batch_id)\n   222→            VALUES (${txnId}, ${t.source_account_id}, ${t.destination_account_id}, ${t.material_id}, ${t.amount}, ${t.unit}, ${t.source_batch_id || null})\n   223→            RETURNING *\n   224→          `;\n   225→          createdTransfers.push(rows[0]);\n   226→        }\n   227→      }\n   228→\n   229→      // Create batches\n   230→      const createdBatches = [];\n   231→      if (batches_created && batches_created.length > 0) {\n   232→        for (const bc of batches_created) {\n   233→          // Create the actual batch first\n   234→          const batchRows = await sql`\n   235→            INSERT INTO batches (material_id, account_id, quantity, unit, originating_transaction_id)\n   236→            VALUES (${bc.material_id}, ${transfers?.[0]?.destination_account_id || null}, ${bc.quantity}, ${bc.unit}, ${txnId})\n   237→            RETURNING *\n   238→          `;\n   239→          const batch = batchRows[0];\n   240→\n   241→          // Create the batches_created record\n   242→          const bcRows = await sql`\n   243→            INSERT INTO batches_created (transaction_id, batch_id, material_id, quantity, unit)\n   244→            VALUES (${txnId}, ${batch.id as string}, ${bc.material_id}, ${bc.quantity}, ${bc.unit})\n   245→            RETURNING *\n   246→          `;\n   247→          createdBatches.push(bcRows[0]);\n   248→        }\n   249→      }\n   250→\n   251→      return new Response(\n   252→        JSON.stringify({\n   253→          ...txn,\n   254→          transfers: createdTransfers,\n   255→          batches_created: createdBatches,\n   256→        }),\n   257→        {\n   258→          status: 201,\n   259→          headers: { \"Content-Type\": \"application/json\" },\n   260→        }\n   261→      );\n   262→    }\n   263→\n   264→    if (req.method === \"DELETE\" && transactionId) {\n   265→      const rows = await sql`\n   266→        DELETE FROM transactions WHERE id = ${transactionId} RETURNING id\n   267→      `;\n   268→      if (rows.length === 0) {\n   269→        return new Response(\n   270→          JSON.stringify({ error: \"Transaction not found\" }),\n   271→          { status: 404, headers: { \"Content-Type\": \"application/json\" } }\n   272→        );\n   273→      }\n   274→      return new Response(JSON.stringify({ id: transactionId }), {\n   275→        status: 200,\n   276→        headers: { \"Content-Type\": \"application/json\" },\n   277→      });\n   278→    }\n   279→\n   280→    return new Response(JSON.stringify({ error: \"Method not allowed\" }), {\n   281→      status: 405,\n   282→      headers: { \"Content-Type\": \"application/json\" },\n   283→    });\n   284→  } catch (err) {\n   285→    console.error(\"Transactions error:\", err);\n   286→    return new Response(JSON.stringify({ error: \"Internal server error\" }), {\n   287→      status: 500,\n   288→      headers: { \"Content-Type\": \"application/json\" },\n   289→    });\n   290→  }\n   291→}\n   292→"}]},"parent_too