nst dateOnly = new Date(d.getFullYear(), d.getMonth(), d.getDate())\n    55→  const timeStr = d.toLocaleTimeString('en-US', { hour: 'numeric', minute: '2-digit' })\n    56→\n    57→  if (dateOnly.getTime() === today.getTime()) {\n    58→    return `Today, ${timeStr}`\n    59→  }\n    60→  if (dateOnly.getTime() === tomorrow.getTime()) {\n    61→    return `Tomorrow, ${timeStr}`\n    62→  }\n    63→  return new Intl.DateTimeFormat('en-US', {\n    64→    month: 'short',\n    65→    day: 'numeric',\n    66→    year: 'numeric',\n    67→  }).format(d)\n    68→}\n    69→\n    70→export function getAuthHeader(): Record<string, string> {\n    71→  const token = localStorage.getItem('token')\n    72→  return token ? { Authorization: `Bearer ${token}` } : {}\n    73→}\n    74→"}]},"parent_tool_use_id":"toolu_014VRTkSHW42529dNWqtjfqu","session_id":"2dec99d4-578f-4560-84e7-863a274d105f","uuid":"510f4bd3-2ff1-4f64-ae64-e4cd2a14c721"}
