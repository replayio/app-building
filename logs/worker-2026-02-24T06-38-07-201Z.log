[2026-02-24T06:26:12.718Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01PzJPFzJ7BMuBCHBw8cCCve","type":"tool_result","content":"     1→import { useEffect, useState, useCallback } from 'react'\n     2→import { useParams } from 'react-router-dom'\n     3→import DealHeader from '@/components/DealHeader'\n     4→import StageProgressBar from '@/components/StageProgressBar'\n     5→import DealHistorySection from '@/components/DealHistorySection'\n     6→import DealMetricsSection from '@/components/DealMetricsSection'\n     7→import WriteupsSection from '@/components/WriteupsSection'\n     8→import LinkedTasksSection from '@/components/LinkedTasksSection'\n     9→import DealAttachmentsSection from '@/components/DealAttachmentsSection'\n    10→import DealContactsSection from '@/components/DealContactsSection'\n    11→\n    12→interface DealInfo {\n    13→  id: string\n    14→  name: string\n    15→  client_id: string\n    16→  client_name: string\n    17→  stage: string\n    18→  value: string | number | null\n    19→  owner: string | null\n    20→  status: string\n    21→  probability: number | null\n    22→  expected_close_date: string | null\n    23→  close_date: string | null\n    24→  created_at: string\n    25→  updated_at: string\n    26→}\n    27→\n    28→export default function DealDetailPage() {\n    29→  const { dealId } = useParams<{ dealId: string }>()\n    30→  const [deal, setDeal] = useState<DealInfo | null>(null)\n    31→  const [loading, setLoading] = useState(true)\n    32→  const [historyRefreshKey, setHistoryRefreshKey] = useState(0)\n    33→\n    34→  const fetchDeal = useCallback(async () => {\n    35→    if (!dealId) return\n    36→    try {\n    37→      const res = await fetch(`/.netlify/functions/deals/${dealId}`)\n    38→      const data = await res.json()\n    39→      setDeal(data)\n    40→    } catch {\n    41→      // silently fail\n    42→    } finally {\n    43→      setLoading(false)\n    44→    }\n    45→  }, [dealId])\n    46→\n    47→  useEffect(() => {\n    48→    fetchDeal()\n    49→  }, [fetchDeal])\n    50→\n    51→  const handleUpdateField = async (field: string, value: string) => {\n    52→    if (!dealId || !deal) return\n    53→    const body: Record<string, unknown> = {}\n    54→    if (field === 'client_name') {\n    55→      body.name = deal.name\n    56→    } else if (field === 'value') {\n    57→      const numericValue = value.replace(/[$,]/g, '')\n    58→      body.value = numericValue || null\n    59→    } else {\n    60→      body[field] = value\n    61→    }\n    62→    try {\n    63→      const res = await fetch(`/.netlify/functions/deals/${dealId}`, {\n    64→        method: 'PUT',\n    65→        headers: { 'Content-Type': 'application/json' },\n    66→        body: JSON.stringify(body),\n    67→      })\n    68→      if (res.ok) {\n    69→        const updated = await res.json()\n    70→        setDeal(updated)\n    71→      }\n    72→    } catch {\n    73→      // silently fail\n    74→    }\n    75→  }\n    76→\n    77→  const handleChangeStage = async (newStage: string) => {\n    78→    if (!dealId || !deal) return\n    79→    try {\n    80→      // Update the deal stage\n    81→      const res = await fetch(`/.netlify/functions/deals/${dealId}`, {\n    82→        method: 'PUT',\n    83→        headers: { 'Content-Type': 'application/json' },\n    84→        body: JSON.stringify({ stage: newStage }),\n    85→      })\n    86→      if (res.ok) {\n    87→        // Record the stage change in history\n    88→        await fetch(`/.netlify/functions/deals/${dealId}/history`, {\n    89→          method: 'POST',\n    90→          headers: { 'Content-Type': 'application/json' },\n    91→          body: JSON.stringify({\n    92→            old_stage: deal.stage,\n    93→            new_stage: newStage,\n    94→            changed_by: deal.owner || 'System',\n    95→          }),\n    96→        })\n    97→\n    98→        const updated = await res.json()\n    99→        setDeal(updated)\n   100→        setHistoryRefreshKey(k => k + 1)\n   101→      }\n   102→    } catch {\n   103→      // silently fail\n   104→    }\n   105→  }\n   106→\n   107→  const handleUpdateMetrics = async (field: string, value: string | number | null) => {\n   108→    if (!dealId) return\n   109→    const body: Record<string, unknown> = { [field]: value }\n   110→    try {\n   111→      const res = await fetch(`/.netlify/functions/deals/${dealId}`, {\n   112→        method: 'PUT',\n   113→        headers: { 'Content-Type': 'application/json' },\n   114→        body: JSON.stringify(body),\n   115→      })\n   116→      if (res.ok) {\n   117→        const updated = await res.json()\n   118→        setDeal(updated)\n   119→      }\n   120→    } catch {\n   121→      // silently fail\n   122→    }\n   123→  }\n   124→\n   125→  if (loading) {\n   126→    return (\n   127→      <div className=\"page-content p-6 max-sm:p-3\" data-testid=\"deal-detail-page\">\n   128→        <p>Loading...</p>\n   129→      </div>\n   130→    )\n   131→  }\n   132→\n   133→  if (!deal) {\n   134→    return (\n   135→      <div className=\"page-content p-6 max-sm:p-3\" data-testid=\"deal-detail-page\">\n   136→        <p>Deal not found</p>\n   137→      </div>\n   138→    )\n   139→  }\n   140→\n   141→  return (\n   142→    <div className=\"page-content p-6 max-sm:p-3\" data-testid=\"deal-detail-page\">\n   143→      <DealHeader\n   144→        dealName={deal.name}\n   145→        clientName={deal.client_name}\n   146→        value={deal.value}\n   147→        owner={deal.owner}\n   148→        stage={deal.stage}\n   149→        onUpdateField={handleUpdateField}\n   150→        onChangeStage={handleChangeStage}\n   151→      />\n   152→\n   153→      <StageProgressBar currentStage={deal.stage} />\n   154→\n   155→      <DealHistorySection dealId={deal.id} refreshKey={historyRefreshKey} />\n   156→\n   157→      <DealMetricsSection\n   158→        probability={deal.probability}\n   159→        expectedCloseDate={deal.expected_close_date}\n   160→        onUpdate={handleUpdateMetrics}\n   161→      />\n   162→\n   163→      <WriteupsSection dealId={deal.id} />\n   164→\n   165→      <LinkedTasksSection dealId={deal.id} clientId={deal.client_id} />\n   166→\n   167→      <DealAttachmentsSection dealId={deal.id} clientId={deal.client_id} />\n   168→\n   169→      <DealContactsSection dealId={deal.id} />\n   170→    </div>\n   171→  )\n   172→}\n   173→"}]},"parent_tool_use_id":"toolu_01MSgTBB3ieRfJPyWmGUc79G","session_id":"5e5af855-f8b5-4faa-8470-bf0544f43eff","uuid":"afcef207-b4da-4b38-a179-f27ce7788777"}
