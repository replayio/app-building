2→\n   183→          const dealsDisplay = od && od.count > 0\n   184→            ? `${od.count} (Value: ${formatValue(Number(od.totalValue))})`\n   185→            : '0'\n   186→\n   187→          const taskDisplay = nt\n   188→            ? `${nt.title} - ${formatTaskDate(nt.dueDate)}`\n   189→            : 'No task scheduled'\n   190→\n   191→          return (\n   192→            <div\n   193→              key={client.id}\n   194→              className=\"clients-table-row\"\n   195→              data-testid={`client-row-${client.id}`}\n   196→            >\n   197→              <div className=\"table-cell\">\n   198→                <button\n   199→                  className=\"client-name-link\"\n   200→                  onClick={() => navigate(`/clients/${client.id}`)}\n   201→                  data-testid={`client-name-${client.id}`}\n   202→                >\n   203→                  {client.name}\n   204→                </button>\n   205→              </div>\n   206→              <div className=\"table-cell table-cell-type\" data-testid={`client-type-${client.id}`}>\n   207→                {client.type}\n   208→              </div>\n   209→              <div className=\"table-cell\">\n   210→                <span\n   211→                  className={`status-badge ${statusColors[client.status] || ''}`}\n   212→                  data-testid={`client-status-${client.id}`}\n   213→                >\n   214→                  {client.status}\n   215→                </span>\n   216→              </div>\n   217→              <div className=\"table-cell table-cell-tags\" data-testid={`client-tags-${client.id}`}>\n   218→                {(client.tags || []).map(tag => (\n   219→                  <span key={tag} className=\"tag-chip\">{tag}</span>\n   220→                ))}\n   221→              </div>\n   222→              <div className=\"table-cell table-cell-contact\" data-testid={`client-contact-${client.id}`}>\n   223→                {contactDisplay}\n   224→   