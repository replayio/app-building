  92→  ]\n    93→\n    94→  const sourceOptions = [\n    95→    { label: 'All', value: '' },\n    96→    ...availableSources.map(s => ({ label: s, value: s })),\n    97→  ]\n    98→\n    99→  return (\n   100→    <div className=\"clients-filters\" data-testid=\"clients-search-and-filters\">\n   101→      <div className=\"clients-search-wrapper\" data-testid=\"search-input-wrapper\">\n   102→        <Search size={16} className=\"clients-search-icon\" />\n   103→        <input\n   104→          type=\"text\"\n   105→          className=\"clients-search-input\"\n   106→          placeholder=\"Search clients by name, tag, or contact...\"\n   107→          value={localSearch}\n   108→          onChange={(e) => setLocalSearch(e.target.value)}\n   109→          data-testid=\"search-input\"\n   110→        />\n   111→        {localSearch && (\n   112→          <button\n   113→            className=\"clients-search-clear\"\n   114→            onClick={() => setLocalSearch('')}\n   115→            data-testid=\"search-clear\"\n   116→          >\n   117→            <X size={14} />\n   118→          </button>\n   119→        )}\n   120→      </div>\n   121→      <div className=\"clients-filter-row\">\n   122→        <FilterDropdown\n   123→          label=\"Status\"\n   124→          value={filters.status}\n   125→          options={statusOptions}\n   126→          onChange={(v) => dispatch(setStatusFilter(v))}\n   127→          testId=\"status-filter\"\n   128→        />\n   129→        <FilterDropdown\n   130→          label=\"Tags\"\n   131→          value={filters.tag}\n   132→          options={tagOptions}\n   133→          onChange={(v) => dispatch(setTagFilter(v))}\n   134→          testId=\"tags-filter\"\n   135→        />\n   136→        <FilterDropdown\n   137→          label=\"Source\"\n   138→          value={filters.source}\n   139→          options={sourceOptions}\n   140→          onChange={(v) => dispatch(setSourceFilter(v))}\n   141→          testId=\"source-filter\"\n   142→        />\n   143→        <FilterDropdown\n   144→          label=\"Sort\"\n   145→          value={filters.sort}\n   146→          options={sortOptions}\n   147→          onChange={(v) => dispatch(setSort(v))}\n   148→          testId=\"sort-filter\"\n   149→        />\n   150→      </div>\n   151→    </div>\n   152→  )\n   153→}\n   154→"}]},"parent_tool_use_id":"toolu_01MSgTBB3ieRfJPyWmGUc79G","session_id":"5e5af855-f8b5-4faa-8470-bf0544f43eff","uuid":"a12af8ce-aeb4-4c8f-9be6-8ae859b79950"}
