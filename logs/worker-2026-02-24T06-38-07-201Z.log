[2026-02-24T06:26:27.161Z] {"type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_01DLGdZ6F5DjqWXaYg84KE3H","type":"tool_result","content":"     1→import { useState, useEffect, useRef } from 'react'\n     2→import { SlidersHorizontal, ChevronDown, X } from 'lucide-react'\n     3→import { useAppDispatch, useAppSelector } from '@/store/hooks'\n     4→import { setFilterType, setFilterText } from '@/store/tasksSlice'\n     5→\n     6→const filterOptions = [\n     7→  { label: 'All Fields', value: 'all' },\n     8→  { label: 'Priority', value: 'priority' },\n     9→  { label: 'Assignee', value: 'assignee' },\n    10→  { label: 'Client', value: 'client' },\n    11→  { label: 'Due Date', value: 'due_date' },\n    12→]\n    13→\n    14→export default function TasksFilter() {\n    15→  const dispatch = useAppDispatch()\n    16→  const filterType = useAppSelector(s => s.tasks.filterType)\n    17→  const filterText = useAppSelector(s => s.tasks.filterText)\n    18→  const [dropdownOpen, setDropdownOpen] = useState(false)\n    19→  const [localText, setLocalText] = useState(filterText)\n    20→  const ref = useRef<HTMLDivElement>(null)\n    21→\n    22→  useEffect(() => {\n    23→    const handler = (e: MouseEvent) => {\n    24→      if (ref.current && !ref.current.contains(e.target as Node)) setDropdownOpen(false)\n    25→    }\n    26→    document.addEventListener('mousedown', handler)\n    27→    return () => document.removeEventListener('mousedown', handler)\n    28→  }, [])\n    29→\n    30→  useEffect(() => {\n    31→    const timer = setTimeout(() => {\n    32→      dispatch(setFilterText(localText))\n    33→    }, 200)\n    34→    return () => clearTimeout(timer)\n    35→  }, [localText, dispatch])\n    36→\n    37→  const [prevReduxText, setPrevReduxText] = useState(filterText)\n    38→  if (filterText !== prevReduxText) {\n    39→    setPrevReduxText(filterText)\n    40→    setLocalText(filterText)\n    41→  }\n    42→\n    43→  const selectedLabel = filterOptions.find(o => o.value === filterType)?.label || 'All Fields'\n    44→\n    45→  const handleTypeChange = (value: string) => {\n    46→    dispatch(setFilterType(value))\n    47→    setLocalText('')\n    48→    setDropdownOpen(false)\n    49→  }\n    50→\n    51→  return (\n    52→    <div className=\"tasks-filter\" data-testid=\"tasks-filter\">\n    53→      <div className=\"filter-dropdown-wrapper\" ref={ref} data-testid=\"tasks-filter-type-dropdown\">\n    54→        <button\n    55→          className=\"filter-trigger\"\n    56→          onClick={() => setDropdownOpen(!dropdownOpen)}\n    57→          data-testid=\"tasks-filter-type-dropdown-trigger\"\n    58→        >\n    59→          <SlidersHorizontal size={14} />\n    60→          <span>{selectedLabel}</span>\n    61→          <ChevronDown size={14} />\n    62→        </button>\n    63→        {dropdownOpen && (\n    64→          <div className=\"filter-dropdown-menu\" data-testid=\"tasks-filter-type-dropdown-menu\">\n    65→            {filterOptions.map(opt => (\n    66→              <button\n    67→                key={opt.value}\n    68→                className={`filter-dropdown-option${opt.value === filterType ? ' selected' : ''}`}\n    69→                onClick={() => handleTypeChange(opt.value)}\n    70→                data-testid={`tasks-filter-type-option-${opt.value}`}\n    71→              >\n    72→                {opt.label}\n    73→              </button>\n    74→            ))}\n    75→          </div>\n    76→        )}\n    77→      </div>\n    78→      <div className=\"tasks-filter-input-wrapper\">\n    79→        <input\n    80→          type=\"text\"\n    81→          className=\"tasks-filter-input\"\n    82→          placeholder=\"Filter...\"\n    83→          value={localText}\n    84→          onChange={(e) => setLocalText(e.target.value)}\n    85→          data-testid=\"tasks-filter-input\"\n    86→        />\n    87→        {localText && (\n    88→          <button\n    89→            className=\"tasks-filter-clear\"\n    90→            onClick={() => setLocalText('')}\n    91→            data-testid=\"tasks-filter-clear\"\n    92→          >\n    93→            <X size={14} />\n    94→          </button>\n    95→        )}\n    96→      </div>\n    97→    </div>\n    98→  )\n    99→}\n   100→"}]},"parent_tool_use_id":"toolu_01MSgTBB3ieRfJPyWmGUc79G","session_id":"5e5af855-f8b5-4faa-8470-bf0544f43eff","uuid":"8856b1d8-5ba5-46f0-9f26-c73e9f274ebe"}
