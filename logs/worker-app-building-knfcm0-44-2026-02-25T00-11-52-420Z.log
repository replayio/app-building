terials.fulfilled, (state, action) => {\n    96→        state.items = action.payload;\n    97→        state.loading = false;\n    98→      })\n    99→      .addCase(fetchMaterials.rejected, (state, action) => {\n   100→        state.loading = false;\n   101→        state.error = action.error.message ?? \"Failed to fetch materials\";\n   102→      })\n   103→      .addCase(fetchMaterialById.pending, (state) => {\n   104→        if (!state.currentMaterial) state.loading = true;\n   105→        state.error = null;\n   106→      })\n   107→      .addCase(fetchMaterialById.fulfilled, (state, action) => {\n   108→        state.currentMaterial = action.payload;\n   109→        state.loading = false;\n   110→      })\n   111→      .addCase(fetchMaterialById.rejected, (state, action) => {\n   112→        state.loading = false;\n   113→        state.error = action.error.message ?? \"Failed to fetch material\";\n   114→      })\n   115→      .addCase(createMaterial.fulfilled, (state, action) => {\n   116→        state.items.push(action.payload);\n   117→      })\n   118→      .addCase(updateMaterial.fulfilled, (state, action) => {\n   119→        const idx = state.items.findIndex((m) => m.id === action.payload.id);\n   120→        if (idx !== -1) state.items[idx] = action.payload;\n   121→        if (state.currentMaterial?.id === action.payload.id) {\n   122→          state.currentMaterial = action.payload;\n   123→        }\n   124→      })\n   125→      .addCase(fetchAccountMaterials.fulfilled, (state, action) => {\n   126→        state.accountMaterials = action.payload;\n   127→      });\n   128→  },\n   129→});\n   130→\n   131→export const { clearCurrentMaterial, clearAccountMaterials } = materialsSlice.actions;\n   132→export const materialsReducer = materialsSlice.reducer;\n   133→"}]},"parent_tool_use_id":"toolu_01Rv3jBJjnBUb6AEfDoS8fpb","session_id":"af064ad8-d415-449b-809b-97262f6720bc","uuid":"6d90d5a0-b84e-4803-8be3-8235b1899e04"}
