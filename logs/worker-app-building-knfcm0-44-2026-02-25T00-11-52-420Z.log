→  | \"transfer\"\n    62→  | \"production\"\n    63→  | \"adjustment\";\n    64→\n    65→/** A double-entry transaction */\n    66→export interface Transaction {\n    67→  id: string;\n    68→  date: string;\n    69→  reference_id: string;\n    70→  description: string;\n    71→  transaction_type: TransactionType;\n    72→  status: \"posted\" | \"draft\" | \"void\";\n    73→  created_by: string;\n    74→  created_at: string;\n    75→  updated_at: string;\n    76→  transfers?: QuantityTransfer[];\n    77→  batches_created?: BatchCreated[];\n    78→}\n    79→\n    80→/** A quantity transfer line within a transaction (double-entry) */\n    81→export interface QuantityTransfer {\n    82→  id: string;\n    83→  transaction_id: string;\n    84→  source_account_id: string;\n    85→  source_account_name?: string;\n    86→  destination_account_id: string;\n    87→  destination_account_name?: string;\n    88→  material_id: string;\n    89→  material_name?: string;\n    90→  amount: number;\n    91→  unit: string;\n    92→  source_batch_id: string | null;\n    93→  source_batch_label?: string;\n    94→}\n    95→\n    96→/** A batch created by a transaction */\n    97→export interface BatchCreated {\n    98→  id: string;\n    99→  transaction_id: string;\n   100→  batch_id: string;\n   101→  material_id: string;\n   102→  material_name?: string;\n   103→  quantity: number;\n   104→  unit: string;\n   105→}\n   106→\n   107→/** Batch lineage: source batches used to create a batch */\n   108→export interface BatchLineage {\n   109→  id: string;\n   110→  batch_id: string;\n   111→  source_batch_id: string;\n   112→  source_material_name?: string;\n   113→  source_account_name?: string;\n   114→  quantity_used: number;\n   115→  unit: string;\n   116→}\n   117→\n   118→/** Low inventory alert */\n   119→export interface LowInventoryAlert {\n   120→  material_id: string;\n   121→  material_name: string;\n   122→  current_quantity: number;\n   123→  reorder_point: number;\n   124→  unit: string;\n   125→  severity: \"warning\" | \"critical\";\n   126→  dismissed: boolean;\n   127→}\n   128→\n   129→/** Dashboard category overview */\n   130→export interface CategoryOverview {\n   131→  category_id: string;\n   132→  category_name: string;\n   133→  total_items: number;\n   134→  total_quantity: number;\n   135→  materials: {\n   136→    id: string;\n   137→    name: string;\n   138→    quantity: number;\n   139→    unit: string;\n   140→  }[];\n   141→}\n   142→\n   143→/** Material stock summary across accounts */\n   144→export interface MaterialStock {\n   145→  material_id: string;\n   146→  total_quantity: number;\n   147→}\n   148→\n   149→/** Account material summary */\n   150→export interface AccountMaterial {\n   151→  material_id: string;\n   152→  material_name: string;\n   153→  category_name: string;\n   154→  unit_of_measure: string;\n   155→  total_quantity: number;\n   156→  batch_count: number;\n   157→}\n   158→"}]},"parent_tool_use_id":"toolu_01Rv3jBJjnBUb6AEfDoS8fpb","session_id":"af064ad8-d415-449b-809b-97262f6720bc","uuid":"bf1386a4-645b-43a2-a247-1fee491c62a9"}
