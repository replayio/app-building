 101→      state.items = [];\n   102→    },\n   103→  },\n   104→  extraReducers: (builder) => {\n   105→    builder\n   106→      .addCase(fetchTransactions.pending, (state) => {\n   107→        if (state.items.length === 0) state.loading = true;\n   108→        state.error = null;\n   109→      })\n   110→      .addCase(fetchTransactions.fulfilled, (state, action) => {\n   111→        state.items = action.payload;\n   112→        state.loading = false;\n   113→      })\n   114→      .addCase(fetchTransactions.rejected, (state, action) => {\n   115→        state.loading = false;\n   116→        state.error = action.error.message ?? \"Failed to fetch transactions\";\n   117→      })\n   118→      .addCase(fetchTransactionById.pending, (state) => {\n   119→        if (!state.currentTransaction) state.loading = true;\n   120→        state.error = null;\n   121→      })\n   122→      .addCase(fetchTransactionById.fulfilled, (state, action) => {\n   123→        state.currentTransaction = action.payload;\n   124→        state.loading = false;\n   125→      })\n   126→      .addCase(fetchTransactionById.rejected, (state, action) => {\n   127→        state.loading = false;\n   128→        state.error = action.error.message ?? \"Failed to fetch transaction\";\n   129→      })\n   130→      .addCase(createTransaction.fulfilled, (state, action) => {\n   131→        state.items.unshift(action.payload);\n   132→      })\n   133→      .addCase(deleteTransaction.fulfilled, (state, action) => {\n   134→        state.items = state.items.filter((t) => t.id !== action.payload);\n   135→      });\n   136→  },\n   137→});\n   138→\n   139→export const { clearCurrentTransaction, clearTransactions } = transactionsSlice.actions;\n   140→export const transactionsReducer = transactionsSlice.reducer;\n   141→"}]},"parent_tool_use_id":"toolu_01GQcso5ScarZqc9rumgghG5","session_id":"af064ad8-d415-449b-809b-97262f6720bc","uuid":"83084824-8882-48ca-af25-34f08d24cbf9"}
