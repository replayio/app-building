5→\n    46→  async function handleSave() {\n    47→    await onUpdate({\n    48→      name: name.trim(),\n    49→      value: parseFloat(value) || 0,\n    50→      owner: owner.trim() || null,\n    51→    })\n    52→    setEditing(false)\n    53→  }\n    54→\n    55→  function handleCancel() {\n    56→    setName(deal.name)\n    57→    setValue(String(deal.value))\n    58→    setOwner(deal.owner ?? '')\n    59→    setEditing(false)\n    60→  }\n    61→\n    62→  function handleChangeStage() {\n    63→    if (selectedStage !== deal.stage) {\n    64→      onStageChange(selectedStage)\n    65→    }\n    66→  }\n    67→\n    68→  return (\n    69→    <div data-testid=\"deal-detail-header\" className=\"border border-border rounded-[6px] p-5 mb-4\">\n    70→      <div className=\"flex items-start justify-between mb-4\">\n    71→        <div className=\"flex-1\">\n    72→          <div className=\"text-[12px] font-medium text-text-muted uppercase tracking-wider mb-1\">\n    73→            Deal Details\n    74→          </div>\n    75→          {editing ? (\n    76→            <input\n    77→              data-testid=\"deal-header-name-input\"\n    78→              type=\"text\"\n    79→              value={name}\n    80→              onChange={(e) => setName(e.target.value)}\n    81→              className=\"text-[18px] font-semibold text-text-primary bg-base border border-border rounded-[5px] px-2 py-1 w-full max-w-[500px] focus:outline-none focus:border-accent\"\n    82→            />\n    83→          ) : (\n    84→            <h1 data-testid=\"deal-header-title\" className=\"text-[18px] font-semibold text-text-primary\">\n    85→              {deal.client_name} - {deal.name}\n    86→            </h1>\n    87→          )}\n    88→        </div>\n    89→        {!editing ? (\n    90→          <button\n    91→            data-testid=\"deal-header-edit-button\"\n    92→            onClick={() => setEditing(true)}\n    93→            className=\"inline-flex items-center justify-center w-7 h-7 rounded-[4px] text-text-muted hover:bg-hover transition-colors duration-100\"\n    94→            title=\"Edit deal\"\n    95→          >\n    96→            <Pencil size={14} strokeWidth={1.75} />\n    97→          </button>\n    98→        ) : (\n    99→          <div className=\"flex items-center gap-1\">\n   100→            <button\n   101→              data-testid=\"deal-header-save-button\"\n   102→              onClick={handleSave}\n   103→              className=\"inline-flex items-center justify-center w-7 h-7 rounded-[4px] text-status-active hover:bg-hover transition-colors duration-100\"\n   104→              title=\"Save\"\n   105→            >\n   106→              <Check size={14} strokeWidth={1.75} />\n   107→            </button>\n   108→            <button\n   109→              data-testid=\"deal-header-cancel-button\"\n   110→              onClick={handleCancel}\n   111→              className=\"inline-flex items-center justify-center w-7 h-7 rounded-[4px] text-status-churned hover:bg-hover transition-colors duration-100\"\n   112→              title=\"Cancel\"\n   113→            >\n   114→              <X size={14} strokeWidth={1.75} />\n   115→            </button>\n   116→          </div>\n   117→        )}\n   118→      </div>\n   119→\n   120→      <div className=\"grid grid-cols-4 max-md:grid-cols-2 max-sm:grid-cols-1 gap-4\">\n   121→        <div>\n   122→          <div className=\"text-[12px] font-medium text-text-muted mb-1\">Client</div>\n   123→          <div data-testid=\"deal-header-client\" className=\"text-[13px] text-text-primary\">{deal.client_name}</div>\n   124→        </div>\n   125→        <div>\n   126→          <div className=\"text-[12px] font-medium text-text-muted mb-1\">Value</div>\n   127→          {editing ? (\n   128→            <input\n   129→              data-testid=\"deal-header-value-input\"\n   130→              type=\"number\"\n   131→              value={value}\n   132→              onChange={(e) => setValue(e.target.value)}\n   133→              className=\"w-full h-[30px] px-2 text-[13px] text-text-primary bg-base border border-border rounded-[5px] focus:outline-none focus:border-accent\"\n   134→            />\n   135→          ) : (\n   136→            <div data-testid=\"deal-header-value\" className=\"text-[13px] text-text-primary font-medium\">{formatValue(Number(deal.value))}</div>\n   137→          )}\n   138→        </div>\n   139→        <div>\n   140→          <div className=\"text-[12px] font-medium text-text-muted mb-1\">Owner</div>\n   141→          {editing ? (\n   142→            availableUsers.length > 0 ? (\n   143→              <FilterSelect\n   144→                testId=\"deal-header-owner-input\"\n   145→                value={owner}\n   146→                onChange={(val) => setOwner(val)}\n   147→                searchable\n   148→                options={[\n   149→                  { value: '', label: '— None —' },\n   150→                  ...availableUsers.map((u) => ({ value: u.name, label: u.name })),\n   151→                ]}\n   152→              />\n   153→            ) : (\n   154→              <input\n   155→                data-testid=\"deal-header-owner-input\"\n   156→                type=\"text\"\n   157→                value={owner}\n   158→                onChange={(e) => setOwner(e.target.value)}\n   159→                className=\"w-full h-[30px] px-2 text-[13px] text-text-primary bg-base border border-border rounded-[5px] focus:outline-none focus:border-accent\"\n   160→              />\n   161→            )\n   162→          ) : (\n   163→            <div data-testid=\"deal-header-owner\" className=\"text-[13px] text-text-primary\">{deal.owner ?? '—'}</div>\n   164→          )}\n   165→        </div>\n   166→        <div>\n   167→          <div className=\"text-[12px] font-medium text-text-muted mb-1\">Stage</div>\n   168→          <div className=\"flex items-center gap-2 flex-wrap\">\n   169→            <FilterSelect\n   170→              testId=\"deal-header-stage-select\"\n   171→              value={selectedStage}\n   172→              onChange={(val) => setSelectedStage(val as DealStage)}\n   173→              options={stageOptions.map((s) => ({ value: s, label: stageLabels[s] }))}\n   174→            />\n   175→            <button\n   176→              data-testid=\"deal-header-change-stage-button\"\n   177→              onClick={handleChangeStage}\n   178→              disabled={selectedStage === deal.stage}\n   179→              className=\"h-[30px] px-3 text-[12px] font-medium text-white bg-accent rounded-[5px] hover:opacity-90 disabled:opacity-40 disabled:cursor-not-allowed transition-opacity duration-100\"\n   180→            >\n   181→              Change Stage\n   182→            </button>\n   183→          </div>\n   184→        </div>\n   185→      </div>\n   186→    </div>\n   187→  )\n   188→}\n   189→"}]},"parent_tool_use_id":"toolu_01PiaV3LBujPAfYathXbdAcx","session_id":"7f686a90-13ca-4f0a-892f-aab054c3ddbb","uuid":"fde7a79d-7580-4bf8-8fbf-2aed1d893d7c"}
