ts.errors.push(`Row ${i + 1}: Type must be \"organization\" or \"individual\"`)\n   133→        continue\n   134→      }\n   135→      const status = row.status?.trim().toLowerCase() || 'prospect'\n   136→      if (!['active', 'inactive', 'prospect', 'churned'].includes(status)) {\n   137→        results.errors.push(`Row ${i + 1}: Status must be one of: active, inactive, prospect, churned`)\n   138→        continue\n   139→      }\n   140→      const tags = row.tags ? row.tags.split(';').map(t => t.trim()).filter(Boolean) : []\n   141→      try {\n   142→        await sql`\n   143→          INSERT INTO clients (name, type, status, tags, source_type, source_detail, campaign, channel, date_acquired)\n   144→          VALUES (${row.name.trim()}, ${type}, ${status}, ${tags}, ${row.source_type?.trim() || null}, ${row.source_detail?.trim() || null}, ${row.campaign?.trim() || null}, ${row.channel?.trim() || null}, ${row.date_acquired?.trim() || null})\n   145→        `\n   146→        results.imported++\n   147→      } catch (err) {\n   148→        results.errors.push(`Row ${i + 1}: ${err instanceof Error ? err.message : 'Database error'}`)\n   149→      }\n   150→    }\n   151→\n   152→    return Response.json(results, { status: 200 })\n   153→  }\n   154→\n   155→  // POST /clients\n   156→  if (req.method === 'POST') {\n   157→    const body = await req.json() as {\n   158→      name: string\n   159→      type: string\n   160→      status?: string\n   161→      tags?: string[]\n   162→      source_type?: string\n   163→      source_detail?: string\n   164→      campaign?: string\n   165→      channel?: string\n   166→      date_acquired?: string\n   167→    }\n   168→\n   169→    const rows = await sql`\n   170→      INSERT INTO clients (name, type, status, tags, source_type, source_detail, campaign, channel, date_acquired)\n   171→      VALUES (${body.name}, ${body.type}, ${body.status ?? 'prospect'}, ${body.tags ?? []}, ${body.source_type ?? null}, ${body.source_detail ?? null}, ${body.campaign ?? null}, ${body.channel ?? null}, ${body.date_acquired || null})\n   172→      RETURNING *\n   173→    `\n   174→\n   175→    return Response.json(rows[0], { status: 201 })\n   176→  }\n   177→\n   178→  // DELETE /clients/:id\n   179→  if (req.method === 'DELETE' && resourceId) {\n   180→    await sql`DELETE FROM clients WHERE id = ${resourceId}`\n   181→    return Response.json({ success: true })\n   182→  }\n   183→\n   184→  // PUT /clients/:id\n   185→  if (req.method === 'PUT' && resourceId) {\n   186→    const body = await req.json() as Record<string, unknown>\n   187→\n   188→    // Get existing client for change detection\n   189→    const existing = await sql`SELECT name, type, status, tags FROM clients WHERE id = ${resourceId}::uuid`\n   190→    if (existing.length === 0) {\n   191→      return new Response(JSON.stringify({ error: 'Not found' }), { status: 404 })\n   192→    }\n   193→    const old = existing[0]\n   194→\n   195→    const dateAcquiredValue = body.date_acquired === '' ? null : (body.date_acquired as string | undefined) ?? null\n   196→\n   197→    const rows = await sql`\n   198→      UPDATE clients SET\n   199→        name = COALESCE(${(body.name as string) ?? null}, name),\n   200→        type = COALESCE(${(body.type as string) ?? null}, type),\n   201→        status = COALESCE(${(body.status as string) ?? null}, status),\n   202→        tags = COALESCE(${(body.tags as string[]) ?? null}, tags),\n   203→        source_type = COALESCE(${(body.source_type as string) ?? null}, source_type),\n   204→        source_detail = COALESCE(${(body.source_detail as string) ?? null}, source_detail),\n   205→        campaign = COALESCE(${(body.campaign as string) ?? null}, campaign),\n   206→        channel = COALESCE(${(body.channel as string) ?? null}, channel),\n   207→        date_acquired = COALESCE(${dateAcquiredValue}, date_acquired),\n   208→        updated_at = NOW()\n   209→      WHERE id = ${resourceId}::uuid\n   210→      RETURNING *\n   211→    `\n   212→\n   213→    if (rows.length === 0) {\n   214→      return new Response(JSON.stringify({ error: 'Not found' }), { status: 404 })\n   215→    }\n   216→\n   217→    const userName = req.user?.name ?? 'System'\n   218→\n   219→    // Create timeline events for field changes\n   220→    if (body.name && body.name !== old.name) {\n   221→      await sql`\n   222→        INSERT INTO timeline_events (client_id, event_type, description, user_name)\n   223→        VALUES (${resourceId}::uuid, 'name_changed', ${'Name Changed: from \\'' + (old.name as string) + '\\' to \\'' + (body.name as string) + '\\''}, ${userName})\n   224→      `\n   225→    }\n   226→\n   227→    if (body.status && body.status !== old.status) {\n   228→      await sql`\n   229→        INSERT INTO timeline_events (client_id, event_type, description, user_name)\n   230→        VALUES (${resourceId}::uuid, 'status_changed', ${'Status Changed: from \\'' + (old.status as string) + '\\' to \\'' + (body.status as string) + '\\''}, ${userName})\n   231→      `\n   232→    }\n   233→\n   234→    if (body.tags) {\n   235→      const oldTags = (old.tags as string[]).sort().join(',')\n   236→      const newTags = (body.tags as string[]).sort().join(',')\n   237→      if (oldTags !== newTags) {\n   238→        await sql`\n   239→          INSERT INTO timeline_events (client_id, event_type, description, user_name)\n   240→          VALUES (${resourceId}::uuid, 'tags_changed', ${'Tags Changed: from [' + (old.tags as string[]).join(', ') + '] to [' + (body.tags as string[]).join(', ') + ']'}, ${userName})\n   241→        `\n   242→      }\n   243→    }\n   244→\n   245→    if (body.type && body.type !== old.type) {\n   246→      await sql`\n   247→        INSERT INTO timeline_events (client_id, event_type, description, user_name)\n   248→        VALUES (${resourceId}::uuid, 'type_changed', ${'Type Changed: from \\'' + (old.type as string) + '\\' to \\'' + (body.type as string) + '\\''}, ${userName})\n   249→      `\n   250→    }\n   251→\n   252→    return Response.json(rows[0])\n   253→  }\n   254→\n   255→  return new Response(JSON.stringify({ error: 'Method not allowed' }), { status: 405 })\n   256→}\n   257→\n   258→export default optionalAuth(handler)\n   259→"}]},"parent_tool_use_id":"toolu_01Xrku3kzdi612UWV1PMFLkB","session_id":"7f686a90-13ca-4f0a-892f-aab054c3ddbb","uuid":"d3fe2e47-e75c-4455-9a18-ec2919c43f23"}
