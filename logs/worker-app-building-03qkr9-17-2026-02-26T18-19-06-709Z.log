→  await sql`INSERT INTO transaction_entries (transaction_id, account_id, entry_type, amount)\n   114→    VALUES (${txn3[0].id}, ${checkingId}, 'debit', 85.00)`;\n   115→  await sql`INSERT INTO transaction_entries (transaction_id, account_id, entry_type, amount)\n   116→    VALUES (${txn3[0].id}, ${opexId}, 'credit', 85.00)`;\n   117→\n   118→  // Seed tags\n   119→  await sql`DELETE FROM tags`;\n   120→  const groceriesTag = await sql`INSERT INTO tags (name) VALUES ('Groceries') RETURNING id`;\n   121→  const salaryTag = await sql`INSERT INTO tags (name) VALUES ('Salary') RETURNING id`;\n   122→  const utilitiesTag = await sql`INSERT INTO tags (name) VALUES ('Utilities') RETURNING id`;\n   123→\n   124→  // Link tags to transactions\n   125→  await sql`INSERT INTO transaction_tags (transaction_id, tag_id)\n   126→    VALUES (${txn1[0].id}, ${groceriesTag[0].id})`;\n   127→  await sql`INSERT INTO transaction_tags (transaction_id, tag_id)\n   128→    VALUES (${txn2[0].id}, ${salaryTag[0].id})`;\n   129→  await sql`INSERT INTO transaction_tags (transaction_id, tag_id)\n   130→    VALUES (${txn3[0].id}, ${utilitiesTag[0].id})`;\n   131→\n   132→  // Seed budget items for Checking Account\n   133→  await sql`INSERT INTO budgets (account_id, name, amount, actual_amount, period)\n   134→    VALUES (${checkingId}, 'Rent', 1500, 1500, 'monthly')`;\n   135→  await sql`INSERT INTO budgets (account_id, name, amount, actual_amount, period)\n   136→    VALUES (${checkingId}, 'Utilities', 250, 200, 'monthly')`;\n   137→  await sql`INSERT INTO budgets (account_id, name, amount, actual_amount, period)\n   138→    VALUES (${checkingId}, 'Progress', 100, 700, 'monthly')`;\n   139→\n   140→  console.log(\"Seed data inserted successfully.\");\n   141→}\n   142→\n   143→seed().catch((err) => {\n   144→  console.error(\"Seed failed:\", err);\n   145→  process.exit(1);\n   146→});\n   147→"}]},"parent_tool_use_id":"toolu_017qQ1njaTEh28HVujpMq1cp","session_id":"9b072fb8-44f1-480a-90bb-ff63